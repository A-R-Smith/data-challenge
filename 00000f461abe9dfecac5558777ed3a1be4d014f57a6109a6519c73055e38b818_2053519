<script language="Javascript"> 
function AddRule()
{
	index=document.getElementById('D1').value;

	switch(index)
	{
		case '01':
		case '02':
						var a_url="52_Add_Access_Rule_mac.html?"+document.getElementById('D1').value;
						Link(a_url);
						break;
		case '03':
		case '04':
						var a_url="52_Add_Access_Rule_ip.html?"+document.getElementById('D1').value;
				  	Link(a_url);
						break;
	}
}
</script>
