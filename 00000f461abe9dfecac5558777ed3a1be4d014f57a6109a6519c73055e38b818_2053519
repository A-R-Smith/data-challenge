<script language="Javascript">
var is_apply = false;

var selectID = new Array();
var selectName=new Array();
var selectStatus=new Array();
var selectLength=new Array();
var totalLength;

var Com_List=new Array();

function packetCom(id,name,size,select,height)
{
	this.name=name;
	this.id=id;
	this.select=select;
	this.size=size;
	this.height=height;
}

function SelectButton(item)
{
	var index=GetSelectIndex(item);

	//alert(index);
	//alert(Com_List[index].select);
	if(Com_List[index].select == 0) 
	{
		var ftype = document.getElementsByName('FrameType');
		
		if(ftype[0].checked)	// L2 ACL
		{ 
			if(document.getElementById('sel_L2ACL').options[0].selected)	// Untagged
			{ 
				if(index < 2)
				{
	 	    			item.style.background= "#ff4040";
	       			Com_List[index].select=1;
	       			ifrACLProSet.ShowPacketComponet(item.id);
				}
			} 
			else	// Tagged
			{ 
				if(index < 4) 
				{
	 	    			item.style.background= "#ff4040";
	 	    			item.style.color="#FFFFFF"
	       			Com_List[index].select=1;
	       			ifrACLProSet.ShowPacketComponet(item.id);
				}
			}
		} 
		else if(ftype[1].checked)	// L3 ACL 
		{
			if(index > 0) 
			{
				item.style.background= "#ff4040";
    				item.style.color="#FFFFFF"
		     		Com_List[index].select=1;
     				ifrACLProSet.ShowPacketComponet(item.id);
			}
		}
		else if(ftype[2].checked)	// IPv6
		{
			if (index < 2)
			{
				/* del for PI-378, 20090408
				if (Com_List[2].select != 1)
				*/
				{
					item.style.background= "#ff4040";
					item.style.color="#FFFFFF"
					Com_List[index].select=1;
					ifrACLProSet.ShowPacketComponet(item.id);
				}
			}
			else
			{
			/*
				if (acl_style != 1)
				{
					if (Com_List[0].select != 1 && Com_List[1].select != 1)
					{
						item.style.background= "#ff4040";
						item.style.color="#FFFFFF"
						Com_List[index].select=1;
						ifrACLProSet.ShowPacketComponet(item.id);
					}
				}
				else
				{
					item.style.background= "#ff4040";
					item.style.color="#FFFFFF"
					Com_List[index].select=1;
					ifrACLProSet.ShowPacketComponet(item.id);
				}
			*/

				item.style.background= "#ff4040";
				item.style.color="#FFFFFF"
				Com_List[index].select=1;
				ifrACLProSet.ShowPacketComponet(item.id);
			}
		}
		else
		{
			item.style.background= "#ff4040";
			item.style.color="#000000"
			Com_List[index].select = 1;
			ifrACLProSet.ShowPacketComponet(item.id);
		}
		
	}
	else
	{
		item.style.background= "#f1f4f5";
		item.style.color="#000000"
		Com_List[index].select=0;	  
		ifrACLProSet.HidePacketComponet(item.id);
	}
	RelocatePoint();
	ifrACLProSet.RelocateCom();
	
} 
function GetSelectIndex(item)
{
	for(var i=0;i<Com_List.length;i++)
	{
	  if(item.id==Com_List[i].id)
	  {
	  	return i;
	  }	
	}
}
function RelocatePoint()
{
	var pos=-1;
	for(var i=Com_List.length-1;i>-1;i--)
	{
		if(Com_List[i].select==1)
		{
			pos=i;
		}
	}
	
	for(var i=0;i<Com_List.length;i++)
	{
		var obj='pointID'+i;
		//MM_findObj(obj).style.visibility='hidden';
		ifrACLScope.SetVisibility(obj,'hidden');
	}
	/*
	if(pos>-1)
	{	
	    var objname='pointID'+pos;	    
	    var obj = MM_findObj(objname);	    
		//obj.style.visibility='visible'; 
		ifrACLScope.SetVisibility(objname,'visible');      
	}
	*/
}
function InitialArray()
{
	Com_List.length=0;
	totalLength=0;
	
	var i=0;
	var j=0;
	

	var ftype = document.getElementsByName('FrameType');
	if(ftype[0].checked)
	{
		if(MM_findObj('sel_L2ACL').selectedIndex==0) 
		{
			Com_List[i++]=new packetCom('Scope1','MAC Address',8,0,160);	
			totalLength+=Com_List[i-1].size;
		} 
		else if(MM_findObj('sel_L2ACL').selectedIndex==1)
		{
			Com_List[i++]=new packetCom('Scope1','MAC Address',6,0,160);	
			totalLength+=Com_List[i-1].size;
			Com_List[i++]=new packetCom('Scope2','VLAN',2,0,80);	
			totalLength+=Com_List[i-1].size;
			Com_List[i++]=new packetCom('Scope3','802.1p',2,0,80);	

		}
		totalLength+=Com_List[i-1].size;
		Com_List[i++]=new packetCom('Scope4','Ethernet Type',12,0,80);
		totalLength+=Com_List[i-1].size;
	
		Com_List[i++]=new packetCom('Scope17','PayLoad',12,0,80);
		totalLength+=Com_List[i-1].size;
	} 
	else if(ftype[1].checked)
	{
		
		Com_List[i++]=new packetCom('Scope18','L2 Header',6,0,180);	
		totalLength+=Com_List[i-1].size;

		//if (acl_style == 2)
		//{
			Com_List[i++]=new packetCom('Scope2','VLAN',2,0,80);
			totalLength+=Com_List[i-1].size;
		//}	
		
		Com_List[i++]=new packetCom('Scope5','IPv4 DSCP',2,0,80);
		totalLength+=Com_List[i-1].size;

		Com_List[i++]=new packetCom('Scope6','IPv4 Address',4,0,160);
		totalLength+=Com_List[i-1].size;
		
		switch (MM_findObj('sel_L3ACL').selectedIndex)
		{
			case 0:
				Com_List[i++]=new packetCom('Scope10','ICMP',2,0,80);
				totalLength+=Com_List[i-1].size;
				break;
			case 1:
				Com_List[i++]=new packetCom('Scope12','IGMP',4,0,80);
				totalLength+=Com_List[i-1].size;
				break;
			case 2:
				Com_List[i++]=new packetCom('Scope13','TCP',4,0,160);
				totalLength+=Com_List[i-1].size;
				//Com_List[i++]=new packetCom('Scope14','TCP Flag',2,0,80);
				//totalLength+=Com_List[i-1].size;
				break;
			case 3:
				Com_List[i++]=new packetCom('Scope15','UDP',4,0,160);
				totalLength+=Com_List[i-1].size;
				break;
			case 4:
				Com_List[i++]=new packetCom('Scope16','Protocol ID',8,0,180);
				totalLength+=Com_List[i-1].size;
				break;
			default:
				Com_List[i++]=new packetCom('Scope16','Protocol ID',8,0,180);
				totalLength+=Com_List[i-1].size;
				break;
		}
	}	
	else if(ftype[2].checked) //IPv6
	{
		Com_List[i++]=new packetCom('Scope7','IPv6 Class',8,0,160);
		totalLength+=Com_List[i-1].size;

		Com_List[i++]=new packetCom('Scope8','IPv6 Flow Label',10,0,80);
		totalLength+=Com_List[i-1].size;
	
		Com_List[i++]=new packetCom('Scope9','IPv6 Address',12,0,80);
		totalLength+=Com_List[i-1].size;
	}
	else  if(ftype[3].checked) //Packet Content
	{
		Com_List[i++]=new packetCom('Scope11','Packet Content',8,0,160);
		totalLength+=Com_List[i-1].size;
	}
}
function getCom(index)
{
	return Com_List(index);
}

function getSelected(index)
{
  return selectStatus[index];
}

function chgL3(item)
{
	if(item.selectedIndex==0)
	{
		MM_findObj('sel_L3ACL').disabled=false;
	}
	else
	{
		MM_findObj('sel_L3ACL').disabled=true;
	}
	
}

function CreateScope()
{	
	InitialArray();
	var obj = MM_findObj('aclScopefr');
	
	obj.style.visibility='visible';
	ifrACLScope.page_refresh();
	ifrACLProSet.ResetAll();
}

function SelFrameType(ftype)
{
	document.form1.profile_type.value = ftype;

	if(ftype == 0)
	{
		document.getElementById('sel_L2ACL').style.display = "block";
		document.getElementById('sel_L3ACL').style.display = "none";
	}
	else if (ftype == 1)
	{
		document.getElementById('sel_L2ACL').style.display = "none";
		document.getElementById('sel_L3ACL').style.display = "block";
		document.getElementById('sel_L2ACL').value = "1";
	}
	else if (ftype == 2)
	{
		document.getElementById('sel_L2ACL').style.display = "none";
		document.getElementById('sel_L3ACL').style.display = "none";
		//document.getElementById('sel_IPv6ACL').style.display = "block";
		//document.getElementById('sel_IPv6ACL').value = "1";
	}
	else if (ftype == 3)
	{
		document.getElementById('sel_L2ACL').style.display = "none";
		document.getElementById('sel_L3ACL').style.display = "none";
		//document.getElementById('sel_IPv6ACL').style.display = "none";
		//document.getElementById('sel_IPv6ACL').value = "1";
	}
	
}
