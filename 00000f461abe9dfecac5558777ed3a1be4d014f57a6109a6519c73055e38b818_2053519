<script type="text/javascript">
	function mechanism2str(i)
	{
		if (i == 0)
			return "Strict";
		else if (i == 1)
			return "Round Robin";
		else
			return "Weight Fair";
	}

function warning()
{
	var i;

	for(i=0; i<QoS_Class_Setting.length; i++)
	{
		if(!checkNumberInput(document.getElementById("max_packet"+i), 0, 255))
		{
			return false;
		}
	}
	document.qos_s.submit();
}
</script>
</head>
