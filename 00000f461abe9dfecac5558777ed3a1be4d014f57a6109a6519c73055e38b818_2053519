	rbottom.location.href = "CfgSwitchMgtPort.html";
}


function OIDCheck(textValue)
{

    ipSplit=textValue.split('.');

        for(i=0; i<ipSplit.length; i++)
        {
          if (isNaN(ipSplit[i]))  return false;
        }

    return true;
 }
 

function OIDValueCheck(textValue)
  {
      ipSplit=textValue.split('.');
        for(i=0; i<ipSplit.length; i++)
       {
         if (ipSplit[i]>4294967295)  return false;
         if (ipSplit[i]< 0 )  return false;
       }
       return true;
 }


function ACLIpMaskCheck(textValue)
{
    if (textValue.indexOf(',') != -1)     return false;
    re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
    Check=textValue.search(re1);
    if(Check==-1)   return false;

    else
    {
       var ipSplit=textValue.split('.');         if(ipSplit.length!=4) return false;
       for(i=0; i<ipSplit.length; i++)
       {
         if (isNaN(ipSplit[i]))  return false;
         if(ipSplit[i]>255)  return false;
         if(ipSplit[i]<0)  return false;

       }

       return true;

    }
 }


function MonitorIPCheck(textValue)
  {

    re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
    Check=textValue.search(re1);
    if(Check==-1)   return false;

    else
    {

       var ipSplit=textValue.split('.');         if(ipSplit.length!=4)
       		return false;

       for(i=0; i<ipSplit.length; i++)
       {
         if (isNaN(ipSplit[i]))  return false;
         if(ipSplit[i]>255 || ipSplit[i]<0)  return false;
       }

       return true;

    }
}

