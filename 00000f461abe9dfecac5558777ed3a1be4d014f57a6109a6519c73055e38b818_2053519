<head>
	<script src="../form.js" type="text/javascript"></script>
	<script src="../checkperm.js" type="text/javascript"></script>
	<script src="../../DataStore/Safeguard.js" type="text/javascript"></script>
	<script src="../../DataStore/Stacking.js" type="text/javascript"></script>
	<script src="../../DataStore/PortSetting.js" type="text/javascript"></script>
	<script src="../../DataStore/CTP.js" type="text/javascript"></script>
	<script language="Javascript">
	function Status_apply()
	{
		document.formCTP_Status.submit();
	}

	function global_apply()
	{
		var f = document.formCTP_Global;

		if (!checkNumberInput(f.interval, 1, 32767))
		{
			return;
		}
		if (f.recover.value == '')
		{
			alert("Please input an integer.");
			f.recover.focus();
			f.recover.select();
			return false;
		}
		else
		{
			if (f.recover.value != 0)
			{
				if (!check_recover(f.recover, 60, 1000000))
				{
					return;
				}
			}
		}
		f.submit();
	}

	function applyForm()
	{
		document.formCTP_Port.submit();
	}

	function check_recover(obj, min_val, max_val)
	{
		var i, ch, len = obj.value.length;
		var ret = true;
		var value = obj.value;

		for (i=0; i < len; i++)
		{
			ch = value.charAt(i);
			if (isNaN(ch))
			{
				alert("Please input an integer.");
				obj.focus();
				obj.select();
				return false;
			}
		}

		if(value == '' || value.search(" ") != -1 || isNaN(value) || value <  min_val || value > max_val )
		 	ret = false;

		if (ret == false)
		{
			eval("alert('The value range is 0 or " + min_val + " - " + max_val + "')");
			obj.focus();
			obj.select();
			return ret;
		}

		return ret;
	}
	</script>
</head>
