<script language="Javascript">
function change_action_Ether(s)
{
	if (s.action.selectedIndex == 1 || s.action.selectedIndex == 2)
	{
		if (acl_style == 1)
		{
			s.pri_flag.checked = false;
			s.pri_flag.disabled = true;
			s.priority.disabled = true;

			s.replace_pri_flag.checked = false;
			s.replace_pri_flag.disabled = true;
			
			s.replace_dscp_flag.checked = false;
			s.replace_dscp_flag.disabled = true;
			s.replace_dscp.disabled = true;
			
			s.no_limit_flag.checked = false;
			s.no_limit_flag.disabled = true;
			s.rx_rate.disabled = true;
		}
	}
	else
	{
		if (acl_style == 1)
		{
			s.pri_flag.disabled = false;
			//s.replace_pri_flag.disabled = false;
			s.replace_dscp_flag.disabled = false;
			s.no_limit_flag.checked = true;
			s.no_limit_flag.disabled = false;
		}
	}
	if (acl_style == 1)
	{
		if(s.action.value!=1)
		{
			s.counter.disabled = true;
		}
		else
		{
			s.counter.disabled = false;
		}
	}
}

change_action_Ether(document.aclruleAdd_Ether);
</script>
