<script language="Javascript">
   function fcheckbox()
   {   
         flag=  document.getElementsByName("radio1")[0].checked;
         if(!flag)
         {
           document.getElementsByName("Priority")[0].readOnly=true;
           document.getElementsByName("DSCPID")[0].readOnly=false;
         }
         else
         {
     	   document.getElementsByName("Priority")[0].readOnly=false;
     	   document.getElementsByName("DSCPID")[0].readOnly=true;
         }
         
   }
</script>
<script language="Javascript">
   function fApply()
   {    
        alert(" Apply ");
     	document.formSTPGlobal.Priority.value = "4";
     	document.formSTPGlobal.DSCPID.value = "4";
   }

</script>

