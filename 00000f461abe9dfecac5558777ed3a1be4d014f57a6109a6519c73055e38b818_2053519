<script language="Javascript">
function chagne_on2()
{
     if((document.getElementById('d2').options.selectedIndex==1) || (document.getElementById('d2').options.selectedIndex==2) || (document.getElementById('d2').options.selectedIndex==3 || document.getElementById('d2').options.selectedIndex==6))
     {
        document.getElementsByName("s2")[0].readOnly=true;
        document.getElementsByName("s2")[0].value="";
     }
     else
     {
      document.getElementsByName("s2")[0].readOnly=false;
      document.getElementsByName("s2")[0].value="";
    
     }
}
function chagne_on3()
{
     if((document.getElementById('d3').options.selectedIndex==1) || (document.getElementById('d3').options.selectedIndex==2))
     {
        document.getElementsByName("s3")[0].readOnly=true;
        document.getElementsByName("s3")[0].value="";
     }
     else
     {
      document.getElementsByName("s3")[0].readOnly=false;
      document.getElementsByName("s3")[0].value="";
     }
}
</script>
