<script language="Javascript">    
	var trClass = '';
	var txtClass = '';
	var tipStr;

	function getTipStr(s)
	{
		if (s == "local_enable")
			return "Keyword";
		else if (s == "none")
			return "Keyword";
		else if (s == "radius")
			return "Built-in&nbsp;Group";
		else if (s == "tacacs+")
			return "Built-in&nbsp;Group";		
		else if (s == "xtacacs")
			return "Built-in&nbsp;Group";
		else if (s == "tacacs")
			return "Built-in&nbsp;Group";
		else if (s == "local")
			return "Keyword";
		else if (s == "")
			return "Empty";
		else
			return "User-defined&nbsp;Group";
	}

	function getShowStr(s)
	{
		if (s == "")
			return "-----";
		else
			return s;
	}

	for (i = 0; i < Enable_Method_Lists.length; i++) {
		if (i%2 == 0){
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else{
			trClass = 'gw';
			txtClass = 'flatGW';
		}

		document.write("<tr class=" + trClass + ">");
		document.write("<td align=left>" + Enable_Method_Lists[i][0] + "</td>");
		tipStr = getTipStr(Enable_Method_Lists[i][1]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Enable_Method_Lists[i][1]) + "</td>");
		tipStr = getTipStr(Enable_Method_Lists[i][2]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Enable_Method_Lists[i][2]) + "</td>");
		tipStr = getTipStr(Enable_Method_Lists[i][3]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Enable_Method_Lists[i][3]) + "</td>");
		tipStr = getTipStr(Enable_Method_Lists[i][4]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Enable_Method_Lists[i][4]) + "</td>");
		document.write("<td><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'></td>");
		if (Enable_Method_Lists[i][0] == 'default')
			document.write("<td><input type=button value='Delete' class=btnInGrayS disabled></td>");
		else
			document.write("<td><input type=button value='Delete' class=btnInGrayS onClick=DeleteIt("+i+");></td>");
		document.write("</tr>");
	}
</script>
 </table>
</div>
</table>
</div>
<script language="Javascript">
	function searchArray(arr, toFind)
	{
		var counting=0;
		if(arr.constructor != Array)
			return false;

		for(var i = 0, len = arr.length; i < len; i++)
		{
			if(arr[i].constructor == Array)
			{
				if (searchArray(arr[i], toFind) )
				return true;
			}
			else if(arr[i] == toFind)
			{
				counting+=1;
			}
		}
		return counting;
	}

	document.formEnalbeGlobal.S2.disabled = true;
	document.formEnalbeGlobal.S3.disabled = true;
	document.formEnalbeGlobal.S4.disabled = true;

	function checkbtn()
	{
		if ("" == Enable_Method[document.formEnalbeGlobal.S1.value])
		{
			document.formEnalbeGlobal.S2.disabled = true;
			document.formEnalbeGlobal.S3.disabled = true;
			document.formEnalbeGlobal.S4.disabled = true;
			Enable_Method[document.formEnalbeGlobal.S2.value] = "";
			Enable_Method[document.formEnalbeGlobal.S3.value] = "";
			Enable_Method[document.formEnalbeGlobal.S4.value] = "";			
		}
		else
		{
			document.formEnalbeGlobal.S2.disabled = false;
		}
		if ("" == Enable_Method[document.formEnalbeGlobal.S2.value])
		{
			document.formEnalbeGlobal.S3.disabled = true;
			document.formEnalbeGlobal.S4.disabled = true;
			Enable_Method[document.formEnalbeGlobal.S3.value] = "";
			Enable_Method[document.formEnalbeGlobal.S4.value] = "";			
		}
		else
		{
			document.formEnalbeGlobal.S3.disabled = false;
		}
		if ("" == Enable_Method[document.formEnalbeGlobal.S3.value])
		{
			document.formEnalbeGlobal.S4.disabled = true;
			Enable_Method[document.formEnalbeGlobal.S4.value] = "";			
		}
		else
		{
			document.formEnalbeGlobal.S4.disabled = false;
		}
		
	}
	
	function EditStart(iTag) {
		var i;
		document.formEnalbeGlobal.T1.value = Enable_Method_Lists[iTag][0];
		if(Enable_Method_Lists[iTag][1]=="")
		{
			document.formEnalbeGlobal.S1.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Enable_Method[i] == Enable_Method_Lists[iTag][1])
					document.formEnalbeGlobal.S1.value=i;
			}
		}
		if(Enable_Method_Lists[iTag][2]=="")
		{
			document.formEnalbeGlobal.S2.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Enable_Method[i] == Enable_Method_Lists[iTag][2])
					document.formEnalbeGlobal.S2.value=i;
			}
		}
		if(Enable_Method_Lists[iTag][3]=="")
		{
			document.formEnalbeGlobal.S3.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Enable_Method[i] == Enable_Method_Lists[iTag][3])
					document.formEnalbeGlobal.S3.value=i;
			}
		}
		if(Enable_Method_Lists[iTag][4]=="")
		{
			document.formEnalbeGlobal.S4.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Enable_Method[i] == Enable_Method_Lists[iTag][4])
					document.formEnalbeGlobal.S4.value=i;
			}
		}
		checkbtn();
	}
	
	function ApplyIt(){
		var m1=document.formEnalbeGlobal.S11.value;
		var m2=document.formEnalbeGlobal.S21.value;
		var m3=document.formEnalbeGlobal.S31.value;
		var m4=document.formEnalbeGlobal.S41.value;

		if (Enable_Method.length != 0)
		{
			m1 = Enable_Method[document.formEnalbeGlobal.S1.value];
		}
		if (Enable_Method.length != 0)
		{
			m2 = Enable_Method[document.formEnalbeGlobal.S2.value];
		}
		if (Enable_Method.length != 0)
		{
			m3 = Enable_Method[document.formEnalbeGlobal.S3.value];
		}
		if (Enable_Method.length != 0)
		{
			m4 = Enable_Method[document.formEnalbeGlobal.S4.value];
		}

		if (!checkTxtInput(document.formEnalbeGlobal.T1))
			return;

		var data_array=new Array();
		data_array[0]=m1;
		data_array[1]=m2;
		data_array[2]=m3;
		data_array[3]=m4;
		if((m1 != '' && searchArray(data_array, m1))>1 || 
			(m2 != '' && searchArray(data_array, m2))>1 || 
			(m3 != '' && searchArray(data_array, m3))>1 || 
			(m4 != '' && searchArray(data_array, m4))>1)
		{
			alert("Some Methods are duplicated");
			return false;
		}

		if ("local_enable" == m1 || "local_enable" == m2 || "local_enable" == m3 || "local_enable" == m4)
			alert("AAA will ignore login methods which after \"local_enable\"");

		if ("none" == m1 || "none" == m2 || "none" == m3 || "none" == m4)
			alert("AAA will ignore login methods which after \"none\"");
		document.formEnalbeGlobal.S11.value = Enable_Method[document.formEnalbeGlobal.S1.value];
		document.formEnalbeGlobal.S21.value = Enable_Method[document.formEnalbeGlobal.S2.value];
		document.formEnalbeGlobal.S31.value = Enable_Method[document.formEnalbeGlobal.S3.value];
		document.formEnalbeGlobal.S41.value = Enable_Method[document.formEnalbeGlobal.S4.value];

		document.formEnalbeGlobal.isClear.value='0';
		document.formEnalbeGlobal.submit();
	}

	function DeleteIt(iTag){
		document.formEnalbeGlobal.T1.value = Enable_Method_Lists[iTag][0];
		document.formEnalbeGlobal.isClear.value='1';
		document.formEnalbeGlobal.submit();
		document.formEnalbeGlobal.S11.value = '-';
		document.formEnalbeGlobal.S21.value = '-';
		document.formEnalbeGlobal.S31.value = '-';
		document.formEnalbeGlobal.S41.value = '-';
	}
</script>
<script src="../checkperm.js" type="text/javascript"></script>
