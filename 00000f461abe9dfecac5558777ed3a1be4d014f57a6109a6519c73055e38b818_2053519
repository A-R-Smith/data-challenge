function ms_info(ms_id,port_no,mac_addr,dev_name,rh_url,rh_target)
{
	this.ms_id=ms_id;
	this.port_no=port_no;
	this.mac_addr=mac_addr;
	this.dev_name=dev_name;
	this.rh_url=rh_url;
	this.rh_target=rh_target;
}
