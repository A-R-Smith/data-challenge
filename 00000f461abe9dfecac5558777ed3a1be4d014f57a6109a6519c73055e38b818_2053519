<script language="Javascript">
var lastidx = -1;

function PrintProfile(index)
{
	var outstr = '';
	
	outstr += "<table name=aclProfileDetail id=aclProfileDetail class=aclProfileDetail2 cellpadding=0 cellspacing=0>";
	for(i=0; i<ACLProfileDetail_List[index].length; i=i+2)
	{
		outstr += "<tr><td class=bh>" + ACLProfileDetail_List[index][i] + "</td>";
		outstr += "<td class=bw style='text-align: left;'>" + ACLProfileDetail_List[index][i+1] + "</td></tr>";
	}
	outstr += "</table>";
	
	document.getElementById('aclPD').innerHTML = outstr;
}

function Swdetail(index)
{
  var obj = MM_findObj('aclPD');
  
  if(lastidx == -1) {
  	PrintProfile(index);
  	obj.style.visibility='visible';
  } else if (lastidx == index) {
  	obj.style.visibility='hidden';
  	lastidx = -1;
  	return;
  } else {
  	PrintProfile(index);
  	obj.style.visibility='visible';
  }
  lastidx = index;
}

function Openrule(index)
{
  var a_url = '52_AccessRule_CoS.html?'+index;	
  Link(a_url);	
}

</script>
