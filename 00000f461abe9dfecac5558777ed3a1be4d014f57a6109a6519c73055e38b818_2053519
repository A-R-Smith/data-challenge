<body class=body228>
	<div id="dhtmltooltip"></div>
	<script type="text/javascript" src="../tooltip.js"></script>
	<form name=formWAC_User_List id=formWAC_User_List method=POST action="../../form/WAC_User_List_Apply">
		<input type=hidden name=isDel id=isDel value='0'>
		<input type=hidden name=isEdit id=isEdit value='0'>
		<input type=hidden name=h_user id=h_user value=''>	
	<table id=mainGray class=mainGray style = "width:100%" cellpadding=0 cellspacing=0 >
        <tr class=gh> 	 	 	 	
            <td width=30%>&nbsp;User Name</td>
            <td width=40%>&nbsp;VLAN Name</td>
            <td width=*></td>
            <td width=*></td>

        </tr>
		<script language="Javascript">    
			var trClass = '';
			var txtClass = '';
			var tipStr;
			
			for(i=0; i<WAC_User.length; i++)
			{		if (i%2 == 0)
				{
					trClass = 'gx';
					txtClass = 'flatGX';
				}
				else
				{
					trClass = 'gw';
					txtClass = 'flatGX';
				}
				document.write("<tr class=" + trClass + ">");		
				tipStr = strNormalize(WAC_User[i][0]);
				document.write("<td align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(WAC_User[i][0], 15)) + "</td>");
				tipStr = strNormalize(WAC_User[i][1]);
				document.write("<td align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(WAC_User[i][1], 32)) + "</td>");
				document.write("  <td align=right><input type=button value='Delete' class=btnText onClick=delit(" + i + ");></td>");
				document.write("</tr>");
			}
		</script>
		
		<script language="javascript">
			function delit(iTag)
			{				
				var s = document.formWAC_User_List;				
				s.isDel.value = '1';
				s.isEdit.value = '0';
				s.h_user.value  = WAC_User[iTag][0];
				//s.h_user.value = s.user_name.value;
				s.submit();
			}		
		</script>
	</table>
	</form>
	</body>
