<script language="Javascript">
function SwitchOption(index)
{
	if (index == 1)
	{
		document.getElementById('gname').disabled = false;
	}
	else
	{
		document.getElementById('gname').disabled = true;
	}
}
function applyform()
{

	if (SIM_Settings[1] != 2)
	{
		if (document.formSIMGlobal.gname.value != '')
		{
			if(!checkTxtInput(document.formSIMGlobal.gname))
			{
				return;
			}
		}
		var interval = document.formSIMGlobal.interval.value;
		var hold = document.formSIMGlobal.hold.value;
		
		if(isNaN(interval) || interval < 30 || interval > 90)
		{
			alert("Discovery Interval is from 30 to 90!");
			document.formSIMGlobal.interval.focus();
			document.formSIMGlobal.interval.select();
			return;
		}

		if(isNaN(hold) || hold < 100 || hold > 255)
		{
			alert("Holdtime is from 100 to 255!");
			document.formSIMGlobal.hold.select();
			document.formSIMGlobal.hold.focus();
			return;
		}
	}
	
	document.formSIMGlobal.submit();
}

if (parent.g_sim_state != SIM_Settings[0] || parent.g_sim_role != SIM_Settings[1])
{
	parent.g_sim_state = SIM_Settings[0]; 
	parent.g_sim_role = SIM_Settings[1]; 
	parent.tree_items.ctExposeItem(0,"../SIM/80_SIM_Settings.html");
	
	parent.frames['tree_items'].location.reload();
}
if (SIM_Settings[1] != 2)
	document.formSIMGlobal.gname.value = SIM_Settings[4];
</script>
</div>
</body>
</html>

