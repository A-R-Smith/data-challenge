            <td colspan=2 width=66%>Log Text</td>
        </tr>
	<script language="Javascript">    
	    var trClass = '';
	    var txtClass = '';
	    
	    for(i=0; i<SystemLogSetting.length; i++)
	    {
	        if (i%2 == 0){
	            trClass = 'gx';
	            txtClass = 'flatGX';
	        }
	        else{
	            trClass = 'gw';
	            txtClass = 'flatGX';
	        }
	        
	        document.write("<tr class=" + trClass + ">");
	        document.write("  <td align=center>" + SystemLogSetting[i][0] + "</td>");
	        document.write("  <td>" + SystemLogSetting[i][1] + "</td>");
	        document.write("  <td colspan=2>" + SystemLogSetting[i][2] + "</td>");
	        document.write("</tr>");	

	    }
		document.write("<tr><td>&nbsp;</td></tr><tr>");
		document.write("  <td align=right colspan=4>");
		document.write("<input type=button name=prebutton id=prebutton class=btnText value='<<Previous' onClick='OnPressPre()'>");
		document.write("	<input type=button name=nextbutton id=nextbutton class=btnText value='Next>>' onClick='OnPressNext()'>");
		document.write("  </td>");
		document.write("</tr>");
	</script>
    </table>
</form>

<script language="javascript">
	function OnPressNext()
	{
		var reUrl="52_Systemlog_table.html?0,2";
		document.location.href=reUrl;
	}
	function OnPressPre()
	{
		var reUrl="52_Systemlog_table.html?0,3";
		document.location.href=reUrl;
	}
	function OnPressClear()
	{
		var reUrl="52_Systemlog_table.html?0,4";
		document.location.href=reUrl;
	}
	function Init()
	{
		if(ButtonPreState=='0')
		{
			document.formMSTI.prebutton.disabled=true;
		}
		if(ButtonNextState=='0')
		{
			document.formMSTI.nextbutton.disabled=true;		
		}
	}
</script>

</body>

