		<script language="Javascript">
			function 	init()
			{
				document.formClearAuthState.port.checked=true;
				document.formClearAuthState.macdress.disabled=true;
				document.formClearAuthState.macdress.style.backgroundColor = "#D4D0C8";
				
			}

			function  onRadClick(oRad)
			{		
				if(oRad.id == 'port')
				{
					document.getElementById("macdress").value = "";
					document.getElementById("macdress").disabled = true;
					document.getElementById("macdress").style.backgroundColor = "#D4D0C8";
					document.getElementById("clear_default_redirpath1").disabled = false;
					document.getElementById("clear_default_redirpath1").style.backgroundColor = "";
					document.getElementById("clear_default_redirpath2").disabled = false;
					document.getElementById("clear_default_redirpath2").style.backgroundColor = "";
					document.getElementById("clear_default_redirpath3").disabled = false;
					document.getElementById("clear_default_redirpath3").style.backgroundColor = "";				
					document.getElementById("fromport").disabled = false;
					document.getElementById("fromport").style.backgroundColor = "";				
					document.getElementById("toport").disabled = false;
					document.getElementById("toport").style.backgroundColor = "";																				
				}
				else if(oRad.id == "mac")
				{
					document.getElementById("macdress").value = "";
					document.getElementById("macdress").disabled = false;
					document.getElementById("macdress").style.backgroundColor = "";
					document.getElementById("clear_default_redirpath1").disabled = true;
					document.getElementById("clear_default_redirpath1").style.backgroundColor = "#D4D0C8";
					document.getElementById("clear_default_redirpath2").disabled = true;
					document.getElementById("clear_default_redirpath2").style.backgroundColor = "#D4D0C8";
					document.getElementById("clear_default_redirpath3").disabled = true;
					document.getElementById("clear_default_redirpath3").style.backgroundColor = "#D4D0C8";			
					document.getElementById("fromport").disabled = true;
					document.getElementById("fromport").style.backgroundColor = "#D4D0C8";				
					document.getElementById("toport").disabled = true;
					document.getElementById("toport").style.backgroundColor = "#D4D0C8";														
				}
		  }



			function	formCheckSetting(s)
			{
				if(!ipCheck(s.virtualIP))
				{
					return false;
				}
				if(!checkNumberInput(s.httpPort, 1, 65535))
				{
					return false;
				}
				if(!checkURLInput(s.default_redirpath))
				{
					return false;
				}
				s.submit();
			}
			function 	checkPort1(s)
			{
				s.toport.value=s.fromport.value;
			}
			function 	checkPort2(s)
			{
				if(s.toport.value<s.fromport.value)
				{
					alert("From Port must greater than To Port!");
					s.toport.value=s.fromport.value;
				}
			}
			function	formCheckClearAuthState(s)
			{

				s.D1.value=0;
				s.D2.value=0;
				s.D3.value=0;
				s.hfromport.value=100;
				s.htoport.value=100;
				if(s.port.checked)
				{
					if(s.clear_default_redirpath1.checked)
					{
						s.D1.value=1;
					}
					if(s.clear_default_redirpath2.checked)
					{
						s.D2.value=1;
					}
					if(s.clear_default_redirpath3.checked)
					{
						s.D3.value=1;
					}	
					s.hfromport.value=s.fromport.value;		
					s.htoport.value=s.toport.value;				
					s.portOrmac.value=1;	
				}	
				else
				{
					if(!MAC_Address_Check(s.macdress))
					{
						return false;
					}
					s.portOrmac.value=0;	
				}								
				s.submit();
			}
			function   formCheckSearchAuthState(s)
			{
				var fromPort=0;
				var toPort=0;
				var authenticated=0;
				var authenticating=0;
				var	 blocked=0;
				fromPort=s.fromport.value;
				toPort=s.toport.value;
				if(s.authenticated.checked)
				{
					authenticated=1;
				}
				if(s.authenticating .checked)
				{
					authenticating =1;
				}
				if(s.blocked.checked)
				{
					blocked=1;
				}		
				location.href="47_WAC_Auth_State_Beyond.html?"+fromPort+"&"+toPort+"&"+authenticated+"&"+authenticating+"&"+blocked+"&1";				
			}

			function	checkURLInput(obj)
			{
				var URLvalue = obj.value;
				var splitURL=URLvalue.split("/");
				var HostIP = splitURL[2];
				if(URLvalue.indexOf("http://")!=0 && URLvalue.indexOf("https://")!=0)
				{
					alert("Invalid URL, should be http://x.x.x.x/...... or https://x.x.x.x/......");
					obj.select();
					obj.focus();		
					return false;
				}
				return true;
			}
			function ipCheckValue(textValue)
			{
				if (textValue.search(" ") != -1)
				{
					alert("Invalid IP Address.");
					return false;
				}

				var Check = textValue.indexOf('.');
				
				if(Check==-1)
				{
					alert("Invalid IP Address.");
					return false;
				}
				else
				{       
					var ipSplit=textValue.split('.');
					if(ipSplit.length!=4)
					{
						alert("Invalid IP Address.");
						return false;
					}

					for(i=0; i<ipSplit.length; i++)
					{
						if(isNaN(ipSplit[i]))
						{
							alert("Invalid IP Address.");
							return false;
						}
					}

					for(i=0; i<ipSplit.length; i++)
					{
						if(ipSplit[i]>255 || ipSplit[i]<0)
						{
							alert("Invalid IP Address.");
							return false;
						}
					}
					return true;
				}
			}

			function  checkedTxt0(num)
			{

				if(num=='0')
				return 'checked'					
				
			}
			function  checkedTxt1(num)
			{
				if(num=='1')
				return 'checked'						
			}
			
		</script>
	</div>

