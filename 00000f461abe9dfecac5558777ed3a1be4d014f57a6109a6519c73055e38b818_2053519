	document.write("<form name=formDelVLAN id=formDelVLAN method='POST' action='../../form/SNMP_v6Host_Table_Apply'>");
	document.write("<tr class=" + trClass + ">");
	document.write("  <td width=35%><input type=hidden name=HostIpv6 id=HostIpv6 value='"+SNMP_Host_Setting[i][0]+"'>" + SNMP_Host_Setting[i][0] + "</td>");
