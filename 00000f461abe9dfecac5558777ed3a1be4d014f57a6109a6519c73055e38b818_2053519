<script language="Javascript">
var lastidx = -1;

parent.acl_rule_list.Clear;
parent.acl_rule_list.length = 0;
for (i = 0; i < ACLRule_Detail.length; i++)
{
	parent.acl_rule_list[i] = ACLRule_Detail[i];
}

for (i = 0; i < parent.acl_profile_list.length; i++)
{
	if (parent.acl_profile_list[i][0] == profileid)
		pid_index = i;
}

function AddRule()
{
	var a_url="52_Add_Access_Rule.html?" + profileid + "," + acl_style;
	Link(a_url);
}

function PrintRule(index)
{
	var outstr = '';
	
	outstr += "<table name=aclProfileDetail id=aclProfileDetail class=aclProfileDetail2 cellpadding=0 cellspacing=0>";
	outstr += "<tr><td colspan=2><b>ACL Rule Details</b></td></tr><tr><td colspan=2>&nbsp;</td></tr>";

	for(i=0; i<ACLRule_Detail[index].length; i++)
	{
		if(ACLRule_Detail[index][i] != '')
		{
			outstr += "<tr><td stype=\"font-size: 11pt; font-weight:bold;\">" + ACLRule_Name[i] + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>";
			if (i == 8)
				outstr += "<td style='text-align: left;'>:&nbsp;" + rp_special_char(ACLRule_Detail[index][i]) + "</td></tr>";
			else
				outstr += "<td style='text-align: left;'>:&nbsp;" + ACLRule_Detail[index][i] + "</td></tr>";
		}
	}
	outstr += "</table>";
	
	document.getElementById('aclPD').innerHTML = outstr;
}

function show_rule_detail(index)
{
	var obj = MM_findObj('aclPD');
  
	if(lastidx == -1) 
	{
		PrintRule(index);
		obj.style.visibility='visible';
	} 
	else if (lastidx == index) 
	{
		obj.style.visibility='hidden';
		lastidx = -1;
	return;
	} 
	else 
	{
		PrintRule(index);
		obj.style.visibility='visible';
	}
	lastidx = index;
}

</script>
