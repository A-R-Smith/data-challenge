	<script language="Javascript">
	var dot1xUser_count;

	function show_counter()
	{
		var str = "<b>802.1X User Table&nbsp;&nbsp;&nbsp;&nbsp;Total Entries: "+ dot1xUser_count+"</b>";
			document.getElementById('dot1xUser_counter').innerHTML = str;
	}

		function checkForm(s)
		{
		
			if(!checkTxtInput(s.E1))
			{
				return false;
			}

			if(s.E2.value != s.E3.value)
			{
				alert('Confirm Password Error!');
				s.elements[2].select();
				s.elements[2].focus();
				return false;
			}
			if(!checkTxtInput(s.E2))
			{
				return false;
			}
			s.submit();
			return true;
		}



	</script>

