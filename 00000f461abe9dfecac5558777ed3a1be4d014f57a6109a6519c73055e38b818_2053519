<script language="Javascript">

function change_vlan_flag(index)
{
	var f = document.formMBA_Local_MAC;
	if (index == 0)
	{
		f.gvlan.disabled = true;
		f.gvlanid.disabled = false;
		f.gvlanid.style.backgroundColor='#FFFFFF';
		f.gvlan.style.backgroundColor='#D4D0C8';		
	}
	else
	{	
		f.gvlan.disabled = false;
		f.gvlanid.disabled = true;
		f.gvlanid.style.backgroundColor='#D4D0C8';
		f.gvlan.style.backgroundColor='#FFFFFF';
	}
}

function init()
{
	change_vlan_flag(1);
}
init();

function applyForm(action_id)
{
	switch (action_id)
	{
		case 1: /* Add */
			var s = document.formMBA_Local_MAC;
		
			if (!MAC_Address_Check(s.mac_addr))
			{
				return false;
			}
			if (!macUcastCheck(s.mac_addr.value))
			{
				alert("Please input an unicast MAC address.");
				s.mac_addr.select();
				return false;
			}
			
			/* if not blank, validate it */
			if ((document.formMBA_Local_MAC.vlan_flag[1].checked) && (document.formMBA_Local_MAC.gvlan.value != ""))
			{
				if (!checkTxtInput(document.formMBA_Local_MAC.gvlan))
				{
					return false;
				}	
				document.formMBA_Local_MAC.h_vlan_flag.value = 1;
			}
			else if(document.formMBA_Local_MAC.gvlanid.value != "")
			{
				if (!checkNumberInput(document.formMBA_Local_MAC.gvlanid, 1, 4094))
				{
					return false;
				}
				document.formMBA_Local_MAC.h_vlan_flag.value = 0;
			}
			else /* ignore blank */
			{
				document.formMBA_Local_MAC.h_vlan_flag.value = 2;
			}

			document.formMBA_Local_MAC.isDel_by_mac.value = '0';
			document.formMBA_Local_MAC.isDel_by_vlan.value = '0';
			document.formMBA_Local_MAC.submit();
			break;
		case 2:
			if (!MAC_Address_Check(document.formMBA_Local_MAC.mac_addr))
			{
				return false;
			}	
			document.formMBA_Local_MAC.gvlan.disabeld = true;
			document.formMBA_Local_MAC.gvlanid.disabeld = true;
			document.formMBA_Local_MAC.isDel_by_mac.value = '1';
			document.formMBA_Local_MAC.isDel_by_vlan.value = '0';
			document.formMBA_Local_MAC.submit();
			break;
		case 3:
			if (formCheckVLAN())
			document.formMBA_Local_MAC.mac_addr.disabeld = true;
			document.formMBA_Local_MAC.isDel_by_mac.value = '0';
			document.formMBA_Local_MAC.isDel_by_vlan.value = '1';
			document.formMBA_Local_MAC.submit();
			break;
		default:
			break;
	}
}

function EditVLANStart(iTag)
{
	var bFLAG = false;
	var iTag_m, i;
	
	for(i=0; i<MBA_Local_MAC.length; i++)
	{
		if(document.getElementsByName('editVLANApply')[i].style.display=='none')
		{
			bFLAG = false
			iTag_m = '';
		}    		
		else
		{
			bFLAG=true;
			iTag_m=i;
			break;
		}
	}
    	if((iTag!=0|iTag!=MBA_Local_MAC.length)&bFLAG==true)
	{
		document.getElementsByName('vlan')[iTag_m].style.display = '';
		document.getElementsByName('vlan_m')[iTag_m].style.display = 'none';
		document.getElementsByName('editVLANButton')[iTag_m].style.display = '';
		document.getElementsByName('editVLANApply')[iTag_m].style.display = 'none';
	}
	document.getElementsByName('editVLANIDButton')[iTag].disabled = true;	
	document.getElementsByName('vlan')[iTag].style.display = 'none';
	document.getElementsByName('vlan_m')[iTag].style.display = '';
	document.getElementsByName('editVLANButton')[iTag].style.display = 'none';
	document.getElementsByName('editVLANApply')[iTag].style.display = '';
}

function EditVLANApplyIt(iTag)
{
	if (!checkTxtInput(document.formmMAC_Edit.vlan_m[iTag]))
	{
		return false;
	}
	var mac,vlan,vlanid;
	var s = document.formmMAC_Edit;		
	
	s.vlan[iTag].style.display = '';
	s.vlan_m[iTag].style.display = 'none';
	s.editVLANButton[iTag].style.display = '';
	s.editVLANApply[iTag].style.display = 'none'; 
	document.formmMAC_Edit.cn_vlan.value = document.formmMAC_Edit.vlan_m[iTag].value;

	mac = MBA_Local_MAC[iTag][0];
	vlan = document.formmMAC_Edit.vlan_m[iTag].value;
	vlanid = MBA_Local_MAC[iTag][2];
	edit_vlan(mac,vlan,vlanid);
}

function edit_vlan( mac,vlan,vlanid)
{
	document.formmMAC_Edit.cn_mac.value = mac;
	document.formmMAC_Edit.cn_vlan.value = vlan;
	document.formmMAC_Edit.cn_vlanid.value = vlanid;
	document.formmMAC_Edit.vlan_flag.value = 1;
	document.formmMAC_Edit.h_vlan_flag.value = 1;
	document.formmMAC_Edit.submit();
}

function EditVLANIDStart(iTag)
{
	var bFLAG = false;
	var iTag_m, i;
	
	for(i=0; i<MBA_Local_MAC.length; i++)
	{
		if(document.getElementsByName('editVLANIDApply')[i].style.display=='none')
		{
			bFLAG = false
			iTag_m = '';
		}    		
		else
		{
			bFLAG=true;
			iTag_m=i;
			break;
		}
	}

    	if((iTag!=0|iTag!=MBA_Local_MAC.length)&bFLAG==true)
	{
		document.getElementsByName('vlanid')[iTag_m].style.display = '';
		document.getElementsByName('vlanid_m')[iTag_m].style.display = 'none';
		document.getElementsByName('editVLANIDButton')[iTag_m].style.display = '';
		document.getElementsByName('editVLANIDApply')[iTag_m].style.display = 'none';
	}
	document.getElementsByName('editVLANButton')[iTag].disabled = true;
	document.getElementsByName('vlanid')[iTag].style.display = 'none';
	document.getElementsByName('vlanid_m')[iTag].style.display = '';
	document.getElementsByName('editVLANIDButton')[iTag].style.display = 'none';
	document.getElementsByName('editVLANIDApply')[iTag].style.display = '';
}

function EditVLANIDApplyIt(iTag)
{
	if (!checkNumberInput(document.formmMAC_Edit.vlanid_m[iTag],1,4094))
	{
		return false;
	}
	var mac,vlan,vlanid;
	var s = document.formmMAC_Edit;		
	
	s.vlanid[iTag].style.display = '';
	s.vlanid_m[iTag].style.display = 'none';
	s.editVLANIDButton[iTag].style.display = '';
	s.editVLANIDApply[iTag].style.display = 'none'; 

	document.formmMAC_Edit.cn_vlanid.value = document.formmMAC_Edit.vlanid_m[iTag].value;

	mac = MBA_Local_MAC[iTag][0];
	vlan = MBA_Local_MAC[iTag][1];
	vlanid = document.formmMAC_Edit.vlanid_m[iTag].value;
	edit(mac,vlan,vlanid);
}

function edit( mac,vlan,vlanid)
{
	document.formmMAC_Edit.cn_mac.value = mac;
	document.formmMAC_Edit.cn_vlan.value = vlan;
	document.formmMAC_Edit.cn_vlanid.value = vlanid;
	document.formmMAC_Edit.vlan_flag.value = 0;
	document.formmMAC_Edit.h_vlan_flag.value = 0;
	document.formmMAC_Edit.submit();
}

function formCheck()
{
	var s = document.formMBA_Local_MAC;

	if (!MAC_Address_Check(s.mac_addr))
	{
		return false;
	}
	if (!macUcastCheck(s.mac_addr.value))
	{
		alert("Please input an unicast MAC address.");
		s.mac_addr.select();
		return false;
	}
	if (document.formMBA_Local_MAC.vlan_flag[1].checked)
	{
		if (!checkTxtInput(document.formMBA_Local_MAC.gvlan))
		{
			return false;
		}	
		document.formMBA_Local_MAC.h_vlan_flag.value = 1;
	}
	else 
	{
		if (!checkNumberInput(document.formMBA_Local_MAC.gvlanid, 1, 4094))
		{
			return false;
		}
		document.formMBA_Local_MAC.h_vlan_flag.value = 0;
	}
	return true;
}

function formCheckVLAN()
{
	if (document.formMBA_Local_MAC.vlan_flag[1].checked)
	{
		if (!checkTxtInput(document.formMBA_Local_MAC.gvlan))
		{
			return false;
		}
		document.formMBA_Local_MAC.vlan_flag.value = 1;
		document.formMBA_Local_MAC.h_vlan_flag.value = 1;		
	}
	else 
	{
		if (!checkNumberInput(document.formMBA_Local_MAC.gvlanid, 1, 4094))
		{
			return false;
		}
		document.formMBA_Local_MAC.vlan_flag.value = 0;
		document.formMBA_Local_MAC.h_vlan_flag.value = 0;
	}
	return true;
}


function gonext()
{
	var url = document.location.search;
	var arg1 = url.split("?")[1].split(",")[1];
	var arg2 = url.split("?")[1].split(",")[2];	
	location.href = "47_MBA_Local_MAC.html?2," + arg1 + "," + arg2;
}

function goprev()
{
	var url = document.location.search;
	var arg1 = url.split("?")[1].split(",")[1];
	var arg2 = url.split("?")[1].split(",")[2];
	
	location.href = "47_MBA_Local_MAC.html?1," + arg1 + "," + arg2;
}

function find_by_mac()
{
	if (!MAC_Address_Check(document.formMBA_Local_MAC.mac_addr))
	{
		return false;
	}
	if (!macUcastCheck(document.formMBA_Local_MAC.mac_addr.value))
	{
		alert("Please input an unicast MAC address.");
		document.formMBA_Local_MAC.mac_addr.select();
		return false;
	}

	location.href = "47_MBA_Local_MAC.html?3,1," + document.formMBA_Local_MAC.mac_addr.value;
}

function find_by_vlan()
{
	if (document.formMBA_Local_MAC.vlan_flag[1].checked)
	{
		if (!checkTxtInput(document.formMBA_Local_MAC.gvlan))
		{
			return false;
		}		
		location.href = "47_MBA_Local_MAC.html?3,2," +escape(document.formMBA_Local_MAC.gvlan.value);

	}
	else 
	{
		if (!checkNumberInput(document.formMBA_Local_MAC.gvlanid, 1, 4094))
		{
			return false;
		}
		location.href = "47_MBA_Local_MAC.html?3,3,"+ document.formMBA_Local_MAC.gvlanid.value;
	}
}

if (MBA_Local_MAC_Btn[2] > 10)
{
	if (MBA_Local_MAC_Btn[0] == '1')
	{
		document.getElementById("bprev").disabled = false;
	}	

	if (MBA_Local_MAC_Btn[1] == '1')
	{
		document.getElementById("bnext").disabled = false;
	}	
}
</script>
</div>
</body>
