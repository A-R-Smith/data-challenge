    <script language="Javascript">
	function getPortNumber_withComma (portMember){
			var portValue = '';
			var firtTime = true;
			var iCounter = 0;	
			var  k;
						//for(k=0; k<portMember.length; k++) {
				/*r(j=1; j<=portMember[k].length; j++){
					if (portMember[k].substring(j-1, j) == '1'){
						iCounter++;
						if(firtTime == false)
							portValue += ', ';
						firtTime = false;
						portValue += '' + (k+1) + ":" + j;
					}
				}*/
			for (k=1; k<=portMember.length; k++){
				if (portMember.substring(k-1, k) == '1'){
				if(firtTime == false)
					portValue += ', ';
				firtTime = false;
				portValue += k;
				}
			}
			return portValue;
		}
    </script>
