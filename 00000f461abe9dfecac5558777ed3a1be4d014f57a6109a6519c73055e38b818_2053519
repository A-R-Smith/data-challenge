				<div id="listBlock">
						<iframe name=ifrTrafficControl id=ifrTrafficControl class=ifrContent src='31_Traffic_Control_table.html' frameborder=0 scrolling='auto'></iframe>
				</div>
			</td></tr>
		</table>

	<script language="Javascript">
		function formCheck(s)
		{	
			var s=document.formtraffic;
			if(!checkNumberInput(s.External_Cost,512,1024000))
			{
				return false;
			}
			if(!checkNumber(s.countdown,5,30) && !checkNumber(s.countdown,0,0))
			{
				alert("Count down is out of range(0 or 5-30)! ");
				s.countdown.select();
				s.countdown.focus();
				return false;
			}
			if(!checkNumberInput(s.interval,5,30))
			{
				return false;
			}
			s.submit();
		   
		}
		 function checkNumber(obj, min_val, max_val)
		{
			var i, ch, len = obj.value.length;
			var ret = true;
			var value = obj.value;

			for (i=0; i < len; i++)
			{
				ch = value.charAt(i);
				if (isNaN(ch))
				{
					return false;
				}
			}

			if(value == '' || value.search(" ") != -1 || isNaN(value) || value <  min_val || value > max_val )
			 	ret = false;

			if (ret == false)
			{
				return ret;
			}

			return ret;
		}
	</script>
<script src="../../DataStore/error_msg.js" type="text/javascript"></script>
