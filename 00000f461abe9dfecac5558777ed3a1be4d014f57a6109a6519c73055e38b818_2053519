<html>
<link rel=stylesheet href="../form.css" type="text/css" />
<head>
	<!--
	-------------------------------------------------------------------
	 Copyright (c) 2007 Alpha Networks Corporation All rights reserved.
	-------------------------------------------------------------------
	-->
	<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">

	<link rel="stylesheet" href="../form.css" type="text/css" />

	<script src="../form.js" type="text/javascript"></script>
	<script src="../../DataStore/Safeguard.js" type="text/javascript"></script>
	<script src="../../DataStore/Stacking.js" type="text/javascript"></script>
	<script src="../checkperm.js" type="text/javascript"></script>
	<script src='../../DataStore/Panel.js' type='text/javascript'></script>
	<script language="Javascript">
		parent.timer_reset=1;
		function addOption(obj, text, value)
		{
			var option=document.createElement("option");
			option.text = text;
			option.value = value;
			try
			{
				obj.add(option,null);
			}
			catch(ex)
			{
				obj.add(option);
			}
		}
		
		function onSelCfgIdChange()
		{
			var oSelCfgId 		= document.getElementById("selCfgId");
			var oLblMedia		= document.getElementById("lblMedia");
			var oSelMedia 		= document.getElementById("selMedia");
			var oLblFilePath 	= document.getElementById("lblFilePath");
			var oBoxFilePath	= document.getElementById("boxFilePath");
			
			if(oSelCfgId.value != "4")	/* save a config to NV-RAM */
			{
				oSelMedia.options.length = 0; /* clear all option */
				addOption(oSelMedia, "NV-RAM" ,"1");
				oSelMedia.value = "1";
				oLblMedia.style.visibility = "hidden";
				oSelMedia.style.visibility = "hidden";
				oLblFilePath.style.visibility = "hidden";
				oBoxFilePath.style.visibility = "hidden";
			}
			else	/* save current config */
			{
				oSelMedia.options.length = 0; /* clear all option */
				addOption(oSelMedia, "NV-RAM" ,"1");
				addOption(oSelMedia, "SD Card" ,"2");
				oSelMedia.value = "1";
				oLblMedia.style.visibility = "visible";
				oSelMedia.style.visibility = "visible";
				oLblFilePath.style.visibility = "visible";
				oBoxFilePath.style.visibility = "visible";
				oSelMedia.value = "2";
			}
			onSelMediaChange();
		}
		
		function onSelMediaChange()
		{
			var oLblMedia		= document.getElementById("lblMedia");
			var oSelMedia 		= document.getElementById("selMedia");
			var oLblFilePath 	= document.getElementById("lblFilePath");
			var oBoxFilePath	= document.getElementById("boxFilePath");
			
			if(oSelMedia.value == "1")	/* NV-RAM */
			{
				oBoxFilePath.style.backgroundColor = "#D4D0C8";
				oBoxFilePath.disabled = true;
				oLblFilePath.style.visibility = "hidden";
				oBoxFilePath.style.visibility = "hidden";
			}
			else /* if(oSelMedia.value == "2")*/	/* SD Card */
			{
				oBoxFilePath.style.backgroundColor = "";
				oBoxFilePath.disabled = false;
				oLblFilePath.style.visibility = "visible";
				oBoxFilePath.style.visibility = "visible";
			}
		}
		
		function btnSaveCfg_Click()
		{
			if(document.getElementById("boxFilePath").disabled == false)	/* save to SD Card */
			{
				if(!checkFilePath(document.getElementById("boxFilePath")))
				{
					return false;
				}
				
				document.getElementById("frmSaveConfig").submit();	/* save current config to file */
			}
			else
			{
				var cfgId = document.getElementById("selCfgId").value;
				var action = cfgId;
				location.href = "81_SaveCfg_GetJava.html?" + action;	/* save a/active config to NV-RAM ?action */
			}
		}
		
		function pageLoad()
		{
			onSelCfgIdChange();
		}
	</script>
</head>


<body onLoad="pageLoad()">
<div id="rightContent" style="overflow:hidden;">

