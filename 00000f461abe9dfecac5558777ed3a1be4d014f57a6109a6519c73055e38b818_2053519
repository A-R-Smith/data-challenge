<script language="Javascript">
	function EditStart(iTag)
	{
		
		document.formAuthSrvHostGlobal.IP_Address.value = Authentication_Server_Host[iTag][0];
		document.formAuthSrvHostGlobal.Protocol.value = Authentication_Server_Host[iTag][1];
		document.formAuthSrvHostGlobal.Port.value = Authentication_Server_Host[iTag][2];
		document.formAuthSrvHostGlobal.TimeOut.value = Authentication_Server_Host[iTag][3];
		document.formAuthSrvHostGlobal.Key.value = Authentication_Server_Host[iTag][4];
		document.formAuthSrvHostGlobal.Retransmit.value = Authentication_Server_Host[iTag][5];
		document.getElementById("IP_Address").readOnly=true;
		document.getElementById("Protocol").disabled=true;
		document.getElementById("isEdit").value = '1';
		document.getElementById("isClear").value = '0';
	}

	function ApplyIt()
	{
		var f = document.formAuthSrvHostGlobal;

		if (!ipCheck(f.IP_Address))
			return;
	
		if ("" != document.formAuthSrvHostGlobal.Port.value)
		{
			if (!checkNumberInput(f.Port, 1, 65535))
				return;
		}
		

		if ("" != document.formAuthSrvHostGlobal.TimeOut.value)
		{
			if (!checkNumberInput(f.TimeOut, 1, 255))
				return;
		}

		if ("" != document.formAuthSrvHostGlobal.Retransmit.value)
		{
			if (!checkNumberInput(f.Retransmit, 1, 255))
				return;
		}

		if ("" != document.formAuthSrvHostGlobal.Key.value)
		{
			if (!checkTxtInput(f.Key))
				return;
		}

		document.formAuthSrvHostGlobal.isClear.value = '0';
		document.formAuthSrvHostGlobal.Protocol.disabled = false;

		document.formAuthSrvHostGlobal.submit();
	}

	function DeleteIt(iTag) {
		document.formAuthSrvHostGlobal.IP_Address.value = Authentication_Server_Host[iTag][0];
		document.formAuthSrvHostGlobal.Protocol.value = Authentication_Server_Host[iTag][1];
		document.formAuthSrvHostGlobal.Port.value = '';
		document.formAuthSrvHostGlobal.TimeOut.value = '';
		document.formAuthSrvHostGlobal.Key.value = '';
		document.formAuthSrvHostGlobal.Retransmit.value = '';
		document.getElementById('isClear').value = '1';
		document.getElementById('isEdit').value = '0';

		document.getElementById("Protocol").disabled=false;
		document.formAuthSrvHostGlobal.submit();
	}
</script>
</body>
