		<div id="selectBlock">	
		<script language="Javascript">
			if(_1x_Global_Enable=='2')
			{
				document.write("<form name=form1xInit id=form1xInit method=POST action='../../form/form1xInitMac'>");
			}
			else if(_1x_Global_Enable=='1')	
			{
				document.write("<form name=form1xInit id=form1xInit method=POST action='../../form/form1xInitPort'>");
			}
			document.write("<table name=tabPortContent id=tabPortContent class=tabPortContent>");
