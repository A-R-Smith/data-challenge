<script language="Javascript">
	var stackID=0;
	var panelName;
	
	function changefront(){	
	  var fobjname= 'frontPanel'+(stackID*2+1);
	  var bobjname= 'frontPanel'+(stackID*2+2); 	 
	  if(document.getElementById(fobjname).style.visibility=='hidden')
	  {
		document.getElementById(fobjname).style.visibility='visible';
		document.getElementById(bobjname).style.visibility='hidden';
	  }
	  else
	  {
		document.getElementById(fobjname).style.visibility='hidden';
		document.getElementById(bobjname).style.visibility='visible';	   
	  }
	}
	
	function changeStack(sel)
	{
		document.getElementById('frontPanel1').style.visibility='hidden';
		document.getElementById('frontPanel2').style.visibility='hidden';
		document.getElementById('frontPanel3').style.visibility='hidden';
		document.getElementById('frontPanel4').style.visibility='hidden';
		document.getElementById('frontPanel5').style.visibility='hidden';
		document.getElementById('frontPanel6').style.visibility='hidden';
		document.getElementById('frontPanel7').style.visibility='hidden';
		document.getElementById('frontPanel8').style.visibility='hidden';
		document.getElementById('frontPanel9').style.visibility='hidden';
		document.getElementById('frontPanel10').style.visibility='hidden';
		document.getElementById('frontPanel11').style.visibility='hidden';
		document.getElementById('frontPanel12').style.visibility='hidden';

		stackID = sel;
		var objname= 'frontPanel'+(stackID*2+1);
		document.getElementById(objname).style.visibility='visible';
		document.getElementById('stack_value').value=sel+1;
		frames['main'].location.reload();
	}	 


function logout()
{
	var msg = 'Do you really want to logout?';
	if(confirm(msg))
	{
		document.formLogout.submit();
	}
}

function onChangeInterval(interval)
{
	switch(interval)
	{
		case '0':
			g_PanelRefreshInterval = '0';
			break;
		case '1':
			g_PanelRefreshInterval="10";	
			break;
		case '2':
			g_PanelRefreshInterval="30";	
			break;
		case '3':
			g_PanelRefreshInterval="60";	
			break;
		case '4':
			g_PanelRefreshInterval="300";	
			break;
		default:
			g_PanelRefreshInterval="10";	
			break;
	}
}

function onApplyInterval()
{
	document.cookie = "panel_interval=" + g_PanelRefreshInterval;
	frames['device_panel'].frames['info_bar'].location.reload();
	timer_reset=1;
	alert("Success!");
}

switch (g_PanelRefreshInterval)
{
	case '0':
		document.getElementById('refresh').selectedIndex = 0;
		break;
	case '10':
		document.getElementById('refresh').selectedIndex = 1;
		break;
	case '30':
		document.getElementById('refresh').selectedIndex = 2;
		break;
	case '60':
		document.getElementById('refresh').selectedIndex = 3;
		break;
	case '300':
		document.getElementById('refresh').selectedIndex = 4;
		break;
}

</script>
