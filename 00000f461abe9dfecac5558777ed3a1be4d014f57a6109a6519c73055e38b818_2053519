	<script language="Javascript">
		parent.timer_reset=1;
		
		/*----------------------------------------------------------------------------------
		 * cmd functions
		 *---------------------------------------------------------------------------------*/
		 
		/* Used to list all files located in a directory of a drive
		 * CLI: dir {<drive_id>} {<pathname 256>} 
		 */
		function cmd_dir(drive_id, pathname)
		{
			window.location.reload();
		}

		/* Used to change the current drive
		 * CLI: change drive <drive_id>
		 */
		/*
		function cmd_change_drive(drive_id)
		{
			document.getElementById("hid_cmd").value = "change_drive";
			document.getElementById("hid_pathname").value = drive_id;
			document.getElementById("frmSDCard_FS").submit();
		}
		*/
		
		/* Used to Delete a file from file system
		 * CLI: del {<drive_id>} <pathname 256>
		 */
		function cmd_del(pathname)
		{
			document.getElementById("hid_cmd").value = "del";
			document.getElementById("hid_pathname").value = pathname;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to rename a file
		 * CLI: rename {<drive_id>} <pathname 256> <filename 256>
		 */
		function cmd_rename(pathname, filename)
		{
			document.getElementById("hid_cmd").value = "rename";
			document.getElementById("hid_pathname").value = pathname;
			document.getElementById("hid_pathname2").value = filename;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to copy a file
		 * CLI: 
		 *	  copy [{<drive_id>} <pathname 256> 
		 *					 | image_id(1) <int 1-2> 
		 *					 | config_id(2) <int 1-2> 
		 *					 | prom(3) | log(4) ]
		 *		   [{<drive_id>} <pathname 256>
		 *					 | image_id(5) <int 1-2>
		 *					 | config_id(6) <int 1-2> | prom(7) ]
		 */
		function cmd_copy(srcPathname, srcFileType, dstPathname, dstFileType)
		{
			/*
			file type
			SD Card File:1
			Image File	:2
			Config File	:3
			Prom File	:4
			Log File	:5
			*/
			document.getElementById("hid_cmd").value = "copy";
			document.getElementById("hid_pathname").value = srcPathname;
			document.getElementById("hid_filetype_src").value = srcFileType;
			document.getElementById("hid_pathname2").value = dstPathname;
			document.getElementById("hid_filetype_dst").value = dstFileType;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to move a file from one location to another location
		 * CLI: move {<drive_id>} <pathname 256> {<drive_id>} <pathname 256>
		 */
		function cmd_move(srcPathname, dstPathname)
		{
			document.getElementById("hid_cmd").value = "move";
			document.getElementById("hid_pathname").value = srcPathname;
			document.getElementById("hid_pathname2").value = dstPathname;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to delete a directory
		 * CLI: rd {<drive_id>} <pathname 256>
		 */
		function cmd_rd(pathname)
		{
			document.getElementById("hid_cmd").value = "rd";
			document.getElementById("hid_pathname").value = pathname;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to display or change current path
		 * CLI: cd {<pathname 256>}
		 */
		function cmd_cd(pathname)
		{
			document.getElementById("hid_cmd").value = "cd";
			document.getElementById("hid_pathname").value = pathname;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* format media
		 * CLI: format <drive> [FAT16(16)|FAT32(32)]{<label_name 8>}
		 */
		function cmd_format(drive, FSType, label_name)
		{
			document.getElementById("hid_cmd").value = "format"
			document.getElementById("hid_pathname").value = drive;
			document.getElementById("hid_FSType").value = FSType;	/* 16, 32 */
			document.getElementById("hid_label_name").value = label_name;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to make a directory
		 * CLI: md {<drive_id>} <pathname 256>
		 */
		function cmd_md(pathname)
		{
			document.getElementById("hid_cmd").value = "md";
			document.getElementById("hid_pathname").value = pathname;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/* Used to upgrade file system to the latest version
		 * CLI: config file_system_version  <version_number 1-2> {<string>}
		 */
		function cdm_file_system_version(version_number)
		{
			document.getElementById("hid_cmd").value = "file_system_version";
			document.getElementById("hid_version_number").value = version_number;
			document.getElementById("frmSDCard_FS").submit();
		}
		
		/*----------------------------------------------------------------------------------
		 * page functions
		 *---------------------------------------------------------------------------------*/
		
		function addOption(obj, text, value)
		{
			var option=document.createElement("option");
			option.text = text;
			option.value = value;
			try
			{
				obj.add(option,null);
			}
			catch(ex)
			{
				obj.add(option);
			}
		}
		
		function onFileSelectChange(index)
		{
			var oSelType1	= document.getElementById("selSrcType");
			var oLblFile1	= document.getElementById("lblSrcFile");
			var oBoxFile1	= document.getElementById("boxSrcPathname_Copy");
			var oSelId1		= document.getElementById("selSrcId");
			var oSelType2	= document.getElementById("selDstType");
			var oLblFile2	= document.getElementById("lblDstFile");
			var oBoxFile2	= document.getElementById("boxDstPathname_Copy");
			var oSelId2		= document.getElementById("selDstId");
			
			if(index == 1)
			{
					switch(oSelType1.value)
					{
						/* SDCard File */
						case "1":
							/* source */
							oLblFile1.innerHTML = "File Name";
							oBoxFile1.style.display = "";
							oBoxFile1.value = "";
							oBoxFile1.disabled = false;
							oSelId1.style.display = "none";
							/* Destination */
							oSelType2.options.length = 0;	/* clear all */
							addOption(oSelType2, "SD Card File", "1");
							addOption(oSelType2, "Image File", "2");
							addOption(oSelType2, "Config File", "3");
							addOption(oSelType2, "Prom File", "4");
							addOption(oSelType2, "Log File", "5");
							oSelType2.value = "1";
							oLblFile2.innerHTML = "File Name";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "none";
							break;
							
						/* Image File */
						case "2":
							/* source */
							oLblFile1.innerHTML = "Image ID";
							oBoxFile1.style.display = "none";
							oBoxFile1.value = "";
							oBoxFile1.disabled = false;
							oSelId1.style.display = "";
							/* Destination */
							oSelType2.options.length = 0;	/* clear all */
							addOption(oSelType2, "SD Card File", "1");
							oSelType2.value = "1";
							oLblFile2.innerHTML = "File Name";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "none";
							break;
							
						/* Config File */
						case "3":
							/* source */
							oLblFile1.innerHTML = "Config ID";
							oBoxFile1.style.display = "none";
							oBoxFile1.value = "";
							oBoxFile1.disabled = false;
							oSelId1.style.display = "";
							/* Destination */
							oSelType2.options.length = 0;	/* clear all */
							addOption(oSelType2, "SD Card File", "1");
							oSelType2.value = "1";
							oLblFile2.innerHTML = "File Name";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "none";
							break;
							
						/* Prom File */
						case "4":
							/* source */
							oLblFile1.innerHTML = "Prom File";
							oBoxFile1.style.display = "";
							oBoxFile1.value = "Prom File";
							oBoxFile1.disabled = true;
							oSelId1.style.display = "none";
							/* Destination */
							oSelType2.options.length = 0;	/* clear all */
							addOption(oSelType2, "SD Card File", "1");
							oSelType2.value = "1";
							oLblFile2.innerHTML = "File Name";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "none";
							break;
							
						/* Log File */
						case "5":
							/* source */
							oLblFile1.innerHTML = "Log File";
							oBoxFile1.style.display = "";
							oBoxFile1.value = "Log File";
							oBoxFile1.disabled = true;
							oSelId1.style.display = "none";
							/* Destination */
							oSelType2.options.length = 0;	/* clear all */
							addOption(oSelType2, "SD Card File", "1");
							oSelType2.value = "1";
							oLblFile2.innerHTML = "File Name";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;

							oSelId2.style.display = "none";
							break;						
						default:
							return false;
					}	/* switch(oSelType1.value) */
			}
			else /* if(index == 2) */
			{
					switch(oSelType2.value)
					{
						/* SDCard File */
						case "1":
							/* source */
							oLblFile2.innerHTML = "File Name";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "none";
							break;
							
						/* Image File */
						case "2":
							/* source */
							oLblFile2.innerHTML = "Image ID";
							oBoxFile2.style.display = "none";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "";
							break;
							
						/* Config File */
						case "3":
							/* source */
							oLblFile2.innerHTML = "Config ID";
							oBoxFile2.style.display = "none";
							oBoxFile2.value = "";
							oBoxFile2.disabled = false;
							oSelId2.style.display = "";
							break;
							
						/* Prom File */
						case "4":
							/* source */
							oLblFile2.innerHTML = "Prom File";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "Prom File";
							oBoxFile2.disabled = true;
							oSelId2.style.display = "none";
							break;
							
						/* Log File */
						case "5":
							/* source */
							oLblFile2.innerHTML = "Log File";
							oBoxFile2.style.display = "";
							oBoxFile2.value = "Log File";
							oBoxFile2.disabled = true;
							oSelId2.style.display = "none";
							break;						
						default:
							return false;
					}	/* switch(oSelType1.value) */
			}
			
			return true;
		}

		function initFileSelect()
		{
			var oSelType1	= document.getElementById("selSrcType");
			var oLblFile1	= document.getElementById("lblSrcFile");
			var oBoxFile1	= document.getElementById("boxSrcPathname_Copy");
			var oSelId1		= document.getElementById("selSrcId");
			var oSelType2	= document.getElementById("selDstType");
			var oLblFile2	= document.getElementById("lblDstFile");
			var oBoxFile2	= document.getElementById("boxDstPathname_Copy");
			var oSelId2		= document.getElementById("selDstId");
			
			/* init source */
			oSelType1.options.length = 0;	/* clear all */
			addOption(oSelType1, "SD Card File", "1");
			addOption(oSelType1, "Image File", "2");
			addOption(oSelType1, "Config File", "3");
			addOption(oSelType1, "Prom File", "4");
			addOption(oSelType1, "Log File", "5");
			oSelType1.value = "1";			/* SD Card File */
			oLblFile1.innerHTML = "File Name";
			oBoxFile1.style.display = "";
			oBoxFile1.value = "";
			oBoxFile1.disabled = false;
			oSelId1.style.display = "none";
			
			/* init Destination */
			oSelType2.options.length = 0;	/* clear all */
			addOption(oSelType2, "SD Card File", "1");
			addOption(oSelType2, "Image File", "2");
			addOption(oSelType2, "Config File", "3");
			addOption(oSelType2, "Prom File", "4");
			/* addOption(oSelType2, "Log File", "5"); */
			oSelType2.value = "1";
			oLblFile2.innerHTML = "File Name";
			oBoxFile2.style.display = "";
			oBoxFile2.value = "";
			oBoxFile2.disabled = false;
			oSelId2.style.display = "none";
		}
		
		function btnGo_Click()
		{
			var pathname = document.getElementById("boxCurrPath").value;
			if(document.getElementById("boxCurrPath").value != "")
			{
				if(!checkPath(document.getElementById("boxCurrPath")))
				{
					return;
				}
			}
			
			cmd_cd(pathname);
		}
		
		function btnMove_Apply_Click()
		{
			var oBoxSrcPathname = document.getElementById("boxSrcPathname_Move");
			var oBoxDstPathname = document.getElementById("boxDstPathname_Move");
			var srcPathname, dstPathname;
			
			/* check */
			if(!checkFilePath(oBoxSrcPathname.value) && !checkFileName(oBoxSrcPathname.value))
			{
				oBoxSrcPathname.select()
				oBoxSrcPathname.focus();
				alert("Invalid file path or file name!");
				return false;
			}
			
			if(!checkFilePath(oBoxDstPathname.value) && !checkFileName(oBoxDstPathname.value))
			{
				oBoxDstPathname.select()
				oBoxDstPathname.focus();
				alert("Invalid file path or file name!");
				return false;
			}
			
			/* get source file */
			if(checkFileName(oBoxSrcPathname.value))	/* if the value is FileName */
			{
				srcPathname = CurrentPath;
				srcPathname += (srcPathname.substring(srcPathname.length-1,srcPathname.length).match(/^[\/\\]$/) == null ? '\\' : '');
				srcPathname += oBoxSrcPathname.value;
				
			}
			else
			{
				srcPathname = oBoxSrcPathname.value;
			}
			
			/* get desination file */
			if(checkFileName(oBoxDstPathname.value))	/* if the value is FileName */
			{
				dstPathname = CurrentPath;
				dstPathname += (dstPathname.substring(dstPathname.length-1,dstPathname.length).match(/^[\/\\]$/) == null ? '\\' : '');
				dstPathname += oBoxDstPathname.value;
				
			}
			else
			{
				dstPathname = oBoxDstPathname.value;
			}
			
			cmd_move(srcPathname, dstPathname);
		}
		
		function btnMove_Cancel_Click()
		{
			document.getElementById("divMove").style.display = "none";
			document.getElementById("divFS").style.display = "";
		}
		 
		function btnPre_Click()
		{
			var arrPathname = CurrentPath.split(/[\/\\]/);
			var pathname = "";
			var driveRegexp = /^[a-zA-Z]:(\\|\/)?$/;	/* C:, C:\ */
			var end = CurrentPath.substring(CurrentPath.length-1,CurrentPath.length).match(/^[\/\\]$/) != null ? arrPathname.length -2 : arrPathname.length -1;
			var i;

			if(!driveRegexp.test(CurrentPath))	/* if isn't root path */
			{
				/* del last path node */
				for(i=0; i < end-1; i++)
				{
					pathname += (arrPathname[i] + '\\');
				}
				pathname += arrPathname[i];	/* end with no '\' */
			}
			else
			{
				pathname = "";
			}

			cmd_cd(pathname);
		}
		
		function btnCreateDir_Click()
		{
			ifrTable_Dir.document.body.scrollTop = 0;
			
			if(document.getElementById('ifrTable_Dir').contentDocument != undefined) /* for firefox */
			{
				document.getElementById('ifrTable_Dir').contentDocument.getElementById('trNewDir').style.display = '';
				document.getElementById('ifrTable_Dir').contentDocument.getElementById('boxNewDirName').select();
				document.getElementById('ifrTable_Dir').contentDocument.getElementById('boxNewDirName').focus();
			}
			else /* for IE */
			{
				ifrTable_Dir.trNewDir.style.display = '';
				ifrTable_Dir.boxNewDirName.select();
				ifrTable_Dir.boxNewDirName.focus();
			}
		}
		
		function btnFormat_Apply_Click()
		{
			if((document.getElementById("boxLabelName").value != "") 
				&& !checkTxtInput(document.getElementById("boxLabelName")))
			{
				return;
			}
			
			var drive = document.getElementById("selDrive_Format").value;
			var FSType = document.getElementById("selFSType").value;
			var label_name = document.getElementById("boxLabelName").value;
			
			cmd_format(drive, FSType, label_name);
		}
		
		function btnFormat_Cancel_Click()
		{
			document.getElementById("divFS").style.display = "";
			document.getElementById("divFormat").style.display = "none";
		}
		
		function btnVersionNum_Click()
		{
			document.getElementById("divFS").style.display = "none";
			document.getElementById("divVersionNum").style.display = "";
		}
		
		function btnVersionNum_Apply_Click()
		{
			var versionNum = document.getElementById("selFSVersion").value;
			cdm_file_system_version(versionNum);
		}
		
		function btnVersionNum_Cancel_Click()
		{
			document.getElementById("divFS").style.display = "";
			document.getElementById("divVersionNum").style.display = "none";
		}

		function btnCopy_Apply_Click()
		{
			var oSelType1	= document.getElementById("selSrcType");
			var oBoxFile1	= document.getElementById("boxSrcPathname_Copy");
			var oSelId1		= document.getElementById("selSrcId");
			var oSelType2	= document.getElementById("selDstType");
			var oBoxFile2	= document.getElementById("boxDstPathname_Copy");
			var oSelId2		= document.getElementById("selDstId");
			var srcPathname, srcFileType;
			var dstPathname, dstFileType;
			var dstFileName;
			var i;

			/* source file */
			switch(oSelType1.value)
			{
				/* SDCard File */
				case "1":
				{
						/* check */
						if(!checkFilePath(oBoxFile1.value) && !checkFileName(oBoxFile1.value))
						{
							oBoxFile1.select()
							oBoxFile1.focus();
							alert("Invalid file path or file name!");
							return false;
						}
						
						/* get source file */
						srcFileType = "1";
						if(checkFileName(oBoxFile1.value))	/* if the value is FileName */
						{
							srcPathname = CurrentPath;
							srcPathname += (srcPathname.substring(srcPathname.length-1,srcPathname.length).match(/^[\/\\]$/) == null ? '\\' : '');
							srcPathname += oBoxFile1.value;
							
						}
						else
						{
							srcPathname = oBoxFile1.value;
						}
						break;
				}
					
				/* Image File */
				case "2":
				{
						srcFileType = "2";
						srcPathname = oSelId1.value;
						break;
				}
					
				/* Config File */
				case "3":
				{
						srcFileType = "3";
						srcPathname = oSelId1.value;
						break;
				}
					
				/* Prom File */
				case "4":
				{
						srcFileType = "4";
						srcPathname = "";
						break;
				}
					
				/* Log File */
				case "5":
				{
						srcFileType = "5";
						srcPathname = "";
						break;
				}	
				default:
					return false;
			}	/*switch(oSelType1.value)*/
			
			/* Destination file */
			switch(oSelType2.value)
			{
				/* SDCard File */
				case "1":
				{
						/* check */
						if(!checkFilePath(oBoxFile2.value) && !checkFileName(oBoxFile2.value))
						{
							oBoxFile2.select()
							oBoxFile2.focus();
							alert("Invalid file path or file name!");
							return false;
						}
						
						/* get Destination file */
						dstFileType = "1";
						if(checkFileName(oBoxFile2.value))	/* if the value is FileName */
						{
							dstPathname = CurrentPath;
							dstPathname += (dstPathname.substring(dstPathname.length-1,dstPathname.length).match(/^[\/\\]$/) == null ? '\\' : '');
							dstPathname += oBoxFile2.value;
							
						}
						else
						{
							dstPathname = oBoxFile2.value;
						}
						
						/* check if overwrite */
						if(false)	/* del this */
						{
						if(checkFileName(oBoxFile2.value))	/* if only filename */
						{
							dstFileName = oBoxFile2.value;
						}
						else
						{
							strArr = oBoxFile2.value.split(/[\\\/]/);
							dstFileName = strArr[strArr.length-1];
						}
						
						for(i=0; i<SDCard_FS.length; i++)
						{
							/* if dstfile already exists */
							if(dstFileName.toUpperCase() == (SDCard_FS[i][0]).toUpperCase())	
							{
								if(oSelType1.value == "1")	/* if srcfile is a SD Card file */
								{
									alert("The same file exists!!");
									return false;
								}
								else
								{
									if(!confirm("Are you sure you want to overwrite the file ?"))
									{
										return false;
									}
									else
									{
										break;
									}
								}
							}
						}	/* end for */
						}	/* del this */
						break;
				}
					
				/* Image File */
				case "2":
				{
						dstFileType = "2";
						dstPathname = oSelId2.value;
						break;
				}
					
				/* Config File */
				case "3":
				{
						dstFileType = "3";
						dstPathname = oSelId2.value;
						break;
				}
					
				/* Prom File */
				case "4":
				{
						dstFileType = "4";
						dstPathname = "";
						break;
				}
					
				/* Log File */
				/*
				case "5":
				{
						dstFileType = "5";
						dstPathname = "";
						break;
				}
				*/	
				default:
					return false;
			}	/*switch(oSelType2.value)*/

			cmd_copy(srcPathname, srcFileType, dstPathname, dstFileType);
		}
		
		function btnCopy_Cancel_Click()
		{
			initFileSelect();
			document.getElementById("divCopy").style.display = "none";
			document.getElementById("divFS").style.display = "";
		}
		
		function btnCopyFlash_Click()
		{
			initFileSelect();
			document.getElementById("divCopy").style.display = "";
			document.getElementById("divFS").style.display = "none";
		}
		
		function page_Load()
		{
			if(FS_StorageMedia_Error != "")
			{
				document.getElementById("divDir").style.display = "none";
				document.getElementById("divDrive").style.display = "";
				document.getElementById("btnPre").disabled = true;
				document.getElementById("btnCreateDir").disabled = true;
				document.getElementById("btnCopyFlash").disabled = true;
			}
			else if((SDCard_FS_Error != "") && (CurrentPath != ""))
			{
				document.getElementById("divDir").style.display = "";
				document.getElementById("divDrive").style.display = "none";
				document.getElementById("btnCreateDir").disabled = true;
			}
			else if(CurrentPath == "")
			{
				document.getElementById("divDir").style.display = "none";
				document.getElementById("divDrive").style.display = "";
				document.getElementById("btnPre").disabled = true;
				document.getElementById("btnCreateDir").disabled = true;
			}
			else
			{
				document.getElementById("divDir").style.display = "";
				document.getElementById("divDrive").style.display = "none";
				document.getElementById("btnPre").disabled = false;
				document.getElementById("btnCreateDir").disabled = false;
			}
		}
	</script>
</head>
