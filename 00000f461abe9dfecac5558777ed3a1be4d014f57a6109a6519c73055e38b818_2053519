<script language="Javascript">
    function delit(){
        formMSTI.submit();
        parent.location.href = '47_Authentication_Server_Host.html';
    }

    function EditStart(iTag){
        document.formMSTI.editButton[iTag].style.display = 'none';
        document.formMSTI.Port[iTag].style.display = 'none';
        document.formMSTI.Port_M[iTag].style.display = '';
        document.formMSTI.Port_M[iTag].select();
        document.formMSTI.Timeout[iTag].style.display = 'none';
        document.formMSTI.Timeout_M[iTag].style.display = '';
        document.formMSTI.Timeout_M[iTag].select();
        document.formMSTI.Retransmit[iTag].style.display = 'none';
        document.formMSTI.Retransmit_M[iTag].style.display = '';
        document.formMSTI.Retransmit_M[iTag].select();
        document.formMSTI.editApply[iTag].style.display = '';
    }
    
    function EditApplyIt(iTag){
        document.formMSTI.editButton[iTag].style.display = '';

        document.formMSTI.Port[iTag].style.display = '';
        document.formMSTI.Port_M[iTag].style.display = 'none';
        document.formMSTI.Timeout[iTag].style.display = '';
        document.formMSTI.Timeout_M[iTag].style.display = 'none';
        document.formMSTI.Retransmit[iTag].style.display = '';
        document.formMSTI.Retransmit_M[iTag].style.display = 'none';
        document.formMSTI.editApply[iTag].style.display = 'none';
        
        document.formMSTI.Port[iTag].value = document.formMSTI.Port_M[iTag].value;
        document.formMSTI.Timeout[iTag].value = document.formMSTI.Timeout_M[iTag].value;
        document.formMSTI.Retransmit[iTag].value = document.formMSTI.Retransmit_M[iTag].value;
    }
