<script language="Javascript">
    function checkSelect(kOne, j, portMember){
        b = portMember.substring(kOne-1, kOne);
        if ( (j == 0 && b == 'U') || (j == 1 && b == 'T') || (j == 2 && b == '0') )
            return true;
        else
            return false;
    }
        function getPortNumber_withComma (portMember, typeChar){
        	var portValue = '';
        	var firtTime = true;
        	var iCounter = 0;	
        	for(j=1; j<=24; j++){
        	    if (portMember.substring(j-1, j) == typeChar){
        	        iCounter++;
        	        if(firtTime == false)
        	            portValue += ',';
        	        firtTime = false;
        	        if(iCounter > 1 && iCounter%8 == 1)
        	            portValue += '<br>';
        	        portValue += '' + j;
        	    }
        	}	
        	return portValue;
        }

    QVlanMatrixLen = QVLAN_Setting.length;

    var VID = QVLAN_Setting[0][0];
    var VLAN_Name = QVLAN_Setting[0][1];
    
    document.write("<tr><td>Port Mirror</td>");
    document.write("    <td width=450>" + VID + "</td>");
    document.write("<tr><td>VLAN Name</td>");
    document.write("    <td>" + VLAN_Name + "</td>");
    document.write("<tr><td>Untag VLAN Ports</td>");
    document.write("  <td>" + getPortNumber_withComma(QVLAN_Setting[0][2], 'U') + "</td>");
    document.write("<tr><td>Tag VLAN Ports</td>");
    document.write("  <td>" + getPortNumber_withComma(QVLAN_Setting[0][2], 'T') + "</td>");
    document.write("<tr><td colspan=2 align=right><input type=button value='Apply' class=btnText onClick='editVID();'>");

    document.write("</table>");

    document.write("<div class=tabVLANContent><table id=mainGray class=mainGray cellpadding=0 cellspacing=0 width=662>");
        document.write("<tr><td>Unit</td>");
        document.write("<td><select class=portSel name=D3 id=D3>");
        for(i=0; i<Unit_Setting.length; i++){
            document.write("<option value='" + Unit_Setting[i] + "'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" + Unit_Setting[i] + "</option>");
        }
        document.write("</select></td>");
        document.write("<tr><td></td>");

    document.write("        <tr class=gh>");
    document.write("            <td width=60>&nbsp; Port</td>");
    document.write("            <td>Select All</td>");

    var portID = '';
    for(i=1; i<25; i++){
        if (i<10) portID = '0' + i;
        else portID = i;
        document.write("        <td>&nbsp;" + portID + "</td>");
    }
    
    document.write("        </tr>");
    //document.write("        <tr><td class=blankbar>&nbsp;</td></tr>");

    var trClass = '';
    var tagName = '';
    var tagType = '';
    
    for(j=0; j<3; j++){
        if (j==2){
            trClass = 'gw'
            tagName = 'Not Member';
            tagType = '0';
        }
        else if (j==1){
            trClass = 'gx'
            tagName = 'Tag';
            tagType = 'T';
        }
        else{
            trClass = 'gw'
            tagName = 'Untag';
            tagType = 'U';
        }

        document.write("    <tr class=" + trClass + ">");
        document.write("        <td>&nbsp; " + tagName + "</td>");
        document.write("        <td><input type=button value='All' class=btnAll onClick=SelAll(" + j + ");></td>");
        
        
        for(k=1; k<25; k++){
            isChecked = '';
            if(checkSelect(k, j, QVLAN_Setting[0][2])){
                isChecked = 'checked';
            }
            document.write("    <td><input type='radio' name='C" + k + "' id='C" + k + "' value='" + tagType + "' " + isChecked + " ></td>");
        }
        
        //document.write("        <tr><td class=blankbar>&nbsp;</td></tr>");
    }
        document.write("<tr><td colspan=26 align=right><input type=button value='Add' class=btnText onClick='VIDportSetApply();'>");
        document.write("</table></div>");
</script>
