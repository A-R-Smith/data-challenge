}

function Openrule(index, acl_style)
{
	var a_url = '52_AccessRule.html?' + index + "," + acl_style + ",0";
	Link(a_url);	
}
function DeleteProfile(idx)
{
	//alert(idx);
	document.ACLProfileForm.pid.value = idx;
	document.ACLProfileForm.isDel_All.value = '0';
	document.ACLProfileForm.set_cpu.value = '0';
	document.ACLProfileForm.submit();
}
