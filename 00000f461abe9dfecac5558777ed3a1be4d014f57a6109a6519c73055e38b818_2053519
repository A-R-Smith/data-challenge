<script language="Javascript">
function formcheck(s)
{
	var no_limitFlag=s.noLimit.selectedIndex;
	if( no_limitFlag !=1)
	{
		if(!checkNumberInput(s.rate,64,1024000))
		return false;
	}
	s.submit();
	return true;
}
function ChangeState(s)
{
	if(s.noLimit.selectedIndex==0)  // on_limit = Disabled
		s.rate.disabled=false;
	else                                       // on_limit = Enabled
		s.rate.disabled=true;
}
</script>


