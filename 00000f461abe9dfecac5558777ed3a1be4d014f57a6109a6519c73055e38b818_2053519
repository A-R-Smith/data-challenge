<script language="Javascript">
	var trClass = '';
	var txtClass = '';

    document.write("<tbody class='scrollContent'>");  
	for(i=0; i<MS_Info.length; i++)
	{
		
		if (i%2 == 0)
		{
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else
		{
			trClass = 'gw';
			txtClass = 'flatGX';
		}

		document.write("<tr class=" + trClass + ">");
		document.write("  <td width=10%><input type=checkbox  name='C" + MS_Info[i][0] + "'></td>");
		document.write("  <td width=5%>" + MS_Info[i][0] + "</td>");
		document.write("  <td width=15%>" + MS_Info[i][1] + "</td>");
		document.write("  <td width=25%>" + MS_Info[i][2] + "</td>");
		document.write("  <td width=30%>" + MS_Info[i][3] + "</td>");
		document.write("  <td>" + MS_Info[i][4] + "</td>");
		document.write("</tr>");
	}

</script>
</tbody>
	</table>
	</div>
	<script language="Javascript">    
		parent.sim_list_count = MS_Info.length;
		parent.show_counter();
	</script>
    </form>
    <script language="javascript">
    var f=document.formMS_List;
	var p=parent.document.formSIM_Action;
    function SIM_Action_Apply(index)
{
	var i, chkid, chk_name, ms_id;
	var s="";

	if(!parent.inputcheck())
		return;
	
	for(i = 0, chkid = 0; i < f.length; i++)
	{
		elm = f.elements[i];
		if(elm.type=="checkbox")
		{
			if(elm.checked)
			{
				ms_id = elm.name.substring(1, elm.name.length);
				s+=(s.length==0 ? ms_id : (","+ms_id));
			}
			chkid++;
		}
	}
	
	if(s.length==0)
	{
		alert("Please select member first.");
		return;
	}
		
	f.sim_data.value=s;
	f.sim_action.value = index;

	parent.location.href = "../../html/SIM_TFTP_Data_Set.html?" + index + "," + f.sim_data.value + " " + p.ip_addr.value + " " + p.file_name.value;
	//location.href = "SIM_TFTP_Data_Set.html?" + index + "," + f.sim_data.value + " " + f.ip_addr.value + " " + f.file_name.value;
}

function SelAll(indexRadio)
{

	var i, chkid;
	var s="";
	
	for(i = 0, chkid = 0; i < f.length; i++)
	{
		elm = f.elements[i];
		if(elm.type=="checkbox")
		{
			elm.checked=true;
			
		}
	}
}

</script>
