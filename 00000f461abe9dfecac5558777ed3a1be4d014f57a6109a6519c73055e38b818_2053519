<script language="Javascript">
function find_entry()
{
	var i, vid;
	var vid = document.formIP_MAC_Blocked.vid.value;
	var mac = document.formIP_MAC_Blocked.mac.value;

	if (!checkNumberInput(document.formIP_MAC_Blocked.vid, 1, 4094))
		return;

	if (!MAC_Address_Check(document.formIP_MAC_Blocked.mac))
		return;
		
	frames['ifrGVRP'].location.href = "30_IP_MAC_Blocked_Table.html?3," + vid + "," + mac;
}

function clear_all()
{
	document.formIP_MAC_Blocked.isDelAll.value = '1';
	document.formIP_MAC_Blocked.submit();
}
function view_all()
{
	location.href = "30_IP_MAC_Blocked.html?0,0,0";
}
function delit(vid, mac)
{
	document.formIP_MAC_Blocked.isDel.value = '1';
	document.formIP_MAC_Blocked.h_vid.value = vid;
	document.formIP_MAC_Blocked.h_mac.value = mac;

	document.formIP_MAC_Blocked.submit();
}
</script>
