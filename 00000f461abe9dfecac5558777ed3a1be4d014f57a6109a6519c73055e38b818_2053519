	        </tr>
	        <tr><td colspan=5><hr align=left name=hr1 id=hr1 class=hr1></td></tr>
    </table>
</form>

</div>
</td></tr>
<tr><td>
<div id="listBlock">
	<table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" width=95%>
	<script language="Javascript">
    document.write("<tr><td colspan=4><b>Total Entries: " + Dot1v_Group_Settings.length +"</b></td></tr>");
    </script>
        <tr class=gh> 	 	 	 	
            <td width=12%>Group ID</td>
            <td width=25%>Group Name</td>
            <td width=15%>Frame Type</td>
            <td width=15%>Protocol Value</td>

            <td></td>
        </tr>
<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
    
    for(i=0; i<Dot1v_Group_Settings.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGX';
        }
        
        document.write("<tr class=" + trClass + ">");
        document.write("  <td>" + Dot1v_Group_Settings[i][0] + "</td>");
        document.write("  <td onMouseover=keytip(unescape('" + strNormalize(Dot1v_Group_Settings[i][1]) +"')); onMouseout='hideddrivetip()'>" +rp_special_char(strTruncate( Dot1v_Group_Settings[i][1], 10)) + "</td>");
        document.write("  <td>" +NnuToString_type(Dot1v_Group_Settings[i][2]) + "</td>");
	document.write("  <td>" + Dot1v_Group_Settings[i][3] + "</td>");
        document.write("  <td align=right><input type=button value='Delete Settings' class=btnText onClick=DelSettings(" + i + ");><input type=button value='Delete Group' class=btnText onClick=DelGroup(" + i + ");></td>");
        document.write("</tr>");
    }
    
</script>
    </table>
</div>
</td></tr>

<script language="Javascript">
function AddGroupInfor()
{
	var s = document.formDot1v_Group_infor;

	if(s.IDorName[0].checked)
	{
		if (!checkNumberInput(s.group, 1, 8))
			return false;
	}
	else if(s.IDorName[1].checked)
	{
		if(!checkTxtInput(s.group))
			return false;
	}
	if (!checkProVal(s))
		return false;
	s.operation_infor.value = '2';
	s.submit();
}
function AddGroup()
{
	var s = document.formDot1v_Group;
	if (!checkNumberInput(s.gid, 1, 8))
		return false;
	//if(!checkTxtInput(s.gName))
		//return false;
	s.operation_group.value = '2';           // 2---add   1---del  3---del all
	s.submit();
	
}
function DelAllGroup()
{
	var s = document.formDot1v_Group;
	s.operation_group.value = '3';   // 2---add   1---del  3---del all
	s.submit();
}

function DelGroup(iTag)
{
	document.formDot1v_Group.operation_group.value = '1';   // 2---add   1---del  3---del all
	document.formDot1v_Group.gid.value = Dot1v_Group_Settings[iTag][0];
	document.formDot1v_Group.gName.value = Dot1v_Group_Settings[iTag][1];
	document.formDot1v_Group.submit();
}

function DelSettings(iTag)
{
	var s = document.formDot1v_Group_infor;
	s.group.value = Dot1v_Group_Settings[iTag][0];
	s.ether_type.value = Dot1v_Group_Settings[iTag][2];
	s.pro_value.value = Dot1v_Group_Settings[iTag][3];
	s.operation_infor.value = '1';
	s.submit();
}

function checkProVal(s)
{
	if(s.pro_value.value.length!=4)
	{
		s.pro_value.focus();
		s.pro_value.select();
		alert("Invalid input!");
		return false;
	}
	if (!checkHexInput(s.pro_value))
		return false;
	
	return true;
}
</script>
</div>
	<script src="../../DataStore/error_msg.js" type="text/javascript"></script>	
</body>
