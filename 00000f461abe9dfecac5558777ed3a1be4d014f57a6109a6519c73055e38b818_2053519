<script language="Javascript">
	function apply()
        {
			if(!checkNumberInput(document.getElementById("telnet_port"), 1, 65535))
			{
				return false;
			}
        	document.formtelnet.submit();
        }
	function changePortState()
	{
		if(document.formtelnet.enabled_flag[0].checked==true)
		{
			document.formtelnet.telnet_port.style.backgroundColor = "";
			document.formtelnet.telnet_port.disabled = false;
		}
		else /* if(document.formtelnet.enabled_flag[1].checked==true) */
		{
			document.formtelnet.telnet_port.style.backgroundColor = "#D4D0C8";
			document.formtelnet.telnet_port.disabled = true;
		}
	}
	changePortState();
</script>
