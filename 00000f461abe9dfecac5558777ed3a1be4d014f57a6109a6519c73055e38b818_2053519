	document.write("<select class=enableSel name=permit_arp_m id=permit_arp_m style='display: none;'>");
	document.write("<option value=0 " + selectedTxt(IP_MAC_Settings[i][5], 'Disabled') + ">Disabled</option>");
	document.write("<option value=1 " + selectedTxt(IP_MAC_Settings[i][5], 'Enabled') + ">Enabled</option>");
	document.write("</select>");
	document.write("  </td>");    
	document.write("  <td align=left>" + IP_MAC_Settings[i][6] + "</td>");
	*/
	//ports
	document.write("  <td align=left><input type=text name=p id=p maxlength=128  class=" + txtClass + " value='" + rp_special_char(strTruncate(IP_MAC_Settings[i][3], 15)) + "' readOnly onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>");
	document.write("  <input type=text name=p_m id=p_m maxlength=128 class=flatWS value='" + IP_MAC_Settings[i][3] + "' style='display: none; width:110'>");
	document.write("</td>")

    //buttons
	if (IP_MAC_Settings[i][0] == "DHCP Snooping")
	{
		document.write("  <td align=right><input type=button name=editButton id=editButton value='Edit' disabled class=btnInGrayS onClick='EditStart(" + i + ");'>");
		document.write("      <input type=button name=editApply id=editApply value='Apply' disabled class=btnInGrayS onClick='EditApplyIt(" + i + ");' style='display: none;'>");

		document.write("<input type=button name=delButton id=delButton value='Delete' disabled class=btnInGrayS onClick=delit(" + i + ");></td>");
	}
	else
	{
		document.write("  <td align=right><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'>");
		document.write("      <input type=button name=editApply id=editApply value='Apply' class=btnInGrayS onClick='EditApplyIt(" + i + ");' style='display: none;'>");
		document.write(" <input type=button name=delButton id=delButton value='Delete' class=btnInGrayS onClick=delit(" + i + ");></td>");
	}
	document.write("</tr>");
    }

	if (IP_MAC_Navigation_Btn[2] != 0)
	{
		document.write("<tr><td colspan=8>&nbsp;</td></tr>");
		document.write("<tr><td colspan=8 align=right><input id=bprev name='bprev' type=button  value='<<Back' class=btnText onClick=goprev() disabled>");
	    document.write("<input id=bnext name='bnext' type=button value='Next>>' class=btnText onClick=gonext() disabled></td>");
	    document.write("</tr>");
	}
	
</script>

</table>
</form>

</div>
</td></tr>
</table>
</div>
<script language="Javascript">//changeForm1280('pass');
function applyForm_Mode_Set()
{
	document.formIP_MAC_ACL_Mode_Settings.submit();
}

function applyForm_Set()
{
	var ob_mac_address = document.formIP_MAC_ACL_Settings.mac_address;
	var ob_ip_addr = document.formIP_MAC_ACL_Settings.ip_address;

	if (!ipCheck(ob_ip_addr))
	{
		return;
	}

	if (!MAC_Address_Check(ob_mac_address))
		return;

	if (document.formIP_MAC_ACL_Settings.all_port.checked == false)
	{
		if (!check_portlist(document.formIP_MAC_ACL_Settings.port))
			return;
	}
	document.formIP_MAC_ACL_Settings.submit();
}

function applyForm_Clear_All()
{
	document.formIP_MAC_ACL_Settings.isDelAll.value = '1';
	document.formIP_MAC_ACL_Settings.submit();
}

function applyForm_Show_All()
{
	location.href = "30_IP_MAC_Settings.html?0,0";
}

function applyForm_Find()
{
	var f = document.formIP_MAC_ACL_Settings;
	
	if (!ipCheck(f.ip_address) || !MAC_Address_Check(f.mac_address))
		return;
	
	location.href = "30_IP_MAC_Settings.html?3," + f.ip_address.value + " " + f.mac_address.value;
}

function gonext()
{
	location.href = "30_IP_MAC_Settings.html?2,0 0";
}

function goprev()
{
	location.href = "30_IP_MAC_Settings.html?1,0 0";
}

function del_ip_mac(ip_addr, mac_addr)
{
	document.formIP_MAC_ACL_Settings.isDel.value = '1';
	document.formIP_MAC_ACL_Settings.cn_ip.value = ip_addr;
	document.formIP_MAC_ACL_Settings.cn_mac.value = mac_addr;
	document.formIP_MAC_ACL_Settings.submit();
}

function checkAllPorts()
{
	var s = document.formIP_MAC_ACL_Settings;

	if (s.all_port.checked == true)
	{
		s.h_all_port.value = '1';
		s.port.disabled = true;
		s.port.style.backgroundColor='#D4D0C8';
	}
	else
	{
		s.h_all_port.value = '0';
		s.port.disabled = false;
		s.port.style.backgroundColor='#FFFFFF';
	}
}

function EditStart(iTag)
{
	//init all
	for(i=0; i<IP_MAC_Settings.length; i++)
	{
		document.getElementsByName("mac_addr")[i].style.display = '';
		document.getElementsByName("mac_addr_m")[i].style.display = 'none';
		//document.getElementsByName("co_mode")[i].style.display = '';
		//document.getElementsByName("co_mode_m")[i].style.display = 'none';
		//document.getElementsByName("permit_arp")[i].style.display = '';
		//document.getElementsByName("permit_arp_m")[i].style.display = 'none';
		document.getElementsByName("p")[i].style.display = '';
		document.getElementsByName("p_m")[i].style.display = 'none';
		document.getElementsByName("editButton")[i].style.display = '';
		document.getElementsByName("editApply")[i].style.display = 'none'; 
	}

    //set current edit
	document.getElementsByName("mac_addr")[iTag].style.display = 'none';
	document.getElementsByName("mac_addr_m")[iTag].style.display = '';
	//document.getElementsByName("co_mode")[iTag].style.display = 'none';
	//document.getElementsByName("co_mode_m")[iTag].style.display = '';
	//document.getElementsByName("permit_arp")[iTag].style.display = 'none';
	//document.getElementsByName("permit_arp_m")[iTag].style.display = '';
	document.getElementsByName("p")[iTag].style.display = 'none';
	document.getElementsByName("p_m")[iTag].style.display = '';
	document.getElementsByName("editButton")[iTag].style.display = 'none';
	document.getElementsByName("editApply")[iTag].style.display = ''; 
}
    
function EditApplyIt(iTag)
{
	if (!MAC_Address_Check(document.getElementsByName("mac_addr_m")[iTag]))
		return;

	if (!check_portlist(document.getElementsByName("p_m")[iTag]))
	{
		return;
	}
	
	document.formIP_MAC_ACL_Settings.isEdit.value = '1';
	document.formIP_MAC_ACL_Settings.cn_ip.value = IP_MAC_Settings[iTag][1];
	document.formIP_MAC_ACL_Settings.cn_mac.value = document.getElementsByName("mac_addr_m")[iTag].value;
	//document.formIP_MAC_ACL_Settings.cn_mode.value = document.getElementsByName("co_mode_m")[iTag].value;
	//document.formIP_MAC_ACL_Settings.hidPermitArp.value = document.getElementsByName("permit_arp_m")[iTag].value;
	document.formIP_MAC_ACL_Settings.cn_port.value = document.getElementsByName("p_m")[iTag].value;
	document.formIP_MAC_ACL_Settings.submit();
}

function delit(iTag)
{
	del_ip_mac(IP_MAC_Settings[iTag][1], document.getElementsByName("mac_addr_m")[iTag].value);
}

checkAllPorts();

if (IP_MAC_Navigation_Btn[0] == '1')
	document.formIP_MAC_Edit.bprev.disabled = false;

if (IP_MAC_Navigation_Btn[1] == '1')
	document.formIP_MAC_Edit.bnext.disabled = false;

</script>
