<script language="Javascript">    
	var trClass = '';
	var txtClass = '';
	var count=0;
	for(i=0; i<ACLRule_Detail.length; i++)
	{
		if (count%2 == 0)
		{
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else
		{
			trClass = 'gw';
			txtClass = 'flatGW';
		}
		
		document.write("<tr class=" + trClass + ">");
		document.write("  <td align=left>" + profileid + "</td>");
		document.write("<td align=left>" + ACLRule_Detail[i][1] + "</td>");
		document.write("  <td align=left>" + ACLRule_Detail[i][2] + "</td>");
		document.write("  <td align=left>" + ACLRule_Detail[i][3] + "</td>"); 			
		document.write("<td><input type=button value='Show Details'  name=ACLRule_Show id=ACLRule_Show class=btnText onClick=show_rule_info(" + i + ")></td>");
		if (parent.acl_profile_list[pid_index][2] == 'IP-MAC-Port Binding')
			document.write("<td><input type=button value='Delete Rules'  name=ACLRuleDelete id=ACLRuleDelete disabled class=btnText onClick=delRule(" + profileid + "," + ACLRule_Detail[i][1] + ")></td>");
		else
			document.write("<td><input type=button value='Delete Rules'  name=ACLRuleDelete id=ACLRuleDelete class=btnText onClick=delRule(" + profileid + "," + ACLRule_Detail[i][1] + ")></td>");
		document.write("</tr>");
		count++;
	}

		document.write("<tr><td>&nbsp;</td></tr>");

	//if (ACL_Rule_Navigation_Btn[2] > 10)
	//{
		document.write("<td align=right colspan=6><input id='bprev' name='bprev' type=button value='<< Back' class=btnText onClick=goprev() disabled>");
