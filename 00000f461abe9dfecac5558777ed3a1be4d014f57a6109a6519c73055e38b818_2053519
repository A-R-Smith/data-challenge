			<script language="Javascript">
			function showHostList(groupId)
			{
				var panstr = "";
				var i;
				var trClass;
				var txtClass;

				panstr += "<table id=mainGray2 class=mainGrayNormal cellpadding=0 cellspacing=0 style=\"width:98%\";><tbody>";
				panstr += "<tr class=gh><td width=15% align=left>&nbsp;IP Address</td><td width=15%>&nbsp;Protocol</td></td>";
				panstr += "<td width=15%></td></tr>"

				if (-1 == groupId)
				{
					document.getElementById('hostpanel').innerHTML = panstr;	
					return;
				}

				for (i = 0; i < Sever_Group[groupId][1].length; i++)
				{
					if (i % 2 == 0)
					{
						trClass = 'gx';
						txtClass = 'flatGX';
					}
					else
					{
						trClass = 'gw';
						txtClass = 'flatGW';
					}

					panstr += "<tr class=" + trClass + ">";	
					panstr += "<td>" + Sever_Group[groupId][1][i][0] + "</td>"
					panstr += "<td>" + Sever_Group[groupId][1][i][1] + "</td>"
					panstr += "<td><input type=button value='Delete' name=deleteHost id=deleteHost class=btnInGrayS onClick='DelHost(" 
						+ groupId + ", " + i + ");'></td></tr>"
				}
				document.getElementById('hostpanel').innerHTML = panstr;		
			}

			</script>
		</td>
	</tr>
	</tbody>
</table>

</div>
</td>
</tr>
</table>
</div>

<script language="Javascript">
	var d2 = document.formVLAN;
	var d = document.formAddVLAN;
	var i;

	var iter=0;
	var page_length = Sever_Group.length % 10;
	ListGroups(page_length * iter, page_length * (iter + 1));

	function ListGroups(start_index, end_index) {
		var trClass = '';
		var txtClass = '';
		var str;
		var i;

		str = "<table cellpadding=\"0\" cellspacing=\"0\" width=98%><tr><td>&nbsp;</td></tr>";
		str +="<tr><td><strong>Total Entries: " + Sever_Group.length + "</td></tr>";
		str += "<tr class=gh>";
		str += "<td width=80%>Group Name</td>";
		str += "<td width=10%></td>";
		str += "<td width=*%></td>";
		str += "</tr>";


		for (i = start_index; i < end_index; i++)
		{
			if (i % 2 == 0)
			{
				trClass = 'gx';
				txtClass = 'flatGX';
			}
			else
			{
				trClass = 'gw';
				txtClass = 'flatGW';
			}

			str += "<tr class=" + trClass + ">";
			str += "  <td>" + rp_special_char(Sever_Group[i][0]) + "</td>";
			str += "  <td width=10%><input type=button name=Rename id=Rename value='Edit' class=btnInGrayS onClick='EditGroup(" + i + ");'></td>";
			if ((Sever_Group[i][0] == 'radius') || (Sever_Group[i][0] == 'tacacs') 
				|| (Sever_Group[i][0] == 'tacacs+') || (Sever_Group[i][0] == 'xtacacs'))
			{
				str += "  <td width=10%><input type=button value='Delete' disabled name=deleteGroup id=deleteGroup class=btnInGrayS></td>";
			}
			else
			{
				str += "  <td width=10%><input type=button value='Delete' name=deleteGroup id=deleteGroup class=btnInGrayS onClick='DelGroup("+ i +");'></td>";
			}
			str += "</tr>";
		}
		str += "</table>";
		document.getElementById('groupContent').innerHTML = str;
	}

	function checkDuplicateHost(group, ipa, proto) {
		var i;

		for (i = 0; i < Sever_Group.length; i++) {
			if (group == Sever_Group[i][0])
			{
				var j;
				for (j = 0; j < Sever_Group[i][1].length; j++)
				{
					if (ipa == Sever_Group[i][1][j][0])
						if (proto == Sever_Group[i][1][j][1])
						{
							alert("The host already exists");
							return false;
						}
				}
			}
		}

		return true;
	}

	function AddHost() {
		if (!checkTxtInput(document.getElementById("GroupCfg"))) {
			return false;
		}

		if (!ipCheck(document.getElementById("IPAddress"))) {
			return false;
		}

		var group = document.getElementById("GroupCfg").value;
		var ipa = document.getElementById("IPAddress").value;
		var proto = document.getElementById("Protocol").value;

		if (!checkDuplicateHost(group, ipa, proto))
			return false;

		document.getElementsByName("DelHost").value = 0;
		document.getElementById("GroupCfg").disabled = false;
		document.host_add.submit();
	}

	function DelHost(gId, hid) {
		document.getElementById('DelHost').value = 1;
		document.getElementById('GroupCfg').value = Sever_Group[gId][0];
		document.getElementById("IPAddress").value = Sever_Group[gId][1][hid][0];
		document.getElementById("Protocol").value = Sever_Group[gId][1][hid][1];

		document.getElementById("GroupCfg").disabled = false;
		document.host_add.submit();
	}

	function checkDuplicateGroup(groupName) {
		var i;

		for (i = 0; i < Sever_Group.length; i++) {
			if (groupName == Sever_Group[i][0])
			{
				alert("The group '" + groupName + "' already exists");
				return false;
			}
		}

		return true;
	}

	function AddGroupApply() {
		if (!checkTxtInput(document.getElementById("Group"))) {
			return false;
		}

		var groupName = document.getElementById("Group").value;

		if (!checkDuplicateGroup(groupName)) {
			return false;
		}
		
		document.getElementsByName("DelGroup").value = 0;
		document.group_add.submit();
	}

	function DelGroup(gId) {
		document.getElementById('DelGroup').value = 1;
		document.getElementById('Group').value = Sever_Group[gId][0];

		document.group_add.submit();
	}

	function CheckGroup() {
		var i;
		var groupName = document.getElementById('GroupCfg').value;

		for (i = 0; i < Sever_Group.length; i++) {
			
			if (groupName == Sever_Group[i][0]) {
				showHostList(i);
				document.getElementById('IPAddress').disabled=false;
				document.getElementById('Protocol').disabled=false;			
				return;
			}
		}

		alert("The authentication group '" + groupName + "' does not exist");
		document.getElementById('IPAddress').disabled=true;
		document.getElementById('Protocol').disabled=true;
		showHostList(-1);
	}

	function EditGroup(groupId) {
		DisplayGroupPane(groupId);
	}

	function DisplayGroupPane(groupId) {
		ChangeItem(1);
		document.getElementById('GroupCfg').value = Sever_Group[groupId][0];
		document.getElementById('GroupCfg').disabled = true;
		showHostList(groupId);
	}

	function ChangeItem(index)
	{
		if(index==1)
		{
			document.getElementById('group1').style.height="0";
			document.getElementById('group1').style.display="none";
			document.getElementById('group2').style.display="block";
			document.getElementById('group2').style.height="200";
			document.getElementById('groupList1').style.background="url(../jstab/TB_Title_dark_Left.jpg)";
			document.getElementById('groupList2').style.background="url(../jstab/TB_Title_dark.jpg)";
			document.getElementById('groupList3').style.background="url(../jstab/TB_Title_dark_Right.jpg)";
			document.getElementById('EditGroup1').style.background="url(../jstab/TB_Title_light_Left.jpg)";
			document.getElementById('EditGroup2').style.background="url(../jstab/TB_Title_light.jpg)";
			document.getElementById('EditGroup3').style.background="url(../jstab/TB_Title_light_Right.jpg)";	

			document.getElementById('GroupCfg').value="";
			document.getElementById('GroupCfg').disabled = false;
			document.getElementById('IPAddress').value="";
			showHostList(-1);
		}
		else if(index==2)
		{
			document.getElementById('group1').style.height="200";
			document.getElementById('group1').style.display="block";
			document.getElementById('group2').style.height="0";
			document.getElementById('group2').style.display="none";
			document.getElementById('groupList1').style.background="url(../jstab/TB_Title_light_Left.jpg)";
			document.getElementById('groupList2').style.background="url(../jstab/TB_Title_light.jpg)";
			document.getElementById('groupList3').style.background="url(../jstab/TB_Title_light_Right.jpg)";
			document.getElementById('EditGroup1').style.background="url(../jstab/TB_Title_dark_Left.jpg)";
			document.getElementById('EditGroup2').style.background="url(../jstab/TB_Title_dark.jpg)";
			document.getElementById('EditGroup3').style.background="url(../jstab/TB_Title_dark_Right.jpg)";			
		}
	}

</script>

