<script language="Javascript">
	var count = 0;
	document.write("<table class=aclAddRule1 cellpadding=0 cellspacing=0>");
	document.write("<tr><td colspan=2><b>Profile Information</b></td></tr>");
	document.write("<tr><td colspan=2>&nbsp;</td></tr>")
	for(j=0; j<parent.acl_profile_list.length; j++)
	{
		if(parent.acl_profile_list[j][0] == Profileid)
		{
			profileidx = j;
			//alert(profileidx);
			if(parent.acl_profile_list[j][1] == 'Ethernet')
			{
				Profile_Type = 0;
			}
			else if(parent.acl_profile_list[j][1] == 'IP')
			{
				Profile_Type = 1;
			}
			else if(parent.acl_profile_list[j][1] == 'IPv6')
			{
				Profile_Type = 2;
			}
			else
			{
				Profile_Type = 3;
			}
			
			for(i=0; i<parent.acl_profile_list[j].length; i++)
			{
				if(parent.acl_profile_list[j][i] != '')
				{
					document.write("<tr><td width=30%>" + ACLProfile_Name[i]+"</td>");	
					document.write("<td >" + parent.acl_profile_list[j][i] + "</td>");
					document.write("</tr>");
				}
			}
		}
	}
	
	document.write("<tr><td colspan=2><hr align=left name=hr1 id=hr1></td></tr>");
	document.write("</table>");
</script>    

