<script src="../../DataStore/Panel.js" type="text/javascript"></script>
<script src="../../DataStore/Switch.js" type="text/javascript"></script>

<script language="Javascript">
if(window.screen.width == 1280)
{
	document.write("<link rel=\"stylesheet\" href=\"..\/base_1280.css\" type=\"text/css\">");
	document.write("<script src='jsPanel.js'><\/script>");
}
else
{
	document.write("<link rel=\"stylesheet\" href=\"..\/base.css\" type=\"text/css\">");
	document.write("<script src='jsPanel_1024.js'><\/script>");
}
</script>

<body>
<script language="Javascript">
	if(window.screen.width == 1280)
		document.write("<img id='banner' src='..\/Icon/banner1280-no.jpg' />");
	else
		document.write("<img id='banner' src='..\/Icon/banner1024-no.jpg' />");
	
	DrawUnit(1);
function redirect()
{
	top.frames['main'].location.href = "../iss/Serial_Port_Settings.html";
}

function go_dlink()
{
	window.open("http://www.dlink.com.tw", null, "resizable =1, height=600,width=800,toolbar=0,location=0,left=" + (window.screen.width-800)/2 + ",top=" + (window.screen.height-600)/2);
}

	 /* var web_timeout = 0; */
	var web_timeout = SysInfo_Setting[3]*60;	/* changed by King Wang */
	var timer = 0;
	
	function PageTimer()
	{
		if (parent.timer_reset == 1)
		{
			timer = 0;
			parent.timer_reset = 0;
		}

		//top.document.title = "logout: "+ timer +"/"+web_timeout;	/* for debug timeout */
		if (timer >= web_timeout && web_timeout != 0)
		{
			timer = 0;
			parent.location.href = "..\/..\/html\/web_timeout.html";
		}

		timer++;
		window.setTimeout("PageTimer()",1000);
	}

	PageTimer();
	
	</script>
	<iframe name='info_bar' id='info_bar' src='../../html/infobar.html'></iframe>
	<div id="logoLink"><a href="javascript:go_dlink()"><img src="../Icon/transparent.gif" width="140px" height="60px" border=0 alt="Go to D-Link Web Site"/></a></div>
	<script type="text/javascript">
