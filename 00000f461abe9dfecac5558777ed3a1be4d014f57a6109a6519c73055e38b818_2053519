			function formCheck(s)
			{
				if((s.agetime_flag.checked == false) && (s.T1.value != ""))
				{
					   if(!checkNumberInput(s.T1,1,1440)) 
					   {
						  return false;
					   }   
				}


				if((s.idletime_flag.checked == false) && (s.T3.value != ""))
				{				  
					   if(!checkNumberInput(s.T3,1,1440)) 
					   {
						  return false;
					   }   	
				}
				
				if(s.T1.value != "")
				{
					if(!checkNumberInput(s.T4,0,300)) 
					{
					  return false;
					}
				}   
				s.submit();
				return true;
			}
			
			function checkBoxState()
			{
				if(document.form1xAuthSetting.agetime_flag.checked == true)
				{
					document.form1xAuthSetting.T1.disabled = true;
					document.form1xAuthSetting.T1.style.backgroundColor = "#D4D0C8";
				}
				else 
				{
					document.form1xAuthSetting.T1.disabled = false;
					document.form1xAuthSetting.T1.style.backgroundColor = "";
					document.form1xAuthSetting.T1.focus();
				}
				
				if(document.form1xAuthSetting.idletime_flag.checked == true)
				{
					document.form1xAuthSetting.T3.disabled = true;
					document.form1xAuthSetting.T3.style.backgroundColor = "#D4D0C8";
				}
				else
				{
					document.form1xAuthSetting.T3.disabled = false;
					document.form1xAuthSetting.T3.style.backgroundColor = "";
					document.form1xAuthSetting.T3.focus();
				}
					document.form1xAuthSetting.T3.disabled = false;
			}


