    <script language="javascript">
    		function NnuToString_type(value)
    		{
    			if(value == '1')
				return "Ethernet II";
			else if(value == '2')
				return "IEEE802.3 SNAP";
			else if(value == '3')
				return "IEEE802.3 LLC";
			else
				return "-";
    		}
