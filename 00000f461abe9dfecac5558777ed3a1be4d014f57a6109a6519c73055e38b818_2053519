<script language="Javascript">
    var d = document.portset;
function apply_port()
{
	//alert(document.getElementById('state').value);
	d.submit();
}
function portRefresh()
{
	frames['ifrPortConfig'].location.reload();
}

function entry()
{
	var i;
	if(document.getElementById('box7').checked)
	{
		for(i=1;i<=7;i++)
		{
			document.getElementById("box"+i).checked=true;
			document.getElementById("s"+i).value=1;
		}

	}
	else if(!document.getElementById('box7').checked)
	{
		for(i=1;i<7;i++)
		{
			document.getElementById("box"+i).checked=false;
			document.getElementById("s"+i).value=0;
		}
	}
}
function entry1()
{
	for(i=1;i<7;i++)
	{
		if(document.getElementById("box"+i).checked)
		{
			document.getElementById("box7").checked=false;
			document.getElementById("s"+i).value=1;
		}
	}
}
</script>
