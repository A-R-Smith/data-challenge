				document.write("<td width=16%>Unit</td>");
				document.write("<td><select class=portSel name=D3 id=D3>");
				for(i=0; i<Unit_Setting.length; i++)
				{
					document.write("<option value='" + Unit_Setting[i] + "'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" + Unit_Setting[i] + "</option>");
				}
				document.write("</select></td>");
			}
		</script>
		<td width=10%>Port</td>
		<script language="Javascript">
			document.write("<td><select class=portSel name=D1 id=D1 >");
			for(i=0; i<Port_Number.length; i++)
			{
				document.write("<option value='" +Port_Number[i] + "'>&nbsp;&nbsp;&nbsp;" + Port_Number[i] + "</option>");
			}
			document.write("</select></td>");		
			document.write("<td width=16%></td>");
			document.write("<td align=right><a href='53_Packet_Received_table.html?1,"+port_num+"' class=blue>View Table</a></td>");
			document.write("<input type=hidden name='port' id='port' value='"+port_num+"'>");	
		</script>
		<td align=right>
		<input type=button value='Apply' class=btnText onClick="apply();">
		<input type=submit value='Clear' class=btnText onClick="return cleartable();">
		</td>
		</tr>
		<tr><td colspan=5><hr align=left name=hr1 id=hr1 class=hr1></td></tr>
		</table>              
		</form>

		</div>
	</td></tr>
	<tr><td>
	<div id="listBlock">

	<script language="Javascript">
