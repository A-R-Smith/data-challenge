<script language="javaScript">
var new_url='';
var msg='';
var sim_action = document.URL.split("?")[1].split(" ")[0];
function redir()
{
	if(msg.length>0)
		alert(msg);

	if(new_url.length>0)
		location.replace(new_url);

	new_url='SIM_TftpStatus.html?'+sim_action;
}

setTimeout("redir()", 300);
</script>
</html>
