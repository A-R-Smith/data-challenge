<script language="Javascript">
function applyform(f, idx)
{
	if (acl_style == 1)
	{
		if (f.auto.checked == false)
		{
			if (!checkNumberInput(f.access_id, 1, 200))
				return;
		}
	}
	else
	{
		if (!checkNumberInput(f.access_id, 1, 100))
			return;
	}


	if (acl_style == 1)
	{
		if (f.auto.checked == true)
			f.h_auto_flag.value='1';
		else
			f.h_auto_flag.value='0';
		
		if (f.pri_flag.checked == true)
		{
			f.h_pri_flag.value = '1';
			if (!checkNumberInput(f.priority, 0, 7))
				return;
		}
		
		if (f.replace_pri_flag.checked == true)
			f.h_rp_pri_flag.value = '1';

		if (f.no_limit_flag.checked == true)
		{
			f.h_no_limit_flag.value = '1'
		}
		else
		{
			if (f.rx_rate.disabled == false)
			{
				if (!checkNumberInput(f.rx_rate, 1, 15625))
					return;
			}	
		}
		
	}
	if (acl_style == 1)
		{
			if (f.replace_dscp_flag.checked == true)
			{
				f.h_rp_dscp_flag.value = '1';
				if (!checkNumberInput(f.replace_dscp, 0, 63))
					return;
			}
			else
				f.h_rp_dscp_flag.value = '0';
		}
		

	if (f.tr_flag.checked == true)
	{
		f.h_tr_flag.value = '1'
		//if(!checkTxtInput(f.tr))
			//return;
	}

	if (TimeRange.length != 0)
		f.h_tr_name.value = TimeRange[f.tr.value][0];
	else
		f.h_tr_name.value = '';

	if (f.all_port.checked == true)
	{
		f.h_all_port_flag.value = '1';
	}
	else
	{
		f.h_all_port_flag.value = '0';
		
		if (!check_portlist(f.port))
			return;
	}
	
	if(idx == 0)
	{
		var valid_rule = false;

		if (f.vlan_name != undefined && f.vlan_name.value.length != 0)
		{
			if(!checkTxtInput(f.vlan_name))
				return;

			valid_rule = true;
		}

		if (f.s_mac != undefined && f.s_mac.value.length != 0)
		{
			if(!MAC_Address_Check(f.s_mac))
				return;

			valid_rule = true;
		}		

		if (f.d_mac != undefined && f.d_mac.value.length != 0)
		{
			if(!MAC_Address_Check(f.d_mac))
				return;

			valid_rule = true;
		}

		if (f.dot_1p != undefined && f.dot_1p.value.length != 0)
		{
			if (!checkNumberInput(f.dot_1p, 0, 7))
				return;

			valid_rule = true;
		}

		if (f.ether_type != undefined && f.ether_type.value.length != 0)
		{
			if (!checkHexInput(f.ether_type))
				return;

			valid_rule = true;
		}

		if (!valid_rule)
		{
			if (acl_style == 1)
				alert("Invalid ACL Rule!");
			else
				alert("Invalid CPU ACL Rule!");
			return;
		}

		document.aclruleAdd_Ether.submit();
	}
	else if(idx == 1)
	{
		var valid_rule = false;
		
		/*if (acl_style == 1)
		{
			if (f.replace_dscp_flag.checked == true)
			{
				f.h_rp_dscp_flag.value = '1';
				if (!checkNumberInput(f.replace_dscp, 0, 63))
					return;
			}
			else
				f.h_rp_dscp_flag.value = '0';
		}
		*/

		//if (acl_style == 2)
		//{
			if (f.vlan_name != undefined && f.vlan_name.value.length != 0)
			{
				if(!checkTxtInput(f.vlan_name))
					return;

				valid_rule = true;
			}
		//}		

		if (f.s_ip != undefined && f.s_ip.value.length != 0)
		{
			if(!ipCheck(f.s_ip))
				return;

			valid_rule = true;
		}

		if (f.d_ip != undefined && f.d_ip.value.length != 0)
		{
			if(!ipCheck(f.d_ip))
				return;

			valid_rule = true;
		}

		if (f.dscp != undefined && f.dscp.value != '')
		{
			if (!checkNumberInput(f.dscp, 0, 63))
				return;

			valid_rule = true;
		}

		if (f.icmp_type != undefined && f.icmp_type.value != '')
		{
			if (!checkNumberInput(f.icmp_type, 0, 255))
				return;

			valid_rule = true;
		}
		if (f.icmp_code != undefined && f.icmp_code.value != '')
		{
			if (!checkNumberInput(f.icmp_code, 0, 255))
				return;

			valid_rule = true;
		}

		if (f.igmp_type != undefined && f.igmp_type.value != '')
		{
			if (!checkNumberInput(f.igmp_type, 0, 255))
				return;

			valid_rule = true;
		}

		if(f.icmp_flag != undefined && f.icmp_flag.checked == true)
		{
			f.h_icmp_flag.value = '1';
			valid_rule = true;
		}
		else
			f.h_icmp_flag.value = '0';

		if(f.igmp_flag != undefined && f.igmp_flag.checked == true)
		{
			valid_rule = true;
			f.h_igmp_flag.value = '1';
		}
		else
			f.h_igmp_flag.value = '0';

		if(f.tcp_flag != undefined && f.tcp_flag.checked == true)
		{
			valid_rule = true;
			f.h_tcp_flag.value = '1';
		}
		else
			f.h_tcp_flag.value = '0';
			
		if(f.udp_flag != undefined && f.udp_flag.checked == true)
		{
			valid_rule = true;
			f.h_udp_flag.value = '1';
		}
		else
			f.h_udp_flag.value = '0';

		if (f.urg_flag != undefined && f.urg_flag.checked == true)
		{
			f.h_urg_flag.value = '1';
			valid_rule = true;
		}
		if (f.ack_flag != undefined && f.ack_flag.checked == true)
		{
			f.h_ack_flag.value = '1';
			valid_rule = true;
		}
		if (f.psh_flag != undefined && f.psh_flag.checked == true)
		{
			f.h_psh_flag.value = '1';
			valid_rule = true;
		}
		if (f.rst_flag != undefined && f.rst_flag.checked == true)
		{
			f.h_rst_flag.value = '1';
			valid_rule = true;
		}
		if (f.syn_flag != undefined && f.syn_flag.checked == true)
		{
			f.h_syn_flag.value = '1';
			valid_rule = true;
		}
		if (f.fin_flag != undefined && f.fin_flag.checked == true)
		{
			f.h_fin_flag.value = '1';
			valid_rule = true;
		}

		if (f.tcp_s_port != undefined && f.tcp_s_port.value != '')
		{
			if (!checkNumberInput(f.tcp_s_port, 0, 65535))
				return;

			valid_rule = true;
		}
		if (f.tcp_d_port != undefined && f.tcp_d_port.value != '')
		{
			if (!checkNumberInput(f.tcp_d_port, 0, 65535))
				return;

			valid_rule = true;
		}

		if (f.udp_s_port != undefined && f.udp_s_port.value != '')
		{
			if (!checkNumberInput(f.udp_s_port, 0, 65535))
				return;

			valid_rule = true;
		}
		if (f.udp_d_port != undefined && f.udp_d_port.value != '')
		{
			if (!checkNumberInput(f.udp_d_port, 0, 65535))
				return;

			valid_rule = true;
		}

		if (f.pro_id != undefined && f.pro_id.value != '')
		{
			if (!checkNumberInput(f.pro_id, 0, 255))
				return;

			valid_rule = true;
		}
		if (f.user_mask != undefined && f.user_mask.value != '')
		{
			if (f.pro_id.value == '')
			{
				alert("You must specify Protocol ID first before specify User Mask.");
				f.pro_id.focus();
				return;
			}
			
			if (!checkHexInput(f.user_mask))
				return;

			valid_rule = true;
		}

		if (!valid_rule)
		{
			if (acl_style == 1)
				alert("Invalid ACL Rule!");
			else
				alert("Invalid CPU ACL Rule!");
			return;
		}
	
		document.aclruleAdd_IP.submit();
	}
	else if(idx == 2)
	{
		var valid_rule = false;
		
		if (f.ip6_class != undefined && f.ip6_class.value != '')
		{
			if (!checkNumberInput(f.ip6_class, 0, 255))
				return;

			valid_rule = true;
		}

		if (f.flow_label != undefined && f.flow_label.value != '')
		{
			if (!checkHexInput(f.flow_label))
				return;

			valid_rule = true;
		}

		if (f.s_ip6 != undefined && f.s_ip6.value != '')
		{
			if (!ipv6_check(f.s_ip6))
				return;

			valid_rule = true;
		}

		if (f.d_ip6 != undefined && f.d_ip6.value != '')
		{
			if (!ipv6_check(f.d_ip6))
				return;

			valid_rule = true;
		}

		if (!valid_rule)
		{
			if (acl_style == 1)
				alert("Invalid ACL Rule!");
			else
				alert("Invalid CPU ACL Rule!");
			return;
		}

		document.aclruleAdd_IPv6.submit();
	}
	else if(idx == 3)
	{
		var valid_rule = false;
		
		if (acl_style == 1)
		{
			if (f.chunk1_flag.checked == true)
			{
				f.h_chunk1_flag.value = '1';
				if (f.user_mask1.value != '')
				{
					if (!checkHexInput(f.user_mask1))
						return;
				}
				
			}
			if (f.chunk2_flag.checked == true)
			{
				f.h_chunk2_flag.value = '1';
				if (f.user_mask2.value != '')
				{
					if (!checkHexInput(f.user_mask2))
						return;
				}
			}
			if (f.chunk3_flag.checked == true)
			{
				f.h_chunk3_flag.value = '1';
				if (f.user_mask3.value != '')
				{
					if (!checkHexInput(f.user_mask3))
						return;
				}
			}
			if (f.chunk4_flag.checked == true)
			{
				f.h_chunk4_flag.value = '1';
				if (f.user_mask4.value != '')
				{
					if (!checkHexInput(f.user_mask4))
						return;
				}
			}
		}
		else
		{
			if (f.C1 != undefined)
			{
				if (f.C1.checked)
				{
					valid_rule = true;
					f.h_offset1_flag.value = '1';
				}
				else
					f.h_offset1_flag.value = '0';
			}

			if (f.C6 != undefined)
			{
				if (f.C6.checked)
				{
					valid_rule = true;
					f.h_offset2_flag.value = '1';
				}
				else
					f.h_offset2_flag.value = '0';
			}

			if (f.C11 != undefined)
			{
				if (f.C11.checked)
				{
					valid_rule = true;
					f.h_offset3_flag.value = '1';
				}
				else
					f.h_offset3_flag.value = '0';
			}

			if (f.C16 != undefined)
			{
				if (f.C16.checked)
				{
					valid_rule = true;
					f.h_offset4_flag.value = '1';
				}
				else
					f.h_offset4_flag.value = '0';
			}

			if (f.C21 != undefined)
			{
				if (f.C21.checked)
				{
					valid_rule = true;
					f.h_offset5_flag.value = '1';
				}
				else
					f.h_offset5_flag.value = '0';
			}

			if (!valid_rule)
			{
				if (acl_style == 1)
					alert("Invalid ACL Rule!");
				else
					alert("Invalid CPU ACL Rule!");
				return;
			}

			if (f.C1 != undefined && f.C1.checked == true)
			{
				if (!checkHexInput(f.C2))
					return false;	
				if (!checkHexInput(f.C3))
					return false;	
				if (!checkHexInput(f.C4))
					return false;	
				if (!checkHexInput(f.C5))
					return false;
			}

			if (f.C6 != undefined && f.C6.checked == true)
			{
				if (!checkHexInput(f.C7))
					return false;
				if (!checkHexInput(f.C8))
					return false;
				if (!checkHexInput(f.C9))
					return false;
				if (!checkHexInput(f.C10))
					return false;
			}

			if (f.C11 != undefined && f.C11.checked == true)
			{
				if (!checkHexInput(f.C12))
					return false;
				if (!checkHexInput(f.C13))
					return false;
				if (!checkHexInput(f.C14))
					return false;
				if (!checkHexInput(f.C15))
					return false;
			}

			if (f.C16 != undefined && f.C16.checked == true)
			{
				if (!checkHexInput(f.C17))
					return false;
				if (!checkHexInput(f.C18))
					return false;
				if (!checkHexInput(f.C19))
					return false;
				if (!checkHexInput(f.C20))
					return false;
			}

			if (f.C21 != undefined && f.C21.checked == true)
			{
				if (!checkHexInput(f.C22))
					return false;
				if (!checkHexInput(f.C23))
					return false;
				if (!checkHexInput(f.C24))
					return false;
				if (!checkHexInput(f.C25))
					return false;
			}
		}
		document.aclruleAdd_Packet_Content.submit();
	}
}
    
function AddRule()
{
	var obj=addruleindex;
	var a_url="52_Add_Access_Rule.html?"+obj.value;
	Link(a_url);
}
</script>
