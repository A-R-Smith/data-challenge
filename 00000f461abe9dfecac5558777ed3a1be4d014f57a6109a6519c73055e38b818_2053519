function selIPV6SrvType(index)
{
	if (1 == index)
	{
		if(document.formACLWizard.TCPSrcPortMask.value.length !=0)
		{
			document.formACLWizard.T100.disabled=true;
			document.formACLWizard.T100.style.backgroundColor='#D4D0C8';
			document.formACLWizard.T200.disabled=true;
			document.formACLWizard.T200.style.backgroundColor='#D4D0C8';
		}
		else
		{
			document.formACLWizard.T100.disabled=false;
			document.formACLWizard.T100.style.backgroundColor='#FFFFFF';
			document.formACLWizard.T200.disabled=false;
			document.formACLWizard.T200.style.backgroundColor='#FFFFFF';
		}
	}
	else if (2 == index)
	{
		if(document.formACLWizard.TCPDesPortMask.value.length !=0)
		{
			document.formACLWizard.T100.disabled=true;
			document.formACLWizard.T100.style.backgroundColor='#D4D0C8';
			document.formACLWizard.T200.disabled=true;
			document.formACLWizard.T200.style.backgroundColor='#D4D0C8';
		}
		else
		{
			document.formACLWizard.T100.disabled=false;
			document.formACLWizard.T100.style.backgroundColor='#FFFFFF';
			document.formACLWizard.T200.disabled=false;
			document.formACLWizard.T200.style.backgroundColor='#FFFFFF';
		}
	}
	if (3 == index)
	{
		if(document.formACLWizard.UDPSrcPortMask.value.length !=0)
		{
			document.formACLWizard.T100.disabled=true;
			document.formACLWizard.T100.style.backgroundColor='#D4D0C8';
			document.formACLWizard.T200.disabled=true;
			document.formACLWizard.T200.style.backgroundColor='#D4D0C8';
		}
		else
		{
			document.formACLWizard.T100.disabled=false;
			document.formACLWizard.T100.style.backgroundColor='#FFFFFF';
			document.formACLWizard.T200.disabled=false;
			document.formACLWizard.T200.style.backgroundColor='#FFFFFF';
		}
	}
	if (4 == index)
	{
		if(document.formACLWizard.UDPDesPortMask.value.length !=0)
		{
			document.formACLWizard.T100.disabled=true;
			document.formACLWizard.T100.style.backgroundColor='#D4D0C8';
			document.formACLWizard.T200.disabled=true;
			document.formACLWizard.T200.style.backgroundColor='#D4D0C8';
		}
		else
		{
			document.formACLWizard.T100.disabled=false;
			document.formACLWizard.T100.style.backgroundColor='#FFFFFF';
			document.formACLWizard.T200.disabled=false;
			document.formACLWizard.T200.style.backgroundColor='#FFFFFF';
		}
	}

	if(document.getElementById("profile_acl").style.display != "none")
	{
		document.getElementById("profile").maxLength = "3";
	}
	else if(document.getElementById("profile_cpu").style.display != "none")
	{
		document.getElementById("profile").maxLength = "1";
	}

	if(document.getElementById("acc_acl").style.display != "none")
	{
		document.getElementById("access").maxLength = "3";
	}
	else if(document.getElementById("acc_cpu").style.display != "none")
	{
		document.getElementById("access").maxLength = "3";
	}
}
</script>
<script src="../checkperm.js" type="text/javascript"></script>
</body>
