<script language="Javascript">
	var d = document.formTrunking;
	tmp_portspan="";
	global_index="";
	var newval;
	var Trunk_Setting_24_tmp=new Array();
	var Trunk_Setting_48_tmp=new Array();
	var count_array=new Array();
	count_array=[0,0,0,0,0,0,0,0];
  
	function getTrunkString(portMember){
			
			var portValue = '';
			var firtTime = true;
			var iCounter = 0;	
			var k,j;
			//if (Stacking_Mode=='0')
			//	portMember.length=1;
			//for(k=0; k<portMember.length; k++) {
			/*	for(j=1; j<=portMember[k].length; j++){
					if (portMember[k].substring(j-1, j) == '1'){
						iCounter++;
						if(firtTime == false)
							portValue += ', ';
						firtTime = false;
						if (Stacking_Mode=='1')
						{
						portValue += '' + (k+1) + ":" + j;
						}
						else
						{
							portValue += '' + j;
						}
					}
				}	
			}*/
			
			//return portValue;
			return getTrunkPortNumber_withComma(portMember);
	}

	function ListPorts(index) {
		var str = '';
		var gid
		gid = parseInt(document.getElementById('GID').value, 10);

		//str += getTrunkString(Trunk_Setting[index][3]);
		str += getTrunkString(newval);
		document.getElementById('portSpan').value = str;		
	}

	function showPorts(index) 
	{
		var trunkidx;
		var unitid;
		var trunkname;
		var i;
		var gid;
		var count=0;

		if(parseInt(document.getElementById('GID').value)> 32)
		{
			for(i=1;i<=parent.port_number;i++)
			{
				document.formTrunking["C"+i].checked=false;
			}
			document.formTrunking['GID'].value=''; 
			return;
		}
		
		if(document.getElementById('GID').value =='')
		{
			alert("Group ID is empty");
			for(i=1;i<=parent.port_number;i++)
			{
				document.formTrunking["C"+i].checked=false;
			}  
			return;
		}

		trunkidx = parseInt(document.getElementById('GID').value, 10);

		if (Stacking_Mode == '1'){
			unitid = parseInt(document.getElementById('editunit').value, 10);
		}
		else
			unitid =1;
		
		newval = '';
		for(i=1; i<=parent.port_number; i++) {
			var radioobj;
			
			radioobj = document.getElementById('C'+i);
				if(radioobj.checked) 
				{
					newval += '1';
					count += 1;
					if(count > 8)
					{
						alert("Maximum 8 ports in a static or LACP group!");
						document.getElementById('C'+index).checked = false;
						return;
					}
				}
				else newval += '0';
		}

/*		Trunk_Setting[trunkidx-1][3] = newval;
		if(document.getElementById('GType').options[0].selected == true)
		{
			Trunk_Setting[trunkidx-1][2] = 1;
		}
		else
		{
			Trunk_Setting[trunkidx-1][2] = 0;
		}
*/ 
		ListPorts(trunkidx-1);
		checkbox_f(index);
	}
	

function ListTrunkPorts() {
	var i, str;
		
	str = "<table cellpadding=0 cellspacing=0 border=0 width=100% >";
	for (i=0; i<Trunk_Setting.length; i++){
		if (i%2 == 0)
			trClass = 'gx';
		else
			trClass = 'gw';
		
		str += "<tr class=" + trClass + ">";
		str += "  <td width=4%>" + Trunk_Setting[i][0] + "</td>"; 
		str += "  <td width=4%>" + getTrunkType(Trunk_Setting[i][2]) + "</td>";
		str += "   <td width=8% align=center>" + floodPortType(Trunk_Setting[i][3])  + "</td>"; 
		str += "  <td width=20% align=center>" + getTrunkPortNumber_withComma(Trunk_Setting[i][4]) + "</td>";
		str += "  <td width=12% align=center>" + getTrunkPortNumber_withComma(Trunk_Setting[i][1]) + "</td>";		
		str += "   <td width=10% align=center>" + floodPortType(Trunk_Setting[i][5])  + "</td>"; 
		str += "   <td width=8% align=center>" + getStatusType(Trunk_Setting[i][6])  + "</td>"; 
		str += "  <td width=8% align=right><input type=button name=modify id=modify value='Edit' class=btnInGrayS onClick='EditTrunkPerUnit(1," + i + ");'/></td>";
		str += "  <td width=8% align=right><input type=button value='Delete' class=btnInGrayS onClick='clearports(\""+ Trunk_Setting[i][0] +"\");'></td>";
		str += "</tr>";
	}
	str += "</table>";
	document.getElementById('TrunkContentPane').innerHTML = str; 
}

	function editchangeunit(unitid) {
		var uid = parseInt(unitid);
		var curr_gid = parseInt(document.getElementById('GID').value);
		var i;		
		
	  i=curr_gid-1;
		DisplayTrunkEditPane(uid, i, 1);
		return 1;
	}

	function GetTrunkIdxFromGid(gid) {
		var i;
		for(i=0; i<Trunk_Setting.length; i++) {
			if(Trunk_Setting[i][0] == gid)
				break;
		}
		if (i==Trunk_Setting.length) return -1;
		return i;
	}

	function CommitPanel() {

		/*if(document.getElementById('GID').value.length <= 0) return;
		trunkidx = GetTrunkIdxFromGid(document.getElementById('GID').value);
		if ()

		newval = '';
		for(i=1; i<=parent.port_number; i++) {
			var radioobj;
			radioobj = document.getElementById('C'+i);
				if(radioobj.checked) newval += '1';
				else newval += '0';
		}
		if (Stacking_Mode =='1')
			unitid = parseInt(document.getElementById('editunit').value);
		else
			unitid = 1;
		gid = parseInt(document.getElementById('GID').value); 
          	Trunk_Setting[trunkidx][3][unitid-1] = newval;
		ListTrunkPorts();
		document.getElementsByName("GID")[0].readOnly=false;
    	
		if(parent.simulation==0){
			document.getElementsByName("isClear").value = 0;
			document.formTrunking.submit();
		}
    	
		settodefault();
		return false;*/
			
			if(Panel_Info[4]==137)		/* DGS3200-24 */
			{
				if(!checkNumberInput(document.formTrunking.GID, 1, 12))
					return false;
			}
			else if(Panel_Info[4]==136)	/* DGS3200-16 */
			{
				if(!checkNumberInput(document.formTrunking.GID, 1, 8))
					return false;
			}
			else						/* DGS3200-10 */
			{
				if(!checkNumberInput(document.formTrunking.GID, 1, 5))
					return false;
			}
		

			document.formTrunking.GType.disabled = false;
			document.formTrunking.Mport.disabled = false;
			document.formTrunking.submit();
	}
	function PressCancel() 
	{
		settodefault();
	}

	function apply() 
	{
		document.formAlgm.submit();
	}
	
	function EditTrunkPerUnit(unitid,trunkidx) {
		  document.formTrunking.GID.readOnly=true;
		  document.formTrunking.GType.disabled=true;
		  document.formTrunking.Mport.disabled=true;
		   document.formTrunking.operation.value = 2;	/* edit */
		  document.formTrunking.commitpanel.value="Apply";
		  DisplayTrunkEditPane(unitid,trunkidx,700);

	}

	function DisplayTrunkEditPane(unitid,trunkidx,duration) {
		var i;
		var uid = unitid;
		var gid;
		var mport_option = parseInt(Trunk_Setting[trunkidx][3]-1,10);
		if(trunkidx < Trunk_Setting.length)
		{
			document.getElementById('GID').value = Trunk_Setting[trunkidx][0];
			document.getElementById('TrunkNameInput').value = Trunk_Setting[trunkidx][1];
			if(Trunk_Setting[trunkidx][2] == '0')
				document.getElementById('GType').options[1].selected = true;
			else
				document.getElementById('GType').options[0].selected = true;
//			document.getElementById('MPort').value = Trunk_Setting[trunkidx][3];
			if (Trunk_Setting[trunkidx][3] == '0')
				document.formTrunking.MPort.options[0].selected = true;
			else
				document.formTrunking.MPort.options[mport_option].selected = true;
			if(Trunk_Setting[trunkidx][6] == '0')
				document.formTrunking.State.options[1].selected = true;
			else
				document.formTrunking.State.options[0].selected = true;
		}
	
		for(i=1;i<=parent.port_number;i++)
		{
			var radioobj;
			radioobj = document.getElementById('C'+i);
			if (Trunk_Setting[trunkidx][4].substring(i-1,i) == '0') 
			{
				radioobj.checked = false;
			} else {
				radioobj.checked = true;
			}
				
		}
		showPorts(global_index);
	}
	
	function ChkAll(){
		var i;
		for(i=1; i<=parent.port_number; i++) {
			document.getElementById('C'+i).checked = true;
		}
	}

	
	function chkDuplicate(sel_J){
		k=0;
		
		for(x=0; x<6; x++)
			if(d.chkPort[sel_J + x*24].checked == true) k=k+1;
		
		if(k>1)
			return 1;
		else
			return 0;
	}

	function chkEight(sel_I){
		k=0;
		for(y=0; y<24; y++ )
			if(d.chkPort[sel_I*24 + y].checked == true) k=k+1;
		
		if(k>8)
			return 1;
		else
			return 0;
	}
	
	function chkIt(iObj, sel_I, sel_J){
		if(chkDuplicate(sel_J)>0 || chkEight(sel_I)>0){
			iObj.checked = false;
		}
		else{
			if(iObj.checked == true)
				iObj.checked = true;
			else
				iObj.checked = false;
		}
	}
	function checkbox_f(index)
	{
	  var count=0,sum=0;
	  var unitid,i;
	  if(Stacking_Mode=='1')
			unitid = parseInt(document.getElementById('editunit').value);
	  else
			unitid = 1;
	  for(i=1;i<parent.port_number;i++)
	  {
		flag=document.formTrunking["C"+i].checked;
		if(flag)
		{ sum++;}
	  }
	  if(unitid==1)
	  {
		count_array[0]=sum;
	  }
	  else if(unitid==2)
	  {
		count_array[1]=sum;
	  }
	  else if(unitid==3)
	  {
		count_array[2]=sum;
	  }
	  else if(unitid==4)
	  {
		count_array[3]=sum;
	  }
	  else if(unitid==5)
	  {
		count_array[4]=sum;
	  }
	  else
	  {
		count_array[5]=sum;
	  }
	  for(i=0;i<6;i++)
	  {
		count+=count_array[i];
	  
	  }
	 
	  if(document.formTrunking.GType.value=='Static' || document.formTrunking.GType.value=='0' )
	  { 
		if(count==8)
		{
		  tmp_portspan=document.formTrunking.portSpan.value;
		}
		if(count >8)
		{ 
		  global_index=index;
		  document.formTrunking["C"+index].checked=false;
		  document.formTrunking.portSpan.value=tmp_portspan;
		}
	  }
	  else
	  {
		if(count==16)
		{
		  tmp_portspan=document.formTrunking.portSpan.value;
		}
		if(count >16)
		{ 
		  global_index=index;
		  document.formTrunking["C"+index].checked=false;
		  document.formTrunking.portSpan.value=tmp_portspan;
		}
	  }
	}
	
	function settodefault()
	{
		//document.formTrunking.editunit.value='01';
		document.formTrunking.GID.value='';
		document.formTrunking.GID.readOnly=false;
		document.formTrunking.operation.value='0';	/* create */
		document.formTrunking.GType.disabled= false;
		document.formTrunking.GType.value= '0';
		document.formTrunking.MPort.disabled= false;
		document.formTrunking.MPort.options[0].selected= true;		
		document.formTrunking.State.value= '0';	
		document.formTrunking.portSpan.value=''; 
		document.formTrunking.commitpanel.value="Add";
		for(i=1;i<=parent.port_number;i++)
		{
		  document.formTrunking["C"+i].checked=false;
		}
		count_array=[0,0,0,0,0,0,0,0];

	}
	
	function clearports(iTag)
	{
		document.formTrunking.GID.value = iTag;
		document.formTrunking.operation.value = 1;	/* del */
		document.formTrunking.submit();
	}
	
//	ListTrunkPorts();
</script>

