<script language="javascript">
function apply()
{
	if (!checkNumberInput(document.policy_set.times, 0, 255))
		return;

	if (!checkNumberInput(document.policy_set.Attempts, 1, 255))
		return;

	document.policy_set.submit();
}
</script>

