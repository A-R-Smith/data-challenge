<script language="Javascript">
function DisplayStart(iTag)
{
	location.href = "28_STPInstance_Table.html?" + iTag;
}

function init()
{
	var i;

	var index = document.URL.split("?")[1];
	if (index != undefined)
	{
		for (i = 0; i <= 11; i++)
		{
			eval("document.formSTP_Instance.E" + i + ".value=STP_Operation_Status[index][" + i + "];");
			eval("document.formSTP_Instance.E" + i + ".readOnly = true");		
		}
	}
}
init();

parent.instance_count = STP_Instance_Setting.length;
parent.show_counter();
</script>
<script src="../../DataStore/error_msg.js" type="text/javascript"></script>
