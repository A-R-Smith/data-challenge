<script>
function show_prev()
{
	location.href = '54_Browse_Router_Port.html?1,0';
}

function show_next()
{
	location.href = '54_Browse_Router_Port.html?2,0';
}

function intial()
{
	var f = document.formIGMPSNPRouter;
	if (Browse_IGMP_SNP_RT[5] == 3)
	{
		f.prev.disabled = false;
		f.next.disabled = false;
	}
	else if (Browse_IGMP_SNP_RT[5] == 1)
		f.prev.disabled = false;
	else if (Browse_IGMP_SNP_RT[5] == 2)
		f.next.disabled = false;
}
intial();
</script>

