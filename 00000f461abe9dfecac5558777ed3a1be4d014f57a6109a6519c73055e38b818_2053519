        function genTxTable(titleName, tableType){
            document.write("<div name=" + tableType + " id=" + tableType + " class=" + tableType + "><table>");
            document.write("    <tr><div><td class=tdSecTitle valign=top>&nbsp;&nbsp;&nbsp;<font style='position: relative; top:1;'>" + titleName + "</font>");
            document.write("        <img name=imgTx id=imgTx class=imgTx src='../Icon/darkblue.jpg'></td></div></tr>");
            document.write("</table></div>");
        }
        function genRxTable(titleName, tableType){
            document.write("<div name=" + tableType + " id=" + tableType + " class=" + tableType + "><table>");
            document.write("    <tr><div><td class=tdSecTitle valign=top>&nbsp;&nbsp;&nbsp;<font style='position: relative; top:1;'>" + titleName + "</font>");
            document.write("        <img name=imgRx id=imgRx class=imgRx src='../Icon/darkblue.jpg'></td></div></tr>");
            document.write("</table></div>");
        }
    </script>
