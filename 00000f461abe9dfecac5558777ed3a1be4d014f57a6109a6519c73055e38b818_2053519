	<script language="javascript">
	function DelIt(iTag,index)
	{
		document.formDHCPDel[iTag].server.value = DHCP_Relay_Table[iTag][index];
		document.formDHCPDel[iTag].submit();
	}
	function checkForm(s)
	{
		if(!ipCheck(s.server))
			return false;
		if(!checkTxtInput(s.IPIFname))
			return false;
		s.submit();
		return true;
	}
	</script>
