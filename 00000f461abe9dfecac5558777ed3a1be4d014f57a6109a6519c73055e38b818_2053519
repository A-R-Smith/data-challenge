<script language="Javascript">
	function err()
	{
		unauth();
		return true;
	}
	function unauth()
	{
		alert("Unauthorized action!");
		location.href="/";
	}

	function urgy()
	{
		if((self==top) && (location.href.indexOf("/RH0")!=-1))
		{
			onerror=err;
			if(window.opener==null)
			{
				unauth();
			}
			else
			{
				if(!window.opener.closed)
				{
				  if(window.opener.parent.parent.urgy==null)
				  {
					unauth();
				  }
				}
				else
				{
					unauth();
				}
			}
			onerror=null;
		}
	}
	urgy();

	if(window.screen.width == 1280){
		document.write("<tr><td height=\"139px\">");
	}else{
		document.write("<tr><td height=\"111px\">");
	}
</script>

