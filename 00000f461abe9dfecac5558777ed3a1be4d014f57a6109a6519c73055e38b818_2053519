<script language="JavaScript">
function ApplyRec(s, action)
{
	if (s.all.checked == false)
	{
		if (!check_portlist(s.portlist))
		{
			return false;
		}
	}
	s.operation.value = action;
	s.submit();
}
function checkAllPorts(s)
{
	if (s.all.checked == true)
	{
		s.all.value = 1;
		s.portlist.style.backgroundColor = "#D4D0C8";
		s.portlist.readOnly = true;
	}
	else
	{
		s.all.value = 0;
		s.portlist.style.backgroundColor = "#FFFFFF";
		s.portlist.readOnly = false;
	}
}
function Apply()
{
	document.ADDRBIND_DHCP.submit();
}

</script>

