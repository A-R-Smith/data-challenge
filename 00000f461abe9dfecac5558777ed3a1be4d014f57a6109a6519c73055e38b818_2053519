	<script language="Javascript">
		parent.timer_reset=1;
		
		function cfgTrunk()
		{
			if(document.all["radEnable"].checked == true)
			{
				document.all["hidCfgTrunk"].value = "1";
			}
			else
			{
				document.all["hidCfgTrunk"].value = "0";
			}

			document.all["frmVlanTrunk_GlbState"].submit();
		}
		
		function initPortsPanl(strPorts)
		{
			var start=-1,end=-1;
			var str="";
			for(k=1;k<=Port_Number.length;k++)
			{
				document.getElementById("C"+k).checked = false;
			}
			for(k=1;k<=strPorts.length;k++)
			{
				if (','==strPorts.substring(k-1, k))
				{
					end = parseInt(str);
					if(-1==start)
					{
						start = end;
					}
					for(n=start;n<=end;n++)
					{
						document.getElementById("C"+n).checked = true;
					}
					start = -1;
					end = -1;
					str = '';
				}
				else if('-'==strPorts.substring(k-1, k))
				{
					start = parseInt(str);
					str = '';
				}
				else
				{
					str+=strPorts.substring(k-1, k);
				}
			}//end for
			
			end = parseInt(str);
			if(-1==start)
			{
				start = end;
			}
			for(n=start;n<=end;n++)
			{
				document.getElementById("C"+n).checked = true;
			}
			start = -1;
			end = -1;
			str = "";
		}

		function showPorts(index)
		{
			var strPorts=""
			for(k=1;k<=Port_Number.length;k++)
			{
				if(false==document.getElementById("C"+k).checked)
				{
					continue;
				}
				
				if(""==strPorts)
				{
					strPorts+=k;
				}
				else
				{
					if(false==document.getElementById("C"+(k-1)).checked)
					{
						strPorts+=","+k;
					}
					else if((k+1>Port_Number.length)||(false==document.getElementById("C"+(k+1)).checked))
					{
						strPorts+="-"+k;
					}
				}
			}
			document.getElementById("boxPorts").value = strPorts;
		}
	
		function cfgPortTrunk()
		{
			document.all["frmVlanTrunk_State"].submit();
		}
		
		function pageLoad()
		{
			/* set golbal state */
			if("Enabled"==VLANTrunk[0])
			{
				document.getElementById("radEnable").checked = true;
				document.getElementById("radDisable").checked = false;
				initPortsPanl(VLANTrunk[1]);
			}
			else
			{
				document.getElementById("radEnable").checked = false;
				document.getElementById("radDisable").checked = true;
				initPortsPanl(VLANTrunk[1]);
			}
		}
	</script>
</head>
