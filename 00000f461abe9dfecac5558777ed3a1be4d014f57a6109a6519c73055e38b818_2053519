	<tr>
    <script language="Javascript">
	if (Stacking_Mode == '1')
	{
		document.write("<td><select class=portSel style=width:100; name=D3 id=D3>");
		for(i=0; i<Unit_Setting.length; i++)
		{
            		document.write("<option value='" + Unit_Setting[i] + "'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" + Unit_Setting[i] + "</option>");
			document.write("</select></td>");
        	}
	}
        document.write("<td><select class=portSel style=width:110; name=D1 id=D1 onchange='chgToPortIndex(this, document.formCTP_Port.D2);'>");
        for(i=0; i<Port_Number.length; i++){
            document.write("<option value='" + Port_Number[i] + "'>" + Port_Number[i] + "</option>");
        }
        document.write("</select></td>");
