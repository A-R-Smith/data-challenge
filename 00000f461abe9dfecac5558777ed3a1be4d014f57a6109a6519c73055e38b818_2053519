<body class=body228>
<form name=formMSTI id=formMSTI>
    <table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" width=100%>
        <tr class=gh>
            <td width=22% align=center>Method List Name</td>
            <td width=12%>Method 1</td>
            <td width=12%>Method 2</td>
            <td width=12%>Method 3</td>
            <td width=12%>Method 4</td>
            <td width=10%></td>
            <td width=10%></td>
        </tr>
        <!--tr><td class=blankbar>&nbsp;</td></tr-->
<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
    
    for(i=0; i<Login_Method_Lists.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGW';
        }
        
        document.write("<tr class=" + trClass + ">");
        document.write("  <td align=center>" + Login_Method_Lists[i][0] + "</td>");
        document.write("  <td><input type=text name=Protocol id=Protocol class=" + txtClass + " value='" + Login_Method_Lists[i][1] + "'>");
        document.write("      <input type=text name=Protocol_M id=Protocol_M class=flatWS value='" + Login_Method_Lists[i][1] + "' style='display: none;'></td>");
        document.write("  <td><input type=text name=Port id=Port class=" + txtClass + " value='" + Login_Method_Lists[i][2] + "'>");
        document.write("      <input type=text name=Port_M id=Port_M class=flatWS value='" + Login_Method_Lists[i][2] + "' style='display: none;'></td>");
        document.write("  <td><input type=text name=Timeout id=Timeout class=" + txtClass + " value='" + Login_Method_Lists[i][3] + "'>");
        document.write("      <input type=text name=Timeout_M id=Timeout_M class=flatWS value='" + Login_Method_Lists[i][3] + "' style='display: none;'></td>");
        document.write("  <td><input type=text name=Retransmit id=Retransmit class=" + txtClass + " value='" + Login_Method_Lists[i][4] + "'>");
        document.write("      <input type=text name=Retransmit_M id=Retransmit_M class=flatWS value='" + Login_Method_Lists[i][4] + "' style='display: none;'></td>");
       	document.write("  <td><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'>");
      	document.write("      <input type=button name=editApply id=editApply value='Apply' class=btnInGrayS onClick='EditApplyIt(" + i + ");' style='display: none;'></td>");
        document.write("  <td><input type=button value='Delete' class=btnInGrayS onClick='delit();'></td>");
        document.write("</tr>");
    }
</script>
    </table>
