<script language="Javascript">
function apply_global()
{
	var f = document.formSMTPGlobal;
	
	if (!ipCheck(f.srv_addr))
		return;
	
	if (!checkNumberInput(f.srv_port, 1, 65535))
		return;

	if (f.mail_addr.value != '')
	{
		if (!checkTxtInput(f.mail_addr))
			return;
	}

	f.submit();
}

function add_receiver()
{
	var f = document.formSMTPList;

	if (!checkTxtInput(f.mail_receiver))
		return false;

	f.isDel.value = '0';
	f.submit();
}

function delit(iTag)
{
	var f = document.formSMTPList;
	f.isDel.value = '1';
	f.index.value = iTag;
	f.submit();
}

document.formSMTPGlobal.mail_addr.value = SMTP_Global[1]; /* changed for PI-194, 20090303*/

</script>
</div>
