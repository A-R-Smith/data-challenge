<script language="JavaScript">
var all_finished = false;
function redir()
{
	var new_url;

	switch (function_id)
	{
		case 1:
			new_url = '80_SIM_List.html?1';
			break;
		case 2:
		case 3:
			new_url = '80_SIM_List.html?2';
			break;
		case 4:
			new_url = '80_SIM_List.html?3';
			break;
	}
	alert('Finish.');
	location.replace(new_url);
}

for(i=0; i<MS_TFTP_Info.length; i++)
{
	if (MS_TFTP_Info[i][7] != '1')
		break;
	
	all_finished = true;
}

if(all_finished)
	setTimeout("redir()", 300);
else
	setTimeout("location.reload()", 2000);
</script>

