	<tr><td height="10px">

    <script language="Javascript">
        genHeaderTable('SNMP User Table');
    </script>

</td></tr>
<tr><td height="10px">
<div id="selectBlock">
<b>Add User</b>
<form name=formAddVLAN id=formAddVLAN method=post action="../../form/SNMP_User_Table_Apply">
<input type=hidden name='isDel' id='isDel' value='0'>
<input type=hidden name='del_name' id='del_name' value=''>

<input type=hidden name='auth_pro' id='auth_pro' value='0'>
<input type=hidden name='priv_pro' id='priv_pro' value='0'>
<input type=hidden name='auth_code' id='auth_code' value=''>
<input type=hidden name='priv_code' id='priv_code' value=''>
    
    <table name=tabSNMPViewTableContent id=tabSNMPViewTableContent class=tabPortContent border=0 width=98%>

        <tr><td width=22%>User Name</td>
          <td width=30%><input class=flatL type="text" name=UName id=UName maxlength=32 style="width:180"></td>
