<script language="Javascript">
function checkPara()
{
	var s = document.formIGMPSNP_Para;

//Query Interval
	if (!checkNumberInput(s.query, 1, 65535))
		return false;

//Max Respones Time
	if (!checkNumberInput(s.maxrp, 1, 25))
		return false;
	
//Robustness
	if (!checkNumberInput(s.robus, 1, 255))
		return false;

//Last Member Query Interval
	if (!checkNumberInput(s.LastMember, 1, 25))
		return false;

//Host Timeout
	if (!checkNumberInput(s.hostTimeout, 1, 16711450))
		return false;

//Route Timeout
	if (!checkNumberInput(s.routeTimeout, 1, 16711450))
		return false;

//Leave Timer
	if (!checkNumberInput(s.leaveTime, 1, 16711450))
		return false;
	
	s.submit();
}

function applyform()
{
	if(parent.simulation==1)
	{
		location.href='23_IGMP_Snooping.html';	
	}
	else
	{
		document.formIGMPRouter_Port.submit();
	}
}
					
function showPorts(index)
{
	var unitid;
	var i;
	var newval;

	var checkboxobj;

	if (Stacking_Mode == '1')
		unitid = parseInt(document.getElementById('D3').value);

	newval = '';
	for(i=1; i<=parent.port_number; i++)
	{
		checkboxobj = document.getElementById('C'+i);
		if(checkboxobj.checked)	 
			newval += '1';
		else
			newval += '0';
	}

	for(i=1;i<=parent.port_number;i++)
	{
		checkboxobj = document.getElementsByName('C'+i);
	}

	IGMP_SNP_Router_Port[2][0] = newval;

	ListPorts();
}

function showForbiddenPorts(index)
{
	var unitid;
	var i;
	var newval;

	var checkboxobj;

	if (Stacking_Mode == '1')
		unitid = parseInt(document.getElementById('D3').value);

	newval = '';
	for(i=1; i<=parent.port_number; i++)
	{
		checkboxobj = document.getElementById('F'+i);
		if(checkboxobj.checked)	 
			newval += '1';
		else
			newval += '0';
	}

	for(i=1;i<=parent.port_number;i++)
	{
		checkboxobj = document.getElementsByName('F'+i);
	}

	IGMP_SNP_Router_Port[3][0] = newval;

	ListForbiddenPorts();
}

function getString(sourceport,typechar)
{
	var portValue = '';
	var firtTime = true;
	var iCounter = 0;	
	var k;
	for(k=0; k<sourceport.length; k++)
	{
		for(j=1; j<=sourceport[k].length; j++)
		{
			if (sourceport[k].substring(j-1, j) == typechar)
			{
				iCounter++;

				firtTime = false;
				if (Stacking_Mode == '1')
					portValue += (k+1) + ":" + j +",";
				else
					portValue += j +",";
			}
		}	
	}
	
	return portValue;
}

function ListPorts()
{
	var str = '';
	str += getString(IGMP_SNP_Router_Port[2],'1');
	document.getElementById('static_router_port').value = str;
}

function ListForbiddenPorts()
{
	var str = '';
	str += getString(IGMP_SNP_Router_Port[3],'1');
	document.getElementById('forbidden_router_port').value = str;
}

function ListDynamicPorts()
{
	var str = '';
	str += getString(IGMP_SNP_Router_Port[4],'1');
	document.getElementById('dynamic_port').value = str;
}

function check_trunk()
{
	var i, j;

	for (i = 0; i < Trunk_Setting.length; i++)
	{
		if (Trunk_Setting[i][6] == '1')
		{
			for (j = 1; j <= parent.port_number; j++)
			{
				if (Trunk_Setting[i][4].substring(j-1, j) == '1' && Trunk_Setting[i][3] != j)
				{
					document.getElementById('C' + j).disabled = true;
					document.getElementById('F' + j).disabled = true;
				}
			}
		}
	}
}

ListPorts();
ListForbiddenPorts();
ListDynamicPorts();
check_trunk();
</script>
