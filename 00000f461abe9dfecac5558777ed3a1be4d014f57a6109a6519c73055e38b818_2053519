<script language="Javascript">
    
function applyform()
{
	document.getElementById("statetype").value =0;
	if(parent.simulation==1)
	{
		alert("OK");
	}
	else
	{
		if (!checkNumberInput(document.getElementById("max_age"), 6, 40))
			return;

		if (STP_Global_Setting[1] <= 2)
		{
			if (!checkNumberInput(document.getElementById("hello_time"), 1, 2))
				return;
		}
		
		
		if(isNaN(document.getElementById("f_delay").value) || document.getElementById("f_delay").value < 4 || document.getElementById("f_delay").value > 30)
		{
			if (!checkNumberInput(document.formSTPGlobal.f_delay, 4, 30))
				return;
		}
		
		if(isNaN(document.getElementById("max_hop").value) || document.getElementById("max_hop").value < 4 || document.getElementById("max_hop").value > 60)
		{
			if (!checkNumberInput(document.getElementById("max_hop"), 4, 60))
				return;
		}

		if(isNaN(document.getElementById("tx_count").value) || document.getElementById("tx_count").value < 1 || document.getElementById("tx_count").value > 10)
		{
			if (!checkNumberInput(document.getElementById("tx_count"), 1, 10))
				return;
		}

		document.getElementById("stp_ver").disabled = false;
            document.getElementById("f_bpdu").disabled = false;
            //document.formSTPGlobal.T1.disabled = false;
            document.getElementById("max_age").disabled = false;
		if (STP_Global_Setting[1] <= 2)
		{
			document.getElementById("hello_time").disabled = false;
			document.getElementById("h_hello").value = document.getElementById("hello_time").value;
		}
		else
		{
			if (STP_Global_Setting[5] == '0')
				document.getElementById("h_hello").value = '2';
			else
				document.getElementById("h_hello").value = STP_Global_Setting[5];
		}
            document.getElementById("f_delay").disabled = false;
            document.getElementById("tx_count").disabled = false;
			document.formSTPGlobal.submit();
			
	}
}
function applystate()
{
	if(document.getElementById("stp_state1").checked)
	{
		document.getElementById("state").value = 1;
	}
	else if(document.getElementById("stp_state2").checked)
	{
		document.getElementById("state").value = 0;
	}
	document.getElementById("statetype").value = 1;
	document.getElementById("formSTPGlobalState").submit();
}

</script>
</div>
