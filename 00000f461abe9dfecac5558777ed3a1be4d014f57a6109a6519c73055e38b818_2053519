<script language="Javascript">
    function delit(){
        formRSTI.submit();
        parent.location.href = '43_RADIUS_Server.html';
    }

    function EditStart(iTag){
        document.formRSTI.editButton[iTag].style.display = 'none';
        document.formRSTI.Successtion[iTag].style.display = 'none';
        document.formRSTI.Successtion_M[iTag].style.display = '';
        document.formRSTI.Successtion_M[iTag].select();
        document.formRSTI.Radius_Server[iTag].style.display = 'none';
        document.formRSTI.Radius_Server_M[iTag].style.display = '';
        document.formRSTI.Radius_Server_M[iTag].select();
        document.formRSTI.Auth_Udp_Port[iTag].style.display = 'none';
        document.formRSTI.Auth_Udp_Port_M[iTag].style.display = '';
        document.formRSTI.Auth_Udp_Port_M[iTag].select();
        document.formRSTI.Acct_Udp_Port[iTag].style.display = 'none';
        document.formRSTI.Acct_Udp_Port_M[iTag].style.display = '';
        document.formRSTI.Acct_Udp_Port_M[iTag].select();
        document.formRSTI.Key[iTag].style.display = 'none';
        document.formRSTI.Key_M[iTag].style.display = '';
        document.formRSTI.Key_M[iTag].select();
        document.formRSTI.Status[iTag].style.display = 'none';
        document.formRSTI.Status_M[iTag].style.display = '';
        document.formRSTI.Status_M[iTag].select();
        document.formRSTI.editApply[iTag].style.display = '';
    }
    
    function EditApplyIt(iTag){
        document.formRSTI.editButton[iTag].style.display = '';

        document.formRSTI.Successtion[iTag].style.display = '';
        document.formRSTI.Successtion_M[iTag].style.display = 'none';
        document.formRSTI.Radius_Server[iTag].style.display = '';
        document.formRSTI.Radius_Server_M[iTag].style.display = 'none';
        document.formRSTI.Auth_Udp_Port[iTag].style.display = '';
        document.formRSTI.Auth_Udp_Port_M[iTag].style.display = 'none';
        document.formRSTI.Acct_Udp_Port[iTag].style.display = '';
        document.formRSTI.Acct_Udp_Port_M[iTag].style.display = 'none';
        document.formRSTI.Key[iTag].style.display = '';
        document.formRSTI.Key_M[iTag].style.display = 'none';
        document.formRSTI.Status[iTag].style.display = '';
        document.formRSTI.Status_M[iTag].style.display = 'none';
        document.formRSTI.editApply[iTag].style.display = 'none';

    }
