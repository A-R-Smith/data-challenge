				<script language="Javascript">
				document.write("<tbody class='scrollContent'>");  
						for(i=0; i<parent.ASP.length; i++)
						{
							if (i%2 == 0)
							{
								trClass = 'gw';
								txtClass = 'flatGX';
							}
							else
							{
								trClass = 'gx';
								txtClass = 'flatGX';
							}
							
							document.write("<tr class=" + trClass + ">");
							document.write('  <td width="25%">' + parent.ASP[i][0] + '</td>');	/* Gateway IP Address */
							document.write('  <td width="25%">' + parent.ASP[i][1] + '</td>');	/* Gateway MAC Address */
							
							/* Ports */
							tipStr = strNormalize(parent.ASP[i][2]);
							/*document.write("<td onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(DualCfgSetting[i][4], 10)) + "</td>");*/
							document.write('  <td  width="40%" onMouseover="keytip(\'' + unescape(tipStr) + '\')" onMouseout="hideddrivetip()">' + rp_special_char(strTruncate(parent.ASP[i][2],50)) + '</td>');

							document.write('  <td width="10%" align="right">');
							document.write('<input type="button" name="btnDelete" value="Delete" class="btnInGrayS" onClick="btnDelete_Click(' + i + ')" />');
