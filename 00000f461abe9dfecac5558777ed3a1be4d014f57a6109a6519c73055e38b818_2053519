<head>
	<!--
	-------------------------------------------------------------------
	 Copyright (c) 2007 Alpha Networks Corporation All rights reserved.
	-------------------------------------------------------------------
	-->
	<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">

	<link rel=stylesheet href="../form.css" type="text/css" />

	<script src="../form.js" type="text/javascript"></script>	
	<script src="../checkperm.js" type="text/javascript"></script>
	<script language="javascript">


	function btnCreateDir_Apply_Click()
	{
		if(!checkTxtInput(document.getElementById("boxNewDirName")))
		{
			return;
		}
		
		var pathname = parent.CurrentPath;
		pathname += pathname.substring(pathname.length-1, pathname.length).match(/^[\/\\]$/) == null ? '\\' : '';
		pathname += document.getElementById("boxNewDirName").value;
		parent.cmd_md(pathname);
	}
	
	function btnCreateDir_Cancel_Click()
	{
		document.getElementById("boxNewDirName").value = "new directory name";
		document.getElementById("trNewDir").style.display = "none";
	}
	
	function aFileName_Click(index)
	{
		var pathname = parent.CurrentPath;
		pathname += pathname.substring(pathname.length-1, pathname.length).match(/^[\/\\]$/) == null ? '\\' : '';
		pathname += parent.SDCard_FS[index][0];
		parent.cmd_cd(pathname);
	}
	
	function btnDelete_Click(index)
	{
		var pathname;
		
		pathname = parent.CurrentPath;
		pathname += pathname.substring(pathname.length-1, pathname.length).match(/^[\/\\]$/) == null ? '\\' : '';
		pathname += parent.SDCard_FS[index][0];
		
		if(parent.SDCard_FS[index][1] == -1)	/* DIR */
		{
			parent.cmd_rd(pathname);
		}
		else
		{
			parent.cmd_del(pathname);
		}
	}
	
	function btnRename_Click(index)
	{
		var pathname, filename;
		var i;
		
		/* if the status is APPLAY, then submit cmd to server */
		if(document.getElementById("boxFileName"+index).style.display == "none")
		{
			/* prepare for next */
			for(i=0; i<parent.SDCard_FS.length; i++)
			{
				document.getElementById("boxFileName"+i).style.display = "none";
				document.getElementById("lblFileName"+i).style.display = "";
				document.getElementById("btnRename"+i).value = "Rename";
			}
			
			document.getElementById("boxFileName"+index).style.display = "";
			document.getElementById("boxFileName"+index).select();
			document.getElementById("boxFileName"+index).focus();
			document.getElementById("lblFileName"+index).style.display = "none";
			document.getElementById("btnRename"+index).value = "Apply";
		}
		else
		{
			if(parent.SDCard_FS[index][1] == -1)	/* if it is DIR */
			{
				if(!checkDirName(document.getElementById("boxFileName"+index)))
				{
					return false;
				}
			}
			else
			{
				if(!checkFileName(document.getElementById("boxFileName"+index)))
				{
					return false;
				}
			}
			
			pathname = parent.CurrentPath;
			pathname += pathname.substring(pathname.length-1, pathname.length).match(/^[\/\\]$/) == null ? '\\' : '';
			pathname += parent.SDCard_FS[index][0];
			filename = document.getElementById("boxFileName"+index).value;
			
			/* check if the file name is exists */
			for(i=0; i<parent.SDCard_FS.length; i++)
			{
				/* if dstfile already exists */
				if(filename.toUpperCase() == (parent.SDCard_FS[i][0]).toUpperCase())	
				{
					alert("The specified file already exists!");
					return false;
				}
			}
			
			parent.cmd_rename(pathname, filename);
		}
	}
	
	function btnCopyTo_Click(index)
	{
		var srcPathname = parent.CurrentPath;
		srcPathname += srcPathname.substring(srcPathname.length-1, srcPathname.length).match(/^[\/\\]$/) == null ? '\\' : '';
		srcPathname += parent.SDCard_FS[index][0];
		
		parent.initFileSelect();
		parent.document.getElementById("divCopy").style.display = "";
		parent.document.getElementById("boxSrcPathname_Copy").value = '1';	/* SDCard File */
		parent.document.getElementById("boxSrcPathname_Copy").value = srcPathname;
		parent.document.getElementById("divFS").style.display = "none";
	}
	
	function btnMoveTo_Click(index)
	{
		var srcPathname = parent.CurrentPath;
		srcPathname += srcPathname.substring(srcPathname.length-1, srcPathname.length).match(/^[\/\\]$/) == null ? '\\' : '';
		srcPathname +=  parent.SDCard_FS[index][0];
		parent.document.getElementById("divMove").style.display = "";
		parent.document.getElementById("boxSrcPathname_Move").value = srcPathname;
		parent.document.getElementById("divFS").style.display = "none";
	}
	
	</script>
   
