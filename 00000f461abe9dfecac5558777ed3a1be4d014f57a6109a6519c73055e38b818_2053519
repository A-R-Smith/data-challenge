    <script language="javascript">
    
    	function convert_int_2_char(value)
    	{
    		if(value == '1')
			return "*";
		else 
			return "&nbsp;&nbsp;";
    	}
	function convert_int_3_char(value)
    	{
    		if(value == '1')
			return "Yes";
		else 
			return "No";
    	}
	function showSize(value)
    	{
    		if(value == '0')
			return "";
		else 
			return value;
    	}
	function showEnpty(value)
	{
    		if(value == '')
			return "(Empty)";
		else 
			return value;	
	}
	</script>
</head>

