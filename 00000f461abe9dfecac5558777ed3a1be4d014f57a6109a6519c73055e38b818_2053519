<script language="Javascript">
	changeForm1280('backup');
    function SwitchOption(sel) {
    	if (sel == 1) {
			
    		document.getElementById('serverip1').disabled = false;
    		document.getElementById('tftpfile1').disabled =false;
    		document.getElementById('uploadlog').disabled = false;
    		document.getElementById('serverip').disabled = true;
    		document.getElementById('tftpfile').disabled = true;
     		document.getElementById('uploadattack').disabled = true;
			document.getElementById('serverip1').focus();
    	} else {
    		document.getElementById('serverip1').disabled = true;
    		document.getElementById('tftpfile1').disabled = true;
    		document.getElementById('uploadlog').disabled = true;
    		document.getElementById('serverip').disabled = false;
    		document.getElementById('tftpfile').disabled = false;
    		document.getElementById('uploadattack').disabled = false;
    		document.getElementById('serverip').focus();
    	}
    }
  SwitchOption(1);	
</script>
