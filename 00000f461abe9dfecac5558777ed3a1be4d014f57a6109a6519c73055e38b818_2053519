var times = 0;
function showBlinkSDCardImg()
{
	var Sdcard_Green = "../Icon/DGS-3200-24/Sdcard_Green.gif";
	if(times%2 == 0)
	{
		document.getElementById("divSDCard").innerHTML = "<img src='../Icon/DGS-3200-24/Sdcard_Green.gif' />";
		times = 1;
	}
	else
	{
		document.getElementById("divSDCard").innerHTML = "";
		times = 0;
	}
	
	window.setTimeout("showBlinkSDCardImg()",300);
}

function DrawUnit(UnitID)
{
	var LedOn_Green			 = "../Icon/DGS-3200/LedGreen.gif";
	var LedOn_Orig			 = "../Icon/DGS-3200/LedOrig.gif";
	var Led_Green_24_Up;
	var Led_Green_24_Down;
	var Led_Orig_24_Up;
	var Led_Orig_24_Down;
	var DGS3224Image;
	var DGS3216Image		 = "../Icon/DGS-3200/DGS-3200_16.jpg";
	var DGS3200Image		 = "../Icon/DGS-3200/DGS-3200_10.jpg";
	var Sdcard_Green;
	var Sdcard_Blink;
	var Sdcard_Red;

	if(Panel_Info[4]==137)
	{
		LedOn_Green			 = "../Icon/DGS-3200-24/LedGreen.gif";
		LedOn_Orig			 = "../Icon/DGS-3200-24/LedOrig.gif";
		Led_Green_24_Up	 	 = "../Icon/DGS-3200-24/Led_Green_Up.gif";
		Led_Green_24_Down	 = "../Icon/DGS-3200-24/Led_Green_Down.gif";
		Led_Orig_24_Up		 = "../Icon/DGS-3200-24/Led_Orig_Up.gif";
		Led_Orig_24_Down	 	 = "../Icon/DGS-3200-24/Led_Orig_Down.gif";
		DGS3224Image		 = "../Icon/DGS-3200-24/DGS-3200_24.jpg";
		Sdcard_Green 		 = "../Icon/DGS-3200-24/Sdcard_Green.gif";
		Sdcard_Blink 		 	= "../Icon/DGS-3200-24/Sdcard_Blink.gif";
		Sdcard_Red 		 	= "../Icon/DGS-3200-24/Sdcard_Red.gif";
	}
	
	document.write("<map name='Panel'>");

	var i;
	if(Panel_Info[4]==137)	/* dgs3200-24 */
	{
		document.write("<div style='position:absolute;left:"+290+"px; top:"+13+"px;'><img border=0 src="+DGS3224Image+"></"+"div>");

		
		for(i=0;i<12;i++)
		{
		
			if((i%2)==0)  //up
			{
				DrawPort(389+(i/2)*31+130, 38+10,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(389+((i-1)/2)*31+130, 57+10,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}

		}

		for(i = 12; i < 20;i++)
		{
			if((i%2)==0)  //up
			{
				DrawPort(528+(i/2-4)*31+130, 38+10,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(528+((i-1)/2-4)*31+130, 57+10,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
		}

		

		//21
		if (Panel_Info[0].substring(20, 21) == 0)
		{
			DrawPort(714+130, 38+10,UnitID,21, 0, 0);	/* no port */
			DrawPort(815+130, 38+10,UnitID,21, 0, 1);	/* no port */
		}
		else
		{
			if (Panel_Info[1].substring(20, 21) == 0)
			{
				DrawPort(714+130, 38+10, UnitID,21, Panel_Info[0].substring(20, 21), 0);
				
			}
			else if (Panel_Info[1].substring(20, 21) == 1)
			{
				
				DrawPort(815+130, 38+10,UnitID,21, Panel_Info[0].substring(20, 21), 1);
			}
		}

		//23
		if (Panel_Info[0].substring(22, 23) == 0)
		{
			DrawPort(745+130, 38+10,UnitID,23, 0, 0);	/* no port */
			DrawPort(853+130, 38+10,UnitID,23, 0, 1);	/* no port */
		}
		else
		{
			if (Panel_Info[1].substring(22, 23) == 0)
			{
				DrawPort(745+130, 38+10, UnitID,23, Panel_Info[0].substring(22, 23), 0);
				
			}
			else if (Panel_Info[1].substring(22, 23) == 1)
			{
				
				DrawPort(853+130, 38+10,UnitID,23, Panel_Info[0].substring(22, 23), 1);
			}
		}

		//22
		if (Panel_Info[0].substring(21, 22) == 0)
		{
			DrawPort(714+130, 57+10,UnitID,22, 0, 0);
			DrawPort(815+130, 57+10,UnitID,22, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(21, 22) == 0)
			{
				DrawPort(714+130, 57+10, UnitID,22, Panel_Info[0].substring(21, 22), 0);
			}
			else if (Panel_Info[1].substring(21, 22) == 1)
			{
				DrawPort(815+130, 57+10,UnitID,22, Panel_Info[0].substring(21, 22), 1);
			}
		}

		//24
		if (Panel_Info[0].substring(23, 24) == 0)
		{
			DrawPort(745+130, 57+10,UnitID,24, 0, 0);
			DrawPort(853+130, 57+10,UnitID,24, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(23, 24) == 0)
			{
				DrawPort(745+130, 57+10, UnitID,24, Panel_Info[0].substring(23, 24), 0);
			}
			else if (Panel_Info[1].substring(23, 24) == 1)
			{
				DrawPort(853+130, 57+10,UnitID,24, Panel_Info[0].substring(23, 24), 1);
			}
		}


		var ledX = 392, ledY = 8;
		for (i=0; i < 20; i++)
		{
			if(i%2 == 0)	/* up */
			{
				DrawLed(ledX+130, ledY+10, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(i, i+1));
				ledX += 15;
			}
			else			/* down */
			{
				DrawLed(ledX+130, ledY+10, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(i, i+1));
				ledX += 16;
			}
			
			if(i == 11)
			{
				ledX += 15;
			}
		}

		//21T
		if (Panel_Info[0].substring(20, 20+1) != 0)
		{
			if (Panel_Info[1].substring(20, 20+1) == 0)	/* if it is not fiber, display at left */
			{
				DrawLed(717+130, 8+10, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(20, 20+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+130, 8+10, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(20, i+1));
			}
		}

		//23T
		if (Panel_Info[0].substring(22, 22+1) != 0)
		{
			if (Panel_Info[1].substring(22, 22+1) == 0)
			{
				DrawLed(717+15+16+130, 8+10, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(22, 22+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+15+23+130, 8+10, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(22, 22+1));
			}
		}

		//22T
		if (Panel_Info[0].substring(21, 21+1) != 0)
		{
			if (Panel_Info[1].substring(21, 21+1) == 0)	/* if it is not fiber, display at left */
			{
				DrawLed(717+15+130, 8+10, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(21, 21+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+15+130, 8+10, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(21, 21+1));
			}
		}

		//24T
		if (Panel_Info[0].substring(23, 23+1) != 0)
		{
			if (Panel_Info[1].substring(23, 23+1) == 0)	/* if it is not fiber, display at left */
			{
				DrawLed(717+15+16+15+130, 8+10, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(23, 23+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+15+23+15+130, 8+10, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(23, 23+1));
			}
		}
		
		// Draw Power led
		x=359;
		y=35;
		document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+LedOn_Green+"></"+"div>");
		// Draw Console led
		switch(Panel_Info[2])
		{
			case '0':   // console led off
				break;
			case '1':   // console led on
				x=359;
				y=47;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;
		}
		// Draw rps led
		switch(Panel_Info[3])
		{
			case '0':   // rps led off
				break;
			case '1':   // rps led on
				x=359;
				y=60;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;
		}
		// Draw SD Card led
		var times = 0;
		switch(SDCard_Status)
		{
			case '0':   		// SD card is not present
				break;
			case '1':   		// SD card is inserted to SD card cage, but we do not read/write it
				x=359;
				y=73;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+LedOn_Green+"></"+"div>");
				x=297;
				y=66;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+Sdcard_Green+"></"+"div>");
				break;
			case '2':			// SD card read/write successfully
				x=359;
				y=73;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+LedOn_Green+"></"+"div>");
				x=297;
				y=66;
				document.write("<div id='divSDCard' style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'></"+"div>");
				x=297;
				y=66;
				/*
				document.write("<div id='divSDCard' style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'>"+Sdcard_Blink+"</"+"div>");
				*/
				document.write("<div id='divSDCard' style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'></"+"div>");
				showBlinkSDCardImg();
				break;
			case '3':			//SD card read/write failed
				x=359;
				y=73;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+LedOn_Green+"></"+"div>");
				x=297;
				y=66;
				document.write("<div style='position:absolute;left:" + (x+130) + "px;top:" + (y+10) + "px;'><img src="+Sdcard_Red+"></"+"div>");
				break;
			default :
				break;
		}
	}
	else if(Panel_Info[4]==136)
	{
		document.write("<div style='position:absolute;left:"+245+";top:"+13+";'><img border=0 src="+DGS3216Image+"></"+"div>");

		
		for(i=0;i<8;i++)
		{
		
			if((i%2)==0)  //up
			{
				DrawPort(442+(i/2)*27, 38,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(442+((i-1)/2)*27, 58,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}

		}


		for(i = 8; i < 14;i++)
		{
			if((i%2)==0)  //up
			{
				DrawPort(579+(i/2-4)*27, 38,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(579+((i-1)/2-4)*27, 58,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
		}

		

		//15
		if (Panel_Info[0].substring(14, 15) == 0)
		{
			DrawPort(660, 38,UnitID,15, 0, 0);
			DrawPort(738, 61,UnitID,15, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(14, 15) == 0)
			{
				DrawPort(660, 38, UnitID,15, Panel_Info[0].substring(14, 15), 0);
				
			}
			else if (Panel_Info[1].substring(14, 15) == 1)
			{
				
				DrawPort(738, 61,UnitID,15, Panel_Info[0].substring(14, 15), 1);
			}
		}

		//16
		if (Panel_Info[0].substring(15, 16) == 0)
		{
			DrawPort(660, 58,UnitID,16, 0, 0);
			DrawPort(797, 61,UnitID,16, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(15, 16) == 0)
			{
				DrawPort(660, 58, UnitID,16, Panel_Info[0].substring(15, 16), 0);
			}
			else if (Panel_Info[1].substring(15, 16) == 1)
			{
				DrawPort(797, 61,UnitID,16, Panel_Info[0].substring(15, 16), 1);
			}
		}

		for (i=0; i < 14; i++)
		{
			if((i%4)==0)  //up
				DrawLed((363+(i/4)*12), 41, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else if((i%4)==1)
				DrawLed((363+((i-1)/4)*12), 55, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else if((i%4)==2)  //up
				DrawLed((363+((i-2)/4)*12), 69, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else if((i%4)==3)
				DrawLed((363+((i-3)/4)*12), 83, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
		}

		//15
		if (Panel_Info[0].substring(14, 15) != 0)
		{
			if (Panel_Info[1].substring(14, 15) == 0)
				DrawLed(399, 41, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(14, 15));
			else
				DrawLed(718, 67, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(14, 15));
		}
		
		//16
		if (Panel_Info[0].substring(15, 16) != 0)
		{
			if (Panel_Info[1].substring(15, 16) == 0)
				DrawLed(399, 83, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(15, 16));
			else
				DrawLed(779, 67, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(15, 16));
		}
		// Draw Power led
		document.write("<div style='position:absolute;left:332;top:79;'><img src="+LedOn_Green+"></"+"div>");
		// Draw Console led
		switch(Panel_Info[2])
		{
			case '0':   // console led off
				break;
			case '1':   // console led on
				document.write("<div style='position:absolute;left:332;top:64;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;

		}
	}
	else if(Panel_Info[4]==135)
	{
		document.write("<div style='position:absolute;left:"+245+";top:"+13+";'><img border=0 src="+DGS3200Image+"></"+"div>");
	for(i=0;i<8;i++)
	{
	
		if((i%2)==0)  //up
		{
			DrawPort_3200(479+(i/2)*27, 40,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
		}
		else  //down
		{
			DrawPort_3200(479+((i-1)/2)*27, 60,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
		}

	}
	//9
	if (Panel_Info[0].substring(8, 9) == 0)
	{
	
		DrawPort_3200(684, 58,UnitID,9, 0, 0);
		DrawPort_3200(640, 64,UnitID,9, 0, 1);
	}
	else
	{
		if (Panel_Info[1].substring(8, 9) == 0)
		{
			DrawPort_3200(685, 58, UnitID,9, Panel_Info[0].substring(8, 9), 0);
		}
		else if (Panel_Info[1].substring(8, 9) == 1)
		{
			DrawPort_3200(640, 64,UnitID,9, Panel_Info[0].substring(8, 9), 1);
		}
	}

	//10
	if (Panel_Info[0].substring(9, 10) == 0)
	{
		DrawPort_3200(797, 58,UnitID,10, 0, 0);
		DrawPort_3200(755, 64,UnitID,10, 0, 1);
	}
	else
	{
		if (Panel_Info[1].substring(9, 10) == 0)
		{
			DrawPort_3200(797, 58, UnitID,10, Panel_Info[0].substring(9, 10), 0);
		}
		else if (Panel_Info[1].substring(9, 10) == 1)
		{
			DrawPort_3200(755, 64,UnitID,10, Panel_Info[0].substring(9, 10), 1);
		}
	}

	for (i=0; i < 8; i++)
	{
		if((i%2)==0)  //up
			DrawLed((410+(i/2)*14), 59, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
		else
			DrawLed((410+((i-1)/2)*14), 74, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
	}
	//9
	if (Panel_Info[0].substring(8, 9) != 0)
	{
		if (Panel_Info[1].substring(8, 9) == 0)
			DrawLed(615, 74, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(8, 9));
		else
			DrawLed(615, 59, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(8, 9));
	}
	
	//10
	if (Panel_Info[0].substring(9, 10) != 0)
	{
		if (Panel_Info[1].substring(9, 10) == 0)
			DrawLed(730, 74, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(9, 10));
		else
			DrawLed(730, 59, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(9, 10));
	}

	// Draw Power led
	document.write("<div style='position:absolute;left:377;top:74;'><img src="+LedOn_Green+"></"+"div>");
	// Draw Console led
	switch(Panel_Info[2])
	{
		case '0':   // console led off
			break;
		case '1':   // console led on
			document.write("<div style='position:absolute;left:377;top:59;'><img src="+LedOn_Green+"></"+"div>");
			break;
		default :
			break;

	}
	
	}
	document.write("</map>");
}

function DrawPort(x,y,UnitID,port,ledValue,PortType)
{
	var port_green,port_orig;

	if(Panel_Info[4]==137)
	{
		/* fix panl image bug */
		var imgWidth;
		
		if(PortType == 0)		/* RJ-45 port */
		{
			if (port == 21 || port == 23)		/* T-Port (up) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
			}
			else if (port == 22 || port == 24)	/* T-Port (down) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
			}
			else								/* Common Port */
			{
				if(port%2 == 1)				/* up */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
				}
				else							/* Down */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
				}
			}

			/* fix panl image bug */
			/*imgWidth = (port==7 || port==8 || port==19 || port==20) ? 27 : 26;	
			imgWidth = (port==21) ? imgWidth-1 : imgWidth;
			y = (port == 9) ? (y+1) : y;
			y = (port == 7) ? (y-1) : y;*/
		}
		else if(PortType == 1)				/* Fiber */
		{
			if(port == 21 || port == 23)
			{
				port_green="../Icon/DGS-3200-24/Fiber_Green_Up.gif";
				port_orig="../Icon/DGS-3200-24/Fiber_Orig_Up.gif";
			}
			else
			{
				port_green="../Icon/DGS-3200-24/Fiber_Green_Down.gif";
				port_orig="../Icon/DGS-3200-24/Fiber_Orig_Down.gif";
			}
		}
		else
		{
			if (port == 21 || port == 23)		/* T-Port (up) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
			}
			else if (port == 22 || port == 24)	/* T-Port (down) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
			}
			else								/* Common Port */
			{
				if(port%2 == 1)				/* up */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
				}
				else							/* Down */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
				}
			}

			imgWidth = (port==7 || port==8 || port==19 || port==20) ? 27 : 26;	
			imgWidth = (port==21) ? imgWidth-1 : imgWidth;
			y = (port == 9) ? (y+1) : y;
			y = (port == 7) ? (y-1) : y;
		}

		switch(ledValue)
		{
			case '2': // link up with speed 1000
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_green+" alt='"+port+"' style='width:" + imgWidth + "px;'></a></"+"div>");
				break;
			case '1': // On  for link port
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_orig+" alt='"+port+"' style='width:" + imgWidth + "px;'></a></"+"div>");
				break;
			case '0': // Off
			default:
				break;
		}
	}
	else
	{
		if(PortType == 0)
		{
			if (port == 15)
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else if (port == 16)
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
			}
			else
			{
				if(port%2 == 1)  // up
				{
					port_green="../Icon/DGS-3200/Port_Up_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
				}
				else     // Down
				{
					port_green="../Icon/DGS-3200/Port_Down_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
				}
			}
		}
		else if(PortType == 1)
		{
			port_green="../Icon/DGS-3200/Fiber_Green.gif";
			port_orig="../Icon/DGS-3200/Fiber_Orig.gif";
		}
		else
		{
			if (port == 15)
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else if (port == 16)
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else
			{
				if(port%2 == 1)  // up
				{
					port_green="../Icon/DGS-3200/Port_Up_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
				}
				else     // Down
				{
					port_green="../Icon/DGS-3200/Port_Down_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
				}
			}
		}
		switch(ledValue)
		{
			case '2': // link up with speed 1000
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_green+" alt='"+port+"' /></a></"+"div>");
				break;
			case '1': // On  for link port
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_orig+" alt='"+port+"' /></a></"+"div>");
				break;
			case '0': // Off
			default:
				break;
		}
	}
}


function DrawPort_3200(x,y,UnitID,port,ledValue,PortType)
{
	var port_green,port_orig;
	
	if(PortType == 0)
	{
		if (port == 9 || port == 10)
		{
			port_green="../Icon/DGS-3200/Port_Down_Green.gif";
			port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
		}
		else
		{
			if(port%2 == 1)  // up
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else     // Down
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
			}
		}
	}
	else if(PortType == 1)
	{
		port_green="../Icon/DGS-3200/Fiber_Green.gif";
		port_orig="../Icon/DGS-3200/Fiber_Orig.gif";
	}
	else
	{
		if (port == 9 || port == 10)
		{
			port_green="../Icon/DGS-3200/Port_Down_Green.gif";
			port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
		}
		else
		{
			if(port%2 == 1)  // up
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else     // Down
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
			}
		}
	}
	
	switch(ledValue)
	{
		case '2': // link up with speed 1000
			document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_green+" alt='"+port+"'></a></"+"div>");
			break;
		case '1': // On  for link port
			document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_orig+" alt='"+port+"'></a></"+"div>");
			break;
		case '0': // Off
		default:
			break;
	}
}

function DrawLed(x1, y1, img1, img2,ledValue)
{
	var imgLed_Green = img1;
	var imgLed_Orig = img2;

	switch(ledValue)
	{
		case '2': // link up with speed 1000
			document.write("<div style='position:absolute;left:"+x1+";top:"+y1+";'><img src="+imgLed_Green+"></"+"div>");
			break;
		case '1': // On  for link port
			document.write("<div style='position:absolute;left:"+x1+";top:"+y1+";'><img src="+imgLed_Orig+"></"+"div>");
			break;
		case '0': // Off
		default:
			break;
	}

}

var times = 0;
function showBlinkSDCardImg()
{
	var Sdcard_Green = "../Icon/DGS-3200-24/Sdcard_Green.gif";
	if(times%2 == 0)
	{
		document.getElementById("divSDCard").innerHTML = "<img src='../Icon/DGS-3200-24/Sdcard_Green.gif' />";
		times = 1;
	}
	else
	{
		document.getElementById("divSDCard").innerHTML = "";
		times = 0;
	}
	
	window.setTimeout("showBlinkSDCardImg()",300);
}

function DrawUnit(UnitID)
{
	var LedOn_Green			 = "../Icon/DGS-3200/LedGreen.gif";
	var LedOn_Orig			 = "../Icon/DGS-3200/LedOrig.gif";
	var Led_Green_24_Up;
	var Led_Green_24_Down;
	var Led_Orig_24_Up;
	var Led_Orig_24_Down;
	var DGS3224Image;
	var DGS3216Image		 = "../Icon/DGS-3200/DGS-3200_16.jpg";
	var DGS3200Image		 = "../Icon/DGS-3200/DGS-3200_10.jpg";
	var Sdcard_Green;
	var Sdcard_Blink;
	var Sdcard_Red;

	if(Panel_Info[4]==137)
	{
		LedOn_Green			 = "../Icon/DGS-3200-24/LedGreen.gif";
		LedOn_Orig			 = "../Icon/DGS-3200-24/LedOrig.gif";
		Led_Green_24_Up	 	 = "../Icon/DGS-3200-24/Led_Green_Up.gif";
		Led_Green_24_Down	 = "../Icon/DGS-3200-24/Led_Green_Down.gif";
		Led_Orig_24_Up		 = "../Icon/DGS-3200-24/Led_Orig_Up.gif";
		Led_Orig_24_Down	 	 = "../Icon/DGS-3200-24/Led_Orig_Down.gif";
		DGS3224Image		 = "../Icon/DGS-3200-24/DGS-3200_24.jpg";
		Sdcard_Green 		 = "../Icon/DGS-3200-24/Sdcard_Green.gif";
		Sdcard_Blink 		 	= "../Icon/DGS-3200-24/Sdcard_Blink.gif";
		Sdcard_Red 		 	= "../Icon/DGS-3200-24/Sdcard_Red.gif";
	}
	
	document.write("<map name='Panel'>");

	var i;
	if(Panel_Info[4]==137)
	{
		document.write("<div style='position:absolute;left:"+160+"px; top:"+3+"px;'><img border=0 src="+DGS3224Image+"></"+"div>");

		
		for(i=0;i<12;i++)
		{
		
			if((i%2)==0)  //up
			{
				DrawPort(389+(i/2)*31, 38,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(389+((i-1)/2)*31, 57,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}

		}

		for(i = 12; i < 20;i++)
		{
			if((i%2)==0)  //up
			{
				DrawPort(528+(i/2-4)*31, 38,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(528+((i-1)/2-4)*31, 57,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
		}

		

		//21
		if (Panel_Info[0].substring(20, 21) == 0)
		{
			DrawPort(714, 38,UnitID,21, 0, 0);	/* no port */
			DrawPort(815, 38,UnitID,21, 0, 1);	/* no port */
		}
		else
		{
			if (Panel_Info[1].substring(20, 21) == 0)
			{
				DrawPort(714, 38, UnitID,21, Panel_Info[0].substring(20, 21), 0);
				
			}
			else if (Panel_Info[1].substring(20, 21) == 1)
			{
				
				DrawPort(815, 38,UnitID,21, Panel_Info[0].substring(20, 21), 1);
			}
		}

		//23
		if (Panel_Info[0].substring(22, 23) == 0)
		{
			DrawPort(745, 38,UnitID,23, 0, 0);	/* no port */
			DrawPort(853, 38,UnitID,23, 0, 1);	/* no port */
		}
		else
		{
			if (Panel_Info[1].substring(22, 23) == 0)
			{
				DrawPort(745, 38, UnitID,23, Panel_Info[0].substring(22, 23), 0);
				
			}
			else if (Panel_Info[1].substring(22, 23) == 1)
			{
				
				DrawPort(853, 38,UnitID,23, Panel_Info[0].substring(22, 23), 1);
			}
		}

		//22
		if (Panel_Info[0].substring(21, 22) == 0)
		{
			DrawPort(714, 57,UnitID,22, 0, 0);
			DrawPort(815, 57,UnitID,22, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(21, 22) == 0)
			{
				DrawPort(714, 57, UnitID,22, Panel_Info[0].substring(21, 22), 0);
			}
			else if (Panel_Info[1].substring(21, 22) == 1)
			{
				DrawPort(815, 57,UnitID,22, Panel_Info[0].substring(21, 22), 1);
			}
		}

		//24
		if (Panel_Info[0].substring(23, 24) == 0)
		{
			DrawPort(745, 57,UnitID,24, 0, 0);
			DrawPort(853, 57,UnitID,24, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(23, 24) == 0)
			{
				DrawPort(745, 57, UnitID,24, Panel_Info[0].substring(23, 24), 0);
			}
			else if (Panel_Info[1].substring(23, 24) == 1)
			{
				DrawPort(853, 57,UnitID,24, Panel_Info[0].substring(23, 24), 1);
			}
		}


		var ledX = 392, ledY = 8;
		for (i=0; i < 20; i++)
		{
			if(i%2 == 0)	/* up */
			{
				DrawLed(ledX, ledY, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(i, i+1));
				ledX += 15;
			}
			else			/* down */
			{
				DrawLed(ledX, ledY, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(i, i+1));
				ledX += 16;
			}
			
			if(i == 11)
			{
				ledX += 15;
			}
		}

		//21T
		if (Panel_Info[0].substring(20, 20+1) != 0)
		{
			if (Panel_Info[1].substring(20, 20+1) == 0)	/* if it is not fiber, display at left */
			{
				DrawLed(717, 8, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(20, 20+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818, 8, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(20, i+1));
			}
		}

		//23T
		if (Panel_Info[0].substring(22, 22+1) != 0)
		{
			if (Panel_Info[1].substring(22, 22+1) == 0)
			{
				DrawLed(717+15+16, 8, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(22, 22+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+15+23, 8, Led_Green_24_Up, Led_Orig_24_Up, Panel_Info[0].substring(22, 22+1));
			}
		}

		//22T
		if (Panel_Info[0].substring(21, 21+1) != 0)
		{
			if (Panel_Info[1].substring(21, 21+1) == 0)	/* if it is not fiber, display at left */
			{
				DrawLed(717+15, 8, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(21, 21+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+15, 8, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(21, 21+1));
			}
		}

		//24T
		if (Panel_Info[0].substring(23, 23+1) != 0)
		{
			if (Panel_Info[1].substring(23, 23+1) == 0)	/* if it is not fiber, display at left */
			{
				DrawLed(717+15+16+15, 8, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(23, 23+1));
			}
			else										/* if it is fiber, display at right */
			{
				DrawLed(818+15+23+15, 8, Led_Green_24_Down, Led_Orig_24_Down, Panel_Info[0].substring(23, 23+1));
			}
		}
		
		// Draw Power led
		document.write("<div style='position:absolute;left:359px;top:35px;'><img src="+LedOn_Green+"></"+"div>");
		// Draw Console led
		switch(Panel_Info[2])
		{
			case '0':   // console led off
				break;
			case '1':   // console led on
				document.write("<div style='position:absolute;left:359px;top:47px;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;
		}
		// Draw rps led
		switch(Panel_Info[3])
		{
			case '0':   // rps led off
				break;
			case '1':   // rps led on
				document.write("<div style='position:absolute;left:359px;top:60px;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;
		}
		// Draw SD Card led
		var times = 0;
		switch(SDCard_Status)
		{
			case '0':   		// SD card is not present
				break;
			case '1':   		// SD card is inserted to SD card cage, but we do not read/write it
				document.write("<div style='position:absolute;left:359px;top:73px;'><img src="+LedOn_Green+"></"+"div>");
				document.write("<div style='position:absolute;left:297px;top:66px;'><img src="+Sdcard_Green+"></"+"div>");
				break;
			case '2':			// SD card read/write successfully
				document.write("<div style='position:absolute;left:359px;top:73px;'><img src="+LedOn_Green+"></"+"div>");
				document.write("<div id='divSDCard' style='position:absolute;left:297px;top:66px;'></"+"div>");
				/*
				document.write("<div id='divSDCard' style='position:absolute;left:297px;top:66px;'>"+Sdcard_Blink+"</"+"div>");
				*/
				document.write("<div id='divSDCard' style='position:absolute;left:297px;top:66px;'></"+"div>");
				showBlinkSDCardImg();
				break;
			case '3':			//SD card read/write failed
				document.write("<div style='position:absolute;left:359px;top:73px;'><img src="+LedOn_Green+"></"+"div>");
				document.write("<div style='position:absolute;left:297px;top:66px;'><img src="+Sdcard_Red+"></"+"div>");
				break;
			default :
				break;
		}
	}
	else if(Panel_Info[4]==136)
	{
		document.write("<div style='position:absolute;left:"+180+";top:"+3+";'><img border=0 src="+DGS3216Image+"></"+"div>");

		
		for(i=0;i<8;i++)
		{
		
			if((i%2)==0)  //up
			{
				DrawPort(377+(i/2)*27, 28,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(377+((i-1)/2)*27, 48,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}

		}

		for(i = 8; i < 14;i++)
		{
			if((i%2)==0)  //up
			{
				DrawPort(514+(i/2-4)*27, 28,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort(514+((i-1)/2-4)*27, 48,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
		}

		

		//15
		if (Panel_Info[0].substring(14, 15) == 0)
		{
			DrawPort(595, 28,UnitID,15, 0, 0);
			DrawPort(673, 51,UnitID,15, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(14, 15) == 0)
			{
				DrawPort(595, 28, UnitID,15, Panel_Info[0].substring(14, 15), 0);
				
			}
			else if (Panel_Info[1].substring(14, 15) == 1)
			{
				
				DrawPort(673, 51,UnitID,15, Panel_Info[0].substring(14, 15), 1);
			}
		}

		//16
		if (Panel_Info[0].substring(15, 16) == 0)
		{
			DrawPort(595, 48,UnitID,16, 0, 0);
			DrawPort(732, 51,UnitID,16, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(15, 16) == 0)
			{
				DrawPort(595, 48, UnitID,16, Panel_Info[0].substring(15, 16), 0);
			}
			else if (Panel_Info[1].substring(15, 16) == 1)
			{
				DrawPort(732, 51,UnitID,16, Panel_Info[0].substring(15, 16), 1);
			}
		}

		for (i=0; i < 14; i++)
		{
			if((i%4)==0)  //up
				DrawLed((298+(i/4)*12), 31, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else if((i%4)==1)
				DrawLed((298+((i-1)/4)*12), 45, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else if((i%4)==2)  //up
				DrawLed((298+((i-2)/4)*12), 59, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else if((i%4)==3)
				DrawLed((298+((i-3)/4)*12), 73, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
		}

		//15
		if (Panel_Info[0].substring(14, 15) != 0)
		{
			if (Panel_Info[1].substring(14, 15) == 0)
				DrawLed(334, 59, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(14, 15));
			else
				DrawLed(653, 57, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(14, 15));
		}
		
		//16
		if (Panel_Info[0].substring(15, 16) != 0)
		{
			if (Panel_Info[1].substring(15, 16) == 0)
				DrawLed(334, 73, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(15, 16));
			else
				DrawLed(714, 57, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(15, 16));
		}
		// Draw Power led
		document.write("<div style='position:absolute;left:267;top:69;'><img src="+LedOn_Green+"></"+"div>");
		// Draw Console led
		switch(Panel_Info[2])
		{
			case '0':   // console led off
				break;
			case '1':   // console led on
				document.write("<div style='position:absolute;left:267;top:54;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;

		}
	}
	else if(Panel_Info[4]==135)
	{
		document.write("<div style='position:absolute;left:"+180+";top:"+3+";'><img border=0 src="+DGS3200Image+"></"+"div>");
		for(i=0;i<8;i++)
		{
		
			if((i%2)==0)  //up
			{
				DrawPort_3200(414+(i/2)*27, 30,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}
			else  //down
			{
				DrawPort_3200(414+((i-1)/2)*27, 50,UnitID,i+1, Panel_Info[0].substring(i, i+1), Panel_Info[1].substring(i, i+1));
			}

		}
		//9
		if (Panel_Info[0].substring(8, 9) == 0)
		{
		
			DrawPort_3200(619, 48,UnitID,9, 0, 0);
			DrawPort_3200(575, 54,UnitID,9, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(8, 9) == 0)
			{
				DrawPort_3200(619, 48, UnitID,9, Panel_Info[0].substring(8, 9), 0);
			}
			else if (Panel_Info[1].substring(8, 9) == 1)
			{
				DrawPort_3200(575, 54,UnitID,9, Panel_Info[0].substring(8, 9), 1);
			}
		}

		//10
		if (Panel_Info[0].substring(9, 10) == 0)
		{
			DrawPort_3200(732, 48,UnitID,10, 0, 0);
			DrawPort_3200(690, 54,UnitID,10, 0, 1);
		}
		else
		{
			if (Panel_Info[1].substring(9, 10) == 0)
			{
				DrawPort_3200(732, 48, UnitID,10, Panel_Info[0].substring(9, 10), 0);
			}
			else if (Panel_Info[1].substring(9, 10) == 1)
			{
				DrawPort_3200(690, 54,UnitID,10, Panel_Info[0].substring(9, 10), 1);
			}
		}

		for (i=0; i < 8; i++)
		{
			if((i%2)==0)  //up
				DrawLed((345+(i/2)*14), 49, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
			else
				DrawLed((345+((i-1)/2)*14), 64, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(i, i+1));
		}
		//9
		if (Panel_Info[0].substring(8, 9) != 0)
		{
			if (Panel_Info[1].substring(8, 9) == 0)
				DrawLed(550, 64, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(8, 9));
			else
				DrawLed(550, 49, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(8, 9));
		}
		
		//10
		if (Panel_Info[0].substring(9, 10) != 0)
		{
			if (Panel_Info[1].substring(9, 10) == 0)
				DrawLed(665, 64, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(9, 10));
			else
				DrawLed(665, 49, LedOn_Green, LedOn_Orig, Panel_Info[0].substring(9, 10));
		}

		// Draw Power led
		document.write("<div style='position:absolute;left:312;top:64;'><img src="+LedOn_Green+"></"+"div>");
		// Draw Console led
		switch(Panel_Info[2])
		{
			case '0':   // console led off
				break;
			case '1':   // console led on
				document.write("<div style='position:absolute;left:312;top:49;'><img src="+LedOn_Green+"></"+"div>");
				break;
			default :
				break;

		}
	}
	document.write("</map>");
}

function DrawPort(x,y,UnitID,port,ledValue,PortType)
{
	var port_green,port_orig;

	if(Panel_Info[4]==137)
	{
		/* fix panl image bug */
		var imgWidth;
		
		if(PortType == 0)		/* RJ-45 port */
		{
			if (port == 21 || port == 23)		/* T-Port (up) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
			}
			else if (port == 22 || port == 24)	/* T-Port (down) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
			}
			else								/* Common Port */
			{
				if(port%2 == 1)				/* up */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
				}
				else							/* Down */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
				}
			}

			/* fix panl image bug */
			/*imgWidth = (port==7 || port==8 || port==19 || port==20) ? 27 : 26;	
			imgWidth = (port==21) ? imgWidth-1 : imgWidth;
			y = (port == 9) ? (y+1) : y;
			y = (port == 7) ? (y-1) : y;*/
		}
		else if(PortType == 1)				/* Fiber */
		{
			if(port == 21 || port == 23)
			{
				port_green="../Icon/DGS-3200-24/Fiber_Green_Up.gif";
				port_orig="../Icon/DGS-3200-24/Fiber_Orig_Up.gif";
			}
			else
			{
				port_green="../Icon/DGS-3200-24/Fiber_Green_Down.gif";
				port_orig="../Icon/DGS-3200-24/Fiber_Orig_Down.gif";
			}
		}
		else
		{
			if (port == 21 || port == 23)		/* T-Port (up) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
			}
			else if (port == 22 || port == 24)	/* T-Port (down) */
			{
				port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
				port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
			}
			else								/* Common Port */
			{
				if(port%2 == 1)				/* up */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Up.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Up.gif";
				}
				else							/* Down */
				{
					port_green="../Icon/DGS-3200-24/Port_Green_Down.gif";
					port_orig="../Icon/DGS-3200-24/Port_Orig_Down.gif";
				}
			}

			imgWidth = (port==7 || port==8 || port==19 || port==20) ? 27 : 26;	
			imgWidth = (port==21) ? imgWidth-1 : imgWidth;
			y = (port == 9) ? (y+1) : y;
			y = (port == 7) ? (y-1) : y;
		}

		switch(ledValue)
		{
			case '2': // link up with speed 1000
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_green+" alt='"+port+"' style='width:" + imgWidth + "px;'></a></"+"div>");
				break;
			case '1': // On  for link port
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_orig+" alt='"+port+"' style='width:" + imgWidth + "px;'></a></"+"div>");
				break;
			case '0': // Off
			default:
				break;
		}
	}
	else
	{
		if(PortType == 0)
		{
			if (port == 15)
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else if (port == 16)
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
			}
			else
			{
				if(port%2 == 1)  // up
				{
					port_green="../Icon/DGS-3200/Port_Up_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
				}
				else     // Down
				{
					port_green="../Icon/DGS-3200/Port_Down_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
				}
			}
		}
		else if(PortType == 1)
		{
			port_green="../Icon/DGS-3200/Fiber_Green.gif";
			port_orig="../Icon/DGS-3200/Fiber_Orig.gif";
		}
		else
		{
			if (port == 15)
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else if (port == 16)
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else
			{
				if(port%2 == 1)  // up
				{
					port_green="../Icon/DGS-3200/Port_Up_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
				}
				else     // Down
				{
					port_green="../Icon/DGS-3200/Port_Down_Green.gif";
					port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
				}
			}
		}

		switch(ledValue)
		{
			case '2': // link up with speed 1000
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_green+" alt='"+port+"'></a></"+"div>");
				break;
			case '1': // On  for link port
				document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_orig+" alt='"+port+"'></a></"+"div>");
				break;
			case '0': // Off
			default:
				break;
		}
	}
	
}


function DrawPort_3200(x,y,UnitID,port,ledValue,PortType)
{
	var port_green,port_orig;
	
	if(PortType == 0)
	{
		if (port == 9 || port == 10)
		{
			port_green="../Icon/DGS-3200/Port_Down_Green.gif";
			port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
		}
		else
		{
			if(port%2 == 1)  // up
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else     // Down
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
			}
		}
	}
	else if(PortType == 1)
	{
		port_green="../Icon/DGS-3200/Fiber_Green.gif";
		port_orig="../Icon/DGS-3200/Fiber_Orig.gif";
	}
	else
	{
		if (port == 9 || port == 10)
		{
			port_green="../Icon/DGS-3200/Port_Down_Green.gif";
			port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
		}
		else
		{
			if(port%2 == 1)  // up
			{
				port_green="../Icon/DGS-3200/Port_Up_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Up_Orig.gif";
			}
			else     // Down
			{
				port_green="../Icon/DGS-3200/Port_Down_Green.gif";
				port_orig="../Icon/DGS-3200/Port_Down_Orig.gif";
			}
		}
	}
	
	switch(ledValue)
	{
		case '2': // link up with speed 1000
			document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_green+" alt='"+port+"'></a></"+"div>");
			break;
		case '1': // On  for link port
			document.write("<div style='position:absolute;left:"+x+";top:"+y+";'><a href='javascript:parent.doPanelPortLink("+UnitID+","+port+")' alt="+port+"><img border=0 src="+port_orig+" alt='"+port+"'></a></"+"div>");
			break;
		case '0': // Off
		default:
			break;
	}
}

function DrawLed(x1, y1, img1, img2,ledValue)
{
	var imgLed_Green = img1;
	var imgLed_Orig = img2;

	switch(ledValue)
	{
		case '2': // link up with speed 1000
			document.write("<div style='position:absolute;left:"+x1+"px; top:"+y1+"px;'><img src='"+imgLed_Green+"' /></"+"div>");
			break;
		case '1': // On  for link port
			document.write("<div style='position:absolute;left:"+x1+";top:"+y1+";'><img src='"+imgLed_Orig+"' /></"+"div>");
			break;
		case '0': // Off
		default:
			break;
	}

}

