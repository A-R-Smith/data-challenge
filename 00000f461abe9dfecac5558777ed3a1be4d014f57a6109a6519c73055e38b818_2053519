<script language="Javascript">
function addOption(obj, text, value)
{
	var option=document.createElement("option");
	option.text = text;
	option.value = value;
	try
	{
		obj.add(option,null);
	}
	catch(ex)
	{
		obj.add(option);
	}
}
			
function checkAuto(index)
{
	var s = document.formACLWizard;

	if (s.auto.checked == true && s.T6.value==1)
	{
		s.h_auto.value = "1";
		s.access.disabled = true;
		s.access.style.backgroundColor='#D4D0C8';
	}
	else
	{
		s.h_auto.value = "0";
		s.access.disabled = false;
		s.access.style.backgroundColor='#FFFFFF';
	}		
}

function applybtn()
{
	if(g_IsHeightVersion)
	{
		if (document.formACLWizard.T6.value=="1" && !checkTxtInput(document.formACLWizard.pro_name))
		{
			return false;
		}
	}
	
	if(document.formACLWizard.T6.value=="1"&&!checkNumberInput(document.formACLWizard.profile,1,200))
	{
		return;
	}
	else if(document.formACLWizard.T6.value=="2"&&!checkNumberInput(document.formACLWizard.profile,1,5))
	{
		return;
	}

	if(document.formACLWizard.T6.value=="1"&& document.formACLWizard.auto.checked == false &&!checkNumberInput(document.formACLWizard.access,1,200))
	{
		return;
	}	
	else if(document.formACLWizard.T6.value=="2" && document.formACLWizard.auto.checked == false &&!checkNumberInput(document.formACLWizard.access,1,100))
	{
		return;
	}
	
	/*changed by beyond zhou 2009.4.2*/
	/*From*/
	if(document.formACLWizard.D1.value==2)
	{
		if(document.formACLWizard.T1.value!=""&&!MAC_Address_Check(document.formACLWizard.T1))
		{
			return;
		}
	}
	if(document.formACLWizard.D1.value==3)
	{	
		if(document.formACLWizard.T10.value!=""&&!ipCheck(document.formACLWizard.T10))
		{
			return;
		}
	}
	if(document.formACLWizard.D1.value==4)
	{		
		if(document.formACLWizard.T100.value!=""&&!ipv6_check(document.formACLWizard.T100))
		{
			return;
		}
	}
	/*To*/
	if(document.formACLWizard.D2.value==2)
	{		
		if(document.formACLWizard.T2.value!=""&&!MAC_Address_Check(document.formACLWizard.T2))
		{
			return;
		}
	}
	if(document.formACLWizard.D2.value==3)
	{		
		if(document.formACLWizard.T20.value!=""&&!ipCheck(document.formACLWizard.T20))
		{
			return;
		}
	}
	if(document.formACLWizard.D2.value==4)
	{		
		if(document.formACLWizard.T200.value!=""&&!ipv6_check(document.formACLWizard.T200))
		{
			return;
		}
	}
	/*changed by beyond zhou end*/	   
	if (document.formACLWizard.S5.value!=""&&!checkNumberInput(document.formACLWizard.S5,0,65535))
	{
		return;
	}
	else if (document.formACLWizard.S6.value!=""&&!checkNumberInput(document.formACLWizard.S6,0,65535))
	{
		return;
	}
	else if (document.formACLWizard.S7.value!=""&&!checkNumberInput(document.formACLWizard.S7,0,65535))
	{
		return;
	}
	else if (document.formACLWizard.S8.value!=""&&!checkNumberInput(document.formACLWizard.S8,0,65535))
	{
		return;
	}
	else if (document.formACLWizard.S10.value!=""&&!checkHexInput(document.formACLWizard.S10))
	{
		return;
	}
	else if (document.formACLWizard.S11.value!=""&&!checkNumberInput(document.formACLWizard.S11,0,7))
	{
		return;
	}
	else if (document.formACLWizard.S12.value!=""&&!checkHexInput(document.formACLWizard.S12))
	{
		return;
	}
	else if (document.formACLWizard.S13.value!=""&&!checkNumberInput(document.formACLWizard.S13,0,255))
	{
		return;
	}
	
	if (document.formACLWizard.A3.value!=""&&!checkNumberInput(document.formACLWizard.A3,1,15625))
	{
		return;
	}
	else if (document.formACLWizard.A4.value!=""&&!checkNumberInput(document.formACLWizard.A4,0,7))
	{
		return;
	}
	else if (document.formACLWizard.A5.value!=""&&!checkNumberInput(document.formACLWizard.A5,0,63))
	{
		return;
	}
	else if (document.formACLWizard.A6.value!=""&&!checkNumberInput(document.formACLWizard.A6,0,7))
	{
		return;
	}
	
	if (document.formACLWizard.D6.options.selectedIndex == 0 || !g_IsHeightVersion)
	{
		if(!check_portlist(document.formACLWizard.port))
		{
			return;
		}
	}
	else if (document.formACLWizard.D6.options.selectedIndex == 1)
	{
		if(!checkTxtInput(document.formACLWizard.vlan_name))
		{
			return;
		}
	}
	else if (document.formACLWizard.D6.options.selectedIndex == 2)
	{
		if(!checkNumberInput(document.formACLWizard.vlan_id,1,4094))
		{
			return;
		}
	}
	
	if(document.formACLWizard.D1.value=="1" && document.formACLWizard.D2.value=="1")
	{
		alert("Invalid Rule");		
		return;
	}
	else if(document.formACLWizard.D1.value=="2"&&document.formACLWizard.T1.value=="" && document.formACLWizard.T2.value==""
			&&document.formACLWizard.S16.value=="" && document.formACLWizard.S10.value==""&&document.formACLWizard.S11.value=="")
	{
		alert("Invalid Rule");
		return;
	}
	else if(document.formACLWizard.D1.value=="3"&&document.formACLWizard.T10.value=="" && document.formACLWizard.T20.value==""
			&&document.formACLWizard.S5.value=="" && document.formACLWizard.S6.value==""&&document.formACLWizard.S7.value==""
			&&document.formACLWizard.S8.value==""&&document.formACLWizard.S9.value==""&&document.formACLWizard.D3.value!="2"
			&&document.formACLWizard.D3.value!="3"&&document.formACLWizard.D3.value!="4"&&document.formACLWizard.D3.value!="7")
	{
		alert("Invalid Rule");
		return;
	}
	else if(g_IsHeightVersion && 
		document.formACLWizard.D1.value=="4" && document.formACLWizard.T100.value=="" && document.formACLWizard.T200.value==""
		&& document.formACLWizard.S12.value=="" && document.formACLWizard.S13.value=="" && document.formACLWizard.TCPSrcPortMask.value=="" &&
		document.formACLWizard.TCPDesPortMask.value=="" && document.formACLWizard.UDPSrcPortMask.value=="" && document.formACLWizard.UDPDesPortMask.value==""&&
		document.formACLWizard.D32.options[3].selected == false && document.formACLWizard.D32.options[6].selected == false)
	{
		alert("Invalid Rule");
		return;
	}
	if ((document.formACLWizard.D1.options[3].selected == true ||document.formACLWizard.D2.options[3].selected == true))
	{
		if (document.formACLWizard.T100.value.length == 0 && document.formACLWizard.T200.value.length == 0)
		{
			if (document.formACLWizard.T6.options[0].selected ==true)
			{
				if (g_IsHeightVersion && document.formACLWizard.D32.options[4].selected == true)
				{
					if (!checkHexInput(document.formACLWizard.TCPSrcPortMask))
					{
						return false;
					}
				}
				
				if (g_IsHeightVersion && document.formACLWizard.D32.options[5].selected == true)
				{
					if (!checkHexInput(document.formACLWizard.TCPDesPortMask))
					{
						return false;
					}
				}
				if (g_IsHeightVersion && document.formACLWizard.D32.options[7].selected == true)
				{
					if (!checkHexInput(document.formACLWizard.UDPSrcPortMask))
					{
						return false;
					}
				}
				if (g_IsHeightVersion && document.formACLWizard.D32.options[8].selected == true)
				{
					if (!checkHexInput(document.formACLWizard.UDPDesPortMask))
					{
						return false;
					}
				}
			}
		}
	}
	
	if(document.formACLWizard.T1.value!=""&&document.formACLWizard.D1.value==2)
	{
		document.formACLWizard.s_mac_flag.value=1;
	}
	else if(document.formACLWizard.T10.value!=""&&document.formACLWizard.D1.value==3)
	{
		document.formACLWizard.s_ip_flag.value=1;
	}
	else if(document.formACLWizard.T100.value!=""&&document.formACLWizard.D1.value==4)
	{
		document.formACLWizard.s_ipv6_flag.value=1;
	}
	
	if(document.formACLWizard.T2.value!=""&&document.formACLWizard.D2.value==2)
	{
		document.formACLWizard.d_mac_flag.value=1;
	}
	else if(document.formACLWizard.T20.value!=""&&document.formACLWizard.D2.value==3)
	{
		document.formACLWizard.d_ip_flag.value=1;
	}
	else if(document.formACLWizard.T200.value!=""&&document.formACLWizard.D2.value==4)
	{
		document.formACLWizard.d_ipv6_flag.value=1;
	}
	
	if(document.formACLWizard.D3.value=='2')
	{
		document.formACLWizard.icmp_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='3')
	{
		document.formACLWizard.igmp_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='4')
	{
		document.formACLWizard.tcp_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='7')
	{
		document.formACLWizard.udp_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='5')
	{
		document.formACLWizard.tcp_s_port_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='6')
	{
		document.formACLWizard.tcp_d_port_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='8')
	{
		document.formACLWizard.udp_s_port_flag.value=1;
	}
	else if(document.formACLWizard.D3.value=='9')
	{
		document.formACLWizard.udp_d_port_flag.value=1;
	}
	else if(document.formACLWizard.D1.value=='3'&&document.formACLWizard.D3.value=='10')
	{
		document.formACLWizard.vlan_flag.value=1;
	}
	
	if(document.formACLWizard.D1.value=='2'&&document.formACLWizard.D31.value=='2')
	{
		document.formACLWizard.vlan_mac_flag.value=1;
	}
	else if(document.formACLWizard.D31.value=='3')
	{
		document.formACLWizard.eth_flag.value=1;
	}
	else if(document.formACLWizard.D31.value=='4')
	{
		document.formACLWizard.dot1_flag.value=1;
	}
	
	if(document.formACLWizard.D32.value=='2')
	{
		document.formACLWizard.flow_flag.value=1;
	}	
	else if(document.formACLWizard.D32.value=='3')
	{
		document.formACLWizard.class_flag.value=1;
	}	
	
	/*add by beyond zhou 2009.3.30*/
	/* checked flow Lable*/
	if(document.formACLWizard.D1.value == 4)
	{
		if(document.formACLWizard.T100.value=='')
		{
			if(document.formACLWizard.D32.value ==2 )
			{
			if(!checkHexInput(document.formACLWizard.S12))
			{				
				return;
			}
			else if(document.formACLWizard.S12.value=="")
			{
				alert("Invalid input!");
				document.formACLWizard.S12.focus();
				return;
				}
			}
		}
	}
	/* checked flow Lable End*/
	
		/* checked class*/
	if(document.formACLWizard.D1.value == 4)
	{
		
		if(document.formACLWizard.T100.value=='')
		{
			if(document.formACLWizard.D32.value ==3 )
			{
				if(!checkNumberInput(document.formACLWizard.S13,0,255))
				{				
					return;
				}
				else if(document.formACLWizard.S13.value=="")
				{
					alert("Invalid input!");
					document.formACLWizard.S13.focus();
					return;
				}
			}
		}
	}
	/* checked class End*/
	document.formACLWizard.submit();
}
function chgToPortIndex1(control,index)
{
	if(!control.options[0].selected)  // non-any
	{
		if(control.options[1].selected ==true)
		{
			init2();
			document.getElementsByName("T10")[0].value="";
			document.getElementsByName("T100")[0].value="";
			document.getElementsByName("T20")[0].value="";
			document.getElementsByName("T200")[0].value="";

			
			
			if(document.formACLWizard.D2.options[0].selected == false)//any
			{
				document.formACLWizard.D2.options[1].selected = true;
				document.getElementsByName("T1")[0].readOnly=false;
				document.getElementsByName("T2")[0].readOnly=false;
				document.getElementById("com11").style.display="none";
				document.getElementById("com21").style.display="none";
				document.getElementById("com111").style.display="none";
				document.getElementById("com211").style.display="none";
				document.getElementById("com1").style.display="block";
				document.getElementById("com2").style.display="block";
			}
			else
			{
				document.getElementsByName("T1")[0].readOnly=false;
				document.getElementById("com11").style.display="none";
				document.getElementById("com111").style.display="none";
				document.getElementById("com1").style.display="block";
			}
			document.getElementById("D31").style.display="block";
			document.getElementById("D3").style.display="none";
			document.getElementById("D32").style.display="none";
			document.formACLWizard.D5.value="1";
			if(document.formACLWizard.D5.disabled==false)
			{
				document.getElementById("AC3").style.display="block";
				document.getElementById("AC4").style.display="none";
				document.getElementById("AC5").style.display="none";
				document.getElementById("AC6").style.display="none";
			}
			document.formACLWizard.D31.value="1"
		}
		else if(control.options[2].selected ==true)
		{
			init2();
			document.getElementsByName("T1")[0].value="";
			document.getElementsByName("T100")[0].value="";
			document.getElementsByName("T2")[0].value="";
			document.getElementsByName("T200")[0].value="";
			
			
			if(document.formACLWizard.D2.options[0].selected ==  false)
			{
				document.formACLWizard.D2.options[2].selected = true;
				document.getElementsByName("T10")[0].readOnly=false;
				document.getElementsByName("T20")[0].readOnly=false;
				document.getElementById('com1').style.display="none";
				document.getElementById('com2').style.display="none";
				document.getElementById('com11').style.display="block";
				document.getElementById('com21').style.display="block";
				document.getElementById('com111').style.display="none";
				document.getElementById('com211').style.display="none";
			}
				
			else
			{
				document.getElementsByName("T10")[0].readOnly=false;
				document.getElementById('com1').style.display="none";
				document.getElementById('com11').style.display="block";
				document.getElementById('com111').style.display="none";
			}
			document.getElementById("D3").style.display="block";
			document.getElementById("D31").style.display="none";
			document.getElementById("D32").style.display="none";
			document.formACLWizard.D5.value="1";
			if(document.formACLWizard.D5.disabled==false)
			{
				document.getElementById("AC3").style.display="block";
				document.getElementById("AC4").style.display="none";
				document.getElementById("AC5").style.display="none";
				document.getElementById("AC6").style.display="none";
			}
			document.formACLWizard.D3.value="1";
		}
				
		else if(control.options[3].selected ==true)
		{
			init2();
			document.getElementsByName("T1")[0].value="";
			document.getElementsByName("T10")[0].value="";
			document.getElementsByName("T2")[0].value="";
			document.getElementsByName("T20")[0].value="";
			
			if(document.formACLWizard.D2.options[0].selected == false)
			{
				document.formACLWizard.D2.options[3].selected = true;
				document.getElementsByName("T100")[0].readOnly=false;
				document.getElementsByName("T200")[0].readOnly=false;
				document.getElementById('com1').style.display="none";
				document.getElementById('com2').style.display="none";
				document.getElementById('com11').style.display="none";
				document.getElementById('com21').style.display="none";
				document.getElementById('com111').style.display="block";
				document.getElementById('com211').style.display="block";
			}			
			else
			{
				document.getElementsByName("T100")[0].readOnly=false;
				document.getElementById('com1').style.display="none";
				document.getElementById('com11').style.display="none";
				document.getElementById('com111').style.display="block";
			}
			document.getElementById("D32").style.display="block";
			document.getElementById("D31").style.display="none";
			document.getElementById("D3").style.display="none";
			document.formACLWizard.D5.value="1";
			if(document.formACLWizard.D5.disabled==false)
			{
				document.getElementById("AC3").style.display="block";
				document.getElementById("AC4").style.display="none";
				document.getElementById("AC5").style.display="none";
				document.getElementById("AC6").style.display="none";
			}
			document.formACLWizard.D32.value="1"
		}
	}
	else  
	{
		document.getElementsByName("T1")[0].value="";
		document.getElementsByName("T10")[0].value="";
		document.getElementsByName("T100")[0].value="";
		document.getElementsByName("T2")[0].value="";
		document.getElementsByName("T20")[0].value="";
		document.getElementsByName("T200")[0].value="";
		

		if(document.formACLWizard.D2.options[0].selected == true)
		{
			document.getElementById("com5").style.display="none";
			document.getElementById("D32").style.display="none";
			document.getElementById("D31").style.display="none";
			document.getElementById("D3").style.display="none";
			document.getElementById("ST5").style.display="none";
			document.getElementById("ST6").style.display="none";
			document.getElementById("ST7").style.display="none";
			document.getElementById("ST8").style.display="none";
			document.getElementById("ST9").style.display="none";
			document.getElementById("ST10").style.display="none";
			document.getElementById("ST11").style.display="none";
			document.getElementById("ST12").style.display="none";
			document.getElementById("ST13").style.display="none";
			document.getElementById("ST14").style.display="none";
			document.getElementById("ST15").style.display="none";
			document.getElementById("ST17").style.display="none";
			document.getElementById("ST18").style.display="none";
			document.getElementById("ST16").style.display="none";
		}
		document.getElementById('com1').style.display="none";
		//document.getElementById('com2').style.display="none";
		document.getElementById('com11').style.display="none";
		//document.getElementById('com21').style.display="none";
		document.getElementById('com111').style.display="none";
		//document.getElementById('com211').style.display="none";
	}
}
function chgToPortIndex2(control,index)
{
	if(!control.options[0].selected)  // non-any
	{
		if(control.options[1].selected ==true)
		{
			init2();
			document.getElementsByName("T10")[0].value="";
			document.getElementsByName("T100")[0].value="";			
			document.getElementsByName("T20")[0].value="";
			document.getElementsByName("T200")[0].value="";
			
			if(document.formACLWizard.D1.options[0].selected == false)
			{
				document.formACLWizard.D1.options[1].selected = true;
				document.getElementsByName("T1")[0].readOnly=false;
				document.getElementsByName("T2")[0].readOnly=false;
				document.getElementById("com11").style.display="none";
				document.getElementById("com21").style.display="none";
				document.getElementById("com111").style.display="none";
				document.getElementById("com211").style.display="none";
				document.getElementById("com1").style.display="block";
				document.getElementById("com2").style.display="block";
			}			
			else
			{
				document.getElementsByName("T2")[0].readOnly=false;
				document.getElementById("com21").style.display="none";
				document.getElementById("com211").style.display="none";
				document.getElementById("com2").style.display="block";
			}
			document.getElementById("D31").style.display="block";
			document.getElementById("D3").style.display="none";
			document.getElementById("D32").style.display="none";
			document.formACLWizard.D5.value="1";
			if(document.formACLWizard.D5.disabled==false)
			{
				document.getElementById("AC3").style.display="block";
				document.getElementById("AC4").style.display="none";
				document.getElementById("AC5").style.display="none";
				document.getElementById("AC6").style.display="none";
			}
			document.formACLWizard.D31.value="1"
		}
		else if(control.options[2].selected ==true)
		{
			init2();
			document.getElementsByName("T2")[0].value="";
			document.getElementsByName("T200")[0].value="";
			document.getElementsByName("T1")[0].value="";
			document.getElementsByName("T100")[0].value="";

			
			if(document.formACLWizard.D1.options[0].selected == false)
			{
				document.formACLWizard.D1.options[2].selected = true;
				document.getElementsByName("T10")[0].readOnly=false;
				document.getElementsByName("T20")[0].readOnly=false;
				document.getElementById('com1').style.display="none";
				document.getElementById('com2').style.display="none";
				document.getElementById('com11').style.display="block";
				document.getElementById('com21').style.display="block";
				document.getElementById('com111').style.display="none";
				document.getElementById('com211').style.display="none";
			}
			else
			{
				document.getElementsByName("T20")[0].readOnly=false;
				document.getElementById('com2').style.display="none";
				document.getElementById('com21').style.display="block";
				document.getElementById('com211').style.display="none";
			}
			document.getElementById("D3").style.display="block";
			document.getElementById("D31").style.display="none";
			document.getElementById("D32").style.display="none";
			document.formACLWizard.D5.value="1";
			if(document.formACLWizard.D5.disabled==false)
			{
				document.getElementById("AC3").style.display="block";
				document.getElementById("AC4").style.display="none";
				document.getElementById("AC5").style.display="none";
				document.getElementById("AC6").style.display="none";
			}
			document.formACLWizard.D3.value="1";
	
		}
		else if(control.options[3].selected ==true)
		{
			init2();
			document.getElementsByName("T2")[0].value="";
			document.getElementsByName("T20")[0].value="";
			document.getElementsByName("T1")[0].value="";
			document.getElementsByName("T10")[0].value="";
			
			
			if(document.formACLWizard.D1.options[0].selected == false)
			{
				document.formACLWizard.D1.options[3].selected = true;
				document.getElementsByName("T100")[0].readOnly=false;
				document.getElementsByName("T200")[0].readOnly=false;
				document.getElementById('com1').style.display="none";
				document.getElementById('com2').style.display="none";
				document.getElementById('com11').style.display="none";
				document.getElementById('com21').style.display="none";
				document.getElementById('com111').style.display="block";
				document.getElementById('com211').style.display="block";
			}
			else
			{
				document.getElementsByName("T200")[0].readOnly=false;
				document.getElementById('com2').style.display="none";
				document.getElementById('com21').style.display="none";
				document.getElementById('com211').style.display="block";
			}
			document.getElementById("D32").style.display="block";
			document.getElementById("D31").style.display="none";
			document.getElementById("D3").style.display="none";
			document.formACLWizard.D5.value="1";
			if(document.formACLWizard.D5.disabled==false)
			{
				document.getElementById("AC3").style.display="block";
				document.getElementById("AC4").style.display="none";
				document.getElementById("AC5").style.display="none";
				document.getElementById("AC6").style.display="none";
			}
			document.formACLWizard.D32.value="1";
			
		}
	}
	else  
	{
		document.getElementsByName("T2")[0].value="";
		document.getElementsByName("T20")[0].value="";
		document.getElementsByName("T200")[0].value="";
		document.getElementsByName("T1")[0].value="";
		document.getElementsByName("T10")[0].value="";
		document.getElementsByName("T100")[0].value="";
		
		
		if(document.formACLWizard.D1.options[0].selected == true)
		{
			document.getElementById("com5").style.display="none";
			document.getElementById("D32").style.display="none";
			document.getElementById("D31").style.display="none";
			document.getElementById("D3").style.display="none";
			document.getElementById("ST5").style.display="none";
			document.getElementById("ST6").style.display="none";
			document.getElementById("ST7").style.display="none";
			document.getElementById("ST8").style.display="none";
			document.getElementById("ST9").style.display="none";
			document.getElementById("ST10").style.display="none";
			document.getElementById("ST11").style.display="none";
			document.getElementById("ST12").style.display="none";
			document.getElementById("ST13").style.display="none";
			document.getElementById("ST14").style.display="none";
			document.getElementById("ST15").style.display="none";
			document.getElementById("ST17").style.display="none";
			document.getElementById("ST18").style.display="none";
			document.getElementById("ST16").style.display="none";
		}
		//document.getElementById('com1').style.display="none";
		document.getElementById('com2').style.display="none";
		//document.getElementById('com11').style.display="none";
		document.getElementById('com21').style.display="none";
		//document.getElementById('com111').style.display="none";
		document.getElementById('com211').style.display="none";
	}
}
