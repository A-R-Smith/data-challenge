<script language="JavaScript">

function ApplyIt(s,action)
{
		if(action=='1')
		{
			if(s.portlist.value !=""&&!check_portlist(s.portlist))
				return false;
		}
		if(action == '3')
			s.port_f.value='0';
		else
			s.port_f.value=s.port.value;
		s.operation.value = action;      /* 1--del  2--find */
		s.submit();
}

function checkAllPorts()
{
	var s = document.dhcpBindEntry;

	if (s.all.checked == true)
	{
		s.portlist.value="";
		s.all_ports.value='1';
		s.portlist.disabled = true;
		s.portlist.style.backgroundColor='#D4D0C8';
	}
	else
	{
		s.portlist.value = '';
		s.all_ports.value='0';
		s.portlist.disabled = false;
		s.portlist.style.backgroundColor='#FFFFFF';
	}
}

</script>
