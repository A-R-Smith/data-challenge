<form name=formBrowse_VLAN id=formBrowse_VLAN>
  <table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" width=100%>
   <tr class='gh'>
   <script>
   if (Stacking_Mode == '1')
   	document.wirte("<td width=30 rowspan='3' >Unit</td>");
    </script>
    <td align=center colSpan=26>Port</td></tr>

<script language="Javascript">
   	document.write("<tr class=gh>");

	for(i=0; i<Port_Number.length; i++)
	{
		if(i >= 26)
		{
			document.write("</tr><tr class=gh>")
		}
		for(i=0; i<Port_Number.length; i++)
		{
			if(parseInt(Port_Number[i]%2)==1)
				document.write("  <td>" + Port_Number[i] + "</td>");
		}

		document.write("</tr><tr class=gh>");

		for(i=0; i<Port_Number.length; i++)
		{
			if(parseInt(Port_Number[i]%2)==0)
				document.write("  <td>" + Port_Number[i] + "</td>");
			}
	}

   	if(Port_Number.length > 26)
   	{
   	   	document.write("</tr>");
   	}

	document.write("</tr>");

	if (Stacking_Mode == '1')
	{
		for(i=0; i<Unit_Setting.length; i++)
		{
			if (i%2 == 0)
				trClass = 'gx';
			else
				trClass = 'gw';

			document.write("<tr class=" + trClass + ">");
			document.write("<td rowspan='2'>" + Unit_Setting[i] + "</td>");

			for(j=0; j<Port_Number.length; j++)
			{
				if(parseInt(Port_Number[j]%2)==1)
					document.write("  <td>"+Router_Port[i][j]+"</td>");
			}

			document.write("</tr><tr class="+trClass+">");
			for(j=0; j<Port_Number.length; j++)
			{
				if(parseInt(Port_Number[j]%2)==0)
				{
					document.write("  <td>"+Router_Port[i][j]+"</td>");
				}
			}
			document.write("</tr>");
		}
	}
	else
	{
		trClass = 'gx';
		document.write("<tr class=" + trClass + ">");

		for(j=0; j<Port_Number.length; j++)
		{
			if(parseInt(Port_Number[j]%2)==1)
			{
				if (Mon_VLAN[4][0].substring(j, j+1) == '0')
					document.write("<td>&nbsp;</td>");
				else
					document.write("  <td>"+Mon_VLAN[4][0].substring(j, j+1)+"</td>");
			}
		}

		document.write("</tr><tr class="+trClass+">");
		for(j=0; j<Port_Number.length; j++)
		{
			if(parseInt(Port_Number[j]%2)==0)
			{
				if (Mon_VLAN[4][0].substring(j, j+1) == '0')
					document.write("<td>&nbsp;</td>");
				else
					document.write("  <td>"+Mon_VLAN[4][0].substring(j, j+1)+"</td>");
			}
		}
		document.write("</tr>");
	}
    
    document.write("</tr>");
</script>
