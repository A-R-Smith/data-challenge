<body class=bodyLogin>
    <script language="Javascript">

function checkUserInput(obj)
{
	var i, ch;
	var len = obj.value.length;
	
	if (obj.value == "")
       {
          	alert("Please input parameter!");
		obj.focus();
		obj.select();
		return false; 
       }
	if(obj.value.search(" ") != -1)
	{
          	alert("The value can not include space!");
		obj.focus();
		obj.select();
		return false; 
       }
	
	for(i = 0; i < len; i++)
	{
		ch = obj.value.charCodeAt(i);
		if(ch > 0x7e || ch < 0x20)
		{	
			alert("Input Error! Unknown characters detected !");
			obj.focus();
			obj.select();
			return false;
		}	  
	}
	return true;
}

function chkUser()
{
	if (!checkUserInput(document.formLogin.user))
		return;

	if (document.formLogin.pass.value.length != 0)
	{
		if (!checkUserInput(document.formLogin.pass))
			return;
	}

	document.formLogin.submit();
}

function clearUser()
{
	document.formLogin.user.value = '';
	document.formLogin.pass.value = '';
}
    </script>
        
     <div name=tabBigTitleLogin id=tabBigTitleLogin class=tabBigTitleLogin><table>
