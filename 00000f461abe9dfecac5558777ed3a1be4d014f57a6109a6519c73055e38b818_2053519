<script language="Javascript">
	var mmode_count = 0;

	function show_counter()
	{
		var str = "<b>Multicast Filtering Mode Table&nbsp;&nbsp;&nbsp;&nbsp;Total Entries: "+ mmode_count + "</b>";
			document.getElementById('mmode_counter').innerHTML = str;
	}


function CheckAll(s)
{
	if (s.all.checked == true)
	{
		s.E1.disabled = true;
		s.isAll.value = '1';
	}
	else
	{
		s.E1.disabled = false;
		s.isAll.value='0';
	}
}

function init()
{
	var s = document.forms[0];
	s.all.checked = false;
	s.isAll.value='0';
}
function checkForm(s)
{
	if (s.all.checked == false)
	{
		if(!checkTxtInput(s.E1))
		{
			return false;
		}
	}
	s.submit();
	return true;
}
</script>
