<body class=body228>
<script language="Javascript">
	  	RunJava();


		function RunJava()
		{
			var s=document.forms[0];

			var UnitNum = 1;
			var portNum=Port_Number.length ;
			var rowNum=1+parseInt(portNum, 10);

			if(portNum > 47)
				document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=2600 HEIGHT=1600>");
			else
				document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=2600 HEIGHT=800>");
			document.write("<param NAME=DataPath value=\"../../html/Hjava_AuthDiagnostics.html?"+iport+"\">" );
			document.write("<param NAME=AppletName value=\"Authenticator Diagnostics\">");
			document.write("<param NAME=TimeIntervalName value=\"Time Interval\">");
			document.write("<param NAME=PortNum value=5>");
			document.write("<param NAME=SlotNum value=",UnitNum,">");
			document.write("<param NAME=MajorTable value=0>");
			document.write("<param NAME=TableNum value=1>");
			document.write("<param NAME=Table0RowNum value=\""+rowNum+"\">");
			document.write("<param NAME=Table0Row0 value=Port>");
			for(i=1; i<=portNum; i++)
				document.write("<param NAME=\"Table0Row"+i+"\" value=\""+i+"\">");
			document.write("<param NAME=ColumNum value=19>");
			document.write("<param NAME=Colum0 value=&nbsp;>");
			document.write("<param NAME=Colum1 value=\"Connect Enter\">");
			document.write("<param NAME=Colum2 value=\"Connect LogOff\">");
			document.write("<param NAME=Colum3 value=\"Auth Enter\">");
			document.write("<param NAME=Colum4 value=\"Auth Success\">");
			document.write("<param NAME=Colum5 value=\"Auth Timeout\">");
			document.write("<param NAME=Colum6 value=\"Auth Fail\">");
			document.write("<param NAME=Colum7 value=\"Auth Reauth\">");
			document.write("<param NAME=Colum8 value=\"Auth Start\">");
			document.write("<param NAME=Colum9 value=\"Auth LogOff\">");
			document.write("<param NAME=Colum10 value=\"Authed Reauth\">");
			document.write("<param NAME=Colum11 value=\"Authed Start\">");
			document.write("<param NAME=Colum12 value=\"Authed LogOff\">");
			document.write("<param NAME=Colum13 value=Responses>");
			document.write("<param NAME=Colum14 value=AccessChallenges>");
			document.write("<param NAME=Colum15 value=OtherReqToSupp>");
			document.write("<param NAME=Colum16 value=NonNakRespFromSupp>");
			document.write("<param NAME=Colum17 value=\"Bac Auth Success\">");
			document.write("<param NAME=Colum18 value=\"Bac Auth Fail\">");
			document.write("</applet>");
		}

	</script>
	<script language="Javascript">
	if(!navigator.javaEnabled())
	{ 
		  document.write("<br>");
		  document.write("You have to install the Java Runtime Environment ( JRE ) to execute the java applet in this page");
	}
	</script>
