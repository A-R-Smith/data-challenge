					<td>
							<div id="divPortList">
							<script language="Javascript">
								function showportpanel(unitid, portnum)
								{
									var panstr = "";
									var ports_per_line = (Port_Number.length > 10) ? Port_Number.length /2 : Port_Number.length;
									var ports_in_line;
									panstr += "<table id=mainGray2 class=mainGrayNormal cellpadding=0 cellspacing=0 style=\"width:100%;\"><tbody>";
									panstr += "<tr class=gh><td width=60>&nbsp; Type</td><td width=80px>Select All</td>";
									// first line ports
									for( i=1; i<=ports_per_line; i++)
									{
										panstr += "<td >&nbsp;" + i + "</td>";
									}
									panstr += "</tr>";
									
									/* 1.Unicast ports */
									panstr += "<tr class=gw><td>Unicast</td><td><input type=button id=btnUAll value=\"Select All\" class=btnAll style='width:70px;' onClick=\"SelAll('U');\" /></td>";
									for(i=1; i<=ports_per_line; i++)
									{
										panstr += "<td><input type='checkbox' name='chkU'"+i+"' id=chkU"+i+" onclick='showPorts("+i+",\"boxUPorts\");'></td>";
									}
									panstr += "</tr>";
									
									/* 1.Multicast ports */
									panstr += "<tr class=gw><td>Multicast</td><td><input type=button id=btnMAll value=\"Select All\" class=btnAll style='width:70px;' onClick=\"SelAll('M');\" /></td>";
									for(i=1; i<=ports_per_line; i++)
									{
										panstr += "<td><input type='checkbox' name='chkM'"+i+"' id=chkM"+i+" onclick='showPorts("+i+",\"boxMPorts\");'></td>";
									}
									panstr += "</tr>";
									
									/* 2.Unicast ports */
									ports_in_line = portnum - ports_per_line;
									if(ports_in_line > 0) {
										panstr += "<tr class=gh><td>&nbsp; </td><td></td>";
										for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
											panstr += "<td>&nbsp;" + i + "</td>";
										for( i=0; i<ports_per_line - ports_in_line; i++)
										panstr += "<td></td>";
										panstr += "</tr>";
										panstr += "<tr class=gw><td>&nbsp;Unicast </td><td></td>";
										for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
											panstr += "<td><input type='checkbox' name='chikU"+i+"' id=chkU"+i+" onclick='showPorts("+i+",\"boxUPorts\");'></td>";
										for( i=0; i<ports_per_line - ports_in_line; i++)
											panstr += "<td></td>";
										panstr += "</tr>";
									}
									
									/* 2.Multicast ports */
									ports_in_line = portnum - ports_per_line;
									if(ports_in_line > 0) {
										panstr += "<tr class=gw><td>&nbsp;Multicast </td><td></td>";
										for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
											panstr += "<td><input type='checkbox' name='chikM"+i+"' id=chkM"+i+" onclick='showPorts("+i+",\"boxMPorts\");'></td>";
										for( i=0; i<ports_per_line - ports_in_line; i++)
											panstr += "<td></td>";
										panstr += "</tr>";
									}

									panstr += "</tbody></table>";
									document.getElementById('divPortList').innerHTML = panstr;		
								}
								showportpanel(1, Port_Number.length);
							</script>
							</div>  <!--div id="divPortList"-->
					</td>
					</tr>
					<tr>
					<td>
							<b>Unicast Filter Ports&nbsp;&nbsp;</b>
							<input class=flatL type="text" name="boxUPorts" id="boxUPorts" readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:auto;text-align:left; width:80%' onMouseOver="ddrivetip(this.value)"; onMouseOut="hideddrivetip()">
					</td>
					</tr>
					<td>
							<b>Multicast Filter Ports&nbsp;&nbsp;</b>
							<input class=flatL type="text" name="boxMPorts" id="boxMPorts" readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:auto;text-align:left; width:80%' onMouseOver="ddrivetip(this.value)"; onMouseOut="hideddrivetip()">
					</td>
					</tr>
				</table>
			</div><!--div id="portpanel" class=tabVLANContentDIV-->
