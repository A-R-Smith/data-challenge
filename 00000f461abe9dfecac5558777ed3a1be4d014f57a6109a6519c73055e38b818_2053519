				<input type=hidden name=CfgId id=CfgId maxlength=2 />
				<script language="Javascript">    
					var trClass = '';
					var txtClass = '';
					for(i=0; i<DualCfgSetting.length; i++)
					{
						if (i%2 == 0)
						{
							trClass = 'gx';
							txtClass = 'flatGX';
						}
						else
						{
							trClass = 'gw';
							txtClass = 'flatGW';
						}
						document.write("<tr class=" + trClass + ">");
						document.write("  <td>"+convert_int_2_char(DualCfgSetting[i][7])+ DualCfgSetting[i][0]+"</td>");
						
						/* filename */
						if(DualCfgSetting[i][8] != "")	/* if SDcard file */
						{
							tipStr = strNormalize(DualCfgSetting[i][8])
							document.write("<td align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(DualCfgSetting[i][8], 10)) + "</td>");
						}
						else
						{
								document.write("<td align=left>&nbsp;" + "--" + "</td>");
						}
						
						document.write("  <td>" + showEnpty(DualCfgSetting[i][1])  + "</td>");	
						document.write("  <td >"+showSize(DualCfgSetting[i][2]) +"</td>");
						document.write("  <td >" + DualCfgSetting[i][3]  + "</td>");
						
						/* from */
						if(DualCfgSetting[i][8] != "")	/* if SDcard file */
						{
							document.write("<td align=left>&nbsp;" + "--" + "</td>");
						}
						else
						{
							tipStr = strNormalize(DualCfgSetting[i][4])
							//document.write("  <td >"+ DualCfgSetting[i][4]+"</td>");
							document.write("<td onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(DualCfgSetting[i][4], 10)) + "</td>");
						}
						
						/* user */
						if(DualCfgSetting[i][8] != "")	/* if SDcard file */
						{
							document.write("<td align=left>&nbsp;" + "--" + "</td>");
						}
						else
						{
							tipStr = strNormalize(DualCfgSetting[i][5])
							document.write("<td onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(DualCfgSetting[i][5], 10)) + "</td>");
						}
					
						document.write("  <td>"+convert_int_3_char(DualCfgSetting[i][6])+"</td>");
						document.write("  <td align=right> ");
						document.write("	<input type=button value='Boot' maxlength=15 class=btnInGrayS onClick='BootIt("+i+")' ");
						if (0 == DualCfgSetting[i][2])
							document.write(" disabled");
						document.write(" />");
						document.write("      <input type=button  value='Active' class=btnInGrayS onClick='ActiveIt("+i+")' ");
						if (0 == DualCfgSetting[i][2])
							document.write(" disabled");
						document.write(" />");
						
						document.write(" <input type=button value='Delete' maxlength=15 class=btnInGrayS onClick='DelIt("+i+")'");
						if (0 == DualCfgSetting[i][2])
							document.write(" disabled");
						document.write(">");
						
						document.write("</td></tr>");
					}
					
					if (Panel_Info[4]==137) /* if dgs3200-24 */
					{
						if (i%2 == 0)
						{
							trClass = 'gx';
						}
						else
						{
							trClass = 'gw';
						}
						
						document.write('<tr class="' + trClass + '">');
						document.write('<td colspan="2">&nbsp;Path Name</td>');
						document.write('<td colspan="6">');
						document.write('<input type="text" id="boxPathname" name="boxPathname" maxlength="254" class="flatL" style="width:90%;" />');
						document.write('</td>');
						document.write('<td align="right">');
						document.write("	<input type=button value='Boot' maxlength=15 class=btnInGrayS onClick='BootIt("+(-1)+")' ");
						document.write(" />");
						document.write("      <input type=button  value='Active' class=btnInGrayS onClick='ActiveIt("+(-1)+")' ");
						document.write(" />");
						document.write("      <input type=button value='Delete' maxlength=15 class=btnInGrayS style='visibility:hidden;' onClick='DelIt("+(-1)+")' ");
						document.write(" />");
						document.write('</td>');
						document.write('</tr>');
					}
					</script>
					</form>
					</table>
					
					<form name=formSSHUser id=formSSHUser></form>
<p>
'*' means the currently actived configuration</p>
<p>(Console) means configuration update through Serial Port(RS232)</p>
<p>(Telnet) means configuration update through TELNET</p>
<p>	(SNMP) means configuration update through SNMP</p>			
<p>(WEB) means configuration update through WEB</p>
<script>
if('1'==ssh_include)
	document.write("<p>(SSH) means configuration update through SSH</p>");
if('1'==gm_include)
	document.write("<p>(SIM) means configuration update through Single IP Management</p>");	
</script>
</div>
