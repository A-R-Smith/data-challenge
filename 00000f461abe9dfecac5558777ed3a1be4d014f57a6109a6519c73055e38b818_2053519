    var trClass = '';
    var txtClass = '';
	document.write("<tbody class='scrollContent'>"); 
    for(i=0; i<DHCP_IP_MAC_Port_Settings.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGW';
        }   

	document.write("<tr class=" + trClass + ">");
	document.write("<td width=30%>" +DHCP_IP_MAC_Port_Settings[i][0]+"</td>");
	document.write("<td width=35%>&nbsp;" +DHCP_IP_MAC_Port_Settings[i][1]+"</td>");
	document.write("<td width=25%>" +DHCP_IP_MAC_Port_Settings[i][2]+"</td>");
