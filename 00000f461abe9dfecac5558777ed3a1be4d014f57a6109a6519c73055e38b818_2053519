<script language="Javascript">    
	function SelAll()
	{
    	var unitid;
    	var i;
    	var newval;
    	
    	var checkboxobj;

    	//unitid = parseInt(document.getElementById('editunit').value);

    	newval = '';
    	for(i=1; i<=parent.port_number; i++) 
	{		    
		    checkboxobj = document.getElementById('C'+i);
				checkboxobj.checked=true;	 
					newval += '1';
				
    	}

    	for(i=1;i<=parent.port_number;i++)
      {
      		checkboxobj = document.getElementsByName('C'+i);
	}
			
 	   	//GuestVlan_Setting[2][unitid-1] = newval;
	    	
  		ListPorts(newval);
	}
 	function getGVLANString(sourceport,typechar)
	{
        	var portValue = '';
        	var firtTime = true;
        	var iCounter = 0;	
        	var k;
        	for(k=0; k<sourceport.length; k++) 
		{
	        	for(j=1; j<=sourceport[k].length; j++)
			{
	        	    if (sourceport[k].substring(j-1, j) == typechar)
				{
	        	        iCounter++;
	        	
	        	        firtTime = false;
	        	        portValue += (k+1) + ":" + j +",";
	        	    }
	        	}	
        	}
        	return portValue;
	}

	function ListPorts(portnumber) 
	{
		var str = '';
		//str += getGVLANString(portnumber,'1');
		str += getPortNumber_withComma(portnumber);
		document.getElementById('PortSpan').value = str;
	}

	function showPorts(index) 
	{
    	var unitid;
    	var i;
    	var newval;
    	
    	var checkboxobj;

	if (Stacking_Mode =='1')
	    	unitid = parseInt(document.getElementById('editunit').value);
	else 
		unitid =1;
    	newval = '';
    	for(i=1; i<=parent.port_number; i++) {		    
		    checkboxobj = document.getElementById('C'+i);
				if(checkboxobj.checked)	 
					newval += '1';
				else
					newval += '0';
    	}

    	for(i=1;i<=parent.port_number;i++)
      {
      	checkboxobj = document.getElementsByName('C'+i);
			}
			
 	   	//GuestVlan_Setting[2][unitid-1] = newval;
	    	
  		ListPorts(newval);
    }

    function editchangeunit(unitid) 
    {
    	var uid = parseInt(unitid);
    	DisplayGVLANEditPane(uid);
    	ListPorts();
    	return 1;
    }

    function DisplayGVLANEditPane(unitid) 
    {
    	var i;
    	var uid = unitid;
    	document.getElementById('editunit').options[unitid-1].selected = true;
    	
    	document.getElementsByName('editunit')[0].checked = true;
	    for(i=1;i<=parent.port_number;i++) {
	    	var checkboxobj;
	    	checkboxobj = document.getElementById('C'+i);
	    	if (GuestVlan_Setting[2][uid-1].substring(i-1,i) == '0')
	    		checkboxobj.checked = false;
	    	else
	    		checkboxobj.checked = true;
  	
	    }
    }

</script>
<SCRIPT language ="Javascript">
function applyform(mode)
{
	if (!checkTxtInput(document.formGuestvlan.Vlanname))
		return;

	if (mode == '1')
		document.formGuestvlan.isClear.value = '0';
	else
		document.formGuestvlan.isClear.value = '1';
	
	document.formGuestvlan.submit();
}

initshowport(Guest_VLAN[1][0]);
document.formGuestvlan.Vlanname.value = Guest_VLAN[0];
</script>

