function init()
{
	var str;
	if(g_IsHeightVersion)
	{
		str = "Apply to";
	}
	else
	{
		str = "Ports";
	}
	document.getElementById("apply_to").innerHTML = str;
	chgToPortIndex5(document.formACLWizard.D5, 0);
	document.getElementById("com5").style.display="none";
	document.formACLWizard.D5.value="1";
	document.getElementById("profile_cpu").style.display="none";
	document.getElementById("profile_acl").style.display="block";
	document.getElementById("acc_cpu").style.display="none";
	document.getElementById("acc_acl").style.display="block";

	document.getElementsByName("s_ip_flag").value=0;
	document.getElementsByName("d_ip_flag").value=0;
	document.getElementsByName("s_ipv6_flag").value=0;
	document.getElementsByName("d_ipv6_flag").value=0;
	document.getElementsByName("s_mac_flag").value=0;
	document.getElementsByName("d_mac_flag").value=0;
	document.getElementsByName("icmp_flag").value=0;
	document.getElementsByName("igmp_flag").value=0;
	document.getElementsByName("tcp_flag").value=0;
	document.getElementsByName("tcp_s_port_flag").value=0;
	document.getElementsByName("tcp_d_port_flag").value=0;
	document.getElementsByName("udp_flag").value=0;
	document.getElementsByName("udp_s_port_flag").value=0;
	document.getElementsByName("udp_d_port_flag").value=0;
	document.getElementsByName("eth_flag").value=0;
	document.getElementsByName("vlan_flag").value=0;
	document.getElementsByName("vlan_mac_flag").value=0;
	document.getElementsByName("dot1_flag").value=0;
	document.getElementsByName("class_flag").value=0;
	document.getElementsByName("flow_flag").value=0;
	document.getElementsByName("h_auto").value=0;
}

