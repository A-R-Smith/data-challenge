<script language="Javascript">
    function FindIt()
    { 
    	var parastr='';
	var portlist;

	if(!check_portlist(document.MacTable.D1))
		return false;
	if(document.MacTable.C1.checked==false &&document.MacTable.C2.checked==false && document.MacTable.C3.checked==false)
	{
		alert("You must check at least a authentication state!" );
		document.MacTable.C1.select();
		document.MacTable.C1.focus();
		return false;
	}

    	if(document.MacTable.C1.checked==true)
			parastr+=",1";
	else
			parastr+=",0";
    	if(document.MacTable.C2.checked==true)
			parastr+=",1";
	else
			parastr+=",0";
    	if(document.MacTable.C3.checked==true)
			parastr+=",1";
	else
			parastr+=",0";	
	portlist = document.MacTable.D1.value;	
    	var reUrl = "77_JWAC_Host_Table.html?0"+parastr+","+portlist;
    	frames['ifrMac'].location.href=reUrl;
    }
    function ClearIt() 
    {    // url ? m,n,x,y,z           --------      m(0-brower 1-pre 2-next 3-delete)  n(portlist) x(C1) y(C2) z(C3)
    	var parastr='';
	var portlist;

	if(!check_portlist(document.MacTable.D1))
		return false;
	if(document.MacTable.C1.checked==false &&document.MacTable.C2.checked==false && document.MacTable.C3.checked==false)
	{
		alert("You must check at least a authentication state!" );
		document.MacTable.C1.select();
		document.MacTable.C1.focus();		
		return false;
	}
    	if(document.MacTable.C1.checked==true)
			parastr+=",1";
	else
			parastr+=",0";
    	if(document.MacTable.C2.checked==true)
			parastr+=",1";
	else
			parastr+=",0";
    	if(document.MacTable.C3.checked==true)
			parastr+=",1";
	else
			parastr+=",0";	
	portlist = document.MacTable.D1.value;	
    	var reUrl = "77_JWAC_Host_Table.html?3"+parastr+","+portlist;
    	frames['ifrMac'].location.href=reUrl;
    }

    function FindAll()
    { 
    	var parastr=',1,1,1';
    	var reUrl = "77_JWAC_Host_Table.html?0"+parastr+",all";
    	frames['ifrMac'].location.href=reUrl;
    }
    function ClearAll()
    { 
    	var parastr=',1,1,1';
    	var reUrl = "77_JWAC_Host_Table.html?3"+parastr+",all";
    	frames['ifrMac'].location.href=reUrl;
    }
</script>
