<script language="Javascript">
var f = document.formSIM_Action;
var sim_list_count;

function show_counter()
{
	var str = "<b>Total Entries: "+ sim_list_count + "</b>";
	document.getElementById('sim_list_counter').innerHTML = str;
}
function inputcheck()
{
	var i, ip;
	
	if (!ipCheck(f.ip_addr))
		return false;
	
	if(f.file_name.value == "" || f.file_name.value.search(" ") != -1)
	{
		alert("Invalid Path \ Filename.")
		f.file_name.select();
		f.file_name.focus();
		return false;
	}
	
	return true;
}



</script>
