			document.location.href=reUrl;

		}
		function Init()
		{
			document.forms[0].D1.selectedIndex = port_num -1;
		}
		function cleartable()
		{
			var szMsg='Clear the counters ?';
			if(confirm(szMsg))
			{
				return true;
			}
			else
				return false;
		}
		function RunJava()
		{
			document.write("<applet codebase=\"../classes\" code=\"MultipleLineChart.class\"  WIDTH=640 HEIGHT=420>");
			document.write("<param NAME=DataPath value=\"../../html/Hjava_PacketsRx.html?"+port_num+"\">" );
			document.write("<param NAME=ChartName value=\"RX Packets Analysis - Port: "+port_num+"\">");
