<script language="Javascript">
function EditStart(iTag)
{
	var bFLAG=false;
	var iTag_m, i;

	if(MSTI_Port_Setting.length !=1 )
	{
		for(i=0; i<MSTI_Port_Setting.length; i++)
		{
			if (document.formMSTIportset.editApply[i].style.display=='none')
			{
				bFLAG=false;
				iTag_m=' ';
			}    		
			else
			{
				bFLAG=true;
				iTag_m = i;
				break;
			}
		}
	
		if((iTag!=0|iTag!=MSTI_Port_Setting.length)&bFLAG==true)
		{	
			document.formMSTIportset.pathCost[iTag_m].style.display = '';
			document.formMSTIportset.pathCost_M[iTag_m].style.display = 'none';
			document.formMSTIportset.pri[iTag_m].style.display = '';
			document.formMSTIportset.pri_M[iTag_m].style.display = 'none';
			document.formMSTIportset.editButton[iTag_m].style.display = '';
			document.formMSTIportset.editApply[iTag_m].style.display = 'none';
		}

		document.formMSTIportset.pathCost[iTag].style.display = 'none';
		document.formMSTIportset.pathCost_M[iTag].style.display = '';
		document.formMSTIportset.pathCost_M[iTag].select();
		
		document.formMSTIportset.pri[iTag].style.display = 'none';
		document.formMSTIportset.pri_M[iTag].style.display = '';

		document.formMSTIportset.pathCost_M[iTag].value =MSTI_Port_Setting[iTag][2];
		document.formMSTIportset.pri_M[iTag].selectedIndex = MSTI_Port_Setting[iTag][3]/16;


		document.formMSTIportset.editButton[iTag].style.display = 'none';
		document.formMSTIportset.editApply[iTag].style.display = '';
	}
	else
	{
		document.formMSTIportset.pathCost.style.display = 'none';
		document.formMSTIportset.pathCost_M.style.display = '';
		document.formMSTIportset.pri.style.display = 'none';
		document.formMSTIportset.pri_M.style.display = '';

		document.formMSTIportset.pathCost_M.value =MSTI_Port_Setting[iTag][2];
		document.formMSTIportset.pri_M.selectedIndex = MSTI_Port_Setting[iTag][3]/16;

		document.formMSTIportset.editButton.style.display = 'none';
		document.formMSTIportset.editApply.style.display = '';
	}
}

function EditApplyIt(iTag)
{
	if(MSTI_Port_Setting.length !=1 )
	{
		if (!checkNumberInput(document.formMSTIportset.pathCost_M[iTag], 1, 200000000))
		{
			return false;
		}
		
		document.formMSTIportset.pathCost[iTag].style.display = '';
		document.formMSTIportset.pathCost_M[iTag].style.display = 'none';
		document.formMSTIportset.pri[iTag].style.display = '';
		document.formMSTIportset.pri_M[iTag].style.display = 'none';

		document.formMSTIportset.editButton[iTag].style.display = '';
		document.formMSTIportset.editApply[iTag].style.display = 'none';
	}
	else
	{
		if (!checkNumberInput(document.formMSTIportset.pathCost_M, 1, 200000000))
		{
			return false;
		}
		
		document.formMSTIportset.pathCost.style.display = '';
		document.formMSTIportset.pathCost_M.style.display = 'none';
		document.formMSTIportset.pri.style.display = '';
		document.formMSTIportset.pri_M.style.display = 'none';

		document.formMSTIportset.editButton.style.display = '';
		document.formMSTIportset.editApply.style.display = 'none';
	}
	
	document.formMSTIportset.D1.value = port;
	document.formMSTIportset.T1.value = MSTI_Port_Setting[iTag][0];
	document.formMSTIportset.T2.value = document.getElementsByName("pathCost_M")[iTag].value;
	document.formMSTIportset.T3.value = document.getElementsByName("pri_M")[iTag].value;
	document.formMSTIportset.submit();

	
}

</script>
</head>

