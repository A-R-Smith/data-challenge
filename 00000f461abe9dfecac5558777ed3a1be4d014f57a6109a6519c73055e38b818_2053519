<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
    document.write("<tbody class='scrollContent'>");  
    for(i=0; i<DHCP_Port_Settings.length;  i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGX';
        }
        
        document.write("<tr class=" + trClass + ">");
        document.write("  <td width=15%>" + DHCP_Port_Settings[i][0] + "</td>");
        document.write("  <td width=30%>" + DHCP_Port_Settings[i][1] + "</td>");
        document.write("</tr>");
    }
    
