    <script src="../../DataStore/TR_ACL.js" type="text/javascript"></script>
    <script language="Javascript">
		var Profileid = document.URL.split("?")[1].split(",")[0];
		var acl_style = document.URL.split("?")[1].split(",")[1];
		var profileidx = 0;
		var Profile_Type = 0; // 0:Ether 1:IP
		</script>
