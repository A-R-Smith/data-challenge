			document.location.href=reUrl;

		}
		function Init()
		{
			document.forms[0].D1.selectedIndex = port_num -1;
		}
		function cleartable()
		{
			var szMsg='Clear the counters ?';
			if(confirm(szMsg))
			{
				return true;
			}
			else
				return false;
		}
		function RunJava()
		{
			document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=600 HEIGHT=350>");
