<script language="Javascript">

function checktype()
{
	if(Profile_Type == 0)
	{
		document.aclruleAdd_Ether.style.display = 'inline';
		document.aclruleAdd_IP.style.display = 'none';
		document.aclruleAdd_IPv6.style.display = 'none';
		document.aclruleAdd_Packet_Content.style.display = 'none';
	}
	else if(Profile_Type == 1)
	{
		document.aclruleAdd_Ether.style.display = 'none';
		document.aclruleAdd_IPv6.style.display = 'none';
		document.aclruleAdd_IP.style.display = 'inline';
		document.aclruleAdd_Packet_Content.style.display = 'none';
	}
	else if(Profile_Type == 2)
	{
		document.aclruleAdd_Ether.style.display = 'none';
		document.aclruleAdd_IP.style.display = 'none';
		document.aclruleAdd_IPv6.style.display = 'inline';
		document.aclruleAdd_Packet_Content.style.display = 'none';
	}
	else if(Profile_Type == 3)
	{
		document.aclruleAdd_Ether.style.display = 'none';
		document.aclruleAdd_IP.style.display = 'none';
		document.aclruleAdd_IPv6.style.display = 'none';
		document.aclruleAdd_Packet_Content.style.display = 'inline';
	}

}

function ck_cpu_packet_content(index)
{
	var i;
	var s = document.aclruleAdd_Packet_Content;
	if (eval("s.C" + index + ".checked") == true)
	{
		eval("s.C"+(index+1) +".disabled = false");
		eval("s.C"+(index+2) +".disabled = false");
		eval("s.C"+(index+3) +".disabled = false");
		eval("s.C"+(index+4) +".disabled = false");
	}
	else
	{
		eval("s.C"+(index+1) +".disabled = true");
		eval("s.C"+(index+2) +".disabled = true");
		eval("s.C"+(index+3) +".disabled = true");
		eval("s.C"+(index+4) +".disabled = true");
	}
}

function chk_function(obj1, obj2,s)
{
	if (obj1.name == 'no_limit_flag' || obj1.name == 'all_port' || obj1.name == 'auto')
		obj2.disabled = obj1.checked ? true : false;
	else if (obj1.name == 'pri_flag')
	{
		obj2.disabled = obj1.checked ? false : true;
		s.replace_pri_flag.disabled = obj1.checked ? false : true;
	}
	else
		obj2.disabled = obj1.checked ? false : true;
}

function check_tcp_flag()
{
	var f = document.aclruleAdd_IP;

	if (f.tcp_flag.checked)
	{
		if (f.tcp_s_port != undefined)
			f.tcp_s_port.disabled = false;

		if (f.tcp_d_port != undefined)
			f.tcp_d_port.disabled = false;

		if (f.urg_flag != undefined)
			f.urg_flag.disabled = false;

		if (f.ack_flag != undefined)
			f.ack_flag.disabled = false;

		if (f.psh_flag != undefined)
			f.psh_flag.disabled = false;

		if (f.rst_flag != undefined)
			f.rst_flag.disabled = false;

		if (f.syn_flag != undefined)
			f.syn_flag.disabled = false;
		
		if (f.fin_flag != undefined)
			f.fin_flag.disabled = false;
	}
	else
	{
		if (f.tcp_s_port != undefined)
			f.tcp_s_port.disabled = true;

		if (f.tcp_d_port != undefined)
			f.tcp_d_port.disabled = true;

		if (f.urg_flag != undefined)
			f.urg_flag.disabled = true;

		if (f.ack_flag != undefined)
			f.ack_flag.disabled = true;

		if (f.psh_flag != undefined)
			f.psh_flag.disabled = true;

		if (f.rst_flag != undefined)
			f.rst_flag.disabled = true;

		if (f.syn_flag != undefined)
			f.syn_flag.disabled = true;
		
		if (f.fin_flag != undefined)
			f.fin_flag.disabled = true;
	}
}

function check_udp_flag()
{
	var f = document.aclruleAdd_IP;

	if (f.udp_flag.checked)
	{
		if (f.udp_s_port != undefined)
			f.udp_s_port.disabled = false;

		if (f.udp_d_port != undefined)
			f.udp_d_port.disabled = false;
	}
	else
	{
		if (f.udp_s_port != undefined)
			f.udp_s_port.disabled = true;

		if (f.udp_d_port != undefined)
			f.udp_d_port.disabled = true;
	}	
}

function check_icmp()
{
	var f = document.aclruleAdd_IP;

	if (f.icmp_flag.checked)
	{
		if (f.icmp_code != undefined)
			f.icmp_code.disabled = false;

		if (f.icmp_type != undefined)
			f.icmp_type.disabled = false;
	}
	else
	{
		if (f.icmp_code != undefined)
			f.icmp_code.disabled = true;

		if (f.icmp_type != undefined)
			f.icmp_type.disabled = true;
	}
}

function check_igmp()
{
	var f = document.aclruleAdd_IP;

	if (f.igmp_flag.checked)
	{
		if (f.igmp_type != undefined)
			f.igmp_type.disabled = false;
	}
	else
	{
		if (f.igmp_type != undefined)
			f.igmp_type.disabled = true;
	}
}

checktype();
