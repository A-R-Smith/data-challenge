<script language="Javascript">
	function getMirrorPortByType_withComma(sourceport, typechar){
		var portValue = '';
		var firtTime = true;
		var iCounter = 0;	
		var k;
		for(k=0; k<sourceport.length; k++) {
			for(j=1; j<=sourceport[k].length; j++){
				if (sourceport[k].substring(j-1, j) == typechar){
					iCounter++;
					if(firtTime == false)
						portValue += ', ';
					firtTime = false;
					portValue += '' + (k+1) + ":" + j;
				}
			}	
		}
		return portValue;
	}
	function getPortNumberby01_withComma (portMember){
			var portValue = '';
			var firtTime = true;
			var iCounter = 0;	
			var  k;
			for (k=1; k<=portMember.length; k++){
				if (portMember.substring(k-1, k) == '1'){
				if(firtTime == false)
					portValue += ', ';
				firtTime = false;
				portValue += k;
				}
			}
			return portValue;
		}
</script>
</head>

