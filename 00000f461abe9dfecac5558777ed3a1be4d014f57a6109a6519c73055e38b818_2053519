	<script language="Javascript">    
		var trClass = '';
		var txtClass = '';

		for(i=0; i<JWAC_Host.length; i++)
		{
		        if (i%2 == 0){
		            trClass = 'gx';
		            txtClass = 'flatGX';
		        }
		        else{
		            trClass = 'gw';
		            txtClass = 'flatGX';
		        }
		        document.write("<form name=formJWACHostDel id=formJWACHostDel method='POST' action='../../form/formJWACHostApply'>");
		        document.write("<tr class=" + trClass + ">");
