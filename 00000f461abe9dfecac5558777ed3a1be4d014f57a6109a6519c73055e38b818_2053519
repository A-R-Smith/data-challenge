	<script language="javascript">
		 function ChangeItem()
		{
			var Url;

			if(document.getElementById('D1').selectedIndex==1)
				{
				Url="77_AttackSystemLog_table.html?1,1";
				frames["ifrSystemLog"].location.href=Url;
				}
			else
				{
				Url="52_Systemlog_table.html?0,1";
				frames["ifrSystemLog"].location.href=Url;		
				}
		}

		function btnFind_Click()
		{
			var Url;
			if(document.getElementById('boxIndex').value != "")
			{
				if(/^[0-9]*[1-9][0-9]*$/.test(document.getElementById('boxIndex').value))	// integer(>0)
				{
					if(document.getElementById('D1').selectedIndex==1)
					{
						Url="77_AttackSystemLog_table.html?1,0," + document.getElementById('boxIndex').value;
					}
					else
					{
						Url="52_Systemlog_table.html?0,0," + document.getElementById('boxIndex').value;
					}
				}
				else
				{
					document.getElementById('boxIndex').focus();
					document.getElementById('boxIndex').select();
					alert("Please input an integer.");
					return false;
				}
			}
			else
			{
				if(document.getElementById('D1').selectedIndex==1)
				{
					Url="77_AttackSystemLog_table.html?1,1";
				}
				else
				{
					Url="52_Systemlog_table.html?0,1";
				}
			}
			
			frames["ifrSystemLog"].location.href=Url;		
		}
	 </script>
</head>


