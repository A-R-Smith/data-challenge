    <script language="Javascript">
	switch (function_id)
	{
		case 1:
			/* genHeaderTable('Download Firmware'); */
			genHeaderTable('Firmware Upgrade');
			break;
		case 2:
			genHeaderTable('Download Configuration');
			break;
		case 3:
			genHeaderTable('Upload Configuration');
			break;
		case 4:
			genHeaderTable('Upload Log File');
			break;
		default:
			function_id = 0;
			genHeaderTable('Firmware Upgrade');
			break;
	}
function draw_progress_bar(percent,table_width,clr_finished,clr_unfinished)
{
	var finished=percent*table_width/100;
	var unfinished=table_width-finished;
	if(finished==0)
		clr_finished=clr_unfinished;
	if(unfinished==0)
		clr_unfinished=clr_finished;
	document.write('<table border=0 cellspacing=1 cellpadding=0 width='+table_width+'>');
	document.write('<tr><td>');
	document.write('<table border=1 cellspacing=0 cellpadding=0 width='+table_width+'>');
	document.write('<tr><td>');
	document.write('<table border=0 cellspacing=0 cellpadding=0 width='+table_width+'>');
	document.write('<td bgcolor="'+clr_finished+'" width='+finished+'><font size=1>&nbsp;</td>');
	document.write('<td bgcolor="'+clr_unfinished+'" width='+unfinished+'><font size=1>&nbsp;</td>');
	document.write('</tr>');
	document.write('</table>');
	document.write('</td></tr>');
	document.write('</table>');
	document.write('</td><td width=30 nowrap align=center><font size=1>'+percent+'%');
	document.write('</td></tr>');
	document.write('</table>');
}
    </script>

</td></tr>
<tr><td height="10px">
<div id="selectBlock">
</div>
</td></tr>

<tr><td>
<div id="listBlock">
