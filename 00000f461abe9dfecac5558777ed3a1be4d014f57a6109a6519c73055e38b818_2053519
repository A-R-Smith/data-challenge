        if(d.C21[2].checked == true) k=k+1;
        if(d.C22[2].checked == true) k=k+1;
        if(d.C23[2].checked == true) k=k+1;
        if(d.C24[2].checked == true) k=k+1;
        
        if(k==24){
            alert('You never set any port.');
            return false;
        }
