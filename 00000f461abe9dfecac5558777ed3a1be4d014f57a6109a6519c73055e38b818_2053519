<script language="Javascript">


	
	function formCheck(s)
	{
		
		if(!checkNumberInput(s.T1,1,16))
		{
			return false;
		}
		if(!checkNumberInput(s.T2,0,65535))
		{
			return false;
		}
		s.submit();
	   	 return true;
	}
	function Init()
	{
		document.formSTPGlobal.S1.value=DHCP_Relay_Setting[0];
		document.formSTPGlobal.T1.value=DHCP_Relay_Setting[1];
		document.formSTPGlobal.T2.value=DHCP_Relay_Setting[2];
		document.formSTPGlobal.S2.value=DHCP_Relay_Setting[3];
		document.formSTPGlobal.S3.value=DHCP_Relay_Setting[4];
		document.formSTPGlobal.S4.value=DHCP_Relay_Setting[5];
	}

</script>

