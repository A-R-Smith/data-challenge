	    		<script language="javascript">
	    			function showRate(value)
	    			{
	    				if(value=='0')
						return "No Limit";
					else
						return value;	    				
	    			}
				function showRadius(value)
				{
					if(value == '1')
						return "(RADIUS)";
					else
						return " ";
				}
			</script>
