	<tr><td height="10px">
		<div id="selectBlock">
		<form name=portset id=portset method=POST action='../../form/formPKT'>
		<table name=tabPortContent id=tabPortContent class=tabPortContent>
		<tr>
		<script language="javascript">
			if(Stacking_Mode!='0')
			{
				document.write("<td width=16%>Unit</td>");
			}
		</script>
		<td width=5%>Port</td>
		<script language="Javascript">
			if(Stacking_Mode!='0')
			{
				document.write("<td><select class=portSel name=D3 id=D3>");
				for(i=0; i<Unit_Setting.length; i++)
				{
					document.write("<option value='" + Unit_Setting[i] + "'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;" + Unit_Setting[i] + "</option>");
				}
