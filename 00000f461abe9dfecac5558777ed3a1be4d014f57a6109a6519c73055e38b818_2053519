<script language="Javascript">
    var trClass = '';
    var txtClass = '';
	document.write("<tbody class='scrollContent'>"); 
	for(i=0; i<IP_MAC_Port_Settings.length; i++)
	{
		if (i%2 == 0)
		{
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else
		{
			trClass = 'gw';
			txtClass = 'flatGX';
		}
	
		document.write("<tr class=" + trClass + ">");
		document.write("  <td width=5%>" + IP_MAC_Port_Settings[i][0] + "</td>");
		document.write("  <td width=15%>" + NumToStrState(IP_MAC_Port_Settings[i][1]) + "</td>");
		document.write("  <td width=10%>" + (IP_MAC_Port_Settings[i][2] == '1' ? " ACL" : "ARP") + "</td>");
		if (is_module_included('ALLOW_ZERO_IP'))
			document.write("  <td width=15%>" + (IP_MAC_Port_Settings[i][3] == '1'?"Allow":"Not Allow") + "</td>");
		document.write("  <td width=15%>" + (IP_MAC_Port_Settings[i][4] == '1'?"Forward":"Not Forward") + "</td>");
		document.write("  <td width=15%>" + (IP_MAC_Port_Settings[i][5]=='0' ? "No Limit" : IP_MAC_Port_Settings[i][5])+"/"+(IP_MAC_Port_Settings[i][6]=='1'?"Stop":"Normal") + "</td>");
		document.write("  <td width=15%>" + numToStr(IP_MAC_Port_Settings[i][7]) + "</td>");
		document.write("</tr>");	
	}
    
    </script>
    </tbody>
    </table>
    </div>
