<head>
	<script src="../form.js" type="text/javascript"></script>	
	<script src="../../DataStore/Safeguard.js" type="text/javascript"></script>
	<script src="../../DataStore/Stacking.js" type="text/javascript"></script>
	<script src="../../DataStore/PortSetting.js" type="text/javascript"></script>
	<script src='../../DataStore/PortSetting48.js' type='text/javascript'></script>
	<script src='../../DataStore/EgressFilter.js' type='text/javascript'></script>
	<script src="../checkperm.js" type="text/javascript"></script>
	<script language="Javascript">
		parent.timer_reset=1;

		function initPortsPanl(strPorts)
		{
			for(i=1;i<=strPorts.length;i++)
			{
				switch(strPorts.substring(i-1,i))
				{
					case "0":
						document.getElementById("chkU"+i).checked = false;
						document.getElementById("chkM"+i).checked = false;
						break;
					case "1":
						document.getElementById("chkU"+i).checked = true;
						document.getElementById("chkM"+i).checked = false;
						break;
					case "2":
						document.getElementById("chkU"+i).checked = false;
						document.getElementById("chkM"+i).checked = true;
						break;
					case "3":
						document.getElementById("chkU"+i).checked = true;
						document.getElementById("chkM"+i).checked = true;
						break;
					default :
						/* no action */
						break;
				}	//end switch
				
			}	//end for
			
			showPorts(null,"boxUPorts");
			showPorts(null,"boxMPorts");
		}

		function showPorts(index,castBoxId)
		{
			var chkId = (castBoxId=="boxUPorts" ? "chkU" : "chkM");
			var strPorts = ""
			for(k=1;k<=Port_Number.length;k++)
			{
				if(false==document.getElementById(chkId+k).checked)
				{
					continue;
				}
				
				if(""==strPorts)
				{
					strPorts+=k;
				}
				else
				{
					if(false==document.getElementById(chkId+(k-1)).checked)
					{
						strPorts+=","+k;
					}
					else if((k+1>Port_Number.length)||(false==document.getElementById(chkId+(k+1)).checked))
					{
						strPorts+="-"+k;
					}
				}
			}	//end for
			document.getElementById(castBoxId).value = strPorts;
		}
		
		function SelAll(strType)
		{
			var chkId = (strType=="U" ? "chkU" : "chkM");
			var boxPortsId = (strType=="U" ? "boxUPorts" : "boxMPorts");
			var btnAll = (strType=="U" ? "btnUAll" : "btnMAll");
			
			if(document.getElementById(btnAll).value == "Select All")
			{
				document.getElementById(btnAll).value = "Clear All";
				for(k=1;k<=Port_Number.length;k++)
				{
					document.getElementById(chkId+k).checked = true;
				}
				document.getElementById(boxPortsId).value = ("1-"+Port_Number.length);
			}
			else
			{
				document.getElementById(btnAll).value = "Select All";
				for(k=1;k<=Port_Number.length;k++)
				{
					document.getElementById(chkId+k).checked = false;
				}
				document.getElementById(boxPortsId).value = "";
			}
		}
		
		function cfgEgrsFil()
		{
			document.getElementById("frmEgrsFil").submit();
		}
		
		function pageLoad()
		{
			initPortsPanl(EgressFilter[0]);
		}
	</script>
</head>


