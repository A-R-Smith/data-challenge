<script language='Javascript'>
    var f = document.formBroadcast;
    if(window.screen.width == 1280){
        document.getElementById('tabPortContent').style.width = 918;
    }
    function chkRate(obj){
        if(obj.selectedIndex == 1){
            f.BcastctrlRate.selectedIndex = 0;
            f.BcastctrlRate.disabled = true;
        }
        else{
            f.BcastctrlRate.disabled = false;
        }
        if(obj.selectedIndex == 0 && f.BcastctrlRate.selectedIndex == 0)
            f.BcastctrlRate.selectedIndex = 1;
    }
    function chkEn(obj){
        if(obj.selectedIndex == 0){
            f.BcastEnabled.selectedIndex = 1;
            obj.disabled = true;
        }
        else
            f.BcastEnabled.selectedIndex = 0;
    }
    chkRate(f.BcastEnabled);
</script>
