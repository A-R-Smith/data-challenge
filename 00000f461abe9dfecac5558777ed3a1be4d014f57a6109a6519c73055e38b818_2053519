    function getQoSColor(qosVal){
        if (qosVal == '3')      return 'QoSHighestColor';
        else if (qosVal == '2') return 'QoSHighColor';
        else if (qosVal == '1') return 'QoSMiddleColor';
        else if (qosVal == '0') return 'QoSLowColor';
    }
    
    function getQoSText(qosVal){
        if (qosVal == '3')      return 'Highest';
        else if (qosVal == '2') return 'High';
        else if (qosVal == '1') return 'Middle';
        else if (qosVal == '0') return 'Low';
    }
    </script>
    </tbody>
