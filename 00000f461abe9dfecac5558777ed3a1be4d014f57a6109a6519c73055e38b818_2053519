<script type="text/javascript">
window.iframeLoad = function(iframe){
	var i;
	var subIframes =  iframe.document ?  iframe.contentWindow.document.getElementsByTagName("iframe") : iframe.contentDocument.getElementsByTagName("iframe");

	/* bind handle to iframes of this sub-page */
	for(i=0; i<subIframes.length; i++)
	{
		window.addEventHandler(subIframes[i], "load", window.resetTimer);
	}
}

function pageLoad()
{
	/* if there is some user action on the browser window, the timeout will be put off */
	/*
	document.onmousemove = new Function('timer_reset = 1;');
	document.onmouseover = new Function('timer_reset = 1;');
	document.onmousedown = new Function('timer_reset = 1;');
	document.onmouseup = new Function('timer_reset = 1;');
	document.onkeydown = new Function('timer_reset = 1;');
	
	frames[0].document.onmousemove = new Function('timer_reset = 1;');
	frames[0].document.onmouseover = new Function('timer_reset = 1;');
	frames[0].document.onmousedown = new Function('timer_reset = 1;');
	frames[0].document.onmouseup = new Function('timer_reset = 1;');
	frames[0].document.onkeydown = new Function('timer_reset = 1;');

	frames[1].document.onmousemove = new Function('timer_reset = 1;');
	frames[1].document.onmouseover = new Function('timer_reset = 1;');
	frames[1].document.onmousedown = new Function('timer_reset = 1;');
	frames[1].document.onmouseup = new Function('timer_reset = 1;');
	frames[1].document.onkeydown = new Function('timer_reset = 1;');

	frames[2].document.onmousemove = new Function('timer_reset = 1;');
	frames[2].document.onmouseover = new Function('timer_reset = 1;');
	frames[2].document.onmousedown = new Function('timer_reset = 1;');
	frames[2].document.onmouseup = new Function('timer_reset = 1;');
	frames[2].document.onkeydown = new Function('timer_reset = 1;');
	*/

	/* if there is some fresh action, the timeout will be put off. 
	 * changed by King Wang for BTD2010011404(bug72), 20100209
	 */
	
	var i;
	var iframes = document.getElementsByTagName("iframe");
	var currFrame;
	for(i=0; i<iframes.length; i++)	/* bind handle to iframes of this page */
	{
		currFrame = iframes[i];
		window.addEventHandler(currFrame, "load", window.resetTimer);
		window.addEventHandler(currFrame, "load", function(){window.iframeLoad(currFrame);} );	// bind onload-handler to sub-iframe
	}
}

window.addEventHandler(window, "load", window.resetTimer);
window.addEventHandler(window, "load", pageLoad);

</script>

