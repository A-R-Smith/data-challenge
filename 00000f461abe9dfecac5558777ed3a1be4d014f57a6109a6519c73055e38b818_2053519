<form name=formSTPGlobal id=formSTPGlobal>

</td></tr>
<tr><td>
<div id="listBlock">
		<font color='red'>(Note:Maximum of 8 entries.) </font>
    <table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" width=100%>
        <tr class=gh>
            <td width=20% align=left>User Name</td>
            <td width=20%>Authentication Mode</td>
            <td width=20%>Host Name</td>
            <td width=30%>Host IP</td>
            <td width=20%></td>
            <td width=10%></td>
        </tr>
        <!--tr><td class=blankbar>&nbsp;</td></tr-->
<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
    
    for(i=0; i<User_Setting.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGW';
        }

        document.write("<tr class=" + trClass + ">");
        document.write("  <td align=left><input type=text name=user_name id=user_name class=" + txtClass + " value='" + User_Setting[i][0] + "'>");
        document.write("  <input type=text name=user_name_m id=user_name_m class=flatWS value='" + User_Setting[i][0] + "' style='display: none;'></td>");
        document.write("  <td><input type=text name=access_right id=access_right class=" + txtClass + " value='" + User_Setting[i][3] + "'>");
        document.write("<select class=enableSel name=access_right_m id=access_right_m  onChange=ChangeStatus(this,"+i+") style='display: none;'>");
        document.write("<option  value='Host-based'>Host-based</option>");
	  document.write("<option value='Password'>Password</option>");
        document.write("<option value='Public Key'>Public Key</option></select></td>");
        document.write("  <td align=left><input type=text name=hostname id=hostname class=" + txtClass + " value='" + User_Setting[i][4] + "'>");
        document.write("  <input type=text name=hostname_m id=hostname_m class=flatWS maxlength=15 value='" + User_Setting[i][4] + "' style='display: none;'></td>");
        document.write("  <td align=left><input type=text name=ipaddr maxlength=15 id=ipaddr class=" + txtClass + " value='" + User_Setting[i][5] + "'>");
        document.write(" <input type=checkbox id=checkbox1_m onClick=ipvalid(this,"+i+") style='display: none;'> <input type=text name=ipaddr_m id=ipaddr_m class=flatWS maxlength=15 value='" + User_Setting[i][5] + "'  style='display: none;'></td>");
        document.write("  <td><input type=button name=editButton id=editButton value='Edit' maxlength=15 class=btnInGrayS onClick='EditStart(" + i + ");'>");
      	document.write("      <input type=button name=editApply id=editApply value='Apply' class=btnInGrayS onClick='EditApplyIt(" + i + ");' style='display: none;'></td>");

        document.write("</tr>");
    }
</script>
    </table>
    <br>
    <b>Note:</b> If you only want to edit the account name or access right without modify the original password, please keep the password column in blank when editing. 
</div>
</form>
</td></tr>
</table>
<script language="Javascript">
    function EditStart(iTag){
      var bFLAG=false;
    	var iTag_m;
    	for(i=0; i<User_Setting.length; i++){
    		if(document.formSTPGlobal.editApply[i].style.display=='none')
    		{
    			bFALG=false
    			iTag_m='';
    		}    		
    		else
    		{
    			bFLAG=true;
    			iTag_m=i;
    			break;
    		}
    	}

    	if((iTag!=0|iTag!=User_Setting.length)&bFLAG==true)
			{				
    		document.formSTPGlobal.user_name[iTag_m].style.display = '';
        document.formSTPGlobal.user_name_m[iTag_m].style.display = 'none';
        document.formSTPGlobal.access_right[iTag_m].style.display = '';
        document.formSTPGlobal.access_right_m[iTag_m].style.display = 'none';
        document.formSTPGlobal.hostname[iTag_m].style.display = '';
        document.formSTPGlobal.hostname_m[iTag_m].style.display = 'none';
        document.formSTPGlobal.ipaddr[iTag_m].style.display = '';
        document.formSTPGlobal.ipaddr_m[iTag_m].style.display = 'none';

        document.formSTPGlobal.checkbox1_m[iTag_m].style.display = 'none';
        document.formSTPGlobal.editButton[iTag_m].style.display = '';
        document.formSTPGlobal.editApply[iTag_m].style.display = 'none'; 
      }
				document.formSTPGlobal.user_name[iTag].style.display = 'none';
        document.formSTPGlobal.user_name_m[iTag].style.display = '';
        document.formSTPGlobal.user_name_m[iTag].select();
        document.formSTPGlobal.access_right[iTag].style.display = 'none';
        document.formSTPGlobal.access_right_m[iTag].style.display = '';
	  document.formSTPGlobal.access_right_m[iTag].value=User_Setting[iTag][3];
        document.formSTPGlobal.hostname[iTag].style.display = 'none';
        document.formSTPGlobal.hostname_m[iTag].style.display = '';
        document.formSTPGlobal.hostname_m[iTag].select();
        document.formSTPGlobal.ipaddr[iTag].style.display = 'none';
        document.formSTPGlobal.ipaddr_m[iTag].style.display = '';
        document.formSTPGlobal.ipaddr_m[iTag].select();

        document.formSTPGlobal.checkbox1_m[iTag].style.display = '';
        document.formSTPGlobal.checkbox1_m[iTag].select();
        document.formSTPGlobal.editButton[iTag].style.display = 'none';
        document.formSTPGlobal.editApply[iTag].style.display = '';
		ChangeStatus(document.formSTPGlobal.access_right_m[iTag],iTag);
		
    }
    function EditApplyIt(iTag){
        document.formSTPGlobal.user_name[iTag].style.display = '';
        document.formSTPGlobal.user_name_m[iTag].style.display = 'none';
        document.formSTPGlobal.access_right[iTag].style.display = '';
        document.formSTPGlobal.access_right_m[iTag].style.display = 'none';
        document.formSTPGlobal.hostname[iTag].style.display = '';
        document.formSTPGlobal.hostname_m[iTag].style.display = 'none';
        document.formSTPGlobal.ipaddr[iTag].style.display = '';
        document.formSTPGlobal.ipaddr_m[iTag].style.display = 'none';
        document.formSTPGlobal.checkbox1_m[iTag].style.display = 'none';
        document.formSTPGlobal.editButton[iTag].style.display = '';
        document.formSTPGlobal.editApply[iTag].style.display = 'none'; 
	  
    }
    function delit(){
    	alert('delete');
    	}
    function apply_check()
    {
       if(document.formSTPGlobal.T1.value=='' || document.formSTPGlobal.T5.value=='')
       {
         alert('User name and Password should not be empty');
       }
       if(document.formSTPGlobal.T5.value!=document.formSTPGlobal.T6.value)
       {
         alert('Password is inconsistent');
       }
       alert('Ok');
    }
	function ChangeStatus(s,index)
	{
		if(s.selectedIndex!=0)
		{
		
			document.formSTPGlobal.hostname_m[index].disabled=true;
			document.formSTPGlobal.ipaddr_m[index].disabled=true;
			document.formSTPGlobal.checkbox1_m[index].disabled=true;
		}
		else
		{
		
			document.formSTPGlobal.hostname_m[index].disabled=false;
			document.formSTPGlobal.ipaddr_m[index].disabled=false;
			document.formSTPGlobal.checkbox1_m[index].disabled=false;
		}
	}
	function ipvalid(s,iTag)
	{
		if(s.checked!=true)
		{
			document.formSTPGlobal.ipaddr_m[iTag].disabled=true;
		}
		else
		{
			document.formSTPGlobal.ipaddr_m[iTag].disabled=false;
		}
	}

</script>
</div>
</body>
