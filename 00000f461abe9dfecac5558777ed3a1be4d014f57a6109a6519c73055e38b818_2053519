                <table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" width=100%>
                    <tr class=gh>
<script language="Javascript">
	if(Port_Setting.length > 24) {
		document.write("<td width=30 rowspan='3' >Unit</td>");
	}else{
		document.write("<td width=30 rowspan='2' >Unit</td>");
	}
</script>
                      <td align=center colSpan=24>Port</td></tr>

<script language="Javascript">
	var flag = true;

   	document.write("<tr class=gh>");
    for(i=0; i<Port_Setting.length; i++){
    	if(i >= 24 && flag){
    	   	document.write("</tr><tr class=gh>");
    	   	flag = false;
		}

		document.write("  <td>" + Port_Setting[i][0] + "</td>");
	}
   	if(Port_Setting.length > 24){
   	   	document.write("</tr>");
   	   	flag = true;
   	}

	document.write("</tr>");


    for(i=0; i<1; i++){
        if (i%2 == 0)
            trClass = 'gx';
        else
            trClass = 'gw';
        
        document.write("<tr class=" + trClass + ">");
		if(Port_Setting.length > 24) {
        	document.write("  <td rowspan='2'>1</td>");
		}else{
        	document.write("  <td>1</td>");
		}

        for(j=0; j<Port_Setting.length; j++){
	    	if(j >= 24 && flag){
	    	   	document.write("</tr><tr class="+trClass+">");
	    	   	flag = false;
			}
            document.write("  <td>"+IGMP_Group_Forward_VID_1[j]+"</td>");
		}
	   	if(Port_Setting.length > 24){
	   	   	document.write("</tr>");
	   	   	flag = true;
	   	}

        document.write("</tr>");
        //document.write("<tr height=1><td class=blankbar height=1>&nbsp;</td></tr>");
    }
    
</script>

                </table>
</form>

