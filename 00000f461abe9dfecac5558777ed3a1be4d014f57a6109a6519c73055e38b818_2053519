<script language="Javascript">
	function searchArray(arr, toFind)
	{
		var counting=0;
		if(arr.constructor != Array)
			return false;

		for(var i = 0, len = arr.length; i < len; i++)
		{
			if(arr[i].constructor == Array)
			{
				if (searchArray(arr[i], toFind) )
				return true;
			}
			else if(arr[i] == toFind)
			{
				counting+=1;
			}
		}
		return counting;
	}

	document.formLoginGlobal.S2.disabled = true;
	document.formLoginGlobal.S3.disabled = true;
	document.formLoginGlobal.S4.disabled = true;

	function checkbtn()
	{
		if ("" == Login_Method[document.formLoginGlobal.S1.value])
		{
			document.formLoginGlobal.S2.disabled = true;
			document.formLoginGlobal.S3.disabled = true;
			document.formLoginGlobal.S4.disabled = true;
			Login_Method[document.formLoginGlobal.S2.value] = "";
			Login_Method[document.formLoginGlobal.S3.value] = "";
			Login_Method[document.formLoginGlobal.S4.value]= "";			
		}
		else
		{
			document.formLoginGlobal.S2.disabled = false;
		}
		if ("" == Login_Method[document.formLoginGlobal.S2.value])
		{
			document.formLoginGlobal.S3.disabled = true;
			document.formLoginGlobal.S4.disabled = true;
			Login_Method[document.formLoginGlobal.S3.value] = "";
			Login_Method[document.formLoginGlobal.S4.value] = "";			
		}
		else
		{
			document.formLoginGlobal.S3.disabled = false;
		}
		if ("" == Login_Method[document.formLoginGlobal.S3.value])
		{
			document.formLoginGlobal.S4.disabled = true;
			Login_Method[document.formLoginGlobal.S4.value] = "";			
		}
		else
		{
			document.formLoginGlobal.S4.disabled = false;
		}
		
	}
	
	function EditStart(iTag) {
		var i;
		document.formLoginGlobal.T1.value = Login_Method_Lists[iTag][0];
		if(Login_Method_Lists[iTag][1]=="")
		{
			document.formLoginGlobal.S1.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Login_Method[i] == Login_Method_Lists[iTag][1])
					document.formLoginGlobal.S1.value=i;
			}
		}
		if(Login_Method_Lists[iTag][2]=="")
		{
			document.formLoginGlobal.S2.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Login_Method[i] == Login_Method_Lists[iTag][2])
					document.formLoginGlobal.S2.value=i;
			}
		}
		if(Login_Method_Lists[iTag][3]=="")
		{
			document.formLoginGlobal.S3.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Login_Method[i] == Login_Method_Lists[iTag][3])
					document.formLoginGlobal.S3.value=i;
			}
		}
		if(Login_Method_Lists[iTag][4]=="")
		{
			document.formLoginGlobal.S4.value=0;
		}
		else
		{
			for(i=0;i<10;i++)
			{
				if(Login_Method[i] == Login_Method_Lists[iTag][4])
					document.formLoginGlobal.S4.value=i;
			}
		}
		checkbtn();
	}
	
	function ApplyIt()
	{
		var m1=document.formLoginGlobal.S11.value;
		var m2=document.formLoginGlobal.S21.value;
		var m3=document.formLoginGlobal.S31.value;
		var m4=document.formLoginGlobal.S41.value;

		if (Login_Method.length != 0)
		{
			m1 = Login_Method[document.formLoginGlobal.S1.value];
		}
		if (Login_Method.length != 0)
		{
			m2 = Login_Method[document.formLoginGlobal.S2.value];
		}
		if (Login_Method.length != 0)
		{
			m3 = Login_Method[document.formLoginGlobal.S3.value];
		}
		if (Login_Method.length != 0)
		{
			m4 = Login_Method[document.formLoginGlobal.S4.value];
		}

		if (!checkTxtInput(document.formLoginGlobal.T1))
			return;

		var data_array=new Array();
		data_array[0]=m1;
		data_array[1]=m2;
		data_array[2]=m3;
		data_array[3]=m4;
		if((m1 != '' && searchArray(data_array, m1))>1 || 
			(m2 != '' && searchArray(data_array, m2))>1 || 
			(m3 != '' && searchArray(data_array, m3))>1 || 
			(m4 != '' && searchArray(data_array, m4))>1)
		{
			alert("Some Methods are duplicated");
			return false;
		}

		if ("local" == m1 || "local" == m2 || "local" == m3 || "local" == m4)
			alert("AAA will ignore login methods which after \"local\"");

		if ("none" == m1 || "none" == m2 || "none" == m3 || "none" == m4)
			alert("AAA will ignore login methods which after \"none\"");

		document.formLoginGlobal.S11.value = Login_Method[document.formLoginGlobal.S1.value];
		document.formLoginGlobal.S21.value = Login_Method[document.formLoginGlobal.S2.value];
		document.formLoginGlobal.S31.value = Login_Method[document.formLoginGlobal.S3.value];
		document.formLoginGlobal.S41.value = Login_Method[document.formLoginGlobal.S4.value];
		document.formLoginGlobal.isClear.value='0';
		document.formLoginGlobal.submit();
	}

	function DeleteIt(iTag)
	{
		document.formLoginGlobal.T1.value = Login_Method_Lists[iTag][0];
		document.formLoginGlobal.isClear.value='1';
		document.formLoginGlobal.submit();
		/*document.formLoginGlobal.S1.value = '-';
		document.formLoginGlobal.S2.value = '-';
		document.formLoginGlobal.S3.value = '-';
		document.formLoginGlobal.S4.value = '-';
		*/
	}
</script>
</body>
