<script language="Javascript">
function gonext()
{
	location.href = "88_TimeRange.html?2";
}

function goprev()
{
	location.href = "88_TimeRange.html?1";
}

function delit(iTag)
{
	document.formTR.isDel.value = '1';
	document.formTR.range_name.value = TimeRange[iTag][0];

	document.formTR.submit();
}
function apply_check()
{
	var s = document.formTR;
	var daylist="";
	
	if(!checkTxtInput(s.range_name))
		return;

	if(s.E9.checked)
	{
		daylist="2,";
	}
	if(s.E10.checked)
	{
		daylist=daylist+"3,"
	}
	if(s.E11.checked)
	{
		daylist=daylist+"4,"
	}
	if(s.E12.checked)
	{
		daylist=daylist+"5,"
	}
	if(s.E13.checked)
	{
		daylist=daylist+"6,"
	}
	if(s.E14.checked)
	{
		daylist=daylist+"7,"
	}
	if(s.E15.checked)
	{
		daylist=daylist+"1,"
	}

	if (daylist == "")
	{
		alert("Week days must be specified!");
		return false;
	}

	var dyale=daylist.substring(0,daylist.length-1);
	s.daylist.value = dyale;

	document.formTR.isDel.value = '0';
	document.formTR.submit();
}

function checkAllDays()
{
	var s = document.formTR;
	var i;

	if (s.E16.checked == true)
	{
		for(i = 9; i < 16; i++)
		{
			eval("s.E"+i+".checked=true");
		}
	}
	else
	{
		for(i = 9; i < 16; i++)
		{
			eval("s.E"+i+".checked=false");
		}
	}
}

if (TR_Navigation_Btn[0] == '1')
	document.formTR.bprev.disabled = false;

if (TR_Navigation_Btn[1] == '1')
	document.formTR.bnext.disabled = false;

</script>
</table>
