function initial()
{
	var f = document.form1;
	var elem = f.elements;
	var i;

	for(i = 0; i < elem.length; i++)
	{
		elem[i].disabled = true;
	}
}

initial();

function enable_all_elem()
{
	var f = document.form1;
	var elem = f.elements;
	var i;

	for(i = 0; i < elem.length; i++)
	{
		elem[i].disabled = false;
	}
}

function go_back()
{
	location.href = "52_AccessProfile.html?" + acl_style;
}

setTimeout('enable_all_elem()', 6000);
	
