<script language="Javascript">
	function EditStart(iTag)
	{
		var bFLAG=false;
		var iTag_m;

		document.formSSHUser[iTag].hostname.value = User_Setting[iTag][2];
		document.formSSHUser[iTag].hostname_m.value = User_Setting[iTag][2];
		
		for(i=0; i<User_Setting.length; i++)
		{
			if(document.formSSHUser[i].editApply.style.display=='none')
			{
				bFALG=false
				iTag_m='';
			}    		
			else
			{
				bFLAG=true;
				iTag_m=i;
				break;
			}
		}

		if((iTag!=0|iTag!=User_Setting.length)&& bFLAG==true)
		{				
			document.formSSHUser[iTag_m].AuthMode.style.display = '';
			document.formSSHUser[iTag_m].AuthMode_m.style.display = 'none';
			document.formSSHUser[iTag_m].hostname.style.display = '';
			document.formSSHUser[iTag_m].hostname_m.style.display = 'none';
			document.formSSHUser[iTag_m].ipaddr.style.display = '';
			document.formSSHUser[iTag_m].ipaddr_m.style.display = 'none';
			document.formSSHUser[iTag_m].checkbox1_m.style.display = 'none';
			document.formSSHUser[iTag_m].editButton.style.display = '';
			document.formSSHUser[iTag_m].editApply.style.display = 'none'; 
		}
		document.formSSHUser[iTag].AuthMode.style.display = 'none';
		document.formSSHUser[iTag].AuthMode_m.style.display = '';
		document.formSSHUser[iTag].AuthMode_m.value=User_Setting[iTag][1];
		document.formSSHUser[iTag].hostname.style.display = 'none';
		document.formSSHUser[iTag].hostname_m.style.display = '';
		document.formSSHUser[iTag].hostname_m.value=User_Setting[iTag][2];
		document.formSSHUser[iTag].ipaddr.style.display = 'none';
		document.formSSHUser[iTag].ipaddr_m.style.display = '';
		document.formSSHUser[iTag].ipaddr_m.value=User_Setting[iTag][3];
		document.formSSHUser[iTag].checkbox1_m.style.display = '';
		if(User_Setting[iTag][4]==1)
			document.formSSHUser[iTag].checkbox1_m.checked=true;
		else
			document.formSSHUser[iTag].checkbox1_m.checked=false;
		document.formSSHUser[iTag].editButton.style.display = 'none';
		document.formSSHUser[iTag].editApply.style.display = '';
		ChangeStatus(document.formSSHUser[iTag].AuthMode_m,iTag);
		
	}
	function EditApplyIt(iTag)
	{
		if(!formCheck(document.formSSHUser[iTag]))
			return false;
		if(document.formSSHUser[iTag].checkbox1_m.checked==true)
			document.formSSHUser[iTag].checkbox1_m.value='1';
		else 
			document.formSSHUser[iTag].checkbox1_m.value='0';
		document.formSSHUser[iTag].AuthMode.style.display = '';
		document.formSSHUser[iTag].AuthMode_m.style.display = 'none';
		document.formSSHUser[iTag].hostname.style.display = '';
		document.formSSHUser[iTag].hostname_m.style.display = 'none';
		document.formSSHUser[iTag].ipaddr.style.display = '';
		document.formSSHUser[iTag].ipaddr_m.style.display = 'none';
		document.formSSHUser[iTag].checkbox1_m.style.display = 'none';
		document.formSSHUser[iTag].editButton.style.display = '';
		document.formSSHUser[iTag].editApply.style.display = 'none'; 
		document.formSSHUser[iTag].submit();
		return true;
	}
	function formCheck(s)
	{
	
		if(!checkTxtInput(s.user_name_m))
			return false;
		if(s.AuthMode_m.selectedIndex == 0)
		{
			if(!checkTxtInput(s.hostname_m))
			{	
				return false;
			}

			if(s.checkbox1_m.checked==true)
			{
				if(!ipCheck(s.ipaddr_m))
				{
					return false;
				}
			}
		}
		return true;
		}

	function ChangeStatus(s,iTag)
	{
		if(s.selectedIndex!=0)
		{
			document.formSSHUser[iTag].hostname_m.disabled=true;
			document.formSSHUser[iTag].ipaddr_m.disabled=true;
			document.formSSHUser[iTag].checkbox1_m.disabled=true;
		}
		else
		{
			document.formSSHUser[iTag].hostname_m.disabled=false;
			document.formSSHUser[iTag].ipaddr_m.disabled=false;
			document.formSSHUser[iTag].checkbox1_m.disabled=false;

			ipvalid(document.formSSHUser[iTag].checkbox1_m,iTag);
		}
	}
	function ipvalid(s,iTag)
	{
		if(s.checked!=true)
		{
			document.formSSHUser[iTag].ipaddr_m.disabled=true;
		}
		else
		{
			document.formSSHUser[iTag].ipaddr_m.disabled=false;
		}
	}
	function Init()
	{
			for(i=0; i<User_Setting.length; i++)                 // init the value of show_item, and the value of the real_item is inited in function editstart();
			{										// when the js_date include ' at the first place,we cann't use <input type=xxx value ="+usersetting[i][o]+"> in tag;
				document.formSSHUser[i].user_name_m.value =  User_Setting[i][0];
				document.formSSHUser[i].AuthMode.value =  User_Setting[i][1];
				document.formSSHUser[i].hostname.value =  strTruncate(User_Setting[i][2], 15);
				document.formSSHUser[i].ipaddr.value =  User_Setting[i][3];
			}
	}

</script>
