<script language="javascript">
function delit(iTag)
{
	var vid = IP_MAC_Blocked[iTag][0];
	var mac = IP_MAC_Blocked[iTag][2];
	
	parent.delit(vid, mac);
}

parent.document.getElementById('total_entry').innerHTML = "<b>Total Entries: "+IP_MAC_Blocked.length + "</b>";
</script>

