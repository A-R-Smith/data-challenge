function find()
{
	var name, ip_addr;
	name = document.formARP_Browse.ipif_name.value;
	ip_addr = document.formARP_Browse.ip_addr.value;
	if (name == '' && ip_addr == '')
	{
		alert("Please input search condition!");
		document.formARP_Browse.ipif_name.select();
		return;
	}

	if (ip_addr != '')
	{
		if (!ipCheck(document.formARP_Browse.ip_addr))
			return;
	}
	
	if(parent.simulation==1)
	{
		refreshThis();
	}
	else
	{
		if (name != '' && ip_addr != '')
			location.href = "54_Browse_ARP.html?3,3," + name + " " + ip_addr;
		else if (name != '' && ip_addr == '')
			location.href = "54_Browse_ARP.html?3,1," + name + " 0";
		else if (name == '' && ip_addr != '')
			location.href = "54_Browse_ARP.html?3,2," + "0 " + ip_addr;
	}
}

function clearAll()
{
	document.formARP_Browse.submit();
}

function showStatic()
{
	location.href = "54_Browse_ARP.html?3,4,0 0";
}

