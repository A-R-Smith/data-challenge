<script language="Javascript">    
    var d = document.STP_PortSetting;

function warning()
{
	var cost = d.External_Cost.value;
	if(STP_Global_Setting[1] == 3)
		var helloTime = d.hello_time.value;

	if (!checkNumberInput(d.External_Cost, 0, 200000000))
		return;
	
	if(STP_Global_Setting[1] == 3)
	{
		if (!checkNumberInput(d.hello_time, 1, 2))
			return;
	}

	d.port_f.value = d.D1.value;
	d.port_t.value = d.D2.value;
	d.submit();
}
</script>
