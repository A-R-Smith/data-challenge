<script language="Javascript">
function EditStart(iTag)
{
	var bFLAG = false;
	var iTag_m, i;
	for(i=0; i<parent.IP_MAC_Settings.length; i++)
	{
		if(document.formIP_MAC_Edit.editApply[i].style.display=='none')
		{
			bFALG = false
			iTag_m = '';
		}    		
		else
		{
			bFLAG=true;
			iTag_m=i;
			break;
		}
	}

    	if((iTag!=0|iTag!=parent.IP_MAC_Settings.length)&bFLAG==true)
	{
		document.formIP_MAC_Edit.mac_addr[iTag_m].style.display = '';
		document.formIP_MAC_Edit.mac_addr_m[iTag_m].style.display = 'none';
		document.formIP_MAC_Edit.p[iTag_m].style.display = '';
		document.formIP_MAC_Edit.p_m[iTag_m].style.display = 'none';
		if(("ACL"==parent.IP_MAC_Settings[iTag_m][0]) && ("ARP"==parent.IP_MAC_Settings[iTag_m][0]))
		{
		document.formIP_MAC_Edit.co_mode[iTag_m].style.display = '';
		document.formIP_MAC_Edit.co_mode_m[iTag_m].style.display = 'none';
		}
		document.formIP_MAC_Edit.editButton[iTag_m].style.display = '';
		document.formIP_MAC_Edit.editApply[iTag_m].style.display = 'none'; 
	}

	document.formIP_MAC_Edit.mac_addr[iTag].style.display = 'none';
	document.formIP_MAC_Edit.mac_addr_m[iTag].style.display = '';
	document.formIP_MAC_Edit.p[iTag].style.display = 'none';
	document.formIP_MAC_Edit.p_m[iTag].style.display = '';
	if(("ACL"==parent.IP_MAC_Settings[iTag][0]) && ("ARP"==parent.IP_MAC_Settings[iTag][0]))
	{
	document.formIP_MAC_Edit.co_mode[iTag].style.display = 'none';
	document.formIP_MAC_Edit.co_mode_m[iTag].style.display = '';
	}
	document.formIP_MAC_Edit.editButton[iTag].style.display = 'none';
	document.formIP_MAC_Edit.editApply[iTag].style.display = '';

}
    
function EditApplyIt(iTag)
{
	if (!MAC_Address_Check(document.formIP_MAC_Edit.mac_addr_m[iTag]))
		return;

	var ip_addr, mac, mode, port;


	document.formIP_MAC_Edit.mac_addr[iTag].style.display = '';
	document.formIP_MAC_Edit.mac_addr_m[iTag].style.display = 'none';
	document.formIP_MAC_Edit.p[iTag].style.display = '';
	document.formIP_MAC_Edit.p_m[iTag].style.display = 'none';
	document.formIP_MAC_Edit.co_mode[iTag].style.display = '';
	document.formIP_MAC_Edit.co_mode_m[iTag].style.display = 'none';
	document.formIP_MAC_Edit.editButton[iTag].style.display = '';
	document.formIP_MAC_Edit.editApply[iTag].style.display = 'none'; 

	//alert("del: " + document.formIP_MAC_Edit.isDel.value);
	//alert("edt: " + document.formIP_MAC_Edit.isEdit.value);

	//document.formIP_MAC_Edit.cn_ip.value = document.formIP_MAC_Edit.ip_addr[iTag].value;
	
	document.formIP_MAC_Edit.cn_mac.value = document.formIP_MAC_Edit.mac_addr_m[iTag].value;
	document.formIP_MAC_Edit.cn_port.value = document.formIP_MAC_Edit.p_m[iTag].value;
	document.formIP_MAC_Edit.cn_mode.value = document.formIP_MAC_Edit.co_mode_m[iTag].value;

	ip_addr = parent.IP_MAC_Settings[iTag][1];
	mac = document.formIP_MAC_Edit.mac_addr_m[iTag].value;
	mode = document.formIP_MAC_Edit.co_mode_m[iTag].value;
	port = document.formIP_MAC_Edit.p_m[iTag].value;

	parent.edit_ip_mac(ip_addr, mac, mode, port);
	//document.formIP_MAC_Edit.isDel.value = '0';
	//document.formIP_MAC_Edit.submit();
}
function delit(iTag)
{
	parent.del_ip_mac(parent.IP_MAC_Settings[iTag][1], document.formIP_MAC_Edit.mac_addr_m[iTag].value);

/*
	document.formIP_MAC_Edit.cn_mac.value = document.formIP_MAC_Edit.mac_addr_m[iTag].value;
	document.formIP_MAC_Edit.cn_port.value = document.formIP_MAC_Edit.p_m[iTag].value;
	document.formIP_MAC_Edit.cn_mode.value = document.formIP_MAC_Edit.co_mode_m[iTag].value;

	document.formIP_MAC_Edit.isDel.value = '1';

	//alert("del: " + document.formIP_MAC_Edit.isDel.value);
	//alert("edt: " + document.formIP_MAC_Edit.isEdit.value);
	document.formIP_MAC_Edit.submit();
*/
}
</script>
