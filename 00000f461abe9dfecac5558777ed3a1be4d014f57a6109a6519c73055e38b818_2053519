<script language="Javascript">
var cur_state = MBA_Global[0];

function apply_state()
{
	if (document.formMBAGlobal_state.state.value != cur_state)
	{
		parent.frames['tree_items'].location.reload();
	}	
	document.formMBAGlobal_state.submit();
}

function applyForm()
{
	
	if (!checkTxtInput(document.formMBAGlobal.pass))
	{
		return false;
	}
	document.formMBAGlobal.submit();
}


function apply_vlan(oBtn)
{
	if(oBtn.id == "btnDel")//delete
	{
		document.formMBAGlobal_vlan.temp.value = 0;
		if (document.formMBAGlobal_vlan.vlan_flag[1].checked)
		{
			if (!checkNumberInput(document.formMBAGlobal_vlan.gvlanid, 1, 4094))
				return false;
			document.formMBAGlobal_vlan.g_vlan_flag.value = 1;
			document.formMBAGlobal_vlan.flag.value = 1;
			document.formMBAGlobal_vlan.temp_name.value = MBA_Global[3];			
		}
		else
		{
			if (!checkTxtInput(document.formMBAGlobal_vlan.gvlan))
				return false;
			document.formMBAGlobal_vlan.g_vlan_flag.value = 0;
			document.formMBAGlobal_vlan.flag.value = 0;
			document.formMBAGlobal_vlan.temp_id.value = MBA_Global[4];		
		}
	}
	else //create
	{
		document.formMBAGlobal_vlan.temp.value = 1;
		if (document.formMBAGlobal_vlan.vlan_flag[0].checked)
		{
			if (!checkTxtInput(document.formMBAGlobal_vlan.gvlan))
				return false;

			document.formMBAGlobal_vlan.h_vlan_flag.value = 1;
		}
		else
		{
			if (!checkNumberInput(document.formMBAGlobal_vlan.gvlanid, 1, 4094))
				return false;

			document.formMBAGlobal_vlan.h_vlan_flag.value = 0;
		}
	}
	if ((document.formMBAGlobal_vlan.ports.value != "") && (oBtn.id != "btnDel") )	
	{
		if (!check_portlist(document.formMBAGlobal_vlan.ports))
			return false;
		document.formMBAGlobal_vlan.temp.value = 2;
	}
	
	document.formMBAGlobal_vlan.submit();
}


function change_vlan_flag(index)
{
	var f = document.formMBAGlobal_vlan;
	if (index == 0)
	{
		f.gvlan.disabled = true;
		f.gvlanid.disabled = false;
		f.gvlanid.style.backgroundColor='#FFFFFF';
		f.gvlan.style.backgroundColor='#D4D0C8';		
	}
	else
	{	
		f.gvlan.disabled = false;
		f.gvlanid.disabled = true;
		f.gvlanid.style.backgroundColor='#D4D0C8';
		f.gvlan.style.backgroundColor='#FFFFFF';
	}
}

function checkInfinite(index)
{
	var d = document.formMBAPort;
	if (d.aging_infinite.checked == true)
	{
		d.aging_time.disabled = true;
		d.aging_time.style.backgroundColor='#D4D0C8';
	}
	else
	{
		d.aging_time.disabled = false;
		d.aging_time.style.backgroundColor='#FFFFFF';
	}
		
}

function checkInfinite_hold(index)
{
	var d = document.formMBAPort;
	if (d.hold_infinite.checked == true)
	{
		d.hold_time.disabled = true;
		d.hold_time.style.backgroundColor='#D4D0C8';
	}
	else
	{
		d.hold_time.disabled = false;
		d.hold_time.style.backgroundColor='#FFFFFF';
	}
		
}
function applyPort()
{
	var d = document.formMBAPort;
	if(d.hold_infinite.checked == false)
	{
		if(!checkNumberInput(d.hold_time, 1, 300))
		{
			return;
		}
	}
	if(d.aging_infinite.checked == false)
	{
		if(!checkNumberInput(d.aging_time, 1, 1440))
		{
			return;
		}
	}
	document.formMBAPort.submit();
}

function init()
{
	change_vlan_flag(1);
	document.formMBAGlobal_vlan.gvlan.value = MBA_Global[3];
	document.formMBAGlobal_vlan.gvlanid.value = MBA_Global[4];
	document.formMBAGlobal.pass.value = MBA_Global[2];
	document.formMBAGlobal.failover.value = MBA_Global_Auth_Failover;
	document.formMBAGlobal.Trap.value = Trap;
}
init();

</script>
