	text-decoration: none;
	font-family: Arial;
	font-size: 11px
}

/* Stack status */
#stackBox {	
	margin: 0 auto;
	overflow:hidden;
	background:rgb(255,255,255)
}

#stackBoxBanner {
	border-width: 0; 
}

.pic { width: 120px; padding-left: 12px;}
.pic p { padding: 2px; border: 1px solid #ddd; background: #fff; margin: 2px; }
.pic img { cursor: pointer; }
