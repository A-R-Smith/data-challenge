<script language="Javascript">
	var trClass = '';
	var txtClass = '';
	var i;

	var tipStr;

	for (i = 0; i < Authentication_Server_Host.length; i++) {
		if (i % 2 == 0) {
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else {
			trClass = 'gw';
			txtClass = 'flatGW';
		}
		document.write("<tr class=" + trClass + ">");
		document.write("  <td align=left>" + Authentication_Server_Host[i][0] + "</td>");
		document.write("  <td align=left>" + Authentication_Server_Host[i][1] + "</td>");
		document.write("  <td align=left>" + Authentication_Server_Host[i][2] + "</td>");
		document.write("  <td align=left>" + Authentication_Server_Host[i][3] + "</td>");

		if ("TACACS+" == Authentication_Server_Host[i][1])
			document.write("  <td align=left>-----</td>");
		else
			document.write("  <td align=left>" + Authentication_Server_Host[i][5] + "</td>");

		if ("TACACS" == Authentication_Server_Host[i][1] || "XTACACS" == Authentication_Server_Host[i][1])
			document.write("  <td align=left onMouseover=keytip('Key&nbsp;is&nbsp;meaningless&nbsp;for&nbsp;TACACS&nbsp;and&nbsp;XTACACS.'); onMouseout='hideddrivetip()'>-----</td>");
		else
		{
			tipStr = strNormalize(Authentication_Server_Host[i][4]);
			document.write("  <td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + strTruncate(Authentication_Server_Host[i][4], 30) + "</td>");
		}
		document.write("  <td><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'>");
		document.write("  </td>");
		document.write("  <td><input type=button value='Delete' class=btnInGrayS onClick=DeleteIt(" + i + ");></td>");
		document.write("</tr>"); 
	}
</script>
   </table>

</div>
</td></tr>
