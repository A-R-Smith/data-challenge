<script language="Javascript">    
	var trClass = '';
	var txtClass = '';
	var tipStr;

	function getTipStr(s)
	{
		if (s == "local_enable")
			return "Keyword";
		else if (s == "none")
			return "Keyword";
		else if (s == "radius")
			return "Built-in&nbsp;Group";
		else if (s == "tacacs+")
			return "Built-in&nbsp;Group";		
		else if (s == "xtacacs")
			return "Built-in&nbsp;Group";
		else if (s == "tacacs")
			return "Built-in&nbsp;Group";
		else if (s == "local")
			return "Keyword";
		else if (s == "")
			return "Empty";
		else
			return "User-defined&nbsp;Group";
	}

	function getShowStr(s)
	{
		if (s == "")
			return "-----";
		else
			return s;
	}

	for (i = 0; i < Login_Method_Lists.length; i++) {
		if (i%2 == 0){
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else{
			trClass = 'gw';
			txtClass = 'flatGW';
		}

		document.write("<tr class=" + trClass + ">");
		document.write("<td align=left>" + Login_Method_Lists[i][0] + "</td>");
		tipStr = getTipStr(Login_Method_Lists[i][1]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Login_Method_Lists[i][1]) + "</td>");
		tipStr = getTipStr(Login_Method_Lists[i][2]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Login_Method_Lists[i][2]) + "</td>");
		tipStr = getTipStr(Login_Method_Lists[i][3]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Login_Method_Lists[i][3]) + "</td>");
		tipStr = getTipStr(Login_Method_Lists[i][4]);
		document.write("<td align=left onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>" + getShowStr(Login_Method_Lists[i][4]) + "</td>");
		document.write("<td><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'></td>");
		if (Login_Method_Lists[i][0] == 'default')
			document.write("<td><input type=button value='Delete' class=btnInGrayS disabled></td>");
		else
			document.write("<td><input type=button value='Delete' class=btnInGrayS onClick=DeleteIt("+i+");></td>");
		document.write("</tr>");
	}
</script>
 </table>
