				{
		        	document.write("  <td align=left>" + JWAC_Host[i][1] + "&nbsp;(P)</td>");
				}
				else
				{
					document.write("  <td align=left>" + JWAC_Host[i][1] + "</td>");
				}
		        document.write("  <td align=center>" + JWAC_Host[i][2] + "</td>");
				if(JWAC_Host[i][4] != 'Authenticated')
				{
					if(JWAC_Host[i][3]!='Infinite')
					{
		        		document.write("  <td align=center>" + JWAC_Host[i][3] + "&nbsp;Sec</td>");
					}
					else
					{
						document.write("  <td align=center>" + JWAC_Host[i][3] + "</td>");
					}
				}
				else
				{
					if(JWAC_Host[i][3]!='Infinite')
					{
		        		document.write("  <td align=center>" + JWAC_Host[i][3] + "&nbsp;Min</td>");
					}
