<script language="Javascript">
function enabled()
{
  for(i=0;i<ACLRule_List.length;i++)
  {
    if(document.getElementsByName("C2")[i].checked)
    {
      parent.document.getElementsByName("ACLWizardApply1")[0].disabled=false;
      break;
    }
  }
}
</script>
