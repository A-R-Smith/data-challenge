<script language="Javascript">    
	var trClass = '';
	var txtClass = '';
 document.write("<tbody class='scrollContent'>"); 
	for(i=0; i<MSTI_VID_LIST.length; i++){
		if (i%2 == 0){
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else{
			trClass = 'gw';
			txtClass = 'flatGW';
		}

		document.write("<tr class=" + trClass + ">");
		document.write("  <td width=18%>" + MSTI_VID_LIST[i][0] + "</td>");
		document.write("  <td width=50%><input type=text name=vidList id=vidList class=" + txtClass + " value='" + MSTI_VID_LIST[i][1] + "' style=\"width:500;\">");
		document.write("      <input type=text name=vidList_M id=vidList_M class=flatWS style='display: none;'></td>");

		if (i == 0)
		{
			document.write("  <td width=30% align=right><input type=button name=editButton id=editButton value='Edit' disabled class=btnInGrayS onClick='EditIt(" + i + ");'>");
			document.write("  <input type=button name=editApply id=editApply value='Apply' class=btnInGrayS disabled onClick='EditApplyIt(" + i + ");' style='display: none; '>");
			document.write("  <input type=button name=delButton id=delButton value='Delete' disabled class=btnInGrayS onClick='delit(" + i + ");'></td>");
		}

		else
		{
			//document.write("  <td><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditIt(" + i + ");'></td>");

			document.write("  <td width=30% align=right><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'>");
			document.write("  <input type=button name=editApply id=editApply value='Apply' class=btnInGrayS onClick='EditApplyIt(" + i + ");' style='display: none; '>");
			document.write(" <input type=button name=delButton id=delButton value='Delete' class=btnInGrayS onClick='delit(" + i + ");'></td>");


		}
		document.write("</tr>");
	}
</script>
</tbody>
</table>
</form>
