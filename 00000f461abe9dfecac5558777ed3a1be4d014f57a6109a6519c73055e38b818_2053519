<script language="Javascript">
    document.formSTPSet.D2.selectedIndex = STP_Port_Setting.length-1;
    function D2_Same(){
        for(i=0;i<24;i++)
            if(document.formSTPSet.D1[i].selected == true)
                document.formSTPSet.D2[i].selected = true;
    }
    
    var f = document.formSTPGlobal;
    function apply(){
        if((f.T1.value=='') || isNaN(f.T1.value) || f.T1.value<0 || f.T1.value>65535 || (f.T1.value.indexOf('.')>-1))
            alert('Invalid Bridge Priority (0 - 65535).');
        else if((f.T2.value=='') || isNaN(f.T2.value) || f.T2.value<6 || f.T2.value>40 || (f.T2.value.indexOf('.')>-1))
            alert('Invalid Bridge Max Age (6 - 40).');
        else if((f.T3.value=='') || isNaN(f.T3.value) || f.T3.value<1 || f.T3.value>10 || (f.T3.value.indexOf('.')>-1))
            alert('Invalid Bridge Hello Time (1 - 10).');
        else if((f.T4.value=='') || isNaN(f.T4.value) || f.T4.value<4 || f.T4.value>30 || (f.T4.value.indexOf('.')>-1))
            alert('Invalid Bridge Forward  Delay (4 - 30).');
        else
            f.submit();
    }
    
    var f2 = document.formSTPSet;
    function apply2(){
        if((f2.P1.value=='') || isNaN(f2.P1.value) || f2.P1.value<1 || f2.P1.value>65535 || (f2.P1.value.indexOf('.')>-1))
            alert('Invalid Path Cost(1 - 65535).');
        else if((f2.P2.value=='') || isNaN(f2.P2.value) || f2.P2.value<0 || f2.P2.value>255 || (f2.P2.value.indexOf('.')>-1))
            alert('Invalid Priority(0 - 255).');
        else
            f2.submit();
    }

    function chkEnable(){
        if(document.formSTPGlobal.R1[0].checked == true){
            document.formSTPGlobal.T1.disabled = false;
            document.formSTPGlobal.T2.disabled = false;
            document.formSTPGlobal.T3.disabled = false;
            document.formSTPGlobal.T4.disabled = false;
            document.formSTPSet.D1.disabled = false;
            document.formSTPSet.D2.disabled = false;
            document.formSTPSet.P1.disabled = false;
            document.formSTPSet.P2.disabled = false;
            document.getElementById("refresh").disabled=false;
            document.getElementById("B1").disabled=false;
       }
       else
       {
            document.formSTPGlobal.T1.disabled = true;
            document.formSTPGlobal.T2.disabled = true;
            document.formSTPGlobal.T3.disabled = true;
            document.formSTPGlobal.T4.disabled = true;
            document.formSTPSet.D1.disabled = true;
            document.formSTPSet.D2.disabled = true;
            document.formSTPSet.P1.disabled = true;
            document.formSTPSet.P2.disabled = true;
            document.getElementById("refresh").disabled=true;
            document.getElementById("B1").disabled=true
       }
    }
    
    chkEnable();
</script>

