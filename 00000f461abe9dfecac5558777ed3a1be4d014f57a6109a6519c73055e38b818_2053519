function IPv6_Select1()
{
	if(document.formACLWizard.T100.value.length !=0)
	{
		//document.getElementsByName("T200")[0].disabled=true;
		//document.formACLWizard.T200.style.backgroundColor='#D4D0C8';	
		
		/* del for PI-294, by King Wang 20090407
		document.formACLWizard.D32.disabled=true;	
		document.formACLWizard.S12.disabled=true;
		document.formACLWizard.S12.style.backgroundColor='#D4D0C8';
		document.formACLWizard.S13.disabled=true;
		document.formACLWizard.S13.style.backgroundColor='#D4D0C8';
		*/
		document.formACLWizard.TCPSrcPortMask.readOnly=true;
		document.formACLWizard.TCPSrcPortMask.style.backgroundColor='#D4D0C8';
		document.formACLWizard.TCPDesPortMask.readOnly=true;
		document.formACLWizard.TCPDesPortMask.style.backgroundColor='#D4D0C8';
		document.formACLWizard.UDPSrcPortMask.readOnly=true;
		document.formACLWizard.UDPSrcPortMask.style.backgroundColor='#D4D0C8';
		document.formACLWizard.UDPDesPortMask.readOnly=true;
		document.formACLWizard.UDPDesPortMask.style.backgroundColor='#D4D0C8';
	
	}
	else
	{
		//document.getElementsByName("T200")[0].disabled=false;
		//document.formACLWizard.T200.style.backgroundColor='#FFFFFF';
		
		/* del for PI-294, by King Wang 20090407
		document.formACLWizard.D32.disabled=false;
		document.formACLWizard.S12.disabled=false;
		document.formACLWizard.S12.style.backgroundColor='#FFFFFF';
		document.formACLWizard.S13.disabled=false;
		document.formACLWizard.S13.style.backgroundColor='#FFFFFF';	
		*/
		document.formACLWizard.TCPSrcPortMask.readOnly=false;
		document.formACLWizard.TCPSrcPortMask.style.backgroundColor='#FFFFFF';
		document.formACLWizard.TCPDesPortMask.readOnly=false;
		document.formACLWizard.TCPDesPortMask.style.backgroundColor='#FFFFFF';
		document.formACLWizard.UDPSrcPortMask.readOnly=false;
		document.formACLWizard.UDPSrcPortMask.style.backgroundColor='#FFFFFF';
		document.formACLWizard.UDPDesPortMask.readOnly=false;
		document.formACLWizard.UDPDesPortMask.style.backgroundColor='#FFFFFF';
	}
}
