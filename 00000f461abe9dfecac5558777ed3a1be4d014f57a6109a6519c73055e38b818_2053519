<script language="Javascript">
function FindAll()
{
	location.href = "54_IGMP_Snooping_Group.html?0:0"; 
}
function applyform()
{
	if (document.formMLD.NameorID.value==2 &&!check_vlanlist(document.formMLD.vid))
	{
		return;
	}
	if (document.formMLD.NameorID.value==1 &&!checkTxtInput(document.formMLD.vname))
	{
		return;
	}

	if(document.formMLD.NameorID.options[0].selected)
		location.href = "54_IGMP_Snooping_Group.html?1" +":"+ escape(document.formMLD.vname.value); 
	else if(document.formMLD.NameorID.options[1].selected) 
		location.href = "54_IGMP_Snooping_Group.html?2" + ":"+document.formMLD.vid.value; 
}
function del()
{
	if (document.formMLD.NameorID_m.value==2)
	{
		if (isNaN(document.formMLD.vid_m.value) || document.formMLD.vid_m.value < 1 ||document.formMLD.vid_m.value > 4094)
		{
			alert("The range of VID List is from 1 to 4094");
			document.formMLD.vid_m.select();
			document.formMLD.vid_m.focus();
			return false;
		}
	}
	if (document.formMLD.NameorID_m.value==1 &&!checkTxtInput(document.formMLD.vname_m))
	{
		return;
	}
	if(document.formMLD.ip_addr.value!="")
	{
		if(!ipCheck(document.formMLD.ip_addr))
		{
			return;
		}
		document.formMLD.delAll_ip.value = 0;
	}
	else
	{
		document.formMLD.delAll_ip.value = 1;
	}
	document.formMLD.delAll.value = 0;
	document.formMLD.submit();
}

function deleteAll()
{
	document.formMLD.delAll.value = 1;
	document.formMLD.submit();
}

</script>
<div id="listBlock">
<span id="cpudiv"></span>
<script language="javascript">
	document.write("<iframe name=ifrGVRP id=ifrGVRP class=ifrPortConfig2 src='54_IGMP_Snooping_Group_table.html' frameborder=0 scrolling='auto'></iframe>");
</script>
</div>

<script>
var snp_group_count;

function show_counter()
{
	var str = "<b>IGMP Snooping Group Table &nbsp;&nbsp;&nbsp;&nbsp;Total Entries: "+ snp_group_count+"</b>";
	document.getElementById('cpudiv').innerHTML = str;
}
function intial()
{
	if(document.getElementById("NameorID").value == 1) /* vlan name */
	{
		document.getElementById("com1").style.display ="none";
		document.getElementById("com11").style.display="block"; /* vlan name */
	}
	else
	{
		document.getElementById("com1").style.display ="block";
		document.getElementById("com11").style.display="none"; /* vlan name */
	}
	
	if(document.getElementById("NameorID_m").value == 1) /* vlan name */
	{
		document.getElementById("com2").style.display ="none";
		document.getElementById("com21").style.display="block";
	}
	else
	{
		document.getElementById("com2").style.display ="block";
		document.getElementById("com21").style.display="none";
	}
}
intial();
function chgToIndex(obj1, obj2)
{
	if(document.getElementById("NameorID").value == 1) /* vlan name */
	{
		document.getElementById("com1").style.display ="none";
		document.getElementById("com11").style.display="block"; /* vlan name */
	}
	else
	{
		document.getElementById("com1").style.display ="block";
		document.getElementById("com11").style.display="none"; /* vlan name */
	}
}

function chgToIndex1(obj1, obj2)
{
	if(document.getElementById("NameorID_m").value == 1) /* vlan name */
	{
		document.getElementById("com2").style.display ="none";
		document.getElementById("com21").style.display="block";
	}
	else
	{
		document.getElementById("com2").style.display ="block";
		document.getElementById("com21").style.display="none";
	}
}

</script>
</div>
</table>
</div>

