	function btnDelete_Click(index)
	{
		parent.document.getElementById("boxIP").value = parent.ASP[index][0];	/* Gateway IP Address */
		parent.document.getElementById("hidOperation").value = "1";				/* delete */
		parent.document.getElementById("frmASP").submit();
	}
