	<script language="Javascript">
		function  vnameToVid(oRad)
		{
			if(oRad.id == "radVlan")
			{
				document.getElementById("radVid").checked = false;
				document.getElementById("radVlan").checked = true;
				document.getElementById("boxVid").value = "";
				document.getElementById("boxVid").disabled = true;
				document.getElementById("boxVid").style.backgroundColor = "#D4D0C8";
				document.getElementById("boxVlan").disabled = false;
				document.getElementById("boxVlan").style.backgroundColor = "";
			}
			else
			{
				document.getElementById("radVlan").checked = false;
				document.getElementById("radVid").checked = true;
				document.getElementById("boxVlan").value = "";
				document.getElementById("boxVlan").disabled = true;
				document.getElementById("boxVlan").style.backgroundColor = "#D4D0C8";
				document.getElementById("boxVid").disabled = false;
				document.getElementById("boxVid").style.backgroundColor = "";
			}
		}
		
		function apply1()
		{
			if( (document.getElementById("radVlan").checked == true) && !checkTxtInput(document.getElementById("boxVlan")) )
			{
				return false;
			}
			else if( (document.getElementById("radVid").checked == true) && !check_vlanlist(document.getElementById("boxVid")) )
			{
				return false;
			}
			document.getElementById("frmSnp1").submit();
		}
		
		function apply2()
		{
			if( checkNumberInput(document.getElementById("boxMaxEntry"),1,256) )
			{
				document.getElementById("frmSnp2").submit();
			}
		}
	</script>
</head>
