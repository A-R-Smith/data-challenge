	<script language="javascript">
		function Num2Str_auth(value)
		{
			if(value == '0')
				return "N/A";
			else if(value == '1')
				return "Authenticated";
			else if(value == '2')
				return "un-Authenticated";
				
		}
		function Num2Str_vlan(value)
		{
			if(value == '0')
				return "N/A";
			else
				return value;				
		}		
	</script>
