	<script language="Javascript">    
	    var trClass = '';
	    var txtClass = '';
	    document.write("<tbody class='scrollContent'>");  
	    for(i=0; i<IGMP_SNP_Group.length; i++)
		{
		        if (i%2 == 0){
		            trClass = 'gx';
		            txtClass = 'flatGX';
		        }
		        else{
		            trClass = 'gw';
		            txtClass = 'flatGX';
		        }
		        
		   document.write("<tr class=" + trClass + ">");
		   document.write("  <td width=5% align=left>" + IGMP_SNP_Group[i][0] + "</td>");	/*  vid */
			tipStr = strNormalize(IGMP_SNP_Group[i][1]);						/* vlan name */
			document.write("<td width=10% align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(IGMP_SNP_Group[i][1],10)) + "</td>");
			document.write("  <td width=15%>" + IGMP_SNP_Group[i][2] + "</td>");			/* Source */
			document.write("  <td width=15%>" + IGMP_SNP_Group[i][3] + "</td>");			/* Group */
			document.write("  <td width=7%>" + IGMP_SNP_Group[i][9] + "</td>");			/* Reports */
			tipStr = strNormalize(IGMP_SNP_Group[i][4])						/* Port Member */
			document.write("<td width=12% align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(IGMP_SNP_Group[i][4], 20)) + "</td>");
			tipStr = strNormalize(IGMP_SNP_Group[i][6])						/* Route Port */
			document.write("<td width=10% align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(IGMP_SNP_Group[i][6], 20)) + "</td>");
			document.write("  <td width=10% >" + IGMP_SNP_Group[i][7] + "</td>");			/* UP Time */
			document.write("  <td width=10% >" + IGMP_SNP_Group[i][8] + "</td>");			/* Expiry Time */
			document.write("  <td width=11% >" + IGMP_SNP_Group[i][5] + "</td>");			/* Mode */
			document.write("</tr>");

	    }
	    
	</script>
	<script language="Javascript">      
		parent.snp_group_count = IGMP_SNP_Group.length;
		parent.show_counter();
	</script>
    </tbody>
    </table>
