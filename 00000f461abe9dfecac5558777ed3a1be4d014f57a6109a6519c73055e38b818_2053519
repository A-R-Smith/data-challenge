<script language="Javascript">
    function apply(s)
    {
    	var i=0;
    	for(i=0; i<s.length-1; i++)
	{
		if(s.elements[i].checked == true)
			s.elements[i].value = '1';
		else
			s.elements[i].value = '0';
	}
	s.submit();
	return true;
    }

</script>
