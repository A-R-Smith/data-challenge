		function aDrive_Click(index)
		{
			var pathname = parent.FS_StorageMedia[index][1]; /* Drive */
			pathname += pathname.substring(parent.CurrentPath.length-1, parent.CurrentPath.length).match(/^[\/\\]$/) == null ? '\\' : '';
			parent.cmd_cd(pathname);
		}
		
		function btnFormat_Click(index)
		{
			parent.document.getElementById("divFS").style.display = "none";
			parent.document.getElementById("divFormat").style.display = "";
			parent.document.getElementById("selDrive_Format").value = parent.FS_StorageMedia[index][1];
		}
		
		function pageLoad()
		{
			var driveRegexp = /^[a-zA-Z]:(\\|\/)?$/;	/* C:, C:\ */
			
