<script language="Javascript">

function delit(iTag)
{
	var f = document.MI_Info;

	if(iTag == -1)
	{
		if(!checkFilePath(document.getElementById("boxPathname")))
		{
			return false;
		}
		if (!confirm("Are you sure to delete firmware: " + document.getElementById("boxPathname").value + " ?"))
		{
			return false;
		}
		f.h_unit.value = 0;
		f.h_pathname.value =  document.getElementById("boxPathname").value;
		f.isDel.value = '1';
		f.submit();
	}
	if(Multi_Image[iTag][8] != "")	/* if SDcard file, added for PI-381, 20090408 */
	{
		if (!confirm("Are you sure to delete firmware: " + Multi_Image[iTag][8] + " ?"))
		{
			return false;
		}
		f.h_unit.value = 0;
		f.h_pathname.value =  Multi_Image[iTag][8];
		f.isDel.value = '1';
		f.submit();
	}
	else if (confirm("Are you sure to delete firmware ID " + Multi_Image[iTag][1] + " ?"))
	{
		f.h_unit.value = Multi_Image[iTag][0];
		f.h_id.value = Multi_Image[iTag][1];
		f.isDel.value = '1';
		f.submit();
	}
}

function set_boot(iTag)
{
	var f = document.MI_Info;

	if(iTag == -1)
	{
		if(!checkFilePath(document.getElementById("boxPathname")))
		{
			return false;
		}
		f.h_unit.value = 0;
		f.h_pathname.value =  document.getElementById("boxPathname").value;
		f.isDel.value = '0';
	}
	else	
	{
		f.h_unit.value = Multi_Image[iTag][0];
		f.h_id.value = Multi_Image[iTag][1];
		f.h_pathname.value  = Multi_Image[iTag][8];
		f.isDel.value = '0';
	}
	
	f.submit();
}

</script>
</div>
</body>
