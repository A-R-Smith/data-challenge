<script language="Javascript">

	function AddIt()
	{
	/*
		if(!checkMulticastIPAddrList(document.formISMGroup.T1.value))
		{
			document.formISMGroup.T1.focus();
			document.formISMGroup.T1.select();
			return false;
		}
		*/
		document.formISMGroup.operation.value='2';
		document.formISMGroup.VlanName.value= vlanname;
		document.formISMGroup.submit();
		return true;
	}
	function DeleteIt(iTag)
	{
		document.formISMGroup.operation.value='1';
		document.formISMGroup.VlanName.value= vlanname;
		document.formISMGroup.T1.value= ISM_Setting_Group[iTag][1];
		document.formISMGroup.submit();
		return true;
	}
