function goprev()
{
	var url = document.URL;
	var arg1 = url.split("?")[1].split(",")[1];
	var arg2 = url.split("?")[1].split(",")[2].split("%20")[0];
	var arg3 = url.split("?")[1].split(",")[2].split("%20")[1];
	location.href = "54_Browse_ARP.html?1," + arg1 + "," + arg2 + " " + arg3;
}

function gonext()
{
	var url = document.URL;
	var arg1 = url.split("?")[1].split(",")[1];
	var arg2 = url.split("?")[1].split(",")[2].split("%20")[0];
	var arg3 = url.split("?")[1].split(",")[2].split("%20")[1];
	location.href = "54_Browse_ARP.html?2," + arg1 + "," + arg2 + " " + arg3;
}
