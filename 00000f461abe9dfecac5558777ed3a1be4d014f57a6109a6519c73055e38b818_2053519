	<input type=hidden name=isDel_by_mac id=isDel value='0'>
	<input type=hidden name=isDel_by_vlan id=isDel value='0'>
	<input type=hidden name='cn_mac' value='0'>
	<input type=hidden name='cn_vlan' value='0'>
	<input type=hidden name='cn_vlanid' value='0'>
	<input type=hidden name="h_vlan_flag" id="h_vlan_flag">
	<input type=hidden name='vlan_flag' id='vlan_flag' value=0>
	<table name=tabPortContent id=tabPortContent class=tabPortContent cellpadding="0" cellspacing="0" >
	<tr>
		<td width=80%>MAC Address&nbsp;<input type=text class=flatL name='mac_addr' maxlength=17 style='width:120;'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		VLAN Name&nbsp;<input type=radio name='vlan_flag' checked value='1' onclick='change_vlan_flag(1)'><input type=text class=flatL name=gvlan id=gvlan maxlength=32 style=width:120>
		&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;VLAN ID&nbsp;<input type=radio name='vlan_flag' value='0' onclick='change_vlan_flag(0)'><input type=text class=flatL name=gvlanid id=gvlanid maxlength=4 style=width:120></td>
		<tr><td></td></tr>
		<tr><td colspan=3><hr align=left name=hr1 id=hr1 class=hr1></td></tr>
		<tr>
		<td colspan=3 align=right>
		<input type=button value='Add' class=btnText onClick="applyForm(1);">&nbsp;
		<input type=button value='Delete By MAC' class=btnText onClick="applyForm(2);">&nbsp;
		<input type=button value='Delete By VLAN' class=btnText onClick="applyForm(3);">&nbsp;
		<input type=button value='Find By MAC' class=btnText onClick="find_by_mac();">&nbsp;
		<input type=button value='Find By VLAN' class=btnText onClick="find_by_vlan();"></td>
		</tr>
	
	</table>
</form>
</div>
</td></tr>
<tr><td>
<div id="listBlock">
	<script language="Javascript">
    document.write("<b>Total Entries: " + MBA_Local_MAC_Btn[2] +"</b>");
    </script>
	<form name=formmMAC_Edit  id=formmMAC_Edit method=post action="../../form/MAC_Edit_Apply">
	<input type=hidden name="h_vlan_flag" id="h_vlan_flag">
	<input type=hidden name='vlan_flag' id='vlan_flag' >
	<table name=mainGray id=mainGray class=mainGray border=0 cellpadding="0" cellspacing="0" width=100%>
        <tr class=gh> 	 	 	 	
            <td width=20%>&nbsp;MAC Address</td>
            <td width=20%>VLAN Name</td>
            <td width=15%>VLAN ID</td>
            <td width=*></td>
        </tr>
<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
    
    for(i=0; i<MBA_Local_MAC.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGW';
        }

	document.write("<tr class=" + trClass + ">");
	tipStr = strNormalize(MBA_Local_MAC[i][1]);
	document.write("  <td>&nbsp;" + MBA_Local_MAC[i][0] + "</td>");
	document.write("  <td align=left><input type=text name=vlan id=vlan maxlength=32  class=" + txtClass + " value='" + rp_special_char(strTruncate(MBA_Local_MAC[i][1], 20)) + "' readOnly onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>");
	document.write("  <input type=text name=vlan_m id=vlan_m maxlength=32 class=flatWS value='" + MBA_Local_MAC[i][1] + "' style='display: none;width:120'></td>");
	document.write("  <td align=left><input type=text name=vlanid id=vlanid maxlength=4  class=" + txtClass + " value='" + MBA_Local_MAC[i][2] + "' readOnly>");
	document.write("  <input type=text name=vlanid_m id=vlanid_m maxlength=4 class=flatWS value='" + MBA_Local_MAC[i][2] + "' style='display: none;width:120'></td>");

	document.write("  <td align=right><input type=button name='editVLANButton' id='editVLANButton' value='Edit By Name' class=btnText onClick='EditVLANStart(" + i + ");'>");
	document.write("<input type=button name='editVLANApply' id='editVLANApply' value='Apply' class=btnText onClick='EditVLANApplyIt(" + i + ");' style='display: none;'>");
	document.write("  <input type=button name='editVLANIDButton' id='editVLANIDButton' value='Edit By ID' class=btnText onClick='EditVLANIDStart(" + i + ");'>");
	document.write("<input type=button name='editVLANIDApply' id='editVLANIDApply' value='Apply' class=btnText onClick='EditVLANIDApplyIt(" + i + ");' style='display: none;'></td>");
	document.write("</tr>");
	
    }
	document.write("<tr><td>&nbsp;</td></tr>");
	document.write("  <input type=text name=cn_mac id=cn_mac style='display: none;'>");
	document.write("  <input type=text name=cn_vlan id=cn_vlan style='display: none;'>");
	document.write("  <input type=text name=cn_vlanid id=cn_vlanid style='display: none;'>");
	if (MBA_Local_MAC_Btn[2] > 0)
	{
		document.write("<tr><td>&nbsp;</td></tr>");
		document.write("<tr><td align=right colspan=5><input id='bprev' name='bprev' type=button value='<< Back' class=btnText onClick=goprev() disabled>");
		document.write("<input id='bnext' name='bnext' type=button value='Next >>' class=btnText onClick=gonext() disabled></td></tr>");
	}
