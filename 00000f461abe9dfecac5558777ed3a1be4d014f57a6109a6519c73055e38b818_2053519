<script language="Javascript">
var d = document.formAddVLAN;
var snmp_user_count;

function AddApply()
{
	if (!checkTxtInput(d.UName))
		return;

	if (!checkTxtInput(d.GName))
		return;

	if (d.auth_pass.disabled == false)
	{
		if (!checkTxtInput(d.auth_pass))
			return;
	}

	if (d.priv_pass.disabled == false)
	{
		if (!checkTxtInput(d.priv_pass))
			return;
	}

	if (d.auth_key.disabled == false)
	{
		if (!checkTxtInput(d.auth_key))
			return;
	}

	if (d.priv_key.disabled == false)
	{
		if (!checkTxtInput(d.priv_key))
			return;
	}

	d.isDel.value = '0';

//Auth Protocol
	if (d.auth_pro_by_pass.disabled == false)
	{
		d.auth_pro.value = d.auth_pro_by_pass.value;
	}
	else if (d.auth_by_key.disabled == false)
	{
		d.auth_pro.value = d.auth_by_key.value;
	}

//Priv Protocol
	if (d.priv_pro_by_pass.disabled == false)
	{
		d.priv_pro.value = d.priv_pro_by_pass.value;
	}
	else if (d.priv_by_key.disabled == false)
	{
		d.priv_pro.value = d.priv_by_key.value;
	}

//Auth Code
	if (d.auth_pass.disabled == false)
	{
		if (d.auth_pro_by_pass.value == 2)
		{
			if (d.auth_pass.value.length < 8 || d.auth_pass.value.length > 16)
			{
				alert("The password length should be 8-16.");
				d.auth_pass.select();
				return;
			}
		}
		else
		{
			if (d.auth_pass.value.length < 8 || d.auth_pass.value.length > 20)
			{
				alert("The password length should be 8-20.");
				d.auth_pass.select();
				return;
			}
		}	
		
		d.auth_code.value = d.auth_pass.value;
	}
	else if (d.auth_key.disabled == false)
	{
		if (d.auth_by_key.value == 2)
		{
			if (d.auth_key.value.length != 32)
			{
				alert("The password length should be 32.");
				d.auth_key.select();
				return;
			}
		}
		else
		{
			if (d.auth_key.value.length != 40)
			{
				alert("The password length should be 40.");
				d.auth_key.select();
				return;
			}
		}
		
		d.auth_code.value = d.auth_key.value;
	}
//Priv Code
	if (d.priv_pass.disabled == false)
	{
		if (d.priv_pass.value.length < 8 || d.priv_pass.value.length > 16)
		{
			alert("The key length should be 8-16.");
			d.priv_pass.select();
			return;
		}
		d.priv_code.value = d.priv_pass.value;
	}
	else if (d.priv_key.disabled == false)
	{
		if (d.priv_key.value.length != 32)
		{
			alert("The key length should be 32.");
			d.priv_key.select();
			return;
		}
		d.priv_code.value = d.priv_key.value;
	}

	d.submit();
}

function delUser()
{
	d.isDel.value = '1';
	d.submit();
}

function checkDES1()
{
	if(d.priv_pro_by_pass.selectedIndex == 0)
	{
		d.priv_pass.disabled=true;
	}
	else
	{
		d.priv_pass.disabled=false;
	}
}

function checkDES2()
{
	if(d.priv_by_key.selectedIndex == 0)
	{
		d.priv_key.disabled=true;
	}
	else
	{
		d.priv_key.disabled=false;
	}
}
	
function checkversion()
{
	if (d.SNMP_Version.selectedIndex == 2)
		d.SNMP_V3_Encryption.disabled = false;
	else
	{
		d.SNMP_V3_Encryption.disabled = true;
		d.auth_pro_by_pass.disabled = true;
		d.auth_pass.disabled = true;
		d.priv_pro_by_pass.disabled = true;
		d.priv_pass.disabled = true;
		d.auth_by_key.disabled = true;
		d.auth_key.disabled = true;
		d.priv_by_key.disabled=true;
		d.priv_key.disabled=true;
	}
}
function checkencryption()
{
	switch(d.SNMP_V3_Encryption.selectedIndex) 
	{
		case 0: 
			d.auth_pro_by_pass.disabled = true;
			d.auth_pass.disabled = true;
			d.priv_pro_by_pass.disabled = true;
			d.priv_pass.disabled = true;
			d.auth_by_key.disabled = true;
			d.auth_key.disabled = true;
			d.priv_by_key.disabled=true;
			d.priv_key.disabled=true;
			break;
		case 1:
			d.auth_pro_by_pass.disabled = false;
			d.auth_pass.disabled = false;
			d.priv_pro_by_pass.disabled = false;
			if (d.priv_pro_by_pass.selectedIndex == 0)
				d.priv_pass.disabled = true;
			else
				d.priv_pass.disabled = false;
			d.auth_by_key.disabled = true;
			d.auth_key.disabled = true;
			d.priv_by_key.disabled=true;
			d.priv_key.disabled=true;
			break;
		case 2:
			d.auth_pro_by_pass.disabled = true;
			d.auth_pass.disabled = true;
			d.priv_pro_by_pass.disabled = true;
			d.priv_pass.disabled = true;
			d.auth_by_key.disabled = false;
			d.auth_key.disabled = false;
			d.priv_by_key.disabled=false;
			if (d.priv_by_key.selectedIndex == 0)
				d.priv_key.disabled=true;
			else
				d.priv_key.disabled=false;
			break;
	}
}
    //document.getElementById('tabSNMPViewTableContent').style.visibility = 'hidden';
function show_counter()
{
	var str = "<b>Total Entries: "+ snmp_user_count+"</b>";
	document.getElementById('snmp_user_counter').innerHTML = str;
}

d.SNMP_V3_Encryption.disabled = true;
d.auth_pro_by_pass.disabled = true;
d.auth_pass.disabled = true;
d.priv_pro_by_pass.disabled = true;
d.priv_pass.disabled = true;
d.auth_by_key.disabled = true;
d.auth_key.disabled = true;
d.priv_by_key.disabled=true;
d.priv_key.disabled=true;
checkversion();
</script>
</div>
</body>
</html>

