							document.write("<td><select class=enableSel name='toport' onchange='checkPort2(document.formSerachAuthState)' id=toport>");
							for(i=0; i<Port_Number.length; i++){
							var ii=i+1;
								document.write("<option value='" + ii+ "'>" + Port_Number[i] + "</option>");
							}
							document.write("       </select></td>");
							document.write("<td><input type=checkbox name=authenticated id=authenticated>Authenticated&nbsp;&nbsp;");
							document.write("<input type=checkbox name=authenticating id=authenticating>Authenticating&nbsp;&nbsp;");
							document.write("<input type=checkbox name=blocked id=blocked>Blocked</td>");
						</script>

