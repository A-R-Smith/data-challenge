<script>
function get_version(v)
{
	var ver= v.split("_");
	var majorver=ver[0].split(".");
	var msg='';
	var verno=0;
	for(i=0; i<majorver.length; i++)
	{
		msg+=majorver[i]+'\n';
		verno+=(majorver[i]*10000/Math.pow(10,i));
	}
	return verno;
}

function java_version(str_ver)
{
	var java_ver= get_version(str_ver)
	alert(java_ver);
	if(java_ver<14200)
	{
		parent.fTopo.location="SIM_null.html";
	}
	else
	{
		parent.fTopo.location="SIM_Topology.html";
	}
}

</script>
