<script language="Javascript">
var lastidx = -1;
function PrintRule(index)
{
	var outstr = '';
	var trClass = 'gx';
	
	outstr += "<table name=mainGray id=mainGray class=mainGray cellpadding=0 cellspacing=0 width=60%>";
	if (acl_style == 1)
		outstr += "<tr class=gh><td colspan=2><b>ACL Rule Details</b></td></tr>";
	else
		outstr += "<tr class=gh><td colspan=2><b>CPU ACL Rule Details</b></td></tr>";

	for(i=0; i<parent.acl_rule_list[index].length; i++)
	{
		if(parent.acl_rule_list[index][i] != '')
		{
			if((acl_style == 2) &&(ACLRule_Name[i]!='Counter'))
			{
				outstr += "<tr class=" + trClass +"><td width=50% stype=\"font-size: 11pt; font-weight:bold;\">" + ACLRule_Name[i] + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>";
				if (i == 8)
					outstr += "<td style='text-align: left;'>" + rp_special_char(parent.acl_rule_list[index][i]) + "</td></tr>";
				else
					outstr += "<td style='text-align: left;'>" + parent.acl_rule_list[index][i] + "</td></tr>";
	
				if (trClass == 'gw')
					trClass = 'gx';
				else
					trClass = 'gw';	
			}
			else if(acl_style == 1)
			{
			outstr += "<tr class=" + trClass +"><td width=50% stype=\"font-size: 11pt; font-weight:bold;\">" + ACLRule_Name[i] + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>";
			if (i == 8)
				outstr += "<td style='text-align: left;'>" + rp_special_char(parent.acl_rule_list[index][i]) + "</td></tr>";
			else
				outstr += "<td style='text-align: left;'>" + parent.acl_rule_list[index][i] + "</td></tr>";

			if (trClass == 'gw')
				trClass = 'gx';
			else
				trClass = 'gw';				
			}
		}
	}
	outstr += "<tr><td></td></tr>";
	outstr += "<tr><td></td></tr>";
	outstr += "<tr><td><input type=button class=btnText value='Show All Rules' onclick=show_all()></td></tr>";
	outstr += "</table>";	
	document.getElementById('aclPD').innerHTML = outstr;
}

</script>
<body><script language="Javascript">genStackIDTip();</script>
<div id=rightContent>    
    <script language="Javascript">
	if (acl_style == 1)
		genHeaderTable('Access Rule Detail Information');
	else
		genHeaderTable('CPU Access Rule Detail Information');
    </script>


<div name='aclPD' id=aclPD class=ifrACLRuleDetail style="visibility:hidden;">
</div>
</div>
<script language="Javascript">
PrintRule(rule_id);
document.getElementById('aclPD').style.visibility='visible';

function show_all()
{
	location.href = '52_AccessRule.html?' + parent.acl_rule_list[rule_id][0] + "," + acl_style + ",0";
}

</script>
</body>
</html>

