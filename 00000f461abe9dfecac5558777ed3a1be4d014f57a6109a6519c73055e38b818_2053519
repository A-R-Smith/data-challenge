	<script language="Javascript">
		parent.timer_reset=1;
		function  vnameOrvid(oRad)
		{
			if(oRad.id == "radVlan_Add")
			{
				document.getElementById("radVid_Add").checked = false;
				document.getElementById("radVlan_Add").checked = true;
				document.getElementById("boxVid_Add").value = "";
				document.getElementById("boxVid_Add").disabled = true;
				document.getElementById("boxVid_Add").style.backgroundColor = "#D4D0C8";
				document.getElementById("boxVlan_Add").disabled = false;
				document.getElementById("boxVlan_Add").style.backgroundColor = "";
				document.getElementById("boxVlan_Add").focus();

			}
			else if(oRad.id == "radVid_Add")
			{
				document.getElementById("radVlan_Add").checked = false;
				document.getElementById("radVid_Add").checked = true;
				document.getElementById("boxVlan_Add").value = "";
				document.getElementById("boxVlan_Add").disabled = true;
				document.getElementById("boxVlan_Add").style.backgroundColor = "#D4D0C8";
				document.getElementById("boxVid_Add").disabled = false;
				document.getElementById("boxVid_Add").style.backgroundColor = "";
				document.getElementById("boxVid_Add").focus();
			}
			else if(oRad.id == "radVlan")
			{
				document.getElementById("radVid").checked = false;
				document.getElementById("radVlan").checked = true;
				document.getElementById("boxVid").value = "";
				document.getElementById("boxVid").disabled = true;
				document.getElementById("boxVid").style.backgroundColor = "#D4D0C8";
				document.getElementById("boxVlan").disabled = false;
				document.getElementById("boxVlan").style.backgroundColor = "";
				document.getElementById("boxVlan").focus();
			}
			else if(oRad.id == "radVid")
			{
				document.getElementById("radVlan").checked = false;
				document.getElementById("radVid").checked = true;
				document.getElementById("boxVlan").value = "";
				document.getElementById("boxVlan").disabled = true;
				document.getElementById("boxVlan").style.backgroundColor = "#D4D0C8";
				document.getElementById("boxVid").disabled = false;
				document.getElementById("boxVid").style.backgroundColor = "";
				document.getElementById("boxVid").focus();
			}
		}
		
		/* set the textbox state by checkbox*/
		/* box:box id(string) */
		/* chk:chk id(string) */
		function setBoxByChk(box,chk)
		{
			if(document.getElementById(chk).checked==true)
			{
				document.getElementById(box).disabled = true;
				document.getElementById(box).value = "all";
				document.getElementById(box).style.backgroundColor = "#D4D0C8";
				document.getElementById("all_flag").value = 1;
			}
			else
			{
				document.getElementById(box).disabled = false;
				document.getElementById(box).value = "";
				document.getElementById(box).style.backgroundColor = "";
				document.getElementById(box).focus();
				document.getElementById("all_flag").value = 0;
			}
		}
		
		function setGstVlan(oBtn)
		{
			if( document.getElementById("radVlan_Add").checked==true )
			{
				if( !checkTxtInput(document.getElementById("boxVlan_Add")) )
				{
					return false;
				}
			}
			else //if( document.getElementById("radVid").checked==true )
			{
				if( !check_vlanlist(document.getElementById("boxVid_Add")) )
				{
					return false;
				}
			}
			if(oBtn.id == "btnAdd")
			{			
				document.all["hidSetOpr"].value = "create";
			}
			else
			{
				document.all["hidSetOpr"].value = "delete";
			}
			document.getElementById("frmAuthen_GstVlan_Set").submit();
		}
		
		function cfgVlan()
		{
			if( document.getElementById("radVlan").checked==true )
			{
				if( !checkTxtInput(document.getElementById("boxVlan")) )
				{
					return false;
				}
			}
			else //if( document.getElementById("radVid").checked==true )
			{
				if (!checkNumberInput(document.frmAuthen_GstVlan.boxVid, 1, 4094))
				{
					return false;
				}
			}			
			
			if(document.getElementById("chkAll").checked==false && document.getElementById("boxPorts").disabled == false)
			{
				if( !check_portlist(document.getElementById("boxPorts")) )
				{
					return false;
				}
			}
			document.getElementById("frmAuthen_GstVlan").submit();
		}		
	</script>
</head>
