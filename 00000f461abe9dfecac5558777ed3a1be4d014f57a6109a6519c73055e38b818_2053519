<script language="Javascript">
function Apply()
{
  alert('Ok');
}
function selectpro(index)
{
	switch(index)
	{
		case '1':
			document.getElementById('icmp').disabled=false;
			document.getElementById('code').disabled=false;
			
			document.getElementById('igmp').disabled=true;
			
			document.getElementById('TCPsrcPort').disabled=true;
			document.getElementById('TCPdstPort').disabled=true;
			
			document.getElementById('UDPsrcPort').disabled=true;
			document.getElementById('UDPdstPort').disabled=true;			
		break;
		case '2':
		document.getElementById('icmp').disabled=true;
			document.getElementById('code').disabled=true;
			
			document.getElementById('igmp').disabled=false;
			
			document.getElementById('TCPsrcPort').disabled=true;
			document.getElementById('TCPdstPort').disabled=true;
			
			document.getElementById('UDPsrcPort').disabled=true;
			document.getElementById('UDPdstPort').disabled=true;
		break;
		case '3':
		document.getElementById('icmp').disabled=true;
			document.getElementById('code').disabled=true;
			
			document.getElementById('igmp').disabled=true;
			
			document.getElementById('TCPsrcPort').disabled=false;
			document.getElementById('TCPdstPort').disabled=false;
			
			document.getElementById('UDPsrcPort').disabled=true;
			document.getElementById('UDPdstPort').disabled=true;
		break;
		case '4':
		document.getElementById('icmp').disabled=true;
			document.getElementById('code').disabled=true;
			
			document.getElementById('igmp').disabled=true;
			
			document.getElementById('TCPsrcPort').disabled=true;
			document.getElementById('TCPdstPort').disabled=true;
			
			document.getElementById('UDPsrcPort').disabled=false;
			document.getElementById('UDPdstPort').disabled=false;
		break;
	}
}
function chgToPortIndex4(control,index)
{ 
    if((control.options.selectedIndex==0) || (control.options.selectedIndex==1))
     {
       //document.getElementsByName("T4")[0].value=''; 
       document.getElementById("com4").style.display="none";
       document.getElementById("com41").style.display="none";
       document.getElementById("com411").style.display="none";
       document.getElementsByName("T4")[0].readOnly=true;
     }
     else if(control.options.selectedIndex==3)
     {
        document.getElementsByName("T4")[0].readOnly=false;
        document.getElementById("com41").style.display="none";
        document.getElementById("com4").style.display="block";
       document.getElementById("com411").style.display="none";
        //document.getElementsByName("T4")[0].value='0~7';
        //document.getElementsByName("T4")[0].style.color = "#666666";
     }
     else if(control.options.selectedIndex==4)
     {
          document.getElementsByName("T4")[0].readOnly=false;
          document.getElementById("com4").style.display="none";
          document.getElementById("com41").style.display="block";
		       document.getElementById("com411").style.display="none";
          //document.getElementsByName("T4")[0].value='0~63';
          //document.getElementsByName("T4")[0].style.color = "#666666";
     }
     else
     {
         document.getElementsByName("T4")[0].readOnly=false;
         document.getElementById("com4").style.display="none";
         document.getElementById("com41").style.display="none";
	       document.getElementById("com411").style.display="block";
         //document.getElementsByName("T4")[0].value=''; 
     }
}
</script>
</form>

</div></body>
