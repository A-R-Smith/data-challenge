<script language="Javascript">

function checkAll(index)
{
	var s = document.formDHCP_IP_MAC_Port_Settings;

	if (s.all_port.checked == true)
	{
		s.h_all_port.value = '1';
		s.port.disabled = true;
		s.port.style.backgroundColor='#D4D0C8';
	}
	else
	{
		s.h_all_port.value = '0';
		s.port.disabled = false;
		s.port.style.backgroundColor='#ffffff';
	}
		
}

function dellist()
{
	var ob_mac_address = document.formDHCP_IP_MAC_Port_Settings.mac_address;
	var ob_ip_addr = document.formDHCP_IP_MAC_Port_Settings.ip_address;
	var port=document.formDHCP_IP_MAC_Port_Settings.port;

	if (!ipCheck(ob_ip_addr))
	{
		return false;
	}
	if (ob_mac_address.value!="" && !MAC_Address_Check(ob_mac_address))
		return false;
	if(!macUcastCheck(ob_mac_address.value))
	{
		alert("Invalid MAC Address.");
		document.formDHCP_IP_MAC_Port_Settings.mac_address.select();
		document.formDHCP_IP_MAC_Port_Settings.mac_address.focus();
		return false;
	}
	if (document.formDHCP_IP_MAC_Port_Settings.all_port.checked == false)
	{
		if(!check_portlist(port))
			return false;
	}
	document.formDHCP_IP_MAC_Port_Settings.isDellist.value=1;
	document.formDHCP_IP_MAC_Port_Settings.submit();
}
function apply()
{
	var ob_mac_address = document.formDHCP_IP_MAC_Port_Settings.mac_address;
	var ob_ip_addr = document.formDHCP_IP_MAC_Port_Settings.ip_address;
	var port=document.formDHCP_IP_MAC_Port_Settings.port;

	if (!ipCheck(ob_ip_addr))
	{
		return false;
	}
	if (ob_mac_address.value!="" && !MAC_Address_Check(ob_mac_address))
		return false;
	if(!macUcastCheck(ob_mac_address.value))
	{
		alert("Invalid MAC Address.");
		document.formDHCP_IP_MAC_Port_Settings.mac_address.select();
		document.formDHCP_IP_MAC_Port_Settings.mac_address.focus();
		return false;
	}
	if (document.formDHCP_IP_MAC_Port_Settings.all_port.checked == false)
	{
		if(!check_portlist(port))
			return false;
	}
	document.formDHCP_IP_MAC_Port_Settings.isDellist.value=0;
	document.formDHCP_IP_MAC_Port_Settings.submit();
}
/*
function clearall()
{

	if (document.formDHCP_IP_MAC_Port_Settings.ip_address.value=="")
	{	
		alert("Please input IP Address");
		return false;
	}
	document.formDHCP_IP_MAC_Port_Settings.isDelAll.value = '1';
	document.formDHCP_IP_MAC_Port_Settings.submit();
}
*/
function delit(ip, mac,port)
{
	document.formDHCP_IP_MAC_Port_Settings.isDel.value = '1';
	document.formDHCP_IP_MAC_Port_Settings.ip_address.value = ip;
	document.formDHCP_IP_MAC_Port_Settings.mac_address.value = mac;
	document.formDHCP_IP_MAC_Port_Settings.port.value = port;
	document.formDHCP_IP_MAC_Port_Settings.submit();
}
</script>

</body>
