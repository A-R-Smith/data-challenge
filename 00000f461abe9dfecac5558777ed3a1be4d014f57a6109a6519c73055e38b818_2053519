<script language="Javascript">
	if(window.screen.width == 1280){
		document.getElementById('treeBox').style.height="100%";
	}

	function closeStackStatus(){
		document.getElementById('stackBox').style.display="none";
		document.getElementById('treeBox').style.height="97%";
	}
	
	function showStackStatus(){
		document.getElementById('stackBox').style.display="block";
		if(window.screen.width == 1280){
			document.getElementById('treeBox').style.height="72%";
		}else{
			document.getElementById('treeBox').style.height="60%";
		}
	}

	function showPullDownMenu(){
		document.getElementById('tag1').style.visibility = 'visible';
		parent.main.hiddenPullDownstackMenu();
	}

	function hiddenPullDownMenu(){
		document.getElementById('tag1').style.visibility = 'hidden';
	}

	function showPullDownStackMenu(){
		document.getElementById('tag2').style.visibility = 'visible';
		hiddenPullDownMenu();
	}

	function hiddenPullDownstackMenu()
	{
		document.getElementById('tag2').style.visibility = 'hidden';
	}
    
	ctItemMouseUp(document.getElementById('ctItemID0').parentNode);
	
