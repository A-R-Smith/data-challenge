<script language="Javascript">
function applyForm()
{
	if (document.formWACGlobal.vlan_name.value.length != 0)
	{
		if (!checkTxtInput(document.formWACGlobal.vlan_name))
			return;
	}


	if (!checkTxtInput(document.formWACGlobal.rdt_page))
		return;

	if(!document.formWACGlobal.timeout_flag.checked)
	{
		if (!checkNumberInput(document.formWACGlobal.timeout,1,1440))
			return;		
	}
	
	document.formWACGlobal.submit();
}

function applyPort()
{
	document.formWACPort.submit();
}

function checkBoxState()
{
	if(document.formWACGlobal.timeout_flag.checked == true)
		document.formWACGlobal.timeout.disabled = true;
	else 
		document.formWACGlobal.timeout.disabled = false;

}
function Init()
{
	document.formWACGlobal.state.value = WAC_Global[0];
	document.formWACGlobal.method.value = WAC_Global[1];
	document.formWACGlobal.vlan_name.value = rp_special_char(WAC_Global[2]);
	document.formWACGlobal.rdt_page.value = rp_special_char(WAC_Global[3]);
	if(WAC_Global[4]== '0')
		document.formWACGlobal.timeout_flag.checked = true;
	else
	{
		document.formWACGlobal.timeout_flag.checked = false;
		document.formWACGlobal.timeout.value = WAC_Global[4];
	}
	checkBoxState();
}

function DelIt()
{
	if(!check_portlist(document.formWACState.portlist))
		return false;
	document.formWACState.operation.value = '1' ;   //  1-----del
	document.formWACState.submit();
}
</script>
