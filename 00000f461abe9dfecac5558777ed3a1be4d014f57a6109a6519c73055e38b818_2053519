<script language="Javascript">
	function EditStart(iTag)
	{
		var bFLAG=false;
		var iTag_m;

	
		document.formSTPGlobal2[iTag].vlanID_show.value = JWAC_User_Settings[iTag][1];
		document.formSTPGlobal2[iTag].vlan_id.value = JWAC_User_Settings[iTag][1];


		for(i=0; i<JWAC_User_Settings.length; i++)
		{
			if(document.formSTPGlobal2[i].editApply.style.display== 'none')
			{
				bFALG=false
				iTag_m='';   //iTag_m is used for what item is apply status just now;
			}    		
			else
			{
				bFLAG=true;
				iTag_m=i;
				break;
			}
		}
		if(bFLAG==true)
		{
			document.formSTPGlobal2[iTag_m].T4_show.style.display = '';
			document.formSTPGlobal2[iTag_m].T4.style.display = 'none';
			if(JWAC_User_VlanStat==1)
			{
				document.formSTPGlobal2[iTag_m].vlanID_show.style.display = '';/* vid */
				document.formSTPGlobal2[iTag_m].vlan_id.style.display = 'none';
			}
			document.formSTPGlobal2[iTag_m].T5.style.display = 'none';
			document.formSTPGlobal2[iTag_m].T6.style.display = 'none';
			document.formSTPGlobal2[iTag_m].editButton.style.display = '';
			document.formSTPGlobal2[iTag_m].editApply.style.display = 'none'; 
		}
		document.formSTPGlobal2[iTag].T4_show.style.display ='none';
		document.formSTPGlobal2[iTag].T4.style.display = '';
		if(JWAC_User_VlanStat==1)
		{
			document.formSTPGlobal2[iTag].vlanID_show.style.display = 'none';
			document.formSTPGlobal2[iTag].vlan_id.style.display = '';
			if(JWAC_User_Settings[iTag][1]!=0)
			{
				document.formSTPGlobal2[iTag].vlan_id.value = JWAC_User_Settings[iTag][1];/* vid */
			}
			else
			{
				document.formSTPGlobal2[iTag].vlan_id.value = "-"
			}
		}
		document.formSTPGlobal2[iTag].T4.select();
		document.formSTPGlobal2[iTag].T5.style.display = '';
		document.formSTPGlobal2[iTag].T5.select();
		document.formSTPGlobal2[iTag].T6.style.display = '';
		document.formSTPGlobal2[iTag].T6.select();
		document.formSTPGlobal2[iTag].editButton.style.display = 'none';
		document.formSTPGlobal2[iTag].editApply.style.display = '';
		document.formSTPGlobal2[iTag].vlan_id.focus();
		document.formSTPGlobal2[iTag].vlan_id.select();
	}
	function EditApplyIt(iTag)
	{
		if(!apply_check(document.formSTPGlobal2[iTag]))
		{
			return false;
		}
		document.formSTPGlobal2[iTag].operation.value='3';
		document.formSTPGlobal2[iTag].submit();
		document.formSTPGlobal2[iTag].T4_show.style.display = '';
		document.formSTPGlobal2[iTag].T4.style.display = 'none';
		document.formSTPGlobal2[iTag].T5.style.display = 'none';
		document.formSTPGlobal2[iTag].T6.style.display = 'none';
		document.formSTPGlobal2[iTag].editButton.style.display = '';
		document.formSTPGlobal2[iTag].editApply.style.display = 'none'; 

		return true;
	}
	function AddIt(s)
	{
		if(!apply_check(s))
		{
			return false;
		}
	
		if(!checkVID(s.vlan_id))
		{
			return false;
		}
		s.operation.value='2';
		s.submit();
	}
	function DelAll(s)
	{
		s.operation.value='4';
		s.submit();
	}
   	function DelIt(iTag)
	{
		document.formSTPGlobal2[iTag].operation.value='1';
		document.formSTPGlobal2[iTag].submit();
		return true;
	}
	function apply_check(s)
	{	

		if(!checkTxtInput(s.T1))
			return false;
		if(!checkTxt(s.T5))
			return false;
		if(s.T5.value!=s.T6.value)
		{
		  	 alert('Password is inconsistent');
		 	return false;
		}

		if(!checkVID(s.vlan_id))
		{
			return false;
		}
		
		return true;
		
	}

	function checkVID(obj)
	{
		/* check vid */
		if(obj.value=="")
		{

		}
		else
		{
			if(isNaN(obj.value) || obj.value.search(" ") != -1 || obj.value<1 || obj.value>4094)
			{
				alert("Invalid VID !");
				return false;
			}
		}

		return true;
	}
	
	function checkTxt(obj)
	{
		var i, ch;
		var len = obj.value.length;
		
		if ( obj.value.search(" ") != -1)
	       {
	          	alert("Input Error!");
			obj.focus();
			obj.select();
			return false; 
	       }
		
		for(i = 0; i < len; i++)
		{
			ch = obj.value.charCodeAt(i);
			if(ch > 0x7e || ch < 0x20)
			{	
				alert("Input Error! Unknown characters detected !");
				obj.focus();
				obj.select();
				return false;
			}	  
		}
		return true;
	}
	function Init()
	{
		for(i=0; i<JWAC_User_Settings.length; i++)
		{
			document.formSTPGlobal2[i].T1.value = JWAC_User_Settings[i][0];
			
			if(JWAC_User_VlanStat==1)
			{
				if(JWAC_User_Settings[i][1] == 0)
				{
					document.formSTPGlobal2[i].vlanID_show.value = '-';
				}
				else
				{
					document.formSTPGlobal2[i].vlanID_show.value = JWAC_User_Settings[i][1];
				}
				document.formSTPGlobal2[i].T4_show.value = JWAC_User_Settings[i][2];
			}
			else
			{
				document.formSTPGlobal2[i].T4_show.value = JWAC_User_Settings[i][2];
			}
		}
	}
</script>
</div>
