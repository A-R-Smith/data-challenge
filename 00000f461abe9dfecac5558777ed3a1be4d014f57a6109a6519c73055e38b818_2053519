<script language="Javascript"> 
//var press=0;
function OnPressNext()
{	
	if(ipress==1)
	{
		ipress = 11;
	}
	else
	{
		ipress = 1;
	}

	var reUrl="47_MBA_AuthState_Table.html?1&"+iport+"&"+ipress;
	window.open(reUrl,"_self","",true);
	//location.replace(reUrl);
	//document.location.href=reUrl;
}
function OnPressPre()
{
	if(ipress==2)
	{
		ipress = 22;
	}
	else
	{
		ipress =2;
	}

	var reUrl="47_MBA_AuthState_Table.html?1&"+iport+"&"+ipress;
	window.open(reUrl,"_self","",true);
	//location.replace(reUrl);
	//document.location.href=reUrl;
}

function ShowBtn(ret)
{
	/* 0-none 1-prebutton 2-nextbutton 3-both botton */
	switch(ret)
	{
		case 0:
			document.getElementById("PrevButton").disabled = true;
			document.getElementById("NextButton").disabled = true;	
			break;
		case 1:
			document.getElementById("PrevButton").disabled = false;
			document.getElementById("NextButton").disabled = true;
			break;
		case 2:
			document.getElementById("PrevButton").disabled = true;
			document.getElementById("NextButton").disabled = false;
			break;
		default :
			document.getElementById("PrevButton").disabled = false;
			document.getElementById("NextButton").disabled = false;
			break;
	}
}

ShowBtn(ret);

