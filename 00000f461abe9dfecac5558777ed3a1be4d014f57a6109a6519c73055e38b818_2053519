/* ----------------------------------------------------- */
/*	Function:	checkASCIICode()
/*	Purpose:	filter the user input
/*	Input:		user input
/*	Output:		None
/*	Return:		error->false
/* ----------------------------------------------------- */
function checkASCIICode(input)
{
	var ch;
	var len;
	
	len=input.value.length;
	
	for(i=0;i<len;i++)
    {
		ch=input.value.charCodeAt(i);
		if(ch>0x7e || ch<0x20)
		{
			alert("Input error! Detected unknown character!");
			input.focus();
			input.select();
			return false;
		}
	}
	return true;
}
/*for single ip check
//
if(self!=top)
parent.parent.urgy();

//
function openRH(url)
{
  open(url);
}
*/
/* ----------------------------------------------------- */
/*	Function:	TRAN_AtoIP()
/*	Purpose:	transfer ip string to int
/*	Input:		ip string
/*	Output:		int
/*	Return:		error->false
/* ----------------------------------------------------- */
function TRAN_AtoIP(str) 
{
    var		ca_var;
    var  	rtn_value;
    var		_str=new Array(4);
	var		i=0;
	var 	j=0;
   
    rtn_value = 0 ;
	_str[j]=0;
    for (i=0;i<str.length;i++)
    {    	
        ca_var = str.charAt(i) ;	//10.1.1.1

        if ( (ca_var >= '0') && (ca_var <= '9') )
        {
            _str[j] = _str[j] * 10 ;
            _str[j] += (ca_var - '0') ;
        }
        else if (ca_var == '.')
		{
			j++;
			_str[j]=0;
		}
        else
            break ;
        
        
    } /* end of while */

    rtn_value = _str[0]*256*256*256+_str[1]*256*256+_str[2]*256+_str[3];
    
    return rtn_value;
}

function genTabOrder()
{
    var count = genTabOrder.arguments.length/2;
    var total_width=0;
    var tabOrdername = new Array();
    var tabOrderURL = new Array(); 	    
    index = genTabOrder.arguments[0]; 
	  	for (var i=1; i< genTabOrder.arguments.length;i++)
  	{
		tabOrdername[(i-1)/2]=genTabOrder.arguments[i++];
		tabOrderURL[(i-1)/2]=genTabOrder.arguments[i];
	}	
	if(index==0)
	{
	  
	   total_width = count*206+count*2+(count)*3;
	}
	{
	  total_width = count*206+count*3+(count)*2;	
	}  
	document.write("<table width="+total_width+" height=30 border=0 cellpadding=0 cellspacing=0 align='center'>");	
	
	
	for(var i=0; i< (tabOrdername.length);i++)
	{
		if(i==index)
		{
			document.write("<td width=3> <img src='../jstab/TB_Title_light_Left.jpg' width=3 height=30></td>");
			document.write("<td class='style1' width=206 height=30 valign=middle align=center onClick='javascript:Link("+tabOrderURL[i]+")'><font color=#FFFFFF>"+tabOrdername[i]+"</font></td>");
			document.write("<td width=2> <img src='../jstab/TB_Title_light_Right.jpg' width=2 height=30></td>");
		}	
		else
		{	
			document.write("<td width=3> <img src='../jstab/TB_Title_dark_Left.jpg' width=3 height=30></td>");
			document.write("<td class='style2' width=133 height=57 valign=middle align=center onClick='javascript:Link("+tabOrderURL[i]+")'><font color=#FFFFFF>"+tabOrdername[i]+"</font></td>");
			document.write("<td width=2> <img src='../jstab/TB_Title_dark_Right.jpg' width=2 height=309></td>");
		}	
	}
	document.write("</tr></table>");
	
}

function genTabHeaderTable(titleName){
    //generate Logout table
    
//    document.write("<table name=tabLogout id=tabLogout class=tabLogout2 ID='tabLogout' onMouseOver='showThis(this);' onMouseDown='closeWin();' onMouseOut='hiddenThis(this);' cellspacing=0>");
//    document.write("    <tr Height=35><td>&nbsp;&nbsp;The current browser connection will<br>");
//    document.write("            &nbsp;&nbsp;be disconnected if you click here.");
//    document.write("</table>");

    //generate back frame table
    document.write("<table name=tabBigFrame id=tabBigFrame class=tabBigFrame2><tr><td>&nbsp;</td></tr></table>"); 
    
    //generate main table frame and big title name 
    document.write("<div name=tabBigTitle id=tabBigTitle class=tabBigTitle2><table>");
    document.write("    <tr><div class=imgBigTitle><td valign=top noWrap>&nbsp;&nbsp;&nbsp;<font class=tdBigTitle>" + titleName + "</font>");
    if(window.screen.width == 1280){
        if(SafeguardEngine_Enable == '1')
            document.write("    <img name=imgBigTitle id=imgBigTitle class=imgBigTitle src='../Icon/lightblue_on_1280.jpg'></td></div></tr>");
        else
            document.write("    <img name=imgBigTitle id=imgBigTitle class=imgBigTitle src='../Icon/lightblue_off_1280.jpg'></td></div></tr>");
    }
    else{
        if(SafeguardEngine_Enable == '1')
            document.write("    <img name=imgBigTitle id=imgBigTitle class=imgBigTitle src='../Icon/lightblue_on.jpg'></td></div></tr>");
        else
            document.write("    <img name=imgBigTitle id=imgBigTitle class=imgBigTitle src='../Icon/lightblue_off.jpg'></td></div></tr>");
    }
    document.write("</table></div>");
}

function Link(page)
{
	location.href="../iss/"+page+"";
}

function changeTabForm1280(inType){
    if(window.screen.width == 1280){
        document.getElementById('tabBigFrame').style.width = 988;
        document.getElementById('tabBigFrame').style.height = 684;
        document.getElementById('tabBigTitle').style.width = 956;
        document.getElementById('tabBigTitle').style.height = 660;
        document.getElementById('imgBigTitle').style.width = 956;
        
        if(inType == 'device'){
            document.getElementById('tabDeviceInfo').style.width = 918;
        }
        else if(inType == 'reset'){
            document.getElementById('tabResetContent').style.width = 918;
        }        
        else if(inType == 'sys'){
            document.getElementById('tabSecTitleIPInfo').style.width = 926;
            document.getElementById('tabIPInfoApply').style.width = 926;
            imgSecTitle();
            document.getElementById('img3thTitle').style.width = 926;
            document.getElementById('tabSecTitleSysInfo').style.width = 926;
            document.getElementById('tabDeviceInfoApply').style.width = 926;
        }        
        else if(inType == 'trap'){
            document.getElementById('tabPortContent').style.width = 918;
            bottomApply();
        }        
        else if(inType == 'port'){
            document.getElementById('tabPortContent').style.width = 918;
            document.getElementById('ifrPortConfig').style.width = 918;
            document.getElementById('ifrPortConfig').style.height = 556;
        }        
