<script language="Javascript">
	  	RunJava();
		
		function RunJava()
		{

			var UnitNum = 1;
			var portNum=parent.iport ;

			document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=2600 HEIGHT=750>");
			document.write("<param NAME=DataPath value=\"../../html/Hjava_AuthDiagnostics_MacBase.html?" +portNum + "\">" );
			document.write("<param NAME=AppletName value=\"Show Authenticator Diagnostics of Port "+portNum+"\">");
			document.write("<param NAME=TimeIntervalName value=\"Time Interval\">");
			document.write("<param NAME=PortNum value=",portNum,">");
			document.write("<param NAME=SlotNum value=",UnitNum,">");    
			document.write("<param NAME=MajorTable value=1>");
			document.write("<param NAME=TableNum value=1>");
			document.write("<param NAME=Table0RowNum value=17>");
			document.write("<param NAME=Table0Row0 value=Index>");
			document.write("<param NAME=Table0Row1 value=1>");
			document.write("<param NAME=Table0Row2 value=2>");
			document.write("<param NAME=Table0Row3 value=3>");
			document.write("<param NAME=Table0Row4 value=4>");
			document.write("<param NAME=Table0Row5 value=5>");
			document.write("<param NAME=Table0Row6 value=6>");
			document.write("<param NAME=Table0Row7 value=7>");
			document.write("<param NAME=Table0Row8 value=8>");
			document.write("<param NAME=Table0Row9 value=9>");
			document.write("<param NAME=Table0Row10 value=10>");
			document.write("<param NAME=Table0Row11 value=11>");
			document.write("<param NAME=Table0Row12 value=12>");
			document.write("<param NAME=Table0Row13 value=13>");
			document.write("<param NAME=Table0Row14 value=14>");
			document.write("<param NAME=Table0Row15 value=15>");
			document.write("<param NAME=Table0Row16 value=16>");    
			document.write("<param NAME=ColumNum value=19>");
			document.write("<param NAME=Colum0 value=&nbsp;>");
			document.write("<param NAME=Colum1 value=\"Connect Enter\">");
			document.write("<param NAME=Colum2 value=\"Connect LogOff\">");
			document.write("<param NAME=Colum3 value=\"Auth Enter\">");
			document.write("<param NAME=Colum4 value=\"Auth Success\">");
			document.write("<param NAME=Colum5 value=\"Auth Timeout\">");
			document.write("<param NAME=Colum6 value=\"Auth Fail\">");
			document.write("<param NAME=Colum7 value=\"Auth Reauth\">");
			document.write("<param NAME=Colum8 value=\"Auth Start\">");
			document.write("<param NAME=Colum9 value=\"Auth LogOff\">");
