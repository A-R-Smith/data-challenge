<script>
function apply_form()
{
	var f = document.app_set;
	f.HL0.value = Login_Method_Lists[f.L0.value][0];
	f.HL1.value = Login_Method_Lists[f.L1.value][0];
	f.HL2.value = Login_Method_Lists[f.L2.value][0];
	f.HL3.value = Login_Method_Lists[f.L3.value][0];

	f.HE0.value = Enable_Method_Lists[f.E0.value][0];
	f.HE1.value = Enable_Method_Lists[f.E1.value][0];
	f.HE2.value = Enable_Method_Lists[f.E2.value][0];
	f.HE3.value = Enable_Method_Lists[f.E3.value][0];

	f.submit();
}
</script>
