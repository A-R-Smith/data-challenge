          <td width=8% class=bh>VLAN ID</td>
          <td width=19% class=bh>VLAN Name</td>
          <td width=15% class=bh>Multicast Group</td>
          <td width=20% class=bh>Multicast MAC address</td>
          <td width=30% class=bh>Port Members</td>
        </tr>
    <script language="Javascript">
    for(i=0; i<IGMP_Multicast_Entry.length; i++){
        document.write("<tr><td class=bw>" + IGMP_Multicast_Entry[i][0] + "</td>");
        document.write("    <td class=bw>" + IGMP_Multicast_Entry[i][1] + "</td>");
        document.write("    <td class=bw>" + IGMP_Multicast_Entry[i][2] + "</td>");
        document.write("    <td class=bw>" + IGMP_Multicast_Entry[i][3] + "</td>");
        document.write("    <td class=bw>" + IGMP_Multicast_Entry[i][4] + "</td>");
        document.write("    <td class=bw>" + getPortNumber_withComma(IGMP_Multicast_Entry[i][5], '1') + "</td>");
    }
    </script>
