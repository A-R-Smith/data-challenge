	<script language="Javascript">
		function formCheck(s)
		{

		    if(!checkNumberInput(s.Session,1,8))
		    {  
		    	return false;
		    }
		    
		    if(!checkNumberInput(s.timeout,120,600))
		    {  
		    	return false;
		    }
		    
		    if(!checkNumberInput(s.failTime,2,20))
		    {  
		    	return false;
		    }
			s.submit();
		    return true;
		}
	</script>
