    <script language="Javascript">
    		function NumToStrState(value)
    		{
    			if(value == '0')
				return "Disabled";
			else if(value == '1')
				return "Enabled (Strict)";
			else if(value =='2')
				return "Enabled (Loose)";
    		}

	     	function numToStr(value)
	     	{
	     		if(value=='0')
				return "No Limit";
			else
				return value;
	     	}
		</script>
        <style type="text/css">
		<!--
		/*============================table head=========================================*/
