    var trClass = '';
    var txtClass = '';
    document.write("<tbody class='scrollContent'>");  
    for(i=0; i<IP_MAC_Blocked.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGW';
        }
        
        document.write("<tr class=" + trClass + ">");
        document.write("<td width=10%>" +IP_MAC_Blocked[i][0]+"</td>");

	document.write("<td width=30% align=left  onMouseover=keytip(unescape('" + strNormalize(IP_MAC_Blocked[i][1]) +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(IP_MAC_Blocked[i][1], 32)) + "</td>");
		
	document.write("<td width=20%>" +IP_MAC_Blocked[i][2]+"</td>");
	document.write("<td width=5%>" +IP_MAC_Blocked[i][3]+"</td>");
	//document.write("<td>" +IP_MAC_Blocked[i][4]+"</td>");
	document.write("  <td width=35% align=right><input type=button value='Delete' class=btnInGrayS onClick=delit(" + i + ");></td>");
	
	document.write("</tr>");
    }
    
</script>
