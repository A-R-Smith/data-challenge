<script language="Javascript">
function intial()
{
	if (parent.is_apply)
	{
		parent.is_apply = false;
		//parent.ifrACLScope.page_refresh();
		//ResetAll();
		parent.location.href = "52_AccessProfile.html?" + acl_style;
	}

}
intial();


function ShowPacketComponet(item)
{
	MM_findObj(item).style.visibility='visible';
}

function HidePacketComponet(item)
{  
  MM_findObj(item).style.visibility='hidden';
  MM_findObj(item).style.top=-1;	
}

function page_refresh()
{
	self.location.reload();	
}

function ResetAll()
{
	var i;
	for(i=1; i<17;i++)
	//for(i=19; i>0;i--)
	{
		if(i == 14)
			continue;
		
		var obj='Scope'+i;
		MM_findObj(obj).style.visibility='hidden';
		MM_findObj(obj).style.top=-1;
		//alert(i);
	}
}

function RelocateCom()
{
	var top=0;
	for(var i=0;i<parent.Com_List.length;i++)
	{
		if(parent.Com_List[i].select==1)
		{		 	
		 	var obj=MM_findObj(parent.Com_List[i].id);
		 	
		 	obj.style.top=top;
		 	top+=parent.Com_List[i].height;
		}
	}
}

function ethernet_apply(f)
{
	if(document.getElementById('Scope1').style.visibility == 'visible')
	{
		var str=document.getElementsByName('textSrcMacMask')[0].value;
		var str1=document.getElementsByName('textDstMacMask')[0].value;

	//MAC Address
		if (f.chkSrcMacMask.checked)
		{
			f.s_mac_flag.value = '1';
			if(!MAC_Address_Check(document.getElementsByName('textSrcMacMask')[0]))
				return false;
		}
		else
		{
			f.s_mac_flag.value = '0';
		}

		if (f.chkDstMacMask.checked)
		{
			f.d_mac_flag.value = '1';
			if(!MAC_Address_Check(document.getElementsByName('textDstMacMask')[0]))
				return false;
		}
		else
		{
			f.d_mac_flag.value = '0';
		}
		
	}
	else
	{
		f.s_mac_flag.value = '0';
		f.d_mac_flag.value = '0';
	}

	if(document.getElementById('Scope2').style.visibility == 'visible')
	{
	//VLAN
		if (f.chkvid.checked)
			f.vlan_flag.value = '1';
		else
			f.vlan_flag.value = '0';
	}
	else
	{
		f.vlan_flag.value = '0';
	}

	if(document.getElementById('Scope3').style.visibility == 'visible')
	{
		//802.1p
		if (f.chk802p.checked)
			f.dot1p_flag.value = '1';
		else
			f.dot1p_flag.value = '0';
	}
	else
	{
		f.dot1p_flag.value = '0';
	}

	if(document.getElementById('Scope4').style.visibility == 'visible')
	{
	//Ethernet Type
		if (f.EtherTypeChk.checked)
			f.ether_flag.value = '1';
		else
			f.ether_flag.value = '0';
	}
	else
	{
		f.ether_flag.value = '0';
	}

	return true;
}

function ip_apply(f)
{
	if(document.getElementById('Scope2').style.visibility == 'visible')
	{
	//VLAN
		if (f.chkvid.checked)
			f.vlan_flag.value = '1';
		else
			f.vlan_flag.value = '0';
	}
	else
	{
		f.vlan_flag.value = '0';
	}

//DSCP
	if(document.getElementById('Scope5').style.visibility == 'visible')
	{
		f.dscp_flag.value = f.iptype.checked ? '1' : '0';
	}
	else
	{
		f.dscp_flag.value = '0';
	}
//IPv4 Address
	if(document.getElementById('Scope6').style.visibility == 'visible')
	{
		var str2=document.getElementsByName('textSrcIPMask')[0].value;
		var str3=document.getElementsByName('textDstIPMask')[0].value;
		if (f.chkSrcIPMask.checked == true)
		{
			f.s_ip_flag.value = '1';
			if(!ACLIPMaskCheck(document.getElementsByName('textSrcIPMask')[0]))
				return false;
		}
		else
			f.s_ip_flag.value = '0';

		if (f.chkDstIPMask.checked == true)
		{
			f.d_ip_flag.value = '1';
			if(!ACLIPMaskCheck(document.getElementsByName('textDstIPMask')[0]))
				return false;
		}
		else
			f.d_ip_flag.value = '0';
		
	}
	else
	{
		f.s_ip_flag.value = '0';
		f.d_ip_flag.value = '0';
	}
//ICMP
	if(document.getElementById('Scope10').style.visibility == 'visible')
	{
		f.icmp_flag.value = f.icmpChk.checked ? '1' : '0';
		//if (acl_style == 2)
		{
			f.icmp_code_flag.value = f.icmp_CodeChk.checked ? '1' : '0';
			f.icmp_type_flag.value = f.icmp_TypeChk.checked ? '1' : '0';
		}	
	}
	else
	{
		f.icmp_flag.value = '0';
		//if (acl_style == 2)
		{
			f.icmp_code_flag.value = '0';
			f.icmp_type_flag.value = '0';
		}	
	}
//IGMP
	if(document.getElementById('Scope12').style.visibility == 'visible')
	{
		f.igmp_flag.value = f.IGMPChk.checked ? '1' : '0';
		//if (acl_style == 2)
			f.igmp_type_flag.value = f.IGMPTypeChk.checked ? '1' : '0';
	}
	else
	{
		f.igmp_flag.value = '0';
		//if (acl_style == 2)
			f.igmp_type_flag.value = '0';
	}
		
//TCP
	if(document.getElementById('Scope13').style.visibility == 'visible')
	{	
		f.h_tcp_flag.value = f.tcp_flag.checked ? '1' : '0';

	//TCP Sorce Port
		if (f.chkSrcTCPMask.checked == true)
		{
			if (!checkHexInput(f.textSrcTCPMask))
				return false;
			f.tcp_s_port_flag.value = '1';
		}
		else
			f.tcp_s_port_flag.value = '0';

	//TCP Destination Port
		if (f.chkDstTCPMask.checked == true)
		{
			if (!checkHexInput(f.textDstTCPMask))
				return false;
			f.tcp_d_port_flag.value = '1';
		}
		else
			f.tcp_d_port_flag.value = '0';

	//TCP Flag Bits
		if (f.TCPFlagChk.checked)
		{
			f.flag_bit_flag.value = '1';
			
			f.urg_flag.value = f.URG_Chk.checked ? '1' : '0';
			f.ack_flag.value = f.ACK_Chk.checked ? '1' : '0';
			f.psh_flag.value = f.PSH_Chk.checked ? '1' : '0';
			f.rst_flag.value = f.RST_Chk.checked ? '1' : '0';
			f.syn_flag.value = f.SYN_Chk.checked ? '1' : '0';
			f.fin_flag.value = f.FIN_Chk.checked ? '1' : '0';
		}
		else
		{
			f.flag_bit_flag.value = '0';
			f.urg_flag.value = '0';
			f.ack_flag.value = '0';
			f.psh_flag.value = '0';
			f.rst_flag.value = '0';
			f.syn_flag.value = '0';
			f.fin_flag.value = '0';
		}
	}
	else
	{
		f.h_tcp_flag.value = '0';
		f.tcp_s_port_flag.value = '0';
		f.tcp_d_port_flag.value = '0';
		f.flag_bit_flag.value = '0';
	}
//UDP Port
	if(document.getElementById('Scope15').style.visibility == 'visible')
	{
		f.h_udp_flag.value = f.udp_flag.checked ? '1' : '0';
		if (f.chkSrcUDPMask.checked == true)
		{
			if (!checkHexInput(f.textSrcUDPMask))
				return false;
			f.udp_s_port_flag.value = '1';
		}
		else
			f.udp_s_port_flag.value = '0';

		if (f.chkDstUDPMask.checked == true)
		{
			if (!checkHexInput(f.textDstUDPMask))
				return false;
			f.udp_d_port_flag.value = '1';
		}
		else
			f.udp_d_port_flag.value = '0';
	}
	else
	{
		f.h_udp_flag.value = '0';
		f.udp_s_port_flag.value = '0';
		f.udp_d_port_flag.value = '0';
	}

/*
//TCP Flag Bit
	if(document.getElementById('Scope14').style.visibility == 'visible')
	{
		f.h_tcp_flag.value = '1';
		if (f.TCPFlagChk.checked)
		{
			f.flag_bit_flag.value = '1';
		}
		else
		{
			f.flag_bit_flag.value = '0';
		}
		f.urg_flag.value = f.URG_Chk.checked ? '1' : '0';
		f.ack_flag.value = f.ACK_Chk.checked ? '1' : '0';
		f.psh_flag.value = f.PSH_Chk.checked ? '1' : '0';
		f.rst_flag.value = f.RST_Chk.checked ? '1' : '0';
		f.syn_flag.value = f.SYN_Chk.checked ? '1' : '0';
		f.fin_flag.value = f.FIN_Chk.checked ? '1' : '0';
		
	}
	else
	{
		f.h_tcp_flag.value = '0';
		f.flag_bit_flag.value = '0';
		f.urg_flag.value = '0';
		f.ack_flag.value = '0';
		f.psh_flag.value = '0';
		f.rst_flag.value = '0';
		f.syn_flag.value = '0';
		f.fin_flag.value = '0';
	}
*/
//Protocol ID
	if(document.getElementById('Scope16').style.visibility == 'visible')
	{
		f.pro_id_flag.value = f.proto_flag.checked ? '1' : '0';
		if (f.proto_flag.checked)
		{
			if (!checkHexInput(f.protocol_id))
				return;

			if (f.chk_user_mask_flag.checked)
			{
				if (!checkHexInput(f.user_mask))
					return;

				f.user_mask_flag.value = '1';
			}
			else
				f.user_mask_flag.value = '0';
		}
	}
	else
	{
		f.pro_id_flag.value = '0';
		f.user_mask_flag.value = '0';
	}
	
	return true;
}

function IPv6_apply(f)
{
//IPv6 Class
	if(document.getElementById('Scope7').style.visibility == 'visible')
	{
		f.class_flag.value = f.IPv6_ClassChk.checked ? '1' : '0';
	}
	else
		f.class_flag.value = '0';
//IPv6 Flow Label
	if(document.getElementById('Scope8').style.visibility == 'visible')
	{
		f.flow_flag.value = f.IPv6_FlowChk.checked ? '1' : '0';
	}
	else
		f.flow_flag.value = '0';
//IPv6 Address
	if(document.getElementById('Scope9').style.visibility == 'visible')
	{
		if (f.IPv6_S_IPChk.checked)
		{
			if (!ipv6_check(f.textSrcIPv6Mask))
				return false;

			f.s_ip6_flag.value = '1';
		}
		else
			f.s_ip6_flag.value = '0';

		if (f.IPv6_D_IPChk.checked)
		{
			if (!ipv6_check(f.textDstIPv6Mask))
				return false;

			f.d_ip6_flag.value = '1';
		}
		else
			f.d_ip6_flag.value = '0';
			
		//f.s_ip6_flag.value = f.IPv6_S_IPChk.checked ? '1' : '0';
		//f.d_ip6_flag.value = f.IPv6_D_IPChk.checked ? '1' : '0';
	}
	else
	{
		f.s_ip6_flag.value = '0';
		f.d_ip6_flag.value = '0';
	}

	return true;
}

function packet_content_apply(f, style)
{
	if (style == 1)
	{
		if(document.getElementById('Scope11').style.visibility == 'visible')
		{
			f.chunk1_flag.value = f.E1.checked ? '1' : '0';
			f.chunk2_flag.value = f.E4.checked ? '1' : '0';
			f.chunk3_flag.value = f.E7.checked ? '1' : '0';
			f.chunk4_flag.value = f.E10.checked ? '1' : '0';

			if (f.E1.checked == true)
			{
				if (!checkNumberInput(f.E2, 0, 31))
					return false;
				if (!checkHexInput(f.E3))
					return false;	
			}

			if (f.E4.checked == true)
			{
				if (!checkNumberInput(f.E5, 0, 31))
					return false;
				if (!checkHexInput(f.E6))
					return false;	
			}

			if (f.E7.checked == true)
			{
				if (!checkNumberInput(f.E8, 0, 31))
					return false;
				if (!checkHexInput(f.E9))
					return false;	
			}

			if (f.E10.checked == true)
			{
				if (!checkNumberInput(f.E11, 0, 31))
					return false;
				if (!checkHexInput(f.E12))
					return false;	
			}
			
		}
		else
		{
			f.chunk1_flag.value = '0';
			f.chunk2_flag.value = '0';
			f.chunk3_flag.value = '0';
			f.chunk4_flag.value = '0';
		}
	}
	else
	{
		if(document.getElementById('Scope11').style.visibility == 'visible')
		{
			f.offset1_flag.value = f.C1.checked ? '1' : '0';
			f.offset2_flag.value = f.C6.checked ? '1' : '0';
			f.offset3_flag.value = f.C11.checked ? '1' : '0';
			f.offset4_flag.value = f.C16.checked ? '1' : '0';
			f.offset5_flag.value = f.C21.checked ? '1' : '0';

			if (f.C1.checked == true)
			{
				if (!checkHexInput(f.C2))
					return false;	
				if (!checkHexInput(f.C3))
					return false;	
				if (!checkHexInput(f.C4))
					return false;	
				if (!checkHexInput(f.C5))
					return false;	
			}

			if (f.C6.checked == true)
			{
				if (!checkHexInput(f.C7))
					return false;
				if (!checkHexInput(f.C8))
					return false;
				if (!checkHexInput(f.C9))
					return false;
				if (!checkHexInput(f.C10))
					return false;
			}

			if (f.C11.checked == true)
			{
				if (!checkHexInput(f.C12))
					return false;
				if (!checkHexInput(f.C13))
					return false;
				if (!checkHexInput(f.C14))
					return false;
				if (!checkHexInput(f.C15))
					return false;
			}

			if (f.C16.checked == true)
			{
				if (!checkHexInput(f.C17))
					return false;
				if (!checkHexInput(f.C18))
					return false;
				if (!checkHexInput(f.C19))
					return false;
				if (!checkHexInput(f.C20))
					return false;
			}

			if (f.C21.checked == true)
			{
				if (!checkHexInput(f.C22))
					return false;
				if (!checkHexInput(f.C23))
					return false;
				if (!checkHexInput(f.C24))
					return false;
				if (!checkHexInput(f.C25))
					return false;
			}
		}
		else
		{
			f.offset1_flag.value = '0';
			f.offset2_flag.value = '0';
			f.offset3_flag.value = '0';
			f.offset4_flag.value = '0';
			f.offset5_flag.value = '0';
		}
	}

	return true;
}

function apply()
{
	var f = document.formTimeProfile;
	var i, found = false, elm;

	for(i = 0; i < f.length; i++)
	{
		elm = f.elements[i];
		if(elm.type=="checkbox")
		{
			if(elm.checked)
			{
				found = true;
				break;
			}
		}
	}

	if (!found)
	{
		alert("Please select at least one mask.");
		return;
	}

	f.profile_id.value = parent.document.form1.pid.value;
	f.profile_type.value = parent.document.form1.profile_type.value;

	if (f.profile_type.value == 0)
	{
		if (ethernet_apply(f))
		{
			parent.is_apply = true;
			f.submit();
		}
	}
	else if (f.profile_type.value == 1)
	{
		if(ip_apply(f))
		{
			parent.is_apply = true;
			f.submit();
		}
	}
	else if (f.profile_type.value == 2)
	{
		if (!IPv6_apply(f))
			return;
		
		parent.is_apply = true;
		f.submit();
	}
	else
	{
		if (!packet_content_apply(f, acl_style))
			return;

		parent.is_apply = true;
		f.submit();
	}
	//parent.ifrACLScope.page_refresh();
	//ResetAll();

/*
	var str=document.getElementsByName('textSrcMacMask')[0].value;
	var str1=document.getElementsByName('textDstMacMask')[0].value;
	var str2=document.getElementsByName('textSrcIPMask')[0].value;
	var str3=document.getElementsByName('textDstIPMask')[0].value;

	if(!document.getElementById('textSrcMacMask').disabled)
	{
		if(!MAC_Address_Check(document.getElementsByName('textSrcMacMask')[0], str))
			return;
	}

	if(!document.getElementById('textDstMacMask').disabled)
	{
		if(!MAC_Address_Check(document.getElementsByName('textDstMacMask')[0], str1))
			return;
	}

	if(!document.getElementById('textSrcIPMask').disabled)
	{
		if(!ipCheck(document.getElementsByName('textSrcIPMask')[0], str2))
			return;
	}

	if(!document.getElementById('textDstIPMask').disabled)
	{
		if(!ipCheck(document.getElementsByName('textDstMacMask')[0], str3))
			return;
	}

	
	
	else
	{
		if(document.getElementById('Scope4').style.visibility == 'visible')
		{
		//alert("visible");
			document.getElementById('EtherTypeChk').disabled=false;
		}
		if(document.getElementById('Scope5').style.visibility == 'visible')
		{
		//alert("visible");
			document.getElementById('iptype').disabled=false;
		}
		if(document.getElementById('Scope12').style.visibility == 'visible')
		{
		//alert("visible");
			document.getElementById('IGMPTypeChk').disabled=false;
		}
		if(document.getElementById('Scope14').style.visibility == 'visible')
		{
		//alert("visible");
			document.getElementById('TCPFlagChk').disabled=false;
		}
		f.submit();
		alert("An Access Profile has been created with Profile ID:"+newprofileid+"!!!!!");
		//parent.location.href='52_AccessProfile.html';
		parent.ifrACLScope.page_refresh();
		ResetAll();
	}
*/
}
  
</script>

