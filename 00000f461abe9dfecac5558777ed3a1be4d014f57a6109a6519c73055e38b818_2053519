<script language="javascript">
	function agingTime(s)
	{
		var str;
		if(s==0)
		{
			str="-";
		}
		else if(s==1)
		{
			str="Infinite";
		}
		else
		{
			str=s;
		}
		return str;
	}
	
	function vid(s)
	{
		var str;
		if(s==0)
		{
			str="-";
		}
		else
		{
			str=s;
		}
		return str;
	}	
</script>
<script language="javascript">
	document.write("<tr class=gh>");
