		parent.timer_reset=1;
		
		function onPageLoad()
		{
			if("Disabled" == DHCPLocalRelay[0])
			{
				document.getElementById("radEnable").checked = true;
				document.getElementById("radDisable").checked = false;
			}
			else
			{
				document.getElementById("radEnable").checked = false;
				document.getElementById("radDisable").checked = true;
			}
		}
		
		function cfgDhcpRelay_G()
		{
			document.getElementById("frmDhcpRelay_G").submit();
		}
		
		function cfgDhcpRelay()
		{
			if( !checkTxtInput(document.getElementById("boxVlan")) )
			{
				return false;
			}
			document.getElementById("frmDhcpRelay").submit();
		}
