}

function ipCheck(textValue)
  {
      if (textValue.indexOf(',') != -1)    return false;//iris chu 04443 modify 2004/11/19 02:22U
  
    re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
    Check=textValue.search(re1);
    if(Check==-1)   return false;

    else
    {

       
       var ipSplit=textValue.split('.');         if(ipSplit.length!=4)
       		return false;
		for(i=0; i<ipSplit.length; i++)
		{
			if(isNaN(ipSplit[i]))  return false;
		}
       	if(eval(ipSplit[0])+eval(ipSplit[1])+eval(ipSplit[2])+eval(ipSplit[3])==0)         		return false;

       	if(eval(ipSplit[0])+eval(ipSplit[1])+eval(ipSplit[2])+eval(ipSplit[3])==1020)         		return false;

       for(i=0; i<ipSplit.length; i++)
       {
         if(ipSplit[i]>255 || ipSplit[i]<0)  return false;
       }

       return true;

    }
 }


function ipCheck2(textValue)
{
	re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
	Check=textValue.search(re1);
	if(Check==-1)   return false;

	else
	{
       var ipSplit=textValue.split('.');  		
		if(ipSplit.length!=4) return false;

		for(i=0; i<ipSplit.length; i++)
		{
			if(isNaN(ipSplit[i]))  return false;
			if(ipSplit[i] > 255)  return false;
			if(ipSplit[i] < 0 )  return false;
		}

		if((ipSplit[0]==255) && (ipSplit[1]==255) && (ipSplit[2]==255)
			&& (ipSplit[3]==255)) return false;
		if(ipSplit[3]==255) return false;
		if(ipSplit[3]==0) return false;
		
		return true;
    }
 }



function gatwayCheck(textValue)
{
	re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
	Check=textValue.search(re1);
	if(Check==-1)   return false;

	else
	{
		//re2=/(\W)/
		var ipSplit=textValue.split('.');  		if(ipSplit.length!=4)
			return false;
		for(i=0; i<ipSplit.length; i++)
		{
			if(isNaN(ipSplit[i]))  return false;
		}

		if(eval(ipSplit[0])+eval(ipSplit[1])+eval(ipSplit[2])+eval(ipSplit[3])==1020)  			return false;

		for(i=0; i<ipSplit.length; i++)
		{
			if(ipSplit[i]>=255 || ipSplit[i]<0)  return false;
		}

		return true;
    }
}

function hostAddrCheck(textValue)
{
	var re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
	
	var check=textValue.search(re1);
	
	if(check == -1)
		return false;
	else
	{
		//var re2=/(\W)/

		var ipString=textValue.split('.');
		if(ipString.length!=4)
			return false;
		/* 255.255.255.255 */
		if(eval(ipString[0])+eval(ipString[1])+eval(ipString[2])+eval(ipString[3])==1020)
			return false;
		/* 127.0.0.0 - 127.255.255.255 */
		else if(ipString[0] == 127)
			return false;
		else
		{
			/* over Class-C is not allowed! */
			if(ipString[0] > 223)
				return false;
		}
		return true;
    }
}


 function ipCheck3(textValue)
  {

    re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
    Check=textValue.search(re1);
    if(Check==-1)   return false;

    else
    {
       var ipSplit=textValue.split('.');  
       ipSplit=textValue.split('.');
       if(ipSplit.length!=4) return false;
       for(i=0; i<ipSplit.length; i++)
       {
	     if(isNaN(ipSplit[i]))  return false;
         if(ipSplit[i]>=255)  return false;
       }

       if(ipSplit[0]==127) return false;         if(ipSplit[0]>=224 && ipSplit[0]<=255) return false; 
       if(ipSplit[0]==128 && ipSplit[1]==0) return false;          if(ipSplit[0]==191 && ipSplit[1]==255) return false;  

       if(ipSplit[3]==255 || ipSplit[3]==0)
        {
           if(ipSplit[0]>=192 && ipSplit[0]<=255) return false;  
           if(ipSplit[2]==255 || ipSplit[2]==0)
           {
           	  if(ipSplit[0]>=128 && ipSplit[0]<=191) return false; 
           	  if(ipSplit[1]==255 || ipSplit[1]==0)
           	  if(ipSplit[0]>=64 && ipSplit[0]<=127) return false;            }

        }

       return true;

    }
 }


 function ipCheckZero(textValue)
  {

    if (textValue.indexOf(',') != -1)     return false;	
    re1=/(\d+)(\W)(\d+)(\W)(\d+)(\W)(\d+)/
    Check=textValue.search(re1);

    if(Check==-1)
       return false;
    else
    {
       var ipSplit=textValue.split('.');  
       if(ipSplit.length!=4) return false;

       for(i=0; i<ipSplit.length; i++)
       {
		 if(isNaN(ipSplit[i]))  return false;
         if(ipSplit[i]<=0)  return false;
       }

       return true;

    }
 }


 function MaskCheck(textValue)
 {
    var Len= textValue.length
    var  flag=1;
   
    for(var i=0; i<Len; i++)
    {
      if(textValue.charAt(i)=='.')  flag=0;

    }

   if (flag==0) return  false;


        re2=/[^A-Fa-f0-9]/

        Check=textValue.search(re2);

     	  if(Check==-1)  return true;
     	  else return  false;

}


 function macCheck(textValue)
 {
   re1=/(-)|(:)|(\s)/
   Check=textValue.search(re1);
   if(Check==-1)
   	return false;
   else{

//         macSplit=textValue.split(re1);
//         if(macSplit.length!=6)
//         {
           macSplit=textValue.split('-');
           if(macSplit.length!=6)
           {
             macSplit=textValue.split(':');
             if(macSplit.length!=6)
             {
               macSplit=textValue.split(' ');
               if(macSplit.length!=6)
                return false;
             }
//           }
        }

         for(i=0; i< macSplit.length; i++)
  		 {
    	  re2=/[A-Fa-f0-9][A-Fa-f0-9]/
     	  Check=macSplit[i].search(re2);
     	  if(Check==-1)  return false;
 		 }

   }
 return true;
}


 function macMcastCheck(textValue)
 {
    var counter=0;
    var ZeroCounter=0;
    for(var i=0; i<12+5;i++)
    {
      if(textValue.charAt(i)=='F' || textValue.charAt(i)=='f')
       counter++;
      if (i>1 && textValue.charAt(i)=='0')
        ZeroCounter++;
    }
    if(counter==12) return false;     else if(ZeroCounter>9)
	return false;      else
    {
   		check=textValue.charAt(1);
    	if((check=='A') ||
    	   (check=='C') ||
    	   (check=='E') ||
    	   (check=='0') ||
           (check=='2') ||
           (check=='4') ||
           (check=='6') ||
           (check=='8'))
           return false;
    }
    return true;
 }


 function macUcastCheck(textValue)
 {

   	check=textValue.charAt(1);
    	if((check=='1') ||
    	   (check=='3') ||
    	   (check=='5') ||
    	   (check=='7') ||
           (check=='8') ||
           (check=='B') ||
           (check=='D') ||
           (check=='F'))
           return false;

    return true;
 }
 

 function UcastMACCheck(textValue)
 {

   	var check1=textValue.charAt(0);
   	var check2=textValue.charAt(1);
   	var counter=0;

   	for(var i=0;i<12+5;i++)
   	{
   		if(textValue.charAt(i)=='0')
   			counter++;
   	}

   	if(counter==12)
   		return false;

   	if(check1 !=0 || check2 !=0)
           return false;

    return true;
 }

