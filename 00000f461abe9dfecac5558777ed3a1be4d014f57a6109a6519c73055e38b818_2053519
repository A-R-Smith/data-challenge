		function OnPressNext()
		{
			var reUrl="77_JWAC_Host_Table.html?1"+","+args[1]+","+args[2]+","+args[3]+","+args[4];
			document.location.href=reUrl;
		}
		function OnPressPre()
		{
			var reUrl="77_JWAC_Host_Table.html?2"+","+args[1]+","+args[2]+","+args[3]+","+args[4];
			document.location.href=reUrl;
		}
		function Init()
		{
			if(displayButton=='1' ||displayButton=='0' )  //0-none 1-prebutton 2-nextbutton 3-both botton
				document.getElementById("NextButton").disabled = true;
			if(displayButton=='2' ||displayButton=='0' )
				document.getElementById("PreButton").disabled = true;
		}
		function DelIt(iTag)
		{
			document.formJWACHostDel[iTag].submit();
		}

	</script>

