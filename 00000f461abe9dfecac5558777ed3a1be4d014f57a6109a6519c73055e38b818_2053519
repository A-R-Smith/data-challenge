<script language="Javascript">	  
	var d = document.formMirror;
	var v = d.sniffer.value;
	
	function getMirroString(sourceport,typechar){
			var portValue = '';
			var firtTime = true;
			var iCounter = 0;	
			var k;
			//for(k=0; k<sourceport.length; k++) {
			//for(j=1; j<=sourceport[k].length; j++){
			//	if (sourceport[k].substring(j-1, j) == typechar){
			for(j=1; j<=sourceport.length; j++){
				if (sourceport.substring(j-1, j) == typechar){
					iCounter++;
					if(firtTime == false)
						portValue += ',';
					firtTime = false;
					//portValue += ' ' + (k+1) + ":" + j +",";
					portValue += j;
				}
			}	
		
			return portValue;
	}

	function ListRxPorts(newval) {
		var str = '';
		str += getMirroString(newval,'B');
		document.getElementById('rxSpan').value = str;
		if (str.length != 0)
			str += ',';
		str += getMirroString(newval,'R');
		document.getElementById('rxSpan').value = str;
	}

	function ListTxPorts(newval) {
		var str = '';
		str += getMirroString(newval,'B');
		document.getElementById('txSpan').value = str;
		if (str.length != 0)
			str += ',';
		str += getMirroString(newval,'T');
		document.getElementById('txSpan').value = str;
	}

	function showPorts(index) {
		var unitid;
		var i;
		var newval;
		var radioobj;

		if (Stacking_Mode == '1')
			unitid = parseInt(document.getElementById('editunit').value);
		else
			unitid = 1;

		newval = '';
		for(i=1; i<=parent.port_number; i++) {			
			radioobj = document.getElementsByName('C'+i);
				if(radioobj[0].checked)	 newval += 'T';
				else if(radioobj[1].checked)	newval += 'R';
				else if(radioobj[2].checked)	newval += 'B';
				else 	newval += '0';
		}
/*			count[unitid] = 0;
		for(i=1;i<=parent.port_number;i++)
	  	{
		radioobj = document.getElementsByName('C'+i);
		
		if(radioobj[0].checked||radioobj[1].checked)
			{
						count[unitid]++;
			}
		else if(radioobj[2].checked)
			{
					count[unitid]+=1;
			}
		}
	*/		
			globalMaxPort=0
			//for(i=1; i<=Unit_Setting.length; i++){
			//	 globalMaxPort+=count[i]
	  //}

	  if(globalMaxPort>8)
	  {
			radioobj = document.getElementsByName('C'+index);
		  radioobj[0].checked=false;
		  radioobj[1].checked=false;
		  radioobj[2].checked=false;
		  radioobj[3].checked=true;
	  }
	  else
	  {
			//Mirror_Setting[2][unitid-1] = newval;
			
				ListTxPorts(newval);
					ListRxPorts(newval);
	   }
	}


	function ListMirrorPorts() {
	var i;
	var str;
		str = "<table cellpadding=0 cellspacing=0 border=0 width=100%>";
	str += "<tr class=gw><td width=120 align=left>TX</td>";
		//str += "  <td>" + getMirrorPortByType_withComma(Mirror_Setting[2],'T') + " " +getMirrorPortByType_withComma(Mirror_Setting[2],'B')+ "</td></tr>";
		str += "  <td>" + getPortNumberby01_withComma(Mirror_Setting[3]) + " </td></tr>";
	str += "<tr class=gx><td width=120 align=left>RX</td>";
		str += "  <td>" + getPortNumberby01_withComma(Mirror_Setting[2]) +  "</td></tr>";
	str += "</table>";
	document.getElementById('SourcePortListPane').innerHTML = str;
  }

	function editchangeunit(unitid) {
		var uid = parseInt(unitid);
		DisplayMirrorEditPane(uid, 1);
		ListTxPortsBy01();
			ListRxPortsBy01();
		return 1;
	}

	function ListTxPortsBy01(){
		var str ='';
		str = getPortNumberby01_withComma(Mirror_Setting[3]);
		document.getElementById('txSpan').value = str;
		}
	function ListRxPortsBy01(){
		var str ='';
		str = getPortNumberby01_withComma(Mirror_Setting[2]);
		document.getElementById('rxSpan').value = str;
		}

	function EditMirrorPerUnit(unitid) {
		DisplayMirrorEditPane(unitid,700);
	}

	function DisplayMirrorEditPane(unitid,duration) {
		var i;
		var uid = unitid;
		if (Stacking_Mode == '1')
		{
			document.getElementById('editunit').options[unitid-1].selected = true;
			document.getElementsByName('editunit')[0].checked = true;
		}
		for(i=1;i<=parent.port_number;i++) {
			var radioobj;
			radioobj = document.getElementsByName('C'+i);
/*			if (Mirror_Setting[2][uid-1].substring(i-1,i) == '0')
				radioobj[3].checked = true;
			else if (Mirror_Setting[2][uid-1].substring(i-1,i) == 'T')
				radioobj[0].checked = true;
			else if (Mirror_Setting[2][uid-1].substring(i-1,i) == 'R')
				radioobj[1].checked = true;
			else
				radioobj[2].checked = true;
*/
			if ((Mirror_Setting[2].substring(i-1, i) == '0') && (Mirror_Setting[3].substring(i-1, i) =='0'))
				radioobj[3].checked = true;
			else if ((Mirror_Setting[2].substring(i-1, i) == '1') && (Mirror_Setting[3].substring(i-1, i) =='1'))
				radioobj[2].checked = true;
			else if (Mirror_Setting[2].substring(i-1, i) == '1')
				radioobj[1].checked = true;
			else if (Mirror_Setting[3].substring(i-1, i) =='1')
				radioobj[0].checked = true;

			
			if (Stacking_Mode == '1')
				TargetUid=document.getElementById('targetUnit').value
			else
				TargetUid=1;

			//if((document.getElementById('sniffer').value==i)||d.mode[1].checked == true)
			if((document.getElementById('sniffer').value==i))
			{
				radioobj[0].disabled = true;
				radioobj[1].disabled = true;
				radioobj[2].disabled = true;
				radioobj[3].disabled = true;
				radioobj[3].checked = true;
			} 
			else 
			{
				radioobj[0].disabled = false;
				radioobj[1].disabled = false;
				radioobj[2].disabled = false;
				radioobj[3].disabled = false;
			}
		}
	}

	function CommitPanel() {
		var unitid;
		var vlanname;
		var i;
		var newval;

		if (Stacking_Mode == '1')
			unitid = parseInt(document.getElementById('editunit').value);
		else
			unitid = 1;
		newval = '';
		for(i=1; i<=parent.port_number; i++) {
			var radioobj;
			radioobj = document.getElementsByName('C'+i);
				if(radioobj[0].checked) newval += 'T';
				else if(radioobj[1].checked) newval += 'R';
				else if(radioobj[2].checked) newval += 'B';
				else newval += '0';
		}
		document.formMirror.submit();
		//Mirror_Setting[2][unitid-1] = newval;
		//ListMirrorPorts();
	}

	
	function apply(){
		    var str = '';
		    str += getMirroString(Mirror_Setting[2],'R');
		    document.getElementById('RX').value = str;
		    str='';
		    str += getMirroString(Mirror_Setting[2],'T');
		    document.getElementById('TX').value = str;
		    str=''
		    str += getMirroString(Mirror_Setting[2],'B');
		    document.getElementById('BOTH').value = str;
		    
        document.formMirror.submit();
	}

	function chkEnable()
	{
		d.sniffer.disabled = false;
		if (Stacking_Mode == '1')
			d.targetUnit.disabled = false;
		for(i=1; i<=parent.port_number; i++) {
			radioobj = document.getElementsByName('C'+i);
			radioobj[0].disabled=false;
			radioobj[1].disabled=false;
			radioobj[2].disabled=false;
			radioobj[3].disabled=false;
		}
		document.getElementById('rxSpan').disabled = false;
		document.getElementById('txSpan').disabled = false;
		if (Stacking_Mode == '1')
			document.getElementById('editunit').disabled =false;

		if (Stacking_Mode == '1')
		{
			unitid=document.getElementById('editunit').value
		}
		else
			unitid=1;
			editchangeunit(unitid)
	}
	
	ListMirrorPorts();
	chkEnable();

</script>

</div>
