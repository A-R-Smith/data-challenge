	function getSpeItemID(objName){
		for(i=1; i<TREE_ITEMS.length-1; i++){
			for(j=1; j<TREE_ITEMS[i].length-5; j++){
				if(TREE_ITEMS[i][4+j].length > 5){
					for(k=1; k<TREE_ITEMS[i][4+j].length-5; k++){
						if(TREE_ITEMS[i][4+j][4+k][1] == objName){
							return i+'+'+j+'+'+k;
						}
					}
				}
				else{
					if(TREE_ITEMS[i][4+j][1] == objName){
						return i+'+'+j;
					}
				}
			}
		}
	}
    
	function calItemMark(objName){
		n = 0;
		s1 = 0;
		s2 = 0;
		s3 = 0;
		aStr = getSpeItemID(objName).split("+");
		for(x=1; x<TREE_ITEMS.length-1; x++){
			n++;
			if(x==aStr[0])
				s1 = n;

			for(y=1; y<TREE_ITEMS[x].length-5; y++){
				n++;
				if((x==aStr[0])&&(y==aStr[1])){
					s2 = n;
				}

				for(z=1; z<TREE_ITEMS[x][y+4].length-5; z++){
					n++;
					if((x==aStr[0])&&(y==aStr[1])&&(z==aStr[2])){
						s3 = n;
					}
				}
			}
		}
		return s1+'+'+s2+'+'+s3;
	}
    
	function selItemMark(objName){
		bStr = calItemMark(objName).split("+");
		
		if(!ctIsFolderOpen(document.getElementById('ctItemID'+bStr[0]).parentNode))
			ctItemMouseUp(document.getElementById('ctItemID'+bStr[0]).parentNode);
		if(bStr[2]==0){
			ctItemMouseUp(document.getElementById('ctItemID'+bStr[1]).parentNode);
		}
		else{
			if(!ctIsFolderOpen(document.getElementById('ctItemID'+bStr[1]).parentNode))
				ctItemMouseUp(document.getElementById('ctItemID'+bStr[1]).parentNode);
			ctItemMouseUp(document.getElementById('ctItemID'+bStr[2]).parentNode);
		}
	} 
</script>
</body>
</html>

