<script language="Javascript">
function chagne_on3()
{
     if((document.getElementById('d3').options.selectedIndex==0) || (document.getElementById('d3').options.selectedIndex==1))
     {
        
        document.getElementsByName("action_label")[0].readOnly=true;
     }
     else
     {
      document.getElementsByName("action_label")[0].readOnly=false;
     }
}
