<script language="Javascript">
function creatUser()
{
	var s = document.formWAC_User;

	if (!checkTxtInput(s.user_name))
	{
		return;
	}

	if (s.pass.value.length != 0)
	{
		if (!checkTxtInput(s.pass))
		{
			return;
		}
	}	
	if (s.pass_confirm.value.length != 0)
	{
		if (!checkTxtInput(s.pass_confirm))
		{
			return;
		}
	}
	if (s.pass.value != s.pass_confirm.value)
	{
		alert("Password input error!");
		s.pass.select();
		return;
	}
	s.isDel.value = '0';
	s.isEdit.value = '0';
	s.h_user.value = s.user_name.value;
	s.submit();
}

function mapVLAN()
{
	var s = document.formWAC_User;

	if (!checkTxtInput(s.vlan_name))
	{
		return;
	}
	s.isDel.value = '0';
	s.isEdit.value = '1';
	s.h_user.value = WAC_User[s.user_name_sel.value][0];
	s.submit();
}

function check(s)
{	if(s=='1')
	{
		if(document.getElementById("vlan").checked)
		{
			document.getElementById("vlan_name").disabled=false;
		}
		else
		{
			document.getElementById("vlan_name").disabled=true;
		}
	}
	else if(s=='2')
	{
		if(document.getElementById("vlanid").checked)
		{
			document.getElementById("vlan_id").disabled=false;
		}
		else
		{
			document.getElementById("vlan_id").disabled=true;
		}
	}
	else if(s=='3')
	{
		if(document.getElementById("clear_vlan").checked)
		{
			document.getElementById("ClearVlan").value=1;
		}
		else
		{
			document.getElementById("ClearVlan").value=2;
		}
	}
}

function init()
{
	document.getElementById("vlan_name").disabled=true;
	document.getElementById("vlan_id").disabled=true;
}
</script>
</div>
</body>
