<script language="Javascript">
    var d = document.formAddVLAN;
 
    function AddApply()
	{

	if(!checkTxtInput(d.VName))
		return false;

	if(d.RVName.value!="")
	{
		if(!checkTxtInput(d.RVName))
			return false;
	}
	if(d.WVName.value!="")
	{
		if(!checkTxtInput(d.WVName))
			return false;
	}
	if(d.NVName.value!="")
	{
		if(!checkTxtInput(d.NVName))
			return false;
	}
	if(d.RVName.value=="" && d.WVName.value=="" && d.NVName.value=="")
	{
		alert("you should write a view at least!");
		return false;
	}
		d.submit();
    }
    
    function checkv3()
	{
    	switch(d.Security_Model.selectedIndex) 
 	{
 		case 0: // v1
 		case 1: // v2
		 	d.Security_Level.disabled=true;		
			break;
 		case 2: // v3
		 	d.Security_Level.disabled=false;		
	 		break;
	}
    }
    d.Security_Level.disabled=true;

	var snmp_group_count;

	function show_counter()
	{
		var str = "<b>Total Entries: "+ snmp_group_count+"</b>";
		document.getElementById('snmp_group_counter').innerHTML = str;
	}

</script>
