		<script language="Javascript">
			function  onRadClick(oRad)
			{		
				if(oRad.id == 'clear_default_redirpath1')
				{	
					document.getElementById("default_redirpath").value = "";
					document.getElementById("default_redirpath").disabled = true;
					document.getElementById("default_redirpath").style.backgroundColor = "#D4D0C8";			
				}
				else if(oRad.id == "clear_default_redirpath0")
				{
					document.getElementById("default_redirpath").value = Default_Redirpath;
					document.getElementById("default_redirpath").disabled = false;
					document.getElementById("default_redirpath").style.backgroundColor = "";
					document.getElementById("default_redirpath").focus();
				}
		  }
			function  isHttps(oRad)
			{	

				if(oRad.id == 'portType0')
				{	
					document.getElementById("httpPort").value = (isHttpPort==1 ? http : "");
					/*
					document.getElementById("httpPort").disabled = false;
					document.getElementById("httpPort").style.backgroundColor = "";
					*/
				}
				else if(oRad.id == "portType1")
				{
					document.getElementById("httpPort").value = (isHttpPort==1 ? "" : https);
					/*
					document.getElementById("httpPort").value = "";
					document.getElementById("httpPort").disabled = true;
					document.getElementById("httpPort").style.backgroundColor = "#D4D0C8";
					*/
				}
		  }		
			function	formCheckSetting(s, oBtn)
			{
				if(oBtn.id == "btnGlbState")
				{
					s.isGlbState.value = "1";
					if(document.getElementById("state1").checked == true)
					{
						s.state.value = "1";
					}
					else
					{
						s.state.value = "0";
					}
					s.submit();
					return true;
				}

				s.isGlbState.value = "0";
				if(s.virtualIP.value != "")
				{
				if(!ipCheck(s.virtualIP))
				{
					return false;
				}
				}

				if(s.httpPort.value != "")
				{
				if(!checkNumberInput(s.httpPort, 1, 65535))
				{
					return false;
				}
				}

				if(document.getElementById("clear_default_redirpath0").checked && (s.default_redirpath.value != ""))
				{
					if(!checkURLInput(s.default_redirpath))
					{
						return false;
					}
				}

				if(s.portType0.checked)
				{
					s.isHttp.value = 1; /* changed form 0. by King Wang, 20081124. */
				}
				if(s.portType1.checked)
				{
					s.isHttp.value = 0; /* changed form 1. by King Wang, 20081124. */
				}		
				s.method.value=s.method1.value;
				s.auth_failover.value=s.auth_failover1.value;			
				
				if(s.clear_default_redirpath0.checked)
				{
					s.isClear.value=0;
				}
				if(s.clear_default_redirpath1.checked)
				{
					s.isClear.value=1;
				}	

				s.submit();
			}

			function	checkURLInput(obj)
			{
				var URLvalue = obj.value;
				var splitURL=URLvalue.split("/");
				var HostIP = splitURL[2];
				if(URLvalue.indexOf("http://")!=0 && URLvalue.indexOf("https://")!=0)
				{
					alert("Invalid URL, should be http://x.x.x.x/...... or https://x.x.x.x/......");
					obj.select();
					obj.focus();		
					return false;
				}
				return true;
			}
			function ipCheckValue(textValue)
			{
				if (textValue.search(" ") != -1)
				{
					alert("Invalid IP Address.");
					return false;
				}

				var Check = textValue.indexOf('.');
				
				if(Check==-1)
				{
					alert("Invalid IP Address.");
					return false;
				}
				else
				{       
					var ipSplit=textValue.split('.');
					if(ipSplit.length!=4)
					{
						alert("Invalid IP Address.");
						return false;
					}

					for(i=0; i<ipSplit.length; i++)
					{
						if(isNaN(ipSplit[i]))
						{
							alert("Invalid IP Address.");
							return false;
						}
					}

					for(i=0; i<ipSplit.length; i++)
					{
						if(ipSplit[i]>255 || ipSplit[i]<0)
						{
							alert("Invalid IP Address.");
							return false;
						}
					}
					return true;
				}
			}
			function Init()
			{
				if(WAC_Global == 0)
				{
					document.getElementById("state1").checked = false;
					document.getElementById("state2").checked = true;
				}
				else
				{
					document.getElementById("state1").checked = true;
					document.getElementById("state2").checked = false;
				}
				document.WAC_Global_Setting.virtualIP.value=virtualIP;
				if(isHttpPort==1)
				{
					document.WAC_Global_Setting.httpPort.value=http;
					document.WAC_Global_Setting.portType0.checked=true;
				}
				else
				{
					document.WAC_Global_Setting.httpPort.value=https;
					document.WAC_Global_Setting.portType1.checked=true;
				}
				document.WAC_Global_Setting.method1.value=Method;
				document.WAC_Global_Setting.auth_failover1.value=Auth_Failover;
				document.WAC_Global_Setting.default_redirpath.value=Default_Redirpath;
			}	
			function  checkedTxt0(num)
			{

				if(num=='0')
				{
					return 'checked';				
				}
			}
			function  checkedTxt1(num)
			{
				if(num=='1')
				{
					return 'checked';		
				}			
			}
			
		</script>
	</div>

</body>
</html>
