<body onLoad=Init() class=body228>
<div id="tableContainer" class="tableContainer"> 
	<table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" style="width:100%" >
     <thead class="fixedHeader">
        <tr class=gh>
					<td width=30% >VLAN Name </td>
					<td width=30%>Multicast Filtering Mode</td>
                     <td width=40%></td>
          </tr>
          </thead>
       			<script language="Javascript">    
			var trClass = '';
			var txtClass = '';	
			document.write("<tbody class='scrollContent'>");  
			for(i=0; i<Multicast_Forward_Model .length; i++)
			{
				 if (i%2 == 0)
				{
				        trClass = 'gx';
				        txtClass = 'flatGX';
				 }
				 else
				{
				        trClass = 'gw';
				        txtClass = 'flatGW';
				}	
				 document.write("<tr class=" + trClass + ">");
				 document.write("<td width=30% align=left>"+ rp_special_char(Multicast_Forward_Model[i][0])+"</td>");
				 document.write("<td width=30% align=left>"+ mode2str(Multicast_Forward_Model[i][1])+"</td>");
				document.write("<td width=40%></td></tr>");
			}

		</script>
		<tr>
			<td colspan=3 align=right><input type=button name=PreButton id=PreButton class=btnInGrayS value='Previous' onClick='OnPressPre()'>
									<input type=button name=NextButton id=NextButton class=btnInGrayS value='Next' onClick='OnPressNext()'></td>
		</tr>
        </tbody>
</table>		
</div>

<script language="Javascript">    
parent.mmode_count = totalEntries;
parent.show_counter();

</script>
<script language="javascript">
	function OnPressNext()
	{
		var reUrl="77_Multi_Filter_Model_Table.html?1";
		document.location.href=reUrl;
	}
	function OnPressPre()
	{
		var reUrl="77_Multi_Filter_Model_Table.html?2";
		document.location.href=reUrl;
	}
	function Init()
	{
		if(displayButton=='1' ||displayButton=='0' )  //0-none 1-prebutton 2-nextbutton 3-both botton
			document.getElementById("NextButton").disabled = true;
		if(displayButton=='2' ||displayButton=='0' )
			document.getElementById("PreButton").disabled = true;
	}

</script>
</body>

