						}
						else{
							trClass = 'gw';
							txtClass = 'flatGW';
						}					
						document.write("<tr class=" + trClass + ">");
						document.write("  <td width=10%>" + IGMPSnooping[i][0] + "</td>");		//VLAN  ID
						document.write("  <td width=20%>" + rp_special_char(IGMPSnooping[i][1]) + "</td>");		//VLAN  Name
						document.write("  <td width='35%'>" + IGMPSnooping[i][2] + "</td>");		//Data Learn State
						document.write("  <td width='35%'>" + IGMPSnooping[i][3] + "</td>");		//Data Learn Aged
						document.write("</tr>");
					}
				</script>
                </tbody>
			</table>
            </div>
</body>
</html>
