<script language="Javascript">
    function delit(){
        alert("delete");
        document.formUnicast.VIDN.value='01';
        document.formUnicast.MACADR.value='';
        document.formUnicast.UnitN.value='01';
        document.formUnicast.PortN.value='01';
    }

    function EditStart(iTag){
        document.formUnicast.VIDN.value=Unicast_Forward_Setting[iTag][0];
        document.formUnicast.MACADR.value=Unicast_Forward_Setting[iTag][1];
        document.formUnicast.UnitN.value=Unicast_Forward_Setting[iTag][2];
        document.formUnicast.PortN.value=Unicast_Forward_Setting[iTag][3];
    }
    
    function ApplyIt(){
    	alert('Ok');
        document.formUnicast.VIDN.value='01';
        document.formUnicast.MACADR.value='';
        document.formUnicast.UnitN.value='01';
        document.formUnicast.PortN.value='01';
        }
function CheckAll(s)
{
	if (s.all.checked == true)
		s.E1.disabled = true;
	else
		s.E1.disabled = false;
}

function init()
{
	var s = document.forms[0];
	s.all.checked = false;
}
</script>
