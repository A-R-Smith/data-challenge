<script>

if (document.URL.indexOf('?') != -1)
{
	var para = document.URL.split("?")[1];

	if (para == 'fail')
	{
		alert("Authentication fail!");
	}
	else
	{
		var name = para.split(",")[1];
		var rdt_page = para.split(",")[2];
		var login_date = para.split(",")[3];
		var login_time = para.split(",")[4];
		var vid = para.split(",")[5];
		var remoteTcpPort = para.split(",")[6];

		window.open("login_ok.html?" + name + "," + login_date + "," + login_time+","+vid+ "," +remoteTcpPort , null, "height=300,width=500,toolbar=0,location=0,left=" + (window.screen.width-500)/2 + ",top=" + (window.screen.height-300)/2);

		function jumpToPage(addr)
		{
			if (addr.search("http://") != -1)
				self.parent.location.replace(addr);
			else
				self.parent.location.replace("http://" + addr);
		}
		window.setTimeout("jumpToPage(rdt_page)", 3000);
	}
}

</script>

