        document.write("  <td><input type=text name=Auth_Udp_Port id=Auth_Udp_Port class=" + txtClass + " value='" + _1x_RADIUS_Server[i][3] + "'>");
        document.write("      <input type=text name=Auth_Udp_Port_M id=Auth_Udp_Port_M class=flatWS value='" + _1x_RADIUS_Server[i][3] + "' style='display: none;'></td>");
        document.write("  <td><input type=text name=Acct_Udp_Port id=Acct_Udp_Port class=" + txtClass + " value='" + _1x_RADIUS_Server[i][4] + "'>");
        document.write("      <input type=text name=Acct_Udp_Port_M id=Acct_Udp_Port_M class=flatWS value='" + _1x_RADIUS_Server[i][4] + "' style='display: none;'></td>");
       	document.write("  <td><input type=text name=Key id=Key class=" + txtClass + " value='" + _1x_RADIUS_Server[i][5] + "'>");
        document.write("      <input type=text name=Key_M id=Key_M class=flatWS value='" + _1x_RADIUS_Server[i][5] + "' style='display: none;'></td>");
       	document.write("  <td><input type=text name=Status id=Status class=" + txtClass + " value='" + _1x_RADIUS_Server[i][6] + "'>");
        document.write("      <input type=text name=Status_M id=Status_M class=flatWS value='" + _1x_RADIUS_Server[i][6] + "' style='display: none;'></td>");
       	document.write("  <td><input type=button name=editButton id=editButton value='Edit' class=btnInGrayS onClick='EditStart(" + i + ");'>");
      	document.write("      <input type=button name=editApply id=editApply value='Apply' class=btnInGrayS onClick='EditApplyIt(" + i + ");' style='display: none;'></td>");
        document.write("  <td><input type=button value='Delete' class=btnInGrayS onClick='delit();'></td>");
        document.write("</tr>");
        
        
    }
