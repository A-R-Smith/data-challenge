			document.write("<div id='logoConsole' style='position:absolute; left:" + (247+130) + "px; top:" + (54+10) + "px;'><a href='javascript:redirect()'><img src='../Icon/transparent.gif' width=29px height=15px border=0 alt='Serial Port Settings'/></a></div>");
		}
		else
		{
			document.write("<div id='logoConsole_RJ45' style='position:absolute; left:186px; top:52px;'><a href='javascript:redirect()'><img src='../Icon/transparent.gif' width=21px height=13px border=0 alt='Serial Port Settings'/></a></div>");
			document.write("<div id='logoConsole' style='position:absolute; left:247px; top:54px;'><a href='javascript:redirect()'><img src='../Icon/transparent.gif' width=29px height=15px border=0 alt='Serial Port Settings'/></a></div>");
		}
	}
	else if(Panel_Info[4]==136)
	{
