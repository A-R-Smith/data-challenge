<script language="Javascript">
	function BootIt(iTag)
	{
		if(iTag == -1)
		{
			if(!checkFilePath(document.formDualCfg.boxPathname))
			{
				return false;
			}
			document.formDualCfg.operation.value='11';	/* boot from SD Card */
			document.formDualCfg.CfgId.value = "0";
		}
		else if(DualCfgSetting[iTag][8] != "")	/* if SD Card config */
		{
			document.formDualCfg.operation.value='11';	/* active SD Card Config */
			document.formDualCfg.CfgId.value = "0";
			document.formDualCfg.boxPathname.value = DualCfgSetting[iTag][8];
		}
		else
		{
			document.formDualCfg.operation.value='1';		/* boot from NV-RAM */
			document.formDualCfg.CfgId.value = DualCfgSetting[iTag][0];
		}
		document.formDualCfg.submit();
	}
	
	function ActiveIt(iTag)
	{
		if(iTag == -1)
		{
			if(!checkFilePath(document.formDualCfg.boxPathname))
			{
				return false;
			}
			document.formDualCfg.operation.value='33';	/* active SD Card Config */
			document.formDualCfg.CfgId.value = "0";
		}
		else if(DualCfgSetting[iTag][8] != "")	/* if SD Card config */
		{
			document.formDualCfg.operation.value='33';	/* active SD Card Config */
			document.formDualCfg.CfgId.value = "0";
			document.formDualCfg.boxPathname.value = DualCfgSetting[iTag][8];
		}
		else
		{
			document.formDualCfg.operation.value='3';		/* active NV-RAM config */
			document.formDualCfg.CfgId.value = DualCfgSetting[iTag][0];
		}
		document.formDualCfg.submit();
	}		
			
	function DelIt(iTag)
	{
		if(iTag == -1)
		{
			if(!checkFilePath(document.formDualCfg.boxPathname))
			{
				return false;
			}
			document.formDualCfg.operation.value='22';	/* del config from SD Card */
			document.formDualCfg.CfgId.value = "0";
		}
		else if(DualCfgSetting[iTag][8] != "")	/* if SD Card config */
		{
			document.formDualCfg.operation.value='22';	/* active SD Card Config */
			document.formDualCfg.CfgId.value = "0";
			document.formDualCfg.boxPathname.value = DualCfgSetting[iTag][8];
		}
		else
		{
			document.formDualCfg.operation.value='2';		/* del config from NV-RAM */
			document.formDualCfg.CfgId.value = DualCfgSetting[iTag][0];
		}
		document.formDualCfg.submit();
	}	
</script>
</div>
<script src="../../DataStore/error_msg.js" type="text/javascript"></script>
</body>
</html>
