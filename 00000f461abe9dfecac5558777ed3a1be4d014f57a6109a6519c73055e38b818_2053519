<script language="Javascript">

function applyForm()
{

	var s=document.formIP_MAC_Port;
	if(IMP_VERSION == '4')
	{
		if(!checkNumberInput(document.getElementById("boxThreshold"), 0, 500))
		{
			return false;
		}
	}
	if(document.getElementById("nolimit").checked == false)
	{
		if(!checkNumberInput(document.getElementById("maxEntry"), 1, 50))
		{
			return false;
		}
	}
	s.submit();
}
</script>
<script language="JavaScript">


function changState()
{
	var s=document.formIP_MAC_Port;
	if(s.nolimit.checked)
	{
		s.maxEntry.disabled=true;
		s.maxEntry.style.backgroundColor='#D4D0C8';
	}
	else
	{
		s.maxEntry.disabled=false;
		s.maxEntry.style.backgroundColor='#FFFFFF';
	}
}

</script>

