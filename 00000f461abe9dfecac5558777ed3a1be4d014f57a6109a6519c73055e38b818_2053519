				document.write("<tr><td>Protocol</td>");
				document.write("	<td><select class=enableSel name=Protocol id=Protocol style='width:100'>");
				for(i=0; i<Protocol_Lists.length; i++){
					document.write("<option value="+Protocol_Lists[i]+" style='width:100'>"+Protocol_Lists[i]+"</option>");
				}
				document.write("	</td><td>Timeout (1-255 sec)</td>");
				document.write("	<td><input class=flatL type=text name=TimeOut id=TimeOut style='width:100' maxlength=3 value=''></td></tr>");
				document.write("<tr><td>Key</td>");
				document.write("	<td><input class=flatL type=password name=Key id=Key style='width:100' maxlength=254 value=''> (Max: 254 characters)</td>");
				document.write("	<td>Retransmit (1-255 times)</td>");
				document.write("	<td><input class=flatL type=text name=Retransmit id=Retransmit style='width:100' maxlength=3 value=''></td>");
				document.write("	<td align=right>");
				document.write("		<input class=flatL type=hidden name=isClear id=isClear value=0>");
				document.write("		<input class=flatL type=hidden name=isEdit id=isEdit value=0>");
				document.write("		<input type=button value='Apply' class=btnText onClick='ApplyIt()';></td></tr>");
				document.write("<tr><td colspan=10><hr align=left name=hr1 id=hr1 class=hr1></td></tr>");
			</script>
			</form>
		</table>
</div>
