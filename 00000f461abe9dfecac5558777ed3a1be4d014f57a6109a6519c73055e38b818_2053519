<script language="Javascript">

function gonext()
{
	location.href = "42_Port_Security_Entry.html?2";
}

function goprev()
{
	location.href = "42_Port_Security_Entry.html?1";
}

function delit(iTag)
{
	var f = document.portSecDel;

	f.h_vname.value = Port_Sec_Entry[iTag][1];
	f.h_mac.value = Port_Sec_Entry[iTag][2];
	f.h_unit.value = Port_Sec_Entry[iTag][3];
	f.h_port.value = Port_Sec_Entry[iTag][4];

	document.portSecDel.submit();
}

function clear_by_port()
{
	document.port_lock_clear.submit();
}

if (Port_Sec_Navigation_Btn[0] == '1')
	document.portSecDel.bprev.disabled = false;

if (Port_Sec_Navigation_Btn[1] == '1')
	document.portSecDel.bnext.disabled = false;

</script>
