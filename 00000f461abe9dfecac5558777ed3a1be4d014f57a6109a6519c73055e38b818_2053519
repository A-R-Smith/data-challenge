<script language="Javascript">

var d = document.formAddVLAN;
 
function ipv6StringCheck(str)
{
	var ret = false;
	var validIPv6Addresses = [
		//preferred
		/^(?:[a-f0-9]{1,4}:){7}[a-f0-9]{1,4}(?:\/\d{1,3})?$/i,

		//various compressed
		/^[a-f0-9]{0,4}::(?:\/\d{1,3})?$/i,
		/^:(?::[a-f0-9]{1,4}){1,6}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){1,6}:(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:)(?::[a-f0-9]{1,4}){1,6}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){2}(?::[a-f0-9]{1,4}){1,5}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){3}(?::[a-f0-9]{1,4}){1,4}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){4}(?::[a-f0-9]{1,4}){1,3}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){5}(?::[a-f0-9]{1,4}){1,2}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){6}(?::[a-f0-9]{1,4})(?:\/\d{1,3})?$/i,


		//IPv6 mixes with IPv4
		/^(?:[a-f0-9]{1,4}:){6}(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i,
		/^:(?::[a-f0-9]{1,4}){0,4}:(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){1,5}:(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:)(?::[a-f0-9]{1,4}){1,4}:(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){2}(?::[a-f0-9]{1,4}){1,3}:(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){3}(?::[a-f0-9]{1,4}){1,2}:(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i,
		/^(?:[a-f0-9]{1,4}:){4}(?::[a-f0-9]{1,4}):(?:\d{1,3}\.){3}\d{1,3}(?:\/\d{1,3})?$/i
	];

	for (var i=0; i<validIPv6Addresses.length; i++)
	{
		if (validIPv6Addresses[i].test(str)) {
			ret = true;
			break;
		}
	}

	if (!ret)
	{
		return false;
	}

	return true;
}

function checkInterface(str)
{
	var i, ch;
	var len = str.length;
	
	if (str == "")
       {
          	alert("Please input parameter!");
		return false; 
       }
	if(str.search(" ") != -1)
	{
          	alert("The value can not include space!");
		return false; 
       }
	
	for(i = 0; i < len; i++)
	{
		ch = str.charCodeAt(i);
		if(ch > 0x7e || ch < 0x20)
		{	
			alert("Input Error! Unknown characters detected !");
			return false;
		}	  
	}
	return true;
}

function AddApply() 
{
	var ipif = d.HostIpv6.value.split('%');
	if(!ipv6StringCheck(ipif[0]))
	{
		alert("Invalid IPv6 Address!");
		d.HostIpv6.focus();
		d.HostIpv6.select();
		return false;
	}

	if(ipif[1] != undefined)
	{
		if(!checkInterface(ipif[1]))
		{
			alert("Invalid Interface Name!")
			d.HostIpv6.focus();
			d.HostIpv6.select();
			return false;
		}
	}

	if(!checkTxtInput(d.VlanName))
		return false;
    	d.submit();
    }

	var snmp_host_count;

	function show_counter()
	{
		var str = "<b>Total Entries: "+ snmp_host_count+"</b>";
		document.getElementById('snmp_v6host_counter').innerHTML = str;
	}
    
</script>
