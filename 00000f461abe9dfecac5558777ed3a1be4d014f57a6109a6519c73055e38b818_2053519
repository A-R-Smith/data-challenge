	<script language="javascript">
		function OnPressNext()
		{	
			if(args[1]=='7')
				args[1] = '0';
			if(args[1]=='4')
				args[1] = '1';
			if(args[1]=='6')
				args[1] = '3';			
			var reUrl="48_MAC_Address_Table_table.html?1"+","+args[1]+","+args[2];
			document.location.href=reUrl;
		}
		function OnPressPre()
		{
			if(args[1]=='7')
				args[1] = '0';
			if(args[1]=='4')
				args[1] = '1';
			if(args[1]=='6')
				args[1] = '3';				
			var reUrl="48_MAC_Address_Table_table.html?2"+","+args[1]+","+args[2];
			document.location.href=reUrl;
		}
		function Init()
		{
			if(displayButton=='1' ||displayButton=='0' )  //0-none 1-prebutton 2-nextbutton 3-both botton
				document.formMSTI.NextButton.disabled = true;
			if(displayButton=='2' ||displayButton=='0' )
				document.formMSTI.PreButton.disabled = true;
		}

	</script>


