	}
	else
	{
		document.write('<div id="logoLink"><a href="javascript:go_dlink()"><img src="../Icon/transparent.gif" width="140px" height="60px" border=0 alt="Go to D-Link Web Site"/></a></div>');
	}
	</script>
	<script>
	if(Panel_Info[4]==137)
	{
