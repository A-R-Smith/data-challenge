        {
            trClass = 'gw';
            txtClass = 'flatGW';
        }
          document.write("<tr class=" + trClass + ">");
          document.write("  <td align=left><input type=checkbox name=C1 id=C1>");
          document.write("  <td align=left>" + table_m[i][0] + "</td>");
          document.write("  <td align=left><a href='52_Access_Rule_Detail_mac.html?+"+i+"' class=blue>" + table_m[i][3] + "</td>");
          document.write("  <td align=left>" + table_m[i][1] + "</td>");
          document.write("  <td align=left>" + table_m[i][2] + "</td>");
          document.write("  <td align=left>" + table_m[i][5] + "</td>"); 
          document.write("</tr>");
          if(i==0)
          {
             document.getElementsByName("C1")[0].checked=true;
          }
    }
    </script>
   </table>
   </td>
         </tr> 
         <tr></tr>
           <td colspan=5 align=left><input type=button value='Delete' class=btnInGray onClick=alert('Delete');></td>
   </table>

   

