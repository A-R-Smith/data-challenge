		<script language="Javascript">
			var d = document.form1xAuthMod;
			var d2 = document.form1xAuthSetting;
			function formCheck(s)
			{
				   if(!checkNumberInput(s.T5,1,65535)) 
				   {
					  return false;
				   }   
				   if(!checkNumberInput(s.T1,0,65535))
				   {
					   return false;
				   }
				   if(!checkNumberInput(s.T2,1,65535))
				   {
					  return false;
				   }   
				   if(!checkNumberInput(s.T3,1,65535))
				   {
					  return false;
				   }

				   if(!checkNumberInput(s.T4,1,10))
				   {
					  return false;
				   }

				   if(!checkNumberInput(s.T6,1,65535))
				   {
					  return false;
				   }
				   s.submit();
				   return true;
			}
			
			function ApplyIt()
			{
				if(_1x_Global_Enable != document.form1xAuthMod.S1.value)
					parent.frames['tree_items'].location.reload();
				document.form1xAuthMod.submit()
			}
			function Init()
			{
				document.form1xAuthMod.S1.value = _1x_Global_Enable;
				document.form1xAuthMod.S2.value = _1x_Auth_Protocol;
				document.form1xAuthMod.failover.value=_1x_Auth_Failover;
			}
			Init();
		</script>
