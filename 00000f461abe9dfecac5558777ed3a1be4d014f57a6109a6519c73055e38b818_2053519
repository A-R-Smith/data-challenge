<script src='../../DataStore/WAC_Global.js' type='text/javascript'></script>
<script src='../../DataStore/jwac_global.js' type='text/javascript'></script>
<script src='../../DataStore/MBA_Global.js' type='text/javascript'></script>
<script src='../../DataStore/Panel.js' type='text/javascript'></script>
<script language="Javascript">
	document.write("<script src='treeitems_24.js' type='text\/javascript'><\/script>");
</script>
<div id=treeBox class=treeBox  style="display:block;">
<table id=ThemeXPTable class='ThemeXPTable' STYLE='position:relatev; top:12; left:12;'>
<tr>
<td id='tree_items'>
<script language='javascript'>
	if(_1x_Global_Enable != '0')
	{
		for(i=6;i<TREE_ITEMS[6].length;i++)
		{
			if(TREE_ITEMS[6][i][1]=='Port Access Control')
			{
				if(_1x_Global_Enable == '1')
				{
			
					TREE_ITEMS[6][i][7] = [null, 'Authenticator State', '../iss/77_AuthState.html', 'main', null];
					TREE_ITEMS[6][i][8] = [null, 'Authenticator Statistics', '../iss/77_AuthStatistics.html', 'main', null];
					TREE_ITEMS[6][i][9] = [null, 'Authenticator Session Statistics', '../iss/77_AuthSessionStatistics.html', 'main', null];
					TREE_ITEMS[6][i][10] = [null, 'Authenticator Diagnostics', '../iss/77_AuthDiag.html', 'main', null];
				}
				else if(_1x_Global_Enable =='2')
				{
					TREE_ITEMS[6][i][7] = [null, 'Authenticator State', '../iss/77_AuthState_Mac.html?1', 'main', null];
					TREE_ITEMS[6][i][8] = [null, 'Authenticator Statistics', '../iss/77_AuthStatistics_Mac.html?1', 'main', null];
					TREE_ITEMS[6][i][9] = [null, 'Authenticator Session Statistics', '../iss/77_AuthSessionStatistics_Mac.html?1', 'main', null];
					TREE_ITEMS[6][i][10] = [null, 'Authenticator Diagnostics', '../iss/77_AuthDiag_Mac.html?1', 'main', null];
				}
				break;
			}
			
		}
		
		for(i=6;i<TREE_ITEMS[4].length;i++)
		{
			if(TREE_ITEMS[4][i]!=null && TREE_ITEMS[4][i][1]=='802.1X')
			{
				if(_1x_Global_Enable != '0')
				{			
					TREE_ITEMS[4][i][8] =[null, 'Initialize Port(s)', '../iss/77_8021x_Init.html?0,0', 'main', null];
					TREE_ITEMS[4][i][9] =[null, 'Reauthenticate Port(s)', '../iss/77_8021x_ReAuth.html?0,0', 'main', null];
					TREE_ITEMS[4][i][10] =[null, 'Guest VLAN', '../iss/42_Guest_VLAN.html', 'main', null];
				}
				else
				{
					TREE_ITEMS[4][i][8] =[null, 'Guest VLAN', '../iss/42_Guest_VLAN.html', 'main', null];
				}
				flag = true;
				break;
			}
		}
	}
</script>
