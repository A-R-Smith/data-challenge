        		<option value="3" selected>V3</option></select></td></tr>
        <tr><td>SNMP V3 Encryption</td>
            <td><select style="width:180" name=SNMP_V3_Encryption id=SNMP_V3_Encryption class=SNMPV3EncryptSel onchange="checkencryption();">
        	<option value="0">None</option>
