<script language="Javascript">

    function ChangMinStatus()
    {
        if(document.formSyslogMode.S1.selectedIndex==1)
        {
        	document.getElementById('T1').disabled = false;
        }
	else
	{
		document.getElementById('T1').disabled = true;
	 }
	 if(document.formSyslogMode.S1.selectedIndex==0)
        {
        	document.getElementById('buttonsave').disabled = false;
        }
	else
	{
		document.getElementById('buttonsave').disabled = true;
	 }
    }
	function Init()
	{
		document.getElementById('operation').value = '0';
		document.getElementById('S1').value = SystemLogTimeMode;
	}
	function checkForm(s)
	{
		if(document.formSyslogMode.S1.selectedIndex==1)
	 	{
	 		if(!checkNumberInput(s.T1, 1, 65535))
			return false;
		}
		s.submit();
	}
	function ApplySave(s)
	{
		document.getElementById('operation').value = '1';
		s.submit();
	}
	
Init();
ChangMinStatus();

</script>
