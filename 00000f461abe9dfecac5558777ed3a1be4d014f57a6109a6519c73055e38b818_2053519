		<script language="Javascript">
			function	formCheckSetting(s)
			{
				if(!ipCheck(s.virtualIP))
					return false;
				if(!checkNumberInput(s.httpPort, 1, 65535))
					return false;
				if(!checkNumberInput(s.delaytime , 0, 10))
					return false;	
				s.submit();
			}
			function formCheckUServer(s)
			{
				   if(!ipCheck(s.UServer)) 
				   {
					  return false;
				   }
				   if(!CheckSubnetMask(s.Mask.value))
				   {
				   	alert("The Netmask is invalid !");
					s.Mask.select();
					s.Mask.focus();
					return false;
				   }
				   s.submit();
			}
			function	formCheckQServer(s)
			{
				if(!checkNumberInput(s.timeout, 5, 300))
					return false;
				if(s.URL.value!="")
				{
					if(!checkURLInput(s.URL))
						return false;		
				}
				s.submit();				
			}
			function	checkURLInput(obj)
			{
				var URLvalue = obj.value;
				var splitURL=URLvalue.split("/");
				var HostIP = splitURL[2];
				if(URLvalue.indexOf("http://")!=0 && URLvalue.indexOf("https://")!=0)
				{
					alert("Invalid URL, should be http://x.x.x.x/...... or https://x.x.x.x/......");
					obj.select();
					obj.focus();		
					return false;
				}
				if(!ipCheckValue(HostIP))
				{
					obj.select();
					obj.focus();		
					return false;
				}
				return true;
			}
			function ipCheckValue(textValue)
			{
				if (textValue.search(" ") != -1)
				{
					alert("Invalid IP Address.");
					return false;
				}

				var Check = textValue.indexOf('.');
				
				if(Check==-1)
				{
					alert("Invalid IP Address.");
					return false;
				}
				else
				{       
					var ipSplit=textValue.split('.');
					if(ipSplit.length!=4)
					{
						alert("Invalid IP Address.");
						return false;
					}

					for(i=0; i<ipSplit.length; i++)
					{
						if(isNaN(ipSplit[i]))
						{
							alert("Invalid IP Address.");
							return false;
						}
					}

					for(i=0; i<ipSplit.length; i++)
					{
						if(ipSplit[i]>255 || ipSplit[i]<0)
						{
							alert("Invalid IP Address.");
							return false;
						}
					}
					return true;
				}
			}
			function DelIt(iTag)
			{
				document.formUServerDel[iTag].submit();
			}
			function Init()
			{
				if(JWAC_state == '0')
				{
					document.getElementById("S1").checked = false;
					document.getElementById("S2").checked = true;
				}
				else
				{
					document.getElementById("S1").checked = true;
					document.getElementById("S2").checked = false;
				}
				
				document.formJWACsetting.virtualIP.value=JWAC_Settings[0];
				document.getElementById("failover").value=failover;
				document.formJWACsetting.httpPort.value = JWAC_Settings[1];
				document.formJWACsetting.UDPFiltering.value=JWAC_Settings[3];
				document.formJWACsetting.ForcibleLogout.value = JWAC_Settings[4];
				document.formJWACsetting.RadiusProtocol.value=JWAC_Settings[5];
				document.formJWACsetting.RedirectState.value = JWAC_Settings[6];
				document.formJWACsetting.destination.value=JWAC_Settings[7];
				document.formJWACsetting.delaytime.value = JWAC_Settings[8];	
				document.formQServer.timeout.value= QServer_Settings[0];
				document.formQServer.monitor.value = QServer_Settings[1];
				document.formQServer.URL.value = QServer_Settings[2];				
			}	
			function CheckSubnetMask(textValue)
			{
				var subIP=textValue.split('.');
				var i;

				if(subIP.length!=4)
					return false;

				for(i=0; i<subIP.length; i++)
					{
						if(isNaN(subIP[i]))  return false;
					}

				if(subIP[0]==0 && subIP[1]==0 && subIP[2]==0 && subIP[3]==0)
					return false;

				for(i=0;i<3;i++)
				{
					if(subIP[i]<subIP[i+1])
						return false;
				}

				for(i=0;i<4;i++)
				{
					if(subIP[i] != 255 && subIP[i] != 254 && subIP[i] != 252 && subIP[i] != 248 && subIP[i] != 240 && subIP[i] != 224
					&& subIP[i] != 192 && subIP[i] != 128 && subIP[i] != 0)
						return false;
				}

				for(i=0;i<3;i++)
				{
					if(subIP[i]!= 255 && subIP[i+1] !=0)
						return false;
				}

				return true;

			}
		</script>
	</div>

