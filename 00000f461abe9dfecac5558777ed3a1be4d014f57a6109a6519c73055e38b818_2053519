<script language="Javascript">
function add_trust_host()
{
	var s = document.formTrustHost;

	if (!ipCheck(s.ip_addr))
		return ;

/* by lightwater 10.25 check net mask */
	if (s.net_mask.value !="")
	{
		if (!MaskCheck(s.net_mask.value))
		{
			alert("Invalid Net Mask !");
			return ;
		}
	}
	
	document.formTrustHost.isDel_All.value = '0';
	document.formTrustHost.isDel.value = '0';
	
	document.formTrustHost.submit();
}

function del_all()
{
	document.formTrustHost.isDel_All.value = '1';
	document.formTrustHost.isDel.value = '0';
	document.formTrustHost.submit();
}

function delit(iTag)
{
	document.formTrustHost.isDel_All.value = '0';
	document.formTrustHost.isDel.value = '1';

	document.formTrustHost.h_ip.value = HostIP_Setting[iTag][1];
	document.formTrustHost.h_mask.value = HostIP_Setting[iTag][2];
	document.formTrustHost.submit();
}
</script>
