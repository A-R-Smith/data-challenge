	<script language="Javascript">
		function getTrunkType(type) 
		{
			if(type=='0')
				return "Static";
			else
				return "LACP";
		}
		function getStatusType(type) 
		{
			if(type=='0')
				return "Disabled";
			else
				return "Enabled";
		}	
		function floodPortType(port) 
		{
			if(port=='0')
				return "";
			else
				return port;
		}	
		function getTrunkPortNumber_withComma (portMember)
		{
			var portValue = '';
			var firtTime = true;
			var iCounter = 0;	
			var  k;
		
			for (k=1; k<=portMember.length; k++){
				if (portMember.substring(k-1, k) == '1'){
				if(firtTime == false)
					portValue += ', ';
				firtTime = false;
				portValue += k;
				}
			}
			return portValue;
		}
	</script>
</head>

