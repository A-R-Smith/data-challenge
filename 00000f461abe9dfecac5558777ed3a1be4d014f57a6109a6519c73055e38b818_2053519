<script language="Javascript">
    function warning()
    {
    	var len=document.pass_set.T1.value.length;
	if(len< 10 || len > 64)
	{
		alert("Engine ID length error ! length is   (10-64)!");
		document.pass_set.T1.select();
		document.pass_set.T1.focus();
		return false;
	}
	if(!checkHexInput(document.pass_set.T1))
		return false;		
	document.pass_set.submit();
    }


</script>
