<script language="Javascript">
function press_enter(event)
{
	if (event.keyCode == 13)
	{
		return false;
	}
}

function applyForm()
{
	if (!checkNumberInput(document.formSSLGlobal.timeout, 60, 86400))
		return false;

	document.formSSLGlobal.submit();
	return true;
}

function applyCipher()
{
	document.getElementById("formSSLCiphersuite").submit();
}

function applyForm1()
{
	var f = document.formSSLCertificate;
	if (!ipCheck(f.R6))
		return false;

	if (!checkTxtInput(f.R7) || !checkTxtInput(f.R8))
		return false;

	document.formSSLCertificate.submit();
	return true;
}

</script>
