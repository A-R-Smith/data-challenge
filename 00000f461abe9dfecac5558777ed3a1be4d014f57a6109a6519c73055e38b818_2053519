<script language="Javascript">
	function showportpanel(unitid, portnum)
	{
		var panstr = "";
		var ports_per_line = (Port_Number.length > 20) ? Port_Number.length /2 : Port_Number.length;
		var ports_in_line;
		
		panstr += "<table id=mainGray2 class=mainGrayNormal cellpadding=0 cellspacing=0 style=\"width:100%;\"><tbody>";
		panstr += "<tr class=gh><td width=60>&nbsp; Port</td><td></td>";
		// first line ports

		for( i=1; i<=ports_per_line; i++)
		{
			if (i < 10)
			{
				panstr += "<td>&nbsp;0" + i + "</td>";
			}
			else
			{
				panstr += "<td>&nbsp;" + i + "</td>";
			}
		}
		panstr += "</tr>";
		panstr += "<tr class=gw><td></td><td></td>";
		for(i=1; i<=ports_per_line; i++)
			panstr += "<td><input type='checkbox' name='C'"+i+"' id=C"+i+" onclick='showPorts("+i+");'></td>";
		panstr += "</tr>";
		
		ports_in_line = portnum - ports_per_line;
		if(ports_in_line > 0) {
			panstr += "<tr class=gh><td>&nbsp; </td><td></td>";
			for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
				panstr += "<td>&nbsp;" + i + "</td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
			panstr += "<tr class=gw><td>&nbsp; </td><td></td>";
			for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
				panstr += "<td><input type='checkbox' name='C"+i+"' id=C"+i+" onclick='showPorts("+i+");'></td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
		}
				
		panstr += "</tbody></table>";
		document.getElementById('portpanel').innerHTML = panstr;		
	}
	/*showportpanel(1, parent.Port_number);*/
	showportpanel(1, Port_Number.length); /* changed by King Wang, 20081212 */
</script>
	</td></tr>
	<tr><td>Ports</td><td colspan=8><input class=flatL type="text" name="portSpan" id=portSpan readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:auto;text-align:left; width=100%' onmouseover="ddrivetip(this.value)"; onMouseout="hideddrivetip()"></td></tr>
	</tbody>
	</table>
</div>
	<p><b><font color='red'>Note:</font></b>&nbsp;Maximum 8 ports in a static trunk or LACP group.</p>
</form>
</div>
