function doSelectPage(pageFile)
{
   	currentPortHTML=''+pageFile;
	gCurrentPortPage=currentPortHTML;
	rbottom.location.href=currentPortHTML+'?0,'+gCurrentUnit+','+gCurrentSlot+','+gCurrentPort;
		
}


