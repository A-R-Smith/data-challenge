<script language="Javascript">
	function showportpanel(unitid, portnum)
	{
		var panstr = "";
		var ports_per_line = (parent.port_number > 20) ? parent.port_number/2 : parent.port_number;

		var i, ports_in_line;
   		 if (unitid > 6) portnum = 32;
		panstr += "<table id=mainGray2 class=mainGrayNormal cellpadding=0 cellspacing=0 width=100%><tbody>";
		panstr += "<tr class=gh><td width=80>&nbsp; Port</td>";
		// first line ports
		for( i=1; i<=ports_per_line; i++)
		{
			if (i < 10)
			{
				panstr += "<td>&nbsp;0" + i + "</td>";
			}
			else
			{
				panstr += "<td>&nbsp;" + i + "</td>";
			}
		}
		panstr += "</tr>";
		panstr += "<tr class=gw><td>&nbsp;<input type=button value=\"All\" class=btnAll onClick=\"SelAll();\"></td>";
		for(i=1; i<=ports_per_line; i++) 
		{
			panstr += "<td><input type='checkbox' name='C"+ i+ "' id=C"+ i+ " onclick='showPorts("+i+");'></td>";
		}
		panstr += "</tr>";
		
		// second line if neccessary
		ports_in_line = portnum - ports_per_line;
		if(ports_in_line > 0) 
		{		
			panstr += "<tr class=gh><td width=80>&nbsp; </td>";
			for( i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td>&nbsp;" + i + "</td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
			panstr += "<tr class=gw><td>&nbsp; </td>";
			for(i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td><input type='checkbox' name='C"+ i+ "' id=C"+ i+ " value='1' onclick='showPorts("+i+");'></td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
		}
		panstr += "</tbody></table>";
		document.getElementById('portpanel').innerHTML = panstr;		
	}
	showportpanel(1, parent.port_number);

	function initshowport(portmember)
	{
		var checkboxobj;
		for(i=1;i<=parent.port_number;i++)
	      {
	      		//if (portmember.substring(i-1, i) == '1')
	      		//	{alert('1');
	      		//		checkboxobj = document.getElementsByName('C'+i);
			//		checkboxobj.checked=true;
					
			//	}
			checkboxobj = document.getElementById('C'+i);
			if (portmember.substring(i-1, i) == '1')
				checkboxobj.checked = true;
			else
				checkboxobj.checked = false;
				
	 	   	//GuestVlan_Setting[2][unitid-1] = newval;
	 		
		}
		ListPorts(Guest_VLAN[1][0]);
	}
	
</script>
	<tr><td>Ports:</td><td><input class=flatL type="text" name="PortSpan" id=PortSpan readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:200;text-align:left; width=100%'
 onmouseover="ddrivetip(this.value)"; onMouseout="hideddrivetip()"></td></tr>
         </td></tr>
