<script language="Javascript">    
function delit(iTag)
{
	parent.delStart(iTag);
}

function EditStart(iTag)
{
	var bFLAG=false;
	var iTag_m, i;

	if(MSTI_VID_LIST.length !=1 )
	{
		for(i=0; i<MSTI_VID_LIST.length; i++)
		{
			if (document.formMSTP_VID_List.editApply[i].style.display=='none')
			{
				bFLAG=false;
				iTag_m=' ';
			}    		
			else
			{
				bFLAG=true;
				iTag_m = i;
				break;
			}
		}
	
		if((iTag!=0|iTag!=MSTI_VID_LIST.length)&bFLAG==true)
		{	
			document.formMSTP_VID_List.vidList[iTag_m].style.display = '';
			document.formMSTP_VID_List.vidList_M[iTag_m].style.display = 'none';
			
			document.formMSTP_VID_List.editButton[iTag_m].style.display = '';
			document.formMSTP_VID_List.editApply[iTag_m].style.display = 'none';
		}

		document.formMSTP_VID_List.vidList[iTag].style.display = 'none';
		document.formMSTP_VID_List.vidList_M[iTag].style.display = '';		
		document.formMSTP_VID_List.vidList_M[iTag].value = MSTI_VID_LIST[iTag][1];
		document.formMSTP_VID_List.vidList_M[iTag].select();

		document.formMSTP_VID_List.editButton[iTag].style.display = 'none';
		document.formMSTP_VID_List.editApply[iTag].style.display = '';
	}
	else
	{
		document.formMSTP_VID_List.vidList.style.display = 'none';
		document.formMSTP_VID_List.vidList_M.style.display = '';
		document.formMSTP_VID_List.vidList_M.value = MSTI_VID_LIST[i][1];
		document.formMSTP_VID_List.vidList_M.select();

		document.formMSTP_VID_List.editButton.style.display = 'none';
		document.formMSTP_VID_List.editApply.style.display = '';
	}

}

function EditApplyIt(iTag)
{
	if(MSTI_VID_LIST.length !=1 )
	{
		if (!check_vlanlist(document.formMSTP_VID_List.vidList_M[iTag]))
		{
			return false;
		}
		
		document.formMSTP_VID_List.vidList[iTag].style.display = '';
		document.formMSTP_VID_List.vidList_M[iTag].style.display = 'none';

		document.formMSTP_VID_List.editButton[iTag].style.display = '';
		document.formMSTP_VID_List.editApply[iTag].style.display = 'none';
	}
	else
	{
		if (!check_vlanlist(document.formMSTP_VID_List.vidList_M))
		{
			return false;
		}
		
		document.formMSTP_VID_List.vidList.style.display = '';
		document.formMSTP_VID_List.vidList_M.style.display = 'none';

		document.formMSTP_VID_List.editButton.style.display = '';
		document.formMSTP_VID_List.editApply.style.display = 'none';
	}
	
	document.formMSTP_VID_List.isEdit.value = 1;
	document.formMSTP_VID_List.isDel.value = 0;
	document.formMSTP_VID_List.T4.value=MSTI_VID_LIST[iTag][0];
	document.formMSTP_VID_List.T5.value=document.getElementsByName("vidList_M")[iTag].value;
	document.formMSTP_VID_List.submit();

}

parent.msti_count = MSTI_VID_LIST.length;
parent.show_counter();

</script>
<script src="../../DataStore/error_msg.js" type="text/javascript"></script>
</body>

