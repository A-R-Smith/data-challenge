<div id="tableContainer" class="tableContainer"> 
			<div id="dhtmltooltip"></div>
			<script type="text/javascript" src="../tooltip.js"></script>
			<table id=mainGray class=mainGray style = "width:100%" cellpadding=0 cellspacing=0>
				<tr class=gh>
					<!--<td >Unit</td>-->
					<td>Drive</td>
					<td>Media Type</td>
					<td>Size (MB)</td>
					<td>Label</td>
					<td>File System Type</td>
					<td><!-- buttons --></td>
				</tr>
				<script language="Javascript">   
					if(parent.FS_StorageMedia_Error != '')
					{
						document.write("<tr>");
						document.write("  <td colspan='6'><b>&nbsp;&nbsp;" + parent.FS_StorageMedia_Error + "</b></td>");
						document.write("</tr>");
					}
					else
					{
						for(i=0; i<parent.FS_StorageMedia.length; i++)
						{
							if (i%2 == 0)
							{
								trClass = 'gx';
								txtClass = 'flatGX';
							}
							else
							{
								trClass = 'gw';
								txtClass = 'flatGX';
							}			
							document.write("<tr class=" + trClass + ">");
							//document.write("  <td>" + parent.FS_StorageMedia[i][0] + "</td>");		//Unit
							document.write("  <td><a id='aDrive' href='javascript:aDrive_Click(" + i + ")'>" + parent.FS_StorageMedia[i][1] + "</a></td>");		//Drive
							document.write("  <td>" + parent.FS_StorageMedia[i][2] + "</td>");		//Media Type
							document.write("  <td>" + parent.FS_StorageMedia[i][3] + "</td>");		//Size
							tipStr = rp_special_char(strNormalize(parent.FS_StorageMedia[i][4]));		//Label
							tipStr = tipStr.replace(/&amp;nbsp;/g, "&nbsp;");
							document.write("<td onMouseover=keytip('" + tipStr +"'); onMouseout='hideddrivetip()'>" + rp_special_char(strTruncate(parent.FS_StorageMedia[i][4], 35)) + "</td>");
							document.write("  <td>" + parent.FS_StorageMedia[i][5] + "</td>");		//File System Type
							document.write("  <td align='right'><input type=button id=btnFormat name=btnFormat value='Format' class='btnInGrayS' onClick='btnFormat_Click(" + i + ")' /></td>");
							document.write("</tr>");
						}
					}
				</script>
			</table>
