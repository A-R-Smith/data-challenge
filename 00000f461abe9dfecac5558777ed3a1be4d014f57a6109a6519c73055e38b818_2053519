	  <script language="javascript">
	 function apply()
	{
		document.auth_set_jp.def_flag.value = 0;
		
		if(!(checkBytesLength(document.getElementById("ptitle"), 128)))
		{
			return false;
		}
		
		if(!(checkBytesLength(document.getElementById("auth_login"), 32)))
		{
			return false;
		}

		if(!(checkBytesLength(document.getElementById("pusername"), 16)))
		{
			return false;
		}

		if(!(checkBytesLength(document.getElementById("ppassword"), 16)))
		{
			return false;
		}
		
		if(!(checkBytesLength(document.getElementById("logout"), 32)))
		{
			return false;
		}

		if(!checkspace(document.auth_set_jp.ptitle.value) || !checkspace(document.auth_set_jp.auth_login.value) ||!checkspace(document.auth_set_jp.pusername.value)
			||!checkspace(document.auth_set_jp.ppassword.value) ||!checkspace(document.auth_set_jp.logout.value))
		{
			return;
		}
		document.auth_set_jp.submit();
	}
	function intial()
	{
		var f = document.auth_set_jp;

		f.ptitle.value = rp_special_char(Customize_Page_Settings_JP[0]);
		f.auth_login.value = rp_special_char(Customize_Page_Settings_JP[1]);
		f.pusername.value = Customize_Page_Settings_JP[2];
		f.ppassword.value = rp_special_char(Customize_Page_Settings_JP[3]);
		f.logout.value = rp_special_char(Customize_Page_Settings_JP[4]);
	}
	intial();
	function changeAll()
	{
		document.auth_set_jp.def_flag.value = 1;
		document.auth_set_jp.submit();
	}
	 function checkspace(obj)
	 {
		var ch = "";
		var i = 1;
		while(i <= obj.length) 
		{
			ch += " ";
			i++;
		}
		if(ch == obj) 
		{
			alert("The parameter can not be null!");
			return false;
		}
		return true;
	}
	  </script>
