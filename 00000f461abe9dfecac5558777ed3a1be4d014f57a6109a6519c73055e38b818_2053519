		<script language="Javascript">
			function	formCheck(s)
			{
				var ipaddress = s.IPAddress.value;
				var timeout = s.timeout.value;
				var times = s.times.value;
				if(!ipCheck(s.IPAddress))
					return false;
				if(!Check_IPAddr( s.IPAddress,false))
					return false;
				if(!checkNumberInput(s.timeout , 1, 99))
					return false;
				if(s.infinite[0].check ==true)
					s.repeattime.value = 0;
				else if(s.infinite[1].checked ==true)
				{
					if(!checkNumberInput(s.times,1,255))
						return false;
					s.repeattime.value = s.times.value;
					
				}
				var ip = ipaddress.split(".");
				document.location.href = "77_Ping_Test_Result.html?"+ ip[0] +","+ip[1]+","+ip[2]+","+ip[3]+","+ s.repeattime.value +","+timeout  ; /* ip[0],ip[1],ip[2],ip[3], repeat, timeout */
			}

			function formCheckIPv6(s)
			{
				var ipaddress = s.IPAddress.value;
				var timeout = s.timeout.value;
				var times = s.times.value;
				var size = s.size.value;
				
				if(!ipv6_check(s.IPAddress))
					return false;
				if(!checkNumberInput(s.timeout , 1, 10))
					return false;
				if(!checkNumberInput(s.size , 1, 6000))
					return false;				
				if(s.infinite[0].check ==true)
					s.repeattime.value = 0;
				else if(s.infinite[1].checked ==true)
				{
					if(!checkNumberInput(s.times,1,255))
						return false;
					s.repeattime.value = s.times.value;
					
				}
				s.submit();
			}
			function checkTimes(s) 
			{
				if (s.infinite[0].checked)
					s.times.disabled = true;
				else
					s.times.disabled = false;
			}
			function Check_IPAddr (obj, zero_ip_allowed)
			{
				var ip_addr=obj.value;
			    	var   retVal = true;
				var  ipTemp;
				var  array_ip = ip_addr.split(".");
				if (!zero_ip_allowed)
				{
					if(parseInt(array_ip[0])+parseInt(array_ip[1])+parseInt(array_ip[2])+parseInt(array_ip[3])==0)
						retVal = false;
				}

				/* Broadcast IP(255.255.255.255). */
			    	if (parseInt(array_ip[0])+parseInt(array_ip[1])+parseInt(array_ip[2])+parseInt(array_ip[3])==1020)
			    	{
			        	retVal = false;
			    	}
				ipTemp =parseInt(array_ip[0],10);
				/* First part is ZERO.(0.XXX.XXX.XXX)*/
			    	if(ipTemp == 0 && (parseInt(array_ip[0])+parseInt(array_ip[1])+parseInt(array_ip[2])+parseInt(array_ip[3])!= 0))
			    	{
			        	retVal = false;
			    	}
				/* Loopback IP(127.XXX.XXX.XXX). */
			    	else if (ipTemp == 127)
			    	{
			        	retVal = false;
			    	}
				/* First part big than 223(223.XXX.XXX.XXX). */
			    	else if(ipTemp >= 224)
			    	{
					retVal = false;
			    	}
				if(!retVal)
				{
					obj.select();
					obj.focus();
					alert("Invalid host address !");
				}
			    	return retVal;
			}
		</script>
	</div>

