	<script language="javascript">
		function OnPressNext()
		{
			var reUrl="77_AttackSystemLog_table.html?1,2";
			document.location.href=reUrl;
		}
		function OnPressPre()
		{
			var reUrl="77_AttackSystemLog_table.html?1,3";
			document.location.href=reUrl;
		}
		function OnPressClear()
		{
			var reUrl="77_AttackSystemLog_table.html?1,4";
			document.location.href=reUrl;
		}
		function Init()
		{
			if(ButtonPreState=='0')
			{
				document.formMSTI.prebutton.disabled=true;
			}
			if(ButtonNextState=='0')
			{
				document.formMSTI.nextbutton.disabled=true;		
			}
		}
	</script>
