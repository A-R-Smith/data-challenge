<script language="Javascript">


function delStart(iTag)
{
	document.formMSTP_VID_List.isDel.value = 1;
	document.formMSTP_VID_List.T4.value=MSTI_VID_LIST[iTag][0];
	document.formMSTP_VID_List.submit();
}
    
function applyform()
{
	var s = document.formMST_CfgIdf;
	if (!checkTxtInput(s.T2))
		return;

	if (!checkNumberInput(s.T3, 0, 65535))
		return;
	
	document.formMST_CfgIdf.submit();
}

function applyform1()
{
	var mstid = document.formMSTP_VID_List.T4;
	var vidlist = document.formMSTP_VID_List.T5;

	if(mstid.value != 'CIST')
	{
		if (!checkNumberInput(document.formMSTP_VID_List.T4, 1, 15))
			return;
	}

	/*
	if(!checkVID(vidlist,vidlist.value))
		return false;
	*/
	/* changed by King Wang, 20090227 */
	if (!check_vlanlist(vidlist))
	{
		return false;
	}

	document.formMSTP_VID_List.isDel.value = 0;
	document.formMSTP_VID_List.submit();
}

var msti_count;

function show_counter()
{
	var str = "<b>Total Entries: "+ msti_count+"</b>";
		document.getElementById('msti_counter').innerHTML = str;
}
</script>
</head>

