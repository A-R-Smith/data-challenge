	   <tr><td>Priv-Protocol by Password</td>
	   <td><select style="width:180" name=priv_pro_by_pass id=priv_pro_by_pass onchange="checkDES1();"  class=SNMPV3EncryptSel>
	   			<option value="1">None</option>
	   			<option value="2">DES</option></select></td>
	   <td>Password</td>
