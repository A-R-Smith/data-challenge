<script>
function show_prev()
{
	parent.location.href = '55_Browse_VLAN.html?1,0';
}

function show_next()
{
	parent.location.href = '55_Browse_VLAN.html?2,0';
}

function intial()
{
	var f = document.formBrowse_VLAN;
	if (Mon_VLAN[5] == 3)
	{
		f.prev.disabled = false;
		f.next.disabled = false;
	}
	else if (Mon_VLAN[5] == 1)
		f.prev.disabled = false;
	else if (Mon_VLAN[5] == 2)
		f.next.disabled = false;
}
intial();
</script>

