<body class=body228>
<form name=formBrowse_Router id=formBrowse_Router>
  <table name=mainGray id=mainGray class=mainGray cellpadding="0" cellspacing="0" width=100%>
   <tr class='gh'>
   <script>
   if (Stacking_Mode == '1')
   	document.wirte("<td width=30 rowspan='3' >Unit</td>");
    </script>
    <td align=center colSpan=26>Port</td></tr>

<script language="Javascript">
	for(i = 0; i < QVLAN_Setting.length; i++)
	{
          	//if(IGMP_SNP_Settings[i][1] == vlanname)
		if(parseInt(QVLAN_Setting[i][0], 10) == vlanid)
		{
          		iTag = i;
          		break;
          	}
          }

   	document.write("<tr class=gh>");

	for(i=0; i<Port_Number.length; i++)
	{
		if(i >= 26)
		{
			document.write("</tr><tr class=gh>");
		}
		for(i=0; i<Port_Number.length; i++)
		{
			if(parseInt(Port_Number[i]%2)==1)
				document.write("  <td>" + Port_Number[i] + "</td>");
		}

		document.write("</tr><tr class=gh>");

		for(i=0; i<Port_Number.length; i++)
		{
			if(parseInt(Port_Number[i]%2)==0)
				document.write("  <td>" + Port_Number[i] + "</td>");
			}
	}

   	if(Port_Number.length > 26)
   	{
   	   	document.write("</tr>");
   	}

	document.write("</tr>");

	if (Stacking_Mode == '1')
	{
		for(i=0; i<Unit_Setting.length; i++)
		{
			if (i%2 == 0)
				trClass = 'gx';
			else
				trClass = 'gw';

			document.write("<tr class=" + trClass + ">");
			document.write("<td rowspan='2'>" + Unit_Setting[i] + "</td>");

			for(j=0; j<Port_Number.length; j++)
			{
				if(parseInt(Port_Number[j]%2)==1)
					document.write("  <td>"+Router_Port[i][j]+"</td>");
			}

			document.write("</tr><tr class="+trClass+">");
			for(j=0; j<Port_Number.length; j++)
			{
				if(parseInt(Port_Number[j]%2)==0)
				{
					document.write("  <td>"+Router_Port[i][j]+"</td>");
				}
			}
			document.write("</tr>");
		}
	}
	else
	{
		trClass = 'gx';
		document.write("<tr class=" + trClass + ">");

		for(j=0; j<Port_Number.length; j++)
		{
			if(parseInt(Port_Number[j]%2)==1)
			{
				if (MLD_SNP_Router_Port[iTag][4][0].substring(j, j+1) == 1)
					document.write("<td>D</td>");
				else if (MLD_SNP_Router_Port[iTag][2][0].substring(j, j+1) == 1 && MLD_SNP_Router_Port[iTag][3][0].substring(j, j+1) == 1)
					document.write("<td>SF</td>");
				else if (MLD_SNP_Router_Port[iTag][2][0].substring(j, j+1) == 1)
					document.write("<td>S</td>");
				else if (MLD_SNP_Router_Port[iTag][3][0].substring(j, j+1) == 1)
					document.write("<td>F</td>");
				else
					document.write("<td>&nbsp;</td>");
			}
		}

		document.write("</tr><tr class="+trClass+">");
		for(j=0; j<Port_Number.length; j++)
		{
			if(parseInt(Port_Number[j]%2)==0)
			{
				if (MLD_SNP_Router_Port[iTag][4][0].substring(j, j+1) == 1)
					document.write("<td>D</td>");
				else if (MLD_SNP_Router_Port[iTag][2][0].substring(j, j+1) == 1 && MLD_SNP_Router_Port[iTag][3][0].substring(j, j+1) == 1)
					document.write("<td>SF</td>");
				else if (MLD_SNP_Router_Port[iTag][2][0].substring(j, j+1) == 1)
					document.write("<td>S</td>");
				else if (MLD_SNP_Router_Port[iTag][3][0].substring(j, j+1) == 1)
					document.write("<td>F</td>");
				else
					document.write("<td>&nbsp;</td>");
			}
		}
		document.write("</tr>");
	}
    
    document.write("</tr>");
</script>
                </table>
                <script language="Javascript">
                  document.write("<br>");
                  document.write("<b>Note:</b>&nbsp;S:Static Router Port ,&nbsp;&nbsp;D:Dynamic Router Port, &nbsp;&nbsp;F:Forbidden Router Port");
                </script>
</form>


