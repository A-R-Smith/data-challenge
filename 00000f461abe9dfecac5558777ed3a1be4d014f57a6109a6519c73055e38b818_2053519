<script language="Javascript">
function chgToPortIndex1(control,index)
{
  if(!control.options[0].selected)  // non-any
  {
     if(control.options[1].selected ==true)   // mac
     {
       if(!document.formSTPGlobal.D2.options[0].selected)      
       {
         document.getElementById('D2').options[1].selected = true;
         document.getElementsByName("T1")[0].readOnly=false;
         document.getElementsByName("T2")[0].readOnly=false;
         document.getElementById("com11").style.display="none";
         document.getElementById("com21").style.display="none";
         document.getElementById("com1").style.display="block";
         document.getElementById("com2").style.display="block";
       }
       else
       {
         document.getElementsByName("T1")[0].readOnly=false;
         document.getElementById("com11").style.display="none";
         document.getElementById("com1").style.display="block";
       }
       var service_obj = document.getElementById('D3');
       service_obj.options.length = 0;
       service_obj.options[0] = new Option('Any', '1');
       service_obj.options[1] = new Option('EtherType', '2');
     }
    else  //IP
    {
       if(!document.formSTPGlobal.D2.options[0].selected)
       {
         document.getElementById('D2').options[2].selected = true;
         document.getElementsByName("T1")[0].readOnly=false;
         document.getElementsByName("T2")[0].readOnly=false;
         document.getElementById('com1').style.display="none";
         document.getElementById('com2').style.display="none";
         document.getElementById('com11').style.display="block";
         document.getElementById('com21').style.display="block";
        }
        else
        {
          document.getElementsByName("T1")[0].readOnly=false;
          document.getElementById("com1").style.display="none";
          document.getElementById("com11").style.display="block";
        }
       var service_obj = document.getElementById('D3');
       service_obj.options.length = 0;
       service_obj.options[0] = new Option('Any', '1');
       service_obj.options[1] = new Option('ICMP', '2');
       service_obj.options[2] = new Option('IGMP', '3');
       service_obj.options[3] = new Option('TCP', '4');
       service_obj.options[4] = new Option('TCP Source Port', '5');
       service_obj.options[5] = new Option('TCP Destination Port', '6');
       service_obj.options[6] = new Option('UDP', '7');
       service_obj.options[7] = new Option('UDP Source Port', '8');
       service_obj.options[8] = new Option('UDP Destination Port', '9');
    }    
  }
  else  //any
  {
       document.getElementsByName("T1")[0].readOnly=true;
        document.getElementById("com1").style.display="none";
       document.getElementById("com11").style.display="none";
       
       if(document.getElementById('D2').value==1)
       	document.getElementById('D3').disabled=false;
  }
   if(document.getElementById("D1").options.selectedIndex==2)
     {
        document.getElementsByName("T3")[0].readOnly=true;
        document.getElementById("com31").style.display="none";   
     }
  globalChg[index]=true  
}
function chgToPortIndex2(control,index)
{
  if(!control.options[0].selected)
  {
    if(control.options[1].selected ==true) 
    {
       if(!document.formSTPGlobal.D1.options[0].selected)
       {
         document.getElementById('D1').options[1].selected = true;
         document.getElementsByName("T1")[0].readOnly=false;
         document.getElementsByName("T2")[0].readOnly=false;
         document.getElementById("com11").style.display="none";
         document.getElementById("com21").style.display="none";
         document.getElementById("com1").style.display="block";
         document.getElementById("com2").style.display="block";
       }
       else
       {
         document.getElementsByName("T2")[0].readOnly=false;
         document.getElementById("com21").style.display="none";
         document.getElementById("com2").style.display="block";
       }
       var service_obj = document.getElementById('D3');
       service_obj.options.length = 0;
       service_obj.options[0] = new Option('Any', '1');
       service_obj.options[1] = new Option('EtherType', '2');
    }
    else   //ip
    {
       if(!document.formSTPGlobal.D1.options[0].selected)
       {
         document.getElementById('D1').options[2].selected = true;
         document.getElementsByName("T1")[0].readOnly=false;
         document.getElementsByName("T2")[0].readOnly=false;
         document.getElementById('com1').style.display="none";
         document.getElementById('com2').style.display="none";
         document.getElementById('com11').style.display="block";
         document.getElementById('com21').style.display="block";
       }
       else
       {
          document.getElementsByName("T2")[0].readOnly=false;
          document.getElementById("com2").style.display="none";
          document.getElementById("com21").style.display="block";
       }
       var service_obj = document.getElementById('D3');
       service_obj.options.length = 0;
       service_obj.options[0] = new Option('Any', '1');
       service_obj.options[1] = new Option('ICMP', '2');
       service_obj.options[2] = new Option('IGMP', '3');
       service_obj.options[3] = new Option('TCP', '4');
       service_obj.options[4] = new Option('TCP Source Port', '5');
       service_obj.options[5] = new Option('TCP Destination Port', '6');
       service_obj.options[6] = new Option('UDP', '7');
       service_obj.options[7] = new Option('UDP Source Port', '8');
       service_obj.options[8] = new Option('UDP Destination Port', '9');
    }
  }
  else
  {
      document.getElementsByName("T2")[0].readOnly=true;
      document.getElementById("com2").style.display="none";
      document.getElementById("com21").style.display="none";

      if(document.getElementById('D1').value==1)
      	document.getElementById('D3').disabled=false;
  }
   if(document.getElementById("D1").options.selectedIndex==2)
     {
        document.getElementsByName("T3")[0].readOnly=true;
        document.getElementById("com31").style.display="none";   
     }
  globalChg[index]=true  
}

function chgToPortIndex3(control,index)
{
     if((control.options.selectedIndex==1) || (control.options.selectedIndex==2) || (control.options.selectedIndex==3 || control.options.selectedIndex==6))
     {
        document.getElementById("com3").style.display="none";
        document.getElementsByName("T3")[0].readOnly=true;
     }
     else if(control.options.selectedIndex==4 || control.options.selectedIndex==5 || control.options.selectedIndex==7 || control.options.selectedIndex==8)
     {
         document.getElementsByName("T3")[0].readOnly=false;
         document.getElementById("com3").style.display="block";
     }
     else
     {
       document.getElementById("com3").style.display="none";
       document.getElementsByName("T3")[0].readOnly=false;
     }
      if((document.getElementById("D3").options.selectedIndex==1 && document.getElementById("D1").options.selectedIndex==1) || (document.getElementById("D3").options.selectedIndex==1 && document.getElementById("D2").options.selectedIndex==1))
     {
        document.getElementsByName("T3")[0].readOnly=false;
        document.getElementById("com31").style.display="block";   
     }
     else
        {document.getElementById("com31").style.display="none";
        document.getElementsByName("T3")[0].readOnly=true;}  
     globalChg[index]=true
}
function chgToPortIndex4(control,index)
{ 
		 if(control.options.selectedIndex==0)
     {
        document.getElementsByName("T4")[0].readOnly=false;
        document.getElementById("com41").style.display="none";
        document.getElementById("com4").style.display="block";
     }
     else if(control.options.selectedIndex==1)
     {
          document.getElementsByName("T4")[0].readOnly=false;
          document.getElementById("com4").style.display="none";
          document.getElementById("com41").style.display="block";
     }
     else
     {
         document.getElementById("com4").style.display="none";
         document.getElementById("com41").style.display="none";
         document.getElementsByName("T4")[0].value=''; 
     }
     globalChg[index]=true
}
function chgToPortIndex5(control,index)
{ 
		 if(control.options.selectedIndex==0)
     {
        document.getElementsByName("Tz")[0].readOnly=false;
        document.getElementById("comz1").style.display="none";
        document.getElementById("comz").style.display="block";
     }
     else if(control.options.selectedIndex==1)
     {
          document.getElementsByName("Tz")[0].readOnly=false;
          document.getElementById("comz").style.display="none";
          document.getElementById("comz1").style.display="block";
     }
     else
     {
         document.getElementById("comz").style.display="none";
         document.getElementById("comz1").style.display="none";
         document.getElementsByName("Tz")[0].value=''; 
     }
     globalChg[index]=true
}
function default_value()
{
   document.getElementsByName("T1")[0].value="";
   document.getElementsByName("T2")[0].value="";
   document.getElementsByName("T3")[0].value="";
   document.getElementsByName("T4")[0].value="";
   document.getElementsByName("T5")[0].value="";
   document.getElementById('D1').options[0].selected=true;
   document.getElementById('D2').options[0].selected=true;
   document.getElementById('D3').options[0].selected=true;
   document.getElementById('D4').options[0].selected=true;
   document.getElementsByName("T1")[0].readOnly=true;
   document.getElementsByName("T2")[0].readOnly=true;
   document.getElementsByName("T4")[0].readOnly=true;
   alert('Ok');
}

