		<script language="Javascript">
			var d = document.form1xAuthMod;
			var d2 = document.form1xAuthSetting;
			function formCheck(s)
			{
				if(s.agetime_flag.checked == false)
				{
					   if(!checkNumberInput(s.T1,1,1440)) 
					   {
						  return false;
					   }   
				}
				if(!checkNumberInput(s.T2,0,10)) 
				{
				  return false;
				}   
				if(s.idletime_flag.checked == false)
				{				  
					   if(!checkNumberInput(s.T3,1,1440)) 
					   {
						  return false;
					   }   	
				}
				if(!checkNumberInput(s.T4,0,300)) 
				{
				  return false;
				}   
				s.submit();
				return true;
			}
			function checkBoxState()
			{
				if(document.form1xAuthSetting.agetime_flag.checked == true)
					document.form1xAuthSetting.T1.disabled = true;
				else 
					document.form1xAuthSetting.T1.disabled = false;
				if(document.form1xAuthSetting.idletime_flag.checked == true)
					document.form1xAuthSetting.T3.disabled = true;
				else 
					document.form1xAuthSetting.T3.disabled = false;
			}


			function Init()
			{
				document.form1xAuthSetting.T1.value = '1440';			
				//document.form1xAuthSetting.T2.value = '10';
				document.form1xAuthSetting.T4.value = '0';
				document.form1xAuthSetting.agetime_flag.checked = false;
				document.form1xAuthSetting.idletime_flag.checked = true;
				checkBoxState();
			}	
		</script>
	</div>
	<script src="../../DataStore/error_msg.js" type="text/javascript"></script>
</body>
