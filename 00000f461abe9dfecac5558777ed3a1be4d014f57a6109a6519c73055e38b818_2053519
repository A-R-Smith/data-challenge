function doPortLink(iFunc,iUnit,iSlot,iPort)
{
	var linkhref;

	if (iUnit==0) gCurrentUnit=1;
	else gCurrentUnit=iUnit;
	if (iSlot==0) gCurrentSlot=1;
	else gCurrentSlot=iSlot;
	if (iPort==0) gCurrentPort=1;
	else gCurrentPort=iPort;

    	rbottom.location.href = gCurrentPortPage + '?0,'+gCurrentUnit+','+gCurrentSlot+','+gCurrentPort;
}
function doUnitLink(iUnit)
{

	if (iUnit==0)
		gCurrentUnit=1;
	else
		gCurrentUnit=iUnit;

	Timer_deviceRefresh=1;		rRefresh.location.href="deviceRefresh.html?"+gCurrentUnit+","+gCurrentSlot;
}

