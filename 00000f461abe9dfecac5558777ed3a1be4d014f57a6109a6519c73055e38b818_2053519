        if(d.C5[2].checked == true) k=k+1;
        if(d.C6[2].checked == true) k=k+1;
        if(d.C7[2].checked == true) k=k+1;
        if(d.C8[2].checked == true) k=k+1;
        if(d.C9[2].checked == true) k=k+1;
        if(d.C10[2].checked == true) k=k+1;
