	  <script language="javascript">
	 function apply()
	{
		document.auth_set.def_flag.value = 0;
		/*
		if(document.auth_set.ptitle.value == "" ||document.auth_set.auth_login.value == "" ||document.auth_set.pusername.value == "" 
		    ||document.auth_set.ppassword.value == "" ||document.auth_set.logout.value == "" )
		{
			alert("The parameter can not be null!");
			return;
		}*/
		if(!checkspace(document.auth_set.ptitle.value) || !checkspace(document.auth_set.auth_login.value) ||!checkspace(document.auth_set.pusername.value)
			||!checkspace(document.auth_set.ppassword.value) ||!checkspace(document.auth_set.logout.value))
		{
			return;
		}
		document.auth_set.submit();
	}
	function intial()
	{
		var f = document.auth_set;

		f.ptitle.value = rp_special_char(Customize_Page_Settings[0]);
		f.auth_login.value = rp_special_char(Customize_Page_Settings[1]);
		f.pusername.value = rp_special_char(Customize_Page_Settings[2]);
		f.ppassword.value = rp_special_char(Customize_Page_Settings[3]);
		f.logout.value = rp_special_char(Customize_Page_Settings[4]);
	}
	intial();
	 function changeAll()
	{
		document.auth_set.def_flag.value = 1;
		document.auth_set.submit();
	}
	 function checkspace(obj)
	 {
		var ch = "";
		var i = 1;
		while(i <= obj.length) 
		{
			ch += " ";
			i++;
		}
		if(ch == obj) 
		{
			alert("The parameter can not be null!");
			return false;
		}
		return true;
	}
	  </script>
