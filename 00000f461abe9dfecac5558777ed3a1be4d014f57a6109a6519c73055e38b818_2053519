<div id="portpanel"></div>
<script language="Javascript">
	function showportpanel(unitid, portnum)
	{
		var panstr = "";
		var ports_per_line = (Port_Number.length > 20) ? Port_Number.length /2 : Port_Number.length;
		var i, ports_in_line;
		panstr += "<table id=mainGray2 class=mainGrayNormal cellpadding=0 cellspacing=0 width=100%><tbody>";
		panstr += "<tr class=gh><td width=80>&nbsp; Sniffer Mode</td>";
		// first line ports
		for( i=1; i<=ports_per_line; i++)
		{
			if (i < 10)
			{
				panstr += "<td>&nbsp;0" + i + "</td>";
			}
			else
			{
				panstr += "<td>&nbsp;" + i + "</td>";
			}
		}
		panstr += "</tr>";
		panstr += "<tr class=gw><td>&nbsp; TX</td>";
		for(i=1; i<=ports_per_line; i++) {
			panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='T' checked disabled onclick='showPorts("+i+");'></td>";
		}
		panstr += "</tr>";
		panstr += "<tr class=gx><td>&nbsp; RX</td>";
		for(i=1; i<=ports_per_line; i++) {
			panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='R' checked disabled onclick='showPorts("+i+");'></td>";
		}
		panstr += "</tr>";
		panstr += "<tr class=gw><td>&nbsp; Both</td>";
		for(i=1; i<=ports_per_line; i++) {
			panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='B' checked disabled onclick='showPorts("+i+");'></td>";
		}
		panstr += "</tr>";
		panstr += "<tr class=gx><td>&nbsp; None</td>";
		for(i=1; i<=ports_per_line; i++) {
			panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='0' checked disabled onclick='showPorts("+i+");'></td>";
		}
		panstr += "</tr>";
		
		// second line if neccessary
		ports_in_line = portnum - ports_per_line;
		if(ports_in_line > 0) {		
			panstr += "<tr class=gh><td width=80>&nbsp; </td>";
			for( i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td>&nbsp;" + i + "</td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
			panstr += "<tr class=gw><td>&nbsp; TX</td>";
			for(i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='T' checked disabled onclick='showPorts("+i+");'></td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
			panstr += "<tr class=gx><td>&nbsp; RX</td>";
			for(i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='R' checked disabled onclick='showPorts("+i+");'></td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
			panstr += "<tr class=gw><td>&nbsp; Both</td>";
			for(i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='B' checked disabled onclick='showPorts("+i+");'></td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";
			panstr += "<tr class=gx><td>&nbsp; None</td>";
			for(i=ports_per_line+1; i<=ports_per_line + ports_in_line; i++)
				panstr += "<td><input type='radio' name='C"+ i+ "' id=C"+ i+ " value='0' checked disabled onclick='showPorts("+i+");'></td>";
			for( i=0; i<ports_per_line - ports_in_line; i++)
				panstr += "<td></td>";
			panstr += "</tr>";

		}		

		panstr += "</tbody></table>";
		document.getElementById('portpanel').innerHTML = panstr;		
	}
	showportpanel(1, parent.port_number);
</script>
</td></tr>
<tr><td width="5%">TX</td><td><input class=flatL type="text" name="txSpan" id=txSpan readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:200;text-align:left; width=100%' disabled	onmouseover="ddrivetip(this.value)"; onMouseout="hideddrivetip()"></td></tr>
<tr><td>RX</td><td><input class=flatL type="text" name="rxSpan" id=rxSpan readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:200;text-align:left; width=100%' disabled	onmouseover="ddrivetip(this.value)"; onMouseout="hideddrivetip()">
        
