<script language="Javascript">
var total_count;
function show_counter()
{
	var str = "<b>Total Entries:&nbsp;"+ total_count + "</b>";
	document.getElementById('dot1v_port_counter').innerHTML = str;
}

function find()
{
	if (!check_portlist(document.formDot1vPort.find_list))
		return;
	
	location.href = "34_Protocol_VLAN_Port.html?1," + document.formDot1vPort.find_list.value;
}

function show_all()
{	
	location.href = "34_Protocol_VLAN_Port.html?0,0";
}
function del_all()
{
	if (!check_portlist(document.formDot1vPort.find_list))
	{
		return;
	}
	
	document.formDot1vPort.DelAll.value = '1';
	document.formDot1vPort.isDel.value = '0';
	document.formDot1vPort.submit();
}

function delit(port, gid)
{
	
	document.formDot1vPort.del_port.value = port;
	document.formDot1vPort.del_gid.value = gid;
	document.formDot1vPort.isDel.value = '1';
	document.formDot1vPort.DelAll.value = '0';
	document.formDot1vPort.submit();
}
function apply_check()
{
	var s = document.formDot1vPort;

	if (s.group_flag[0].checked)
	{
		s.h_group_flag.value = 0;
		if (Dot1v_Group_Settings.length != 0)
		{
			s.groupname.value = Dot1v_Group_Settings[s.group_name.value][1];
		}
		else
		{
			alert("Protocol group id does not exist!");
			return false;
		}
	}
	else
	{
		s.h_group_flag.value = 1;
		if (Dot1v_Group_Settings.length != 0)
		{
			s.groupname.value = Dot1v_Group_Settings[s.group_name.value][1];
		}
		else
		{
			alert("Protocol group name does not exist!");
			return false;
		}
	}

	if (s.vlan_flag[0].checked)
	{
		if (!checkNumberInput(s.vid, 1, 4094))
			return;

		s.h_vlan_flag.value = 0;
	}
	else
	{
		if (!checkTxtInput(s.vname))
			return;

		s.h_vlan_flag.value = 1;
	}

	if (s.all_port.checked == false)
	{
		if (!check_portlist(s.port))
			return false;
	}



/*
	if (s.all_group.checked == false)
	{
		if (s.group.selectedIndex == 0)
		{
			if(isNaN(gid) || gid < 1 || gid > 16)
			{
				alert("Protocol Group ID range : 1-16 !");
				s.gid.select();
				s.gid.focus();
				s.gid.value = 1;
				return false;
			}
		}
		else
		{
			if (!checkTxtInput(s.gid))
				return false;
		}
	}

	if (s.action.selectedIndex == 0)
	{
		if (s.vlan_sel.selectedIndex == 0)
		{
			if (!checkNumberInput(s.vlan_value, 1, 4094))
				return;
		}
		else if (s.vlan_sel.selectedIndex == 1)
		{
			if (!checkTxtInput(s.vlan_value))
				return;
		}
	}

	if (s.all_port.checked == false)
	{
		if (!check_portlist(s.port))
			return false;
	}
*/
		
	document.formDot1vPort.isDel.value = '0';
	document.formDot1vPort.DelAll.value = '0';
	document.formDot1vPort.submit();
}

function checkAll(index)
{
	var s = document.formDot1vPort;

	switch (index)
	{
		case 1:
			if (s.all_port.checked == true)
			{
				document.formDot1vPort.all_port_flag.value = '1'
				s.port.readOnly = true;
				s.port.style.backgroundColor='#D4D0C8';
			}
			else
			{
				document.formDot1vPort.all_port_flag.value = '0'
				s.port.readOnly = false;
				s.port.style.backgroundColor='#FFFFFF';
			}
				
			break;
		case 2:
			if (s.all_group.checked == true)
			{
				document.formDot1vPort.all_gid_flag.value = '1'
				s.group.disabled = true;
				s.gid.readOnly = true;
			}
			else
			{
				document.formDot1vPort.all_gid_flag.value = '0'
				s.group.disabled = false;
				s.gid.readOnly = false;
			}
				
			break;
		default:
			break;
	}
}

/*
function changeAction(control,obj)
{
	var s = document.formDot1vPort;
	
	if (control.selectedIndex == 1)
	{
		obj.value=1;
		s.vlan_sel.disabled = true;
		s.vlan_value.disabled = true;
		s.all_group.disabled = false;
	}
	else
	{
		s.vlan_sel.disabled = false;
		s.vlan_value.disabled = false;
		s.all_group.disabled = true;
	}
}

function changeGroup()
{
	var s = document.formDot1vPort;
	if (s.group.selectedIndex == 0)
	{
		s.id_flag.value = 1;
		s.name_flag.value = 0;
		
	}
	else if (s.group.selectedIndex == 1)
	{
		s.name_flag.value = 1;
		s.id_flag.value = 0;
		
	}	
}

function changevlan()
{
	var s = document.formDot1vPort;
	if (s.group.vlan_sel == 0)
	{
		s.vlan_flag.value = 0;
		
	}
	else if (s.group.selectedIndex == 1)
	{
		s.vlan_flag.value = 1;
	}	
}
*/

function change_group_flag(index)
{
	var f = document.formDot1vPort;
	if (index == 0)
	{
		f.group_id.disabled = false;
		f.group_name.disabled = true;
	}
	else
	{
		f.group_id.disabled = true;
		f.group_name.disabled = false;
	}
}

function change_vlan_flag(index)
{
	var f = document.formDot1vPort;
	if (index == 0)
	{
		f.vid.disabled = false;
		f.vname.disabled = true;
		f.vname.style.backgroundColor='#D4D0C8';
		f.vid.style.backgroundColor='#FFFFFF';
	}
	else
	{
		f.vid.disabled = true;
		f.vname.disabled = false;
		f.vid.style.backgroundColor='#D4D0C8';
		f.vname.style.backgroundColor='#FFFFFF';
	}
}

function init()
{
	var s = document.formDot1vPort;
	
	change_group_flag(0);
	change_vlan_flag(0);
}
init();
//changeAction(document.formDot1vPort.action, document.formDot1vPort.group);

</script>
</div>
