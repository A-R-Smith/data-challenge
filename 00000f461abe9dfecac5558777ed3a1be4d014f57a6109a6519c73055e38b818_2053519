vnameOrvid(document.getElementById("radVlan"));
setBoxByChk('boxPorts','chkAll');

/*
if(window.screen.height < 1024)
{
	document.getElementById("ifrTable").style.height="160px";
}
else
{
	document.getElementById("ifrTable").style.height="380px";
}
*/

function checkobj(obj)
{
	if(obj.value == 1)
	{
		document.getElementById('boxPorts').disabled = true;
		document.getElementById('boxPorts').style.backgroundColor = "#D4D0C8";
		document.getElementById('chkAll').disabled = true;
	}
	else
	{
		document.getElementById('boxPorts').disabled = false;
		document.getElementById('boxPorts').style.backgroundColor = "";
		document.getElementById('chkAll').disabled = false;
	}
}
checkobj(document.getElementById("rad"));
</script>
