	<script language="Javascript">
		function Choose_Check(value)
		{
			if(value == 1)
			{
				document.getElementById('Radius_Server_6').disabled = true;
				document.getElementById('Radius_Server_4').disabled = false;
			}
			else 
			{
				document.getElementById('Radius_Server_4').disabled = true;
				document.getElementById('Radius_Server_6').disabled = false;
			}
		}
	</script>
	<script language="Javascript">
		function EditStart(iTag)
		{
			document.formRADIUSGlobal.Succession.value = _1x_RADIUS_Server[iTag][0];
			if(_1x_RADIUS_Server[iTag][8] == 0)
			{
				document.formRADIUSGlobal.Radius_Server_6.value = '';
				Choose_Check(1);
				document.getElementById('Radius_Server_Choose0').checked=false;
				document.getElementById('Radius_Server_Choose1').checked=true;
				document.formRADIUSGlobal.Radius_Server_4.value = _1x_RADIUS_Server[iTag][1];
			}
			else
			{
				document.formRADIUSGlobal.Radius_Server_4.value = '';
				Choose_Check(0);
				document.getElementById('Radius_Server_Choose1').checked=false;
				document.getElementById('Radius_Server_Choose0').checked=true;
				document.formRADIUSGlobal.Radius_Server_6.value = _1x_RADIUS_Server[iTag][1];
			}
			document.formRADIUSGlobal.Auth_Udp_Port.value = _1x_RADIUS_Server[iTag][2];
			document.formRADIUSGlobal.Acct_Udp_Port.value = _1x_RADIUS_Server[iTag][3];
			document.formRADIUSGlobal.Timeout.value = _1x_RADIUS_Server[iTag][5];
			document.formRADIUSGlobal.Retransmit.value = _1x_RADIUS_Server[iTag][6];
		}
		function formCheck(s)
		{
			var Key=s.Key.value;

			if(document.getElementById('Radius_Server_6').disabled == true)
			{
				if(!ipCheck(s.Radius_Server_4))
				{
					return false;
				}
			}
			else
			{
				if(!ipv6_check(s.Radius_Server_6))
				{
					return false;
				}
			}
			if(!checkNumberInput(s.Auth_Udp_Port,1,65535) )
			{  
				return false;
			}

			if(!checkNumberInput(s.Acct_Udp_Port,1,65535) )
			{  
				return false;
			}    
			if(!checkNumberInput(s.Timeout,1,255) )
			{  
				return false;
			}
			if(!checkNumberInput(s.Retransmit,1,255) )
			{  
				return false;
			}

			if(!checkTxtInput(s.Key))
			{
				return false;
			}
			s.submit();
			return true;
		}
		


	</script>
	<script src="../../DataStore/error_msg.js" type="text/javascript"></script>
