function goEdit(iTag)
{
	location.href = "23_MLD_Router.html?"+MLD_SNP_Settings[iTag][0];
}
    
function goView(iTag)
{
	location.href = '54_MLD_Snooping_Group.html?' + MLD_SNP_Settings[iTag][0] + ",0";
}


