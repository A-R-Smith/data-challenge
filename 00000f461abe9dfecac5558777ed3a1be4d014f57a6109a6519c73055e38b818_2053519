	<script language="Javascript">
		function  setStat(oRad)
		{
			if(oRad.id == "radAdd")
			{
				/* for add */
				document.all["boxIp"].disabled = false;
				document.all["boxIp"].style.backgroundColor = "";
				document.all["boxIp"].focus();
				document.all["boxMac"].disabled = false;
				document.all["boxMac"].style.backgroundColor = "";
				document.all["boxPort"].disabled = false;
				document.all["boxPort"].style.backgroundColor = "";
				/* for del */
				document.all["boxIp_Del"].disabled = true;
				document.all["boxIp_Del"].style.backgroundColor = "#D4D0C8";
				document.all["boxMac_Del"].disabled = true;
				document.all["boxMac_Del"].style.backgroundColor = "#D4D0C8";
				document.all["boxPort_Del"].disabled = true;
				document.all["boxPort_Del"].style.backgroundColor = "#D4D0C8";
				/* for filter */
				document.all["boxPort_Filter"].disabled = true;
				document.all["boxPort_Filter"].style.backgroundColor = "#D4D0C8";
				document.all["selStat"].disabled = true;
				document.all["selStat"].style.backgroundColor = "#D4D0C8";
			}
			else if(oRad.id == "radDel")
			{
				/* for add */
				document.all["boxIp"].disabled = true;
				document.all["boxIp"].style.backgroundColor = "#D4D0C8";
				document.all["boxMac"].disabled = true;
				document.all["boxMac"].style.backgroundColor = "#D4D0C8";
				document.all["boxPort"].disabled = true;
				document.all["boxPort"].style.backgroundColor = "#D4D0C8";
				/* for del */
				document.all["boxIp_Del"].disabled = false;
				document.all["boxIp_Del"].style.backgroundColor = "";
				document.all["boxIp_Del"].focus();
				document.all["boxMac_Del"].disabled = false;
				document.all["boxMac_Del"].style.backgroundColor = "";
				document.all["boxPort_Del"].disabled = false;
				document.all["boxPort_Del"].style.backgroundColor = "";
				/* for filter */
				document.all["boxPort_Filter"].disabled = true;
				document.all["boxPort_Filter"].style.backgroundColor = "#D4D0C8";
				document.all["selStat"].disabled = true;
				document.all["selStat"].style.backgroundColor = "#D4D0C8";
			}
			else /* if(oRad.id == "radFilter") */
			{
				/* for add */
				document.all["boxIp"].disabled = true;
				document.all["boxIp"].style.backgroundColor = "#D4D0C8";
				document.all["boxMac"].disabled = true;
				document.all["boxMac"].style.backgroundColor = "#D4D0C8";
				document.all["boxPort"].disabled = true;
				document.all["boxPort"].style.backgroundColor = "#D4D0C8";
				/* for del */
				document.all["boxIp_Del"].disabled = true;
				document.all["boxIp_Del"].style.backgroundColor = "#D4D0C8";
				document.all["boxMac_Del"].disabled = true;
				document.all["boxMac_Del"].style.backgroundColor = "#D4D0C8";
				document.all["boxPort_Del"].disabled = true;
				document.all["boxPort_Del"].style.backgroundColor = "#D4D0C8";
				/* for filter */
				document.all["boxPort_Filter"].disabled = false;
				document.all["boxPort_Filter"].style.backgroundColor = "";
				document.all["boxPort_Filter"].focus();
				document.all["selStat"].disabled = false;
				document.all["selStat"].style.backgroundColor = "";
			}
		}

		function cfgDhcpFilter()
		{
			if( document.getElementById("radAdd").checked==true )
			{
				if( !ipCheck(document.getElementById("boxIp")) )
				{
					return false;
				}
				if(document.all["boxMac"].value != "")
				{
					if( !MAC_Address_Check(document.all["boxMac"]) )
					{
						return false;
					}
				}
				if( !check_portlist(document.all["boxPort"]) )
				{
				alert(document.all["boxPort"].value);
					return false;
				}
				document.all["hidOpr"].value = "add";
			}
			else if( document.getElementById("radDel").checked==true )
			{
				if( !ipCheck(document.getElementById("boxIp_Del")) )
				{
					return false;
				}
				if(document.all["boxMac_Del"].value != "")
				{
					if( !macCheck(document.all["boxMac_Del"].value) )
					{
						return false;
					}
				}
				if( !check_portlist(document.all["boxPort_Del"]) )
				{
					return false;
				}
				document.all["hidOpr"].value = "del";
			}
			else //if(document.getElementById("radFilter").checked==true)
			{
				if( !check_portlist(document.getElementById("boxPort_Filter")) )
				{
					return false;
				}
				document.all["hidOpr"].value = "filter";
			}
			document.getElementById("frmDHCPServerScreening").submit();
		}
	</script>
</head>
