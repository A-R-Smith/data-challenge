			var s=document.forms[0];

			var UnitNum = 1;
			var portNum=Port_Number.length ;
			var rowNum=1+parseInt(portNum, 10);

			if(portNum>47)
				document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=1500 HEIGHT=1600>");
			else
				document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=1500 HEIGHT=800>");
			document.write("<param NAME=DataPath value=\"../../html/Hjava_AuthSession.html?"+iPort+"\">" );
			document.write("<param NAME=AppletName value=\"Authenticator Session Statistics\">");
			document.write("<param NAME=TimeIntervalName value=\"Time Interval\">");
			document.write("<param NAME=PortNum value=5>");
			document.write("<param NAME=SlotNum value=",UnitNum,">");
			document.write("<param NAME=MajorTable value=0>");
			document.write("<param NAME=TableNum value=1>");
			document.write("<param NAME=Table0RowNum value=\""+rowNum+"\">");
			document.write("<param NAME=Table0Row0 value=Port>");
			for(i=1; i<=portNum; i++)
				document.write("<param NAME=\"Table0Row"+i+"\" value=\""+i+"\">");
			document.write("<param NAME=ColumNum value=10>");
			document.write("<param NAME=Colum0 value=&nbsp;>");
			document.write("<param NAME=Colum1 value=\"Octets RX\">");
			document.write("<param NAME=Colum2 value=\"Octets TX\">");
			document.write("<param NAME=Colum3 value=\"Frames RX\">");
			document.write("<param NAME=Colum4 value=\"Frames TX\">");
			document.write("<param NAME=Colum5 value=ID>");
			document.write("<param NAME=Colum6 value=AuthenticMethod>");
			document.write("<param NAME=Colum7 value=Time>");
			document.write("<param NAME=Colum8 value=TerminateCause>");
			document.write("<param NAME=Colum9 value=UserName>");
			document.write("</applet>");
		}

	</script>
