function del_all()
{
	document.ACLProfileForm.isDel_All.value = '1';
	document.ACLProfileForm.set_cpu.value = '0';
	document.ACLProfileForm.submit();
}

function set_cpu_filter()
{
	document.ACLProfileForm.set_cpu.value = '1';
	document.ACLProfileForm.isDel_All.value = '0';
	document.ACLProfileForm.submit();
}

