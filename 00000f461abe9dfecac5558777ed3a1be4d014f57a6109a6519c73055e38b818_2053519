	<script language="Javascript">
	  	RunJava();
		Init();
		function apply()
		{
			var port=document.forms[0].D1.value;
			var reUrl="53_Packet_UMB_cast_table.html?1,"+port;
			document.location.href=reUrl;

		}
		function Init()
		{
			document.forms[0].D1.selectedIndex = port_num -1;
		}
		function cleartable()
		{
			var szMsg='Clear the counters ?';
			if(confirm(szMsg))
			{
				return true;
			}
			else
				return false;
		}
		function RunJava()
		{
			document.write("<applet codebase=\"../classes\" code=\"MultiTable5.class\"  WIDTH=600 HEIGHT=400>");
			document.write("<param NAME=DataPath value=\"../../html/Hjava_Packets.html?"+port_num+"\">" );
			document.write("<param NAME=PortNum value="+port_num+">");
			document.write("<param NAME=SlotNum value=",1,">");
			document.write("<param NAME=appletName value=\"Port: "+port_num+"\">");
			document.write("<param NAME=MajorTable value=0>");
			document.write("<param NAME=TableNum value=3>");
			document.write("<param NAME=Table0RowNum value=3>");
			document.write("<param NAME=Table0Row0 value=RX&nbsp;Packets>");
			document.write("<param NAME=Table0Row1 value=Bytes>");
			document.write("<param NAME=Table0Row2 value=Packets>");
			document.write("<param NAME=Table1RowNum value=4>");
			document.write("<param NAME=Table1Row0 value=RX&nbsp;Packets>");
			document.write("<param NAME=Table1Row2 value=Multicast>");
			document.write("<param NAME=Table1Row3 value=Broadcast>");
			document.write("<param NAME=Table1Row1 value=Unicast>");
			document.write("<param NAME=Table2RowNum value=3>");
			document.write("<param NAME=Table2Row0 value=TX&nbsp;Packets>");
			document.write("<param NAME=Table2Row1 value=Bytes>");
			document.write("<param NAME=Table2Row2 value=Packets>");
			document.write("<param NAME=ColumNum value=3>");
			document.write("<param NAME=Colum0 value=Packets>");
			document.write("<param NAME=Colum1 value=Total>");
			document.write("<param NAME=Colum2 value=Total/sec>");
			document.write("</applet>");
		}

	</script>
