<script language="Javascript">
var i;

parent.acl_profile_list.Clear;
parent.acl_profile_list.length = 0;
for (i = 0; i < ACLProfile_Detail.length; i++)
{
	parent.acl_profile_list[i] = ACLProfile_Detail[i];
}

var lastidx = -1;
if (acl_style == 1)
	parent.max_acl_profile_num = (ACLProfile_Num >= 14) ? 200 : 6;
else
	parent.max_acl_profile_num = 5;

function PrintProfile(index)
{
	var outstr = '';

	if (acl_style == 2)
		outstr += "<table name=aclProfileDetail id=aclProfileDetail class=aclProfileDetail cellpadding=0 cellspacing=0>";
	else
		outstr += "<table name=aclProfileDetail id=aclProfileDetail class=aclProfileDetail2 cellpadding=0 cellspacing=0>";
	outstr += "<tr><td colspan=2><b>Profile Details</b></td></tr><tr><td colspan=2>&nbsp;</td></tr>";

	for(i=0; i<ACLProfile_Detail[index].length; i++)
	{
		if(ACLProfile_Detail[index][i] != '')
		{
			outstr += "<tr><td stype=\"font-size: 11pt; font-weight:bold;\">" + ACLProfile_Name[i] + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>";
			outstr += "<td style='text-align: left;'>: &nbsp;" + ACLProfile_Detail[index][i] + "</td></tr>";
		}
	}
	outstr += "</table>";
	
	document.getElementById('aclPD').innerHTML = outstr;
}

function Swdetail(profile_index)
{
location.href = "52_AccessProfile_Info.html?" + profile_index + "," + acl_style;
/*
