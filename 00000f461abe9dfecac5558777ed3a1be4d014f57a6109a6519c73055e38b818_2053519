<script language="Javascript">    
    function delit(iTag)
    {
		document.formVLAN[iTag].CName.value=SNMP_Community_Setting[iTag][0] ;
		document.formVLAN[iTag].submit();
    }

	parent.snmp_comm_count = SNMP_Community_Setting.length;
	parent.show_counter();

</script>
