<script language="Javascript">
function PrintProfile(index)
{
	var outstr = '';
	var trClass = 'gx';
	
	outstr += "<table name=mainGray id=mainGray class=mainGray cellpadding=0 cellspacing=0 width=60%>";
	if (acl_style == 1)
		outstr += "<tr class=gh><td colspan=2><b>ACL Profile Details</b></td></tr>";
	else
		outstr += "<tr class=gh><td colspan=2><b>CPU ACL Profile Details</b></td></tr>";

	for(i=0; i<parent.acl_profile_list[index].length; i++)
	{
		if(parent.acl_profile_list[index][i] != '')
		{
			outstr += "<tr class=" + trClass +"><td width=30% stype=\"font-size: 11pt; font-weight:bold;\">" + ACLProfile_Name[i] + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>";
			outstr += "<td style='text-align: left;'>" + parent.acl_profile_list[index][i] + "</td></tr>";

			if (trClass == 'gw')
				trClass = 'gx';
			else
				trClass = 'gw';
		}
	}

	outstr += "<tr><td></td></tr>";
	outstr += "<tr><td></td></tr>";
	outstr += "<tr><td><input type=button class=btnText value='Show All Profiles' onclick=show_all() style='width:110;'></td></tr>";
	outstr += "</table>";
	
	document.getElementById('aclPD').innerHTML = outstr;
}

</script>
