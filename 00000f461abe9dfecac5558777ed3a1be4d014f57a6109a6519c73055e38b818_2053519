<script language="Javascript">
function delRule(profile_id, access_id)
{
	document.aclrulebrowser.pid.value = profile_id;
	document.aclrulebrowser.access_id.value = access_id;
	document.aclrulebrowser.submit();
}

function Openrule(index)
{
	var a_url = '52_AccessRule.html?'+index;	
	Link(a_url);	
}

function show_rule_info(rule_id)
{
	location.href = "52_AccessRule_Info.html?" + rule_id + "," + acl_style;
}

function gonext()
{
	location.href = "52_AccessRule.html?" + profileid + "," + acl_style + ",2";
}

function goprev()
{
	location.href = "52_AccessRule.html?" + profileid + "," + acl_style + ",1";
}

if (ACL_Rule_Navigation_Btn[0] == 1)
	document.aclrulebrowser.bprev.disabled = false;

if (ACL_Rule_Navigation_Btn[1] == 1)
	document.aclrulebrowser.bnext.disabled = false;

function go_back()
{
	location.href = "52_AccessProfile.html?" + acl_style;
}


</script>
</body>
