							{
								document.write("<option value='" + Port_Number[i] + "'>&nbsp;&nbsp;&nbsp;" + Port_Number[i] + "</option>");
							}
							document.write("</select></td>");
							document.write("    <td width=8%>To Port</td>");
							document.write("<td width=8%><select class=portSel name=D2 id=D2 onchange='chkFromPortRight(document.form1xInit.D1, this);'>");
							for(i=0; i<Port_Number.length; i++)
							{
								document.write("<option value='" + Port_Number[i] + "'>&nbsp;&nbsp;&nbsp;" + Port_Number[i] + "</option>");
							}
							document.write("</select></td>");
							if(_1x_Global_Enable=='2')
							{
								document.write("<td width=12%>MAC Address</td>");
