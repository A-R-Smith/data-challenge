<script language="Javascript">
	var trClass = '';
	var txtClass = '';
	var i;
	var str,str2;

    function displayvlan(start_index,end_index)
    {
        str2='';
        str2+="<table name=mainGray id=mainGray cellpadding=0 cellspacing=0 style=\"width:98%;\">";
        str2+="<tr class=gh>";
        str2+="<td width=5% >VID</td>";
        str2+="<td width=15%>VLAN Name</td>";
        str2+="<td width=15%>Leave Timer</td>";
        str2+="<td width=15%>Host Timeout</td>";
        str2+="<td width=15%>Router Timeout</td>";
        str2+="<td width=20%>State</td>";
        str2+="<td width=*>Parameter Settings</td>";
        //str2+="<td width=*>Multicast Entry Table</td>";
        str2+="</tr>";

      for(i=start_index; i<end_index; i++)
      {
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGW';
        }
 
        str2+="<tr class=" + trClass + ">";
        str2+="<td>" + IGMP_SNP_Settings[i][0] + "</td>";
	str2 += "<td align=left  onMouseover=keytip(unescape('" + strNormalize(IGMP_SNP_Settings[i][1]) +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(IGMP_SNP_Settings[i][1], 10)) + "</td>";	
      //  str2+="<td>" + rp_special_char(IGMP_SNP_Settings[i][1]) + "</td>";
        str2+="<td>" + IGMP_SNP_Settings[i][5] + "</td>";
        str2+="<td>" + IGMP_SNP_Settings[i][3] + "</td>";
        str2+="<td>" + IGMP_SNP_Settings[i][4] + "</td>";
        str2+="<td>" + enabledTxt(IGMP_SNP_Settings[i][2]) + "</td>";
	str2+="<td><input type=button name=RouterEdit id=RouterEdit class=btnText value='Edit'  onclick='goEdit("+i+")'></td>";
        //str2+="<td><input type=button name=MultiCEdit id=MultiCEdit class=btnText value='View'  onclick='goView("+i+")'></td>";
        str2+="</tr>";
   
      }

       str2+="<tr><td>&nbsp;</td></tr><tr><td colspan=8 align=right><input id=bprev name='bprev' type=button  value='<<Back' class=btnText onClick=goprev() disabled>";
       str2+="<input id=bnext name='bnext' type=button value='Next>>' class=btnText onClick=gonext() disabled></t></tr>";
       str2+="</table>";

       document.getElementById('vlandiv').innerHTML=str2;
     }

	displayvlan(0, IGMP_SNP_Settings.length);
	
       </script>
       </form>
       
       </div>
</td></tr>
</table>
</div>

<script language="Javascript">    
function goEdit(iTag)
{
	location.href = "23_IGMP_Router.html?"+IGMP_SNP_Settings[iTag][0];
}
    
function goView(iTag)
{
	location.href = '54_IGMP_Snooping_Group.html?' + IGMP_SNP_Settings[iTag][0] + ",0";
}


function gonext()
{
	location.href = "23_IGMP_Snooping.html?2";
}

function goprev()
{
	location.href = "23_IGMP_Snooping.html?1";
}
 </script>


