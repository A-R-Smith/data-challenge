var rh_prefix= new Array("RH00","RH0","RH");
function open_remote(rhid)
{
	var rh_url='/'+rhid+'/';
//+++++++Arthur Chow modify single IP pop error 2004/11/30 03:15U	
/*	
	var rh_target='_'+rhid;
	if(self.rh_win==null)
	{
		self.rh_win= new Array();
	}
	if(self.rh_win[rh_target]==null || self.rh_win[rh_target].closed)
	{
		self.rh_win[rh_target]=open(rh_url,rh_target);
	}
	g_remote_win=self.rh_win[rh_target];
	setTimeout("set_focus_to_remote()", 780);
*/
	parent.parent.openRH(rh_url);	
}

<!-- RpSample -->
var command= new Array("nop", "add", "delete", "dp_interval", "hold_time", "commander", "candidate");
//	[Options:]
//		add			: for CS only, add a CaS to it's GM group via member_id with password.
//		delete		: for CS only, remove a MS from it's GM group via member_id.
//		dp-interval	: config it's dp-interval in sec
//		holdtime	: config it's holdtime in sec
//		commander	: as CS, do nothing.
//					  as MS, do nothing.
//					  as CaS, convert role_state to CS 
//		candidate	: as CS, convert role_state to CaS.
//					  as MS, convert role_state to CaS.
//					  as CaS, do nothing. 
<!-- RpEnd -->
var command= new Array("nop", "add", "delete");
function device_action(index,rhid,pwd)
{
	var password=(pwd!=null)?pwd:"";
	var cgicmd="../../html/SIM_Config.html?"+command[parseInt(index)]+" "+rhid+" "+password;
	parent.fPoll.location.href=cgicmd;
}

function refreshView()
{
	callJavaFunction("appSIM", "refreshView");
}

<!-- RpSample -->
// function callJavaFunction()
//  parameters:
//   arguments[0]: sAppletName, applet name
//   arguments[1]: sFunction, java function name
//   arguments[2]: the parameter of the called function
//     .
//     .
//     .
//   arguments[n]: the parameter of the called function
<!-- RpEnd -->
function callJavaFunction()
{
	var iSize= arguments.length;
	if( iSize < 2)
	{
		alert( "callJavaFunction() error!" );
		return;
	}
	var sAppletName= arguments[0];
	var sFunction= arguments[1];
	var ret;
	var i;

	sFunction+= "(";
	for( i=2; i< iSize; i++)
	{
		sFunction+= arguments[i];
		if( i< iSize - 1)
			sFunction+= ",";
	}
	sFunction+= ")";

	var sEval="ret= document.applets['"+sAppletName+"']."+sFunction;
	eval(sEval);
	//alert(ret);
	//return ret;
}

</script>
</html>

