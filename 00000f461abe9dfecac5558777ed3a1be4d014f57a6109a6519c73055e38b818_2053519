<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
    
	for(i=0; i<MS_TFTP_Info.length; i++)
	{
		if (i%2 == 0)
		{
			trClass = 'gx';
			txtClass = 'flatGX';
		}
		else
		{
			trClass = 'gw';
			txtClass = 'flatGW';
		}

		document.write("<tr class=" + trClass + ">");
		document.write("  <td>&nbsp;&nbsp;" + MS_TFTP_Info[i][0] + "</td>");
		document.write("  <td>" + MS_TFTP_Info[i][1] + "</td>");
		document.write("  <td>" + MS_TFTP_Info[i][2] + "</td>");
		document.write("  <td>" + MS_TFTP_Info[i][3] + "</td>");
		document.write("  <td>");
		if (parseInt(MS_TFTP_Info[i][6], 10) != 0)
			draw_progress_bar(parseInt(MS_TFTP_Info[i][6], 10), 100, '#808080','#e0e0e0');
		else
			document.write(MS_TFTP_Info[i][5]);
		
		document.write("  </td>");
		document.write("</tr>");
	}
