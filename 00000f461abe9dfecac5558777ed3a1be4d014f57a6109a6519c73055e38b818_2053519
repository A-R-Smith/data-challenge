								document.write("<td width=* align=right>");
								document.write("<input type=button value='Apply' class=btnText onClick='formCheck(document.forms[0]); '>");
								function Checkstatus(s)
								{
									if(s.C1.checked==false)
									{
										s.T1.disabled=true;
										s.T1.style.backgroundColor='#D4D0C8';
									}
									else
									{
										s.T1.disabled=false;
										s.T1.style.backgroundColor='#ffffff';
									}
								}
								function Init()
								{
									var s=document.forms[0];
									s.C1.checked=false;
									s.T1.disabled=true;
									s.T1.style.backgroundColor='#D4D0C8';
								}	
								Init();								
								function formCheck(s)
								{
								    
								    var macAddr = s.T1;
		  	  						
									if(s.C1.checked==true)
									{
										s.C1.value = '1';
										if(!MAC_Address_Check(macAddr)) 
										{
											return false;
										}		
									}
									else
										s.C1.value = '0';
									s.submit();
									return true;		   
								}   
							}

							
							else
							{
								document.write("<td width=12%></td>");
								document.write("<td width=25%></td>");		
								document.write("<td width=* align=right>");
								document.write("<input type=submit value='Apply' class=btnText ></td>");						
								
							}
							
						</script>
					</tr>
