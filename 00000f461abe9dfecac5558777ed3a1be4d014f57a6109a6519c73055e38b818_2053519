		document.write("<script src="+linkUrl+" type=\"text\/javascript\"><\/script>");
	 </script>	
       <script language="Javascript">
		function mode2str(value)
	 	{
	 		if(value=='1')
				return 'Forward Unregistered Groups';
			else if(value == '2')
				return 'Filter Unregistered Groups';
	 	}
	 </script>		 	
