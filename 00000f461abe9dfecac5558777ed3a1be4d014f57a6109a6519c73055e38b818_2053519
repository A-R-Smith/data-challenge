<script language="Javascript">

function delit(iTag)
{
	var ip = DHCP_IP_MAC_Port_Settings[iTag][0];
	var mac = DHCP_IP_MAC_Port_Settings[iTag][1];
	var port = DHCP_IP_MAC_Port_Settings[iTag][2];
	
	parent.delit(ip, mac,port);
}
</script>

