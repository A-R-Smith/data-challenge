function chgToPortIndex5(control,index)
{
	if(control.options.selectedIndex==0) 
	{
		init4();
		document.getElementById("AC3").style.display="block";
		document.getElementById("AC4").style.display="none";
		document.getElementById("AC5").style.display="none";
		document.getElementById("AC6").style.display="none";
		document.getElementsByName("A3")[0].readOnly=false;
	}
	else if(control.options.selectedIndex==1) 
	{
		init4();
		document.getElementById("AC3").style.display="none";
		document.getElementById("AC4").style.display="block";
		document.getElementById("AC5").style.display="none";
		document.getElementById("AC6").style.display="none";
		document.getElementsByName("A4")[0].readOnly=false;
	}
	else if(control.options.selectedIndex==2) 
	{
		init4();
		document.getElementById("AC3").style.display="none";
		document.getElementById("AC4").style.display="none";
		document.getElementById("AC5").style.display="block";
		document.getElementById("AC6").style.display="none";
		document.getElementsByName("A5")[0].readOnly=false;
	}
	else if(control.options.selectedIndex==3) 
	{
		init4();
		document.getElementById("AC3").style.display="none";
		document.getElementById("AC4").style.display="none";
		document.getElementById("AC5").style.display="none";
		document.getElementById("AC6").style.display="block";
		document.getElementsByName("A6")[0].readOnly=false;
	}
}
function chgToPortIndex8(control,index)
{
	if(control.options[0].selected ==true) 
	{
		var str = "Apply to Ports";
		document.getElementById("cpu_port").innerHTML = str;
		
		var service_obj = document.getElementById('D32');
		service_obj.options.length = 3;
		service_obj.options[0] = new Option('Any', '1');
		service_obj.options[1] = new Option('Flow Lable', '2');
		service_obj.options[2] = new Option('Class', '3');
		document.getElementById('D3').options[0].selected = true;
		document.getElementById('D31').options[0].selected = true;
		document.getElementById('D32').options[0].selected = true;
		//document.getElementById('D2').options[0].selected = true;
		/*service_obj.options[3] = new Option('TCP All', '4');
		service_obj.options[4] = new Option('TCP Source Port', '5');
		service_obj.options[5] = new Option('TCP Destination Port', '6');
		service_obj.options[6] = new Option('UDP All', '7');
		service_obj.options[7] = new Option('UDP Source Port', '8');
		service_obj.options[8] = new Option('UDP Destination Port', '9');*/
		var service_obj;
		if(g_IsHeightVersion)
		{
			service_obj = document.getElementById('D4');
			service_obj.options.length = 0;
			service_obj.options[0] = new Option('Permit', '1');
			service_obj.options[1] = new Option('Deny', '2');
		}
		
		/*var service_obj = document.getElementById('D6');
		service_obj.options.length = 0;
		service_obj.options[0] = new Option('Ports', '1');*/
	
		document.formACLWizard.D5.disabled=false;
		if(g_IsHeightVersion)
		{
			document.formACLWizard.pro_name.value="";
			document.formACLWizard.pro_name.disabled=true;
			document.formACLWizard.pro_name.style.backgroundColor="#D4D0C8";
		}
		document.formACLWizard.D5.value="1";
		document.getElementById("AC3").style.display="block";
		document.getElementById("AC3").style.display="";
		document.getElementById("AC4").style.display="none";
		document.getElementById("AC5").style.display="none";
		document.getElementById("AC6").style.display="none";
		if(g_IsHeightVersion)
		{
			document.getElementById("D6").style.display="none";
		}
		document.getElementById("profile_cpu").style.display="none";
		document.getElementById("profile_acl").style.display="";
		document.getElementById("acc_cpu").style.display="none";
		document.getElementById("acc_acl").style.display="";
		document.formACLWizard.h_auto.value = "0";
		//document.formACLWizard.auto.disabled=true;
		document.getElementById("cpu_port").style.display="block";
		if(g_IsHeightVersion)
		{
			document.getElementById("apply_to").style.display="none";
		}
		document.getElementById("AC7").style.display="block";
		document.getElementById("AC8").style.display="none";
		document.getElementById("AC9").style.display="none";
		document.getElementById("ST5").style.display="none";
		document.getElementById("ST6").style.display="none";
		document.getElementById("ST7").style.display="none";
		document.getElementById("ST8").style.display="none";
		document.getElementById("ST9").style.display="none";
		document.getElementById("ST10").style.display="none";
		document.getElementById("ST11").style.display="none";
		document.getElementById("ST12").style.display="none";
		document.getElementById("ST13").style.display="none";
		document.getElementById("ST14").style.display="none";
		document.getElementById("ST15").style.display="none";
		document.getElementById("ST17").style.display="none";
		document.getElementById("ST18").style.display="none";
		document.getElementById("ST16").style.display="none";

		document.formACLWizard.auto.disabled=false;	
		document.formACLWizard.auto.checked=false;
		document.formACLWizard.access.disabled =false;
		document.formACLWizard.access.style.backgroundColor="#FFFFFF";
		if(!g_IsHeightVersion)
		{
			document.getElementById("D4").options[2] = null;
		}
	}
	else 
	{
		
		var str;
		if(g_IsHeightVersion)
		{
			str = "Apply to";
		}
		else
		{
			str = "Ports";
		}
		document.getElementById("AC3").style.display="none";
		document.getElementById("AC4").style.display="none";
		document.getElementById("AC5").style.display="none";
		document.getElementById("AC6").style.display="none";
		document.getElementById("apply_to").innerHTML = str;
		//document.formACLWizard.D5.value="1";
		//document.getElementById("AC3").style.display="block";
		var service_obj = document.getElementById('D32');
		service_obj.options.length = 0;
		service_obj.options[0] = new Option('Any', '1');
		service_obj.options[1] = new Option('Flow Lable', '2');
		service_obj.options[2] = new Option('Class', '3');
		/*service_obj.options[3] = new Option('TCP All', '4');
		service_obj.options[4] = new Option('TCP Source Port', '5');
		service_obj.options[5] = new Option('TCP Destination Port', '6');
		service_obj.options[6] = new Option('UDP All', '7');
		service_obj.options[7] = new Option('UDP Source Port', '8');
		service_obj.options[8] = new Option('UDP Destination Port', '9');*/
		document.getElementById('D3').options[0].selected = true;
		document.getElementById('D31').options[0].selected = true;
		document.getElementById('D32').options[0].selected = true;
		//document.getElementById('D2').options[0].selected = true;
		var service_obj;
		if(g_IsHeightVersion)
		{
			service_obj = document.getElementById('D4');
			service_obj.options.length = 0;
			service_obj.options[0] = new Option('Permit', '1');
			service_obj.options[1] = new Option('Deny', '2');
			service_obj.options[2] = new Option('Mirror', '3');
		}
		
		var service_obj;
		if(g_IsHeightVersion)
		{
			service_obj = document.getElementById('D6');
			service_obj.options.length = 0;
			service_obj.options[0] = new Option('Ports', '1');
			service_obj.options[1] = new Option('VLAN Name', '2');
			service_obj.options[2] = new Option('VLAN ID', '3');
		}

		document.formACLWizard.D5.disabled=true;
		if(g_IsHeightVersion)
		{
			document.formACLWizard.pro_name.disabled=false;
			document.formACLWizard.pro_name.style.backgroundColor="#FFFFFF";
		}
		document.getElementById("profile_cpu").style.display="";
		document.getElementById("profile_acl").style.display="none";
		document.getElementById("acc_cpu").style.display="";
		document.getElementById("acc_acl").style.display="none";
		document.formACLWizard.h_auto.value = "1";
		document.getElementById("AC7").style.display="block";
		if(g_IsHeightVersion)
		{
			document.getElementById("D6").style.display="block";
		}
		document.getElementById("apply_to").style.display="block";
		document.getElementById("cpu_port").style.display="none";
		document.getElementById("AC8").style.display="none";
		document.getElementById("AC9").style.display="none";
		document.getElementById("ST5").style.display="none";
		document.getElementById("ST6").style.display="none";
		document.getElementById("ST7").style.display="none";
		document.getElementById("ST8").style.display="none";
		document.getElementById("ST9").style.display="none";
		document.getElementById("ST10").style.display="none";
		document.getElementById("ST11").style.display="none";
		document.getElementById("ST12").style.display="none";
		document.getElementById("ST13").style.display="none";
		document.getElementById("ST14").style.display="none";
		document.getElementById("ST15").style.display="none";
		document.getElementById("ST17").style.display="none";
		document.getElementById("ST18").style.display="none";
		document.getElementById("ST16").style.display="none";
		document.formACLWizard.auto.disabled=true;	
		document.formACLWizard.auto.checked=false;
		document.formACLWizard.access.disabled =false;
		document.formACLWizard.access.style.backgroundColor="#FFFFFF";

		if(!g_IsHeightVersion)
		{
			document.getElementById("D4").options.length = 0;
			addOption(document.getElementById("D4"), "Permit", "1");
			addOption(document.getElementById("D4"), "Deny", "3");
			addOption(document.getElementById("D4"), "Mirror", "2");
			document.getElementById("D4").options[0].selected = true;
		}
	}

	if(document.getElementById("profile_acl").style.display != "none")
	{
		document.getElementById("profile").maxLength = "3";
	}
	else if(document.getElementById("profile_cpu").style.display != "none")
	{
		document.getElementById("profile").maxLength = "1";
	}

	if(document.getElementById("acc_acl").style.display != "none")
	{
		document.getElementById("access").maxLength = "3";
	}
	else if(document.getElementById("acc_cpu").style.display != "none")
	{
		document.getElementById("access").maxLength = "3";
	}
}

function chgToPortIndex9(control,index)
{
	if(control.options[0].selected ==true) 
	{
		document.formACLWizard.D5.value="1";
		if(document.formACLWizard.T6.value==1)
		{
			document.getElementById("AC3").style.display="block";
			document.formACLWizard.D5.disabled=false;
		}

	}
	else 
	{
		document.getElementById("AC3").style.display="none";
		document.getElementById("AC4").style.display="none";
		document.getElementById("AC5").style.display="none";
		document.getElementById("AC6").style.display="none";
		document.formACLWizard.D5.disabled=true;
	}
}
function chgToPortIndex10(control,index)
{
	if (control.options.selectedIndex == 0)
	{
		document.getElementById("AC7").style.display="block";
		document.getElementById("AC8").style.display="none";
		document.getElementById("AC9").style.display="none";
	}
	else if (control.options.selectedIndex == 1)
	{
		document.getElementById("AC7").style.display="none";
		document.getElementById("AC8").style.display="block";
		document.getElementById("AC9").style.display="none";
	}
	else if (control.options.selectedIndex == 2)
	{
		document.getElementById("AC7").style.display="none";
		document.getElementById("AC8").style.display="none";
		document.getElementById("AC9").style.display="block";
	}
}


