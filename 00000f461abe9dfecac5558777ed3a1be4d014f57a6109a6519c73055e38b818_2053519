	<script language="Javascript">
		function apply_state()
		{
			document.ISM_State.submit();
		}
	
		function EditStart(iTag)
		{
			ChangeShow(2);
			document.formISM.VlanName.value = ISM_Setting[iTag][1];
			document.formISM.VID.value = ISM_Setting[iTag][0];
			if(ISM_Setting[iTag][3] == "Disabled")
			{
				document.formISM.state.value = 0;
			}
			else
			{
				document.formISM.state.value = 1;
			}
			document.formISM.ReplaceIP.value = ISM_Setting[iTag][2];
			document.formISM.MemPort.value = ISM_Setting[iTag][4];
			document.formISM.taggedPort.value = ISM_Setting[iTag][6];
			document.formISM.SourcePort.value = ISM_Setting[iTag][5];
		}
	
		function CancelIt()
		{
			Init();
		}
		
		function Modify(Itag)
		{
			//document.location.href="77_ISM_Group_List.html?Vname="+ISM_Setting[Itag][1]+"";
			document.location.href="77_ISM_Group_List.html?"+Itag;
			
		}

		function FormCheck(s)
		{
			var member = s.MemPort.value;
			var taggedport = s.taggedPort.value;
			var source = s.SourcePort.value;
			var replaceip=s.ReplaceIP.value
			if(!checkNumberInput(s.VID, 2, 4094))
			{
				return false;
			}	
			if(!checkTxtInput(s.VlanName))
			{
				return false;
			}
			if(!ipCheck_Setup(replaceip))
			{
				alert("Invalid IP Address!");
				s.ReplaceIP.focus();
				s.ReplaceIP.select();
				return false;
			}
			if(member != "")
			{
				if(!check_portlist(s.MemPort))
					return false;
			}
			if(taggedport != "")
			{
				if(!check_portlist(s.taggedPort))
					return false;
			}
			if(source != "")
			{
				if(!check_portlist(s.SourcePort))
					return false;
			}
			s.submit();
			return true;
		}

		function Init()
		{			
			ChangeShow(1);
			document.formISM.VlanName.value = '';
			document.formISM.VID.value = '';
			document.formISM.state.value ='0';
			document.formISM.ReplaceIP.value = '0.0.0.0';
			document.formISM.MemPort.value = '';
			document.formISM.taggedPort.value = '';
			document.formISM.SourcePort.value = '';
			document.formISM.taggedPort.value = '';
		}

		function ChangeShow(index)
		{	
			if(index==1)  //init
			{
				document.formISM.VlanName.readOnly = false;
				document.formISM.VID.readOnly = false;
				document.formISM.state.disabled =true;
				document.formISM.ReplaceIP.disabled = true;
				document.formISM.MemPort.disabled = true;
				document.formISM.taggedPort.disabled = true;
				document.formISM.SourcePort.disabled = true;
				document.formISM.Apply.style.display = 'none';
				document.formISM.Add.style.display = '';
				document.formISM.operation.value = '2';	
				document.getElementById('ReplaceIP').style.backgroundColor='#D4D0C8';
				document.getElementById('MemPort').style.backgroundColor='#D4D0C8';
				document.getElementById('taggedPort').style.backgroundColor='#D4D0C8';
				document.getElementById('SourcePort').style.backgroundColor='#D4D0C8';
				
			}
			else if(index=2)  //after press edit
			{
				document.formISM.VlanName.readOnly = true;
				document.formISM.VID.readOnly = true;
				document.formISM.state.disabled =false;
				document.formISM.ReplaceIP.disabled = false;
				document.formISM.MemPort.disabled = false;
				document.formISM.taggedPort.disabled = false;
				document.formISM.SourcePort.disabled = false;
				document.formISM.Apply.style.display = '';
				document.formISM.Add.style.display = 'none';
				document.formISM.operation.value = '3';	
				document.getElementById('ReplaceIP').style.backgroundColor='#FFFFFF';
				document.getElementById('MemPort').style.backgroundColor='#FFFFFF';
				document.getElementById('taggedPort').style.backgroundColor='#FFFFFF';
				document.getElementById('SourcePort').style.backgroundColor='#FFFFFF';
			}

			else
			{
				document.formISM.VlanName.value = '';
				document.formISM.VID.value = '';
				document.formISM.state.value = '0';
				document.formISM.state.disabled =true;
				document.formISM.ReplaceIP.value = '0.0.0.0';
				document.formISM.ReplaceIP.disabled = true;
				document.formISM.MemPort.value = '';
				document.formISM.MemPort.disabled = true;
				document.formISM.taggedPort.value = '';
				document.formISM.taggedPort.disabled = true;
				document.formISM.SourcePort.value = '';
				document.formISM.SourcePort.disabled = true;
				document.formISM.operation.value = '2';		
				document.getElementById('ReplaceIP').style.backgroundColor='#D4D0C8';
				document.getElementById('MemPort').style.backgroundColor='#D4D0C8';
				document.getElementById('taggedPort').style.backgroundColor='#D4D0C8';
				document.getElementById('SourcePort').style.backgroundColor='#D4D0C8';
			
			}

		}
		function DeleteIt(s)
		{
			s.submit();
		}
		
		function ipCheck_Setup(textValue)
		{
			if (textValue.search(" ") != -1)
			{
				return false;
			}

			if (textValue == 0)
			{
				return false;
			}
			
			var Check = textValue.indexOf('.');
			if(Check==-1)
			{
				return false;
			}
			else
			{
				var ipSplit=textValue.split('.');
				if(ipSplit.length!=4)
				{
					return false;
				}
				for(i=0; i<ipSplit.length; i++)
				{
					if(isNaN(ipSplit[i]))  return false;
				}

				if(eval(ipSplit[0])+eval(ipSplit[1])+eval(ipSplit[2])+eval(ipSplit[3])==1020)  <!-- RpSample -->// ip=255.255.255.255<!-- RpEnd -->
		       	{
					return false;
				}

				for(i=0; i<ipSplit.length; i++)
				{
					if(ipSplit[i] > 255 || ipSplit[i]<0)
						return false;
				}

				return true;
			}
		}
	</script>
