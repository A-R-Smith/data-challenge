<script language="Javascript">    
    var trClass = '';
    var txtClass = '';
	var i;
    
    for(i=0; i<Multi_Image.length; i++){
        if (i%2 == 0){
            trClass = 'gx';
            txtClass = 'flatGX';
        }
        else{
            trClass = 'gw';
            txtClass = 'flatGX';
        }
        
	document.write("<tr class=" + trClass + ">");
	document.write("  <td>&nbsp;" + Multi_Image[i][2] + "</td>");
	
	if(Multi_Image[i][8] != "")	/* if SDcard file */
	{
		tipStr = strNormalize(Multi_Image[i][8])
		document.write("<td align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(Multi_Image[i][8], 10)) + "</td>");
	}
	else
	{
			document.write("<td align=left>&nbsp;" + "--" + "</td>");
	}
	
	document.write("  <td>" + Multi_Image[i][3] + "</td>");
	document.write("  <td>" + Multi_Image[i][4] + "</td>");
	document.write("  <td>" + Multi_Image[i][5] + "</td>");

	if(Multi_Image[i][8] != "")	/* if SDcard file */
	{
		document.write("<td align=left>&nbsp;" + "--" + "</td>");
	}
	else
	{
		tipStr = strNormalize(Multi_Image[i][6])	//from
		document.write("<td align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(Multi_Image[i][6],20)) + "</td>");
	}

	if(Multi_Image[i][8] != "")	/* if SDcard file */
	{
		document.write("<td align=left>&nbsp;" + "--" + "</td>");
	}
	else
	{
		tipStr = strNormalize(Multi_Image[i][7])	//user
		document.write("<td align=left  onMouseover=keytip(unescape('" + tipStr +"')); onMouseout='hideddrivetip()'>&nbsp;" + rp_special_char(strTruncate(Multi_Image[i][7], 10)) + "</td>");
	}
	

	if (Multi_Image[i][3] != '')
	{
		document.write("  <td><input type=button value='Set Boot' class=btnInGrayS onClick=set_boot(" + i + ");></td>");
		document.write("  <td><input type=button value='Delete' class=btnInGrayS onClick=delit(" + i + ");></td>");
	}
	else
	{
		document.write("  <td><input type=button value='Set Boot' class=btnInGrayS disabled onClick=set_boot(" + i + ");></td>");
		document.write("  <td><input type=button value='Delete' class=btnInGrayS disabled onClick=delit(" + i + ");></td>");
	}
	
	document.write("</tr>");
    }
	
	if (Panel_Info[4]==137) /* if dgs3200-24 */
	{
		if (i%2 == 0)
		{
			trClass = 'gx';
		}
		else
		{
			trClass = 'gw';
		}
		
		document.write('<tr class="' + trClass + '">');
		document.write('<td>&nbsp;Path Name</td>');
		document.write('<td colspan="5">');
		document.write('<input type="text" id="boxPathname" name="boxPathname" maxlength="254" class="flatL" style="width:95%;" />');
		document.write('</td>');
		document.write('<td>&nbsp;</td>');
		document.write('<td align="left">');
		document.write('<input type=button value="Set Boot" maxlength=15 class=btnInGrayS onClick="set_boot('+(-1)+')" />');
		document.write('</td>');
		document.write("  <td><input type=button value='Delete' class=btnInGrayS style='visibility:hidden;' onClick=delit(" + (-1) + ");></td>");
		document.write('</tr>');
	}
</script>
    </table>
    <P>
'*' Means Boot Up Firmware</P>
(Console) Means Firmware Update Through Serial Port (RS232)</P><P>
(Telnet) Means Firmware Update Through TELNET</P><P>
(SNMP) Means Firmware Update Through SNMP</P><P>
(WEB) Means Firmware Update Through WEB</P><P>
(SSH) Means Firmware Update Through SSH</P><P>
(SIM) Means Firmware Update Through Single IP Management</P>

