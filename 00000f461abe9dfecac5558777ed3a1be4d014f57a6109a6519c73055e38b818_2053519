function ckclick12()
{
	if(document.getElementsByName('TCPFlagChk')[0].checked)
	{
		document.getElementsByName('all_flat_bit')[0].disabled=false;
		document.getElementsByName('URG_Chk')[0].disabled=false;
		document.getElementsByName('ACK_Chk')[0].disabled=false;
		document.getElementsByName('PSH_Chk')[0].disabled=false;
		document.getElementsByName('RST_Chk')[0].disabled=false;
		document.getElementsByName('SYN_Chk')[0].disabled=false;
		document.getElementsByName('FIN_Chk')[0].disabled=false;
	}
	else
	{
		document.getElementsByName('all_flat_bit')[0].disabled=true;
		document.getElementsByName('URG_Chk')[0].disabled=true;
		document.getElementsByName('ACK_Chk')[0].disabled=true;
		document.getElementsByName('PSH_Chk')[0].disabled=true;
		document.getElementsByName('RST_Chk')[0].disabled=true;
		document.getElementsByName('SYN_Chk')[0].disabled=true;
		document.getElementsByName('FIN_Chk')[0].disabled=true;
	}
}

function ck_packet_content(index)
{
	var s = document.formTimeProfile;
	if(eval("s.E"+index+".checked") == true)
	{
		eval("s.E"+(index+1) +".disabled = false");
		eval("s.E"+(index+2) +".disabled = false");
	}
	else
	{
		eval("s.E"+(index+1) +".disabled = true");
		eval("s.E"+(index+2) +".disabled = true");
	}
}

function ck_cpu_packet_content(index)
{
	var i;
	var s = document.formTimeProfile;
	if (eval("s.C" + index + ".checked") == true)
	{
		eval("s.C"+(index+1) +".disabled = false");
		eval("s.C"+(index+2) +".disabled = false");
		eval("s.C"+(index+3) +".disabled = false");
		eval("s.C"+(index+4) +".disabled = false");
	}
	else
	{
		eval("s.C"+(index+1) +".disabled = true");
		eval("s.C"+(index+2) +".disabled = true");
		eval("s.C"+(index+3) +".disabled = true");
		eval("s.C"+(index+4) +".disabled = true");
	}
}

function check_tcp_flag()
{
	var f = document.formTimeProfile;

	if (f.tcp_flag.checked)
	{
		f.chkSrcTCPMask.disabled = false;
		f.chkDstTCPMask.disabled = false;
		f.TCPFlagChk.disabled = false;
	}
	else
	{
		f.chkSrcTCPMask.disabled = true;
		f.chkDstTCPMask.disabled = true;
		f.TCPFlagChk.disabled = true;
	}
}

function check_icmp()
{
	var f = document.formTimeProfile;

	if (f.icmpChk.checked)
	{
		f.icmp_TypeChk.disabled = false;
		f.icmp_CodeChk.disabled = false;
	}
	else
	{
		f.icmp_TypeChk.disabled = true;
		f.icmp_CodeChk.disabled = true;
	}
}

function check_igmp()
{
	var f = document.formTimeProfile;

	if (f.IGMPChk.checked)
	{
		f.IGMPTypeChk.disabled = false;
	}
	else
	{
		f.IGMPTypeChk.disabled = true;
	}
}

function check_all_flag_bit()
{
	var s = document.formTimeProfile;
	if (s.all_flat_bit.checked)
	{
		document.getElementsByName('URG_Chk')[0].checked=true;
		document.getElementsByName('ACK_Chk')[0].checked=true;
		document.getElementsByName('PSH_Chk')[0].checked=true;
		document.getElementsByName('RST_Chk')[0].checked=true;
		document.getElementsByName('SYN_Chk')[0].checked=true;
		document.getElementsByName('FIN_Chk')[0].checked=true;
	}
	else
	{
		document.getElementsByName('URG_Chk')[0].checked=false;
		document.getElementsByName('ACK_Chk')[0].checked=false;
		document.getElementsByName('PSH_Chk')[0].checked=false;
		document.getElementsByName('RST_Chk')[0].checked=false;
		document.getElementsByName('SYN_Chk')[0].checked=false;
		document.getElementsByName('FIN_Chk')[0].checked=false;
	}
}

function check_udp_flag()
{
	var f = document.formTimeProfile;

	if (f.udp_flag.checked)
	{
		f.chkSrcUDPMask.disabled = false;
		f.chkDstUDPMask.disabled = false;
	}
	else
	{
		f.chkSrcUDPMask.disabled = true;
		f.chkDstUDPMask.disabled = true;
	}
}

function check_proto_flag()
{
	var f = document.formTimeProfile;

	if (f.proto_flag.checked)
	{
		f.protocol_id.disabled = false;
		f.chk_user_mask_flag.disabled = false;
	}
	else
	{
		f.protocol_id.disabled = true;
		f.chk_user_mask_flag.disabled = true;
	}
}

