<script language="JavaScript" type="text/javascript">

var g_remote_win=null;

function set_focus_to_remote()
{
	if(g_remote_win!=null)
	{
		g_remote_win.focus();
	}
}

function open_remote(rh_url,rh_target)
{
	if(self.rh_win==null)
	{
		self.rh_win= new Array();
	}
	if(self.rh_win[rh_target]==null || self.rh_win[rh_target].closed)
	{
		self.rh_win[rh_target]=open(rh_url,rh_target);
	}
	g_remote_win=self.rh_win[rh_target];
	setTimeout("set_focus_to_remote()", 780);
}

function get_radio_checked()
{
	var i, chkid;
	for(i=0, chkid=0; i<f0.length; i++)
	{
		var elm=f0.elements[i];
		if(elm.type=="radio")
		{
			if(elm.checked)
			{
				return chkid;
			}
			chkid++;
		}
	}
	return -1;
}

function click_upload()
{
	var chkid=get_radio_checked();
	if(chkid==-1)
	{
		alert("Please select member first.");
		return false;
	}
	
	if(!inputcheck())
		return false;
		
	f0.SIM_Data.value=g_ms_info[chkid].ms_id;
	return true;
}

function click_upload_log()
{
	var i, chkid;
	var s="";
	for(i=0, chkid=0; i<f0.length; i++)
	{
		elm=f0.elements[i];
		if(elm.type=="checkbox")
		{
			if(elm.checked)
			{
				var ms_id=g_ms_info[chkid].ms_id;
				s+=(s.length==0 ? ms_id : (","+ms_id));
			}
			chkid++;
		}
	}
	
	if(s.length==0)
	{
		alert("Please select member first.");
		return false;
	}
	
	if(!inputcheck())
		return false;
		
	f0.SIM_Data.value=s;
	return true;
}

function click_download()
{
	var chkid=get_radio_checked();
	if(chkid==-1)
	{
		alert("Please select member first.");
		return false;
	}
	
	if(!inputcheck())
		return false;
		
	f0.SIM_Data.value=g_ms_info[chkid].ms_id;
	return true;
}

function click_download_firmware()
{
	var i, chkid;
	var s="";
	for(i=0, chkid=0; i<f0.length; i++)
	{
		elm=f0.elements[i];
		if(elm.type=="checkbox")
		{
			if(elm.checked)
			{
				var ms_id=g_ms_info[chkid].ms_id;
				s+=(s.length==0 ? ms_id : (","+ms_id));
			}
			chkid++;
		}
	}
	
	if(s.length==0)
	{
		alert("Please select member first.");
		return false;
	}
	
	if(!inputcheck())
		return false;
		
	f0.SIM_Data.value=s;
	return true;
}

function inputcheck()
{
	var i, ip;
	
	for(i=0; i<4; i++)
	{
		ip=eval("f0.SIM_TftpSrvIp"+i);
		
		if(isNaN(ip.value) || ip.value < 0 || ip.value > 255 || ip.value == "" || ip.value.search(" ") != -1)
		{
			alert("Invalid IP Address.");
			ip.select();
			ip.focus();
			return false
		}
		
		if(i == 0 && ip.value == 0)
		{
			alert("Invalid IP Address.");
			ip.select();
			ip.focus();
			return false
		}
	}
	
	if(f0.SIM_TftpFilename.value == "" || f0.SIM_TftpFilename.value.search(" ") != -1)
	{
		alert("Invalid Path \ Filename.")
		f0.SIM_TftpFilename.select();
		f0.SIM_TftpFilename.focus();
		return false;
	}
	
	return true;
}
function init()
{
<!-- RpDynamicDisplay RpItemCount=1 RpGetType=complex RpGetPtr=DD_SIM_Action_Upgrade -->
<!-- RpItemGroup -->
f0.btnDownload.onclick=click_download_firmware;
<!-- RpLastItemInGroup -->
<!-- RpLastItemInGroup -->

<!-- RpDynamicDisplay RpItemCount=1 RpGetType=complex RpGetPtr=DD_SIM_Action_CfgFile -->
<!-- RpItemGroup -->
f0.btnUpload.onclick=click_download_firmware;
f0.btnDownload.onclick=click_download_firmware;
<!-- RpLastItemInGroup -->
<!-- RpLastItemInGroup -->


<!-- RpDynamicDisplay RpItemCount=1 RpGetType=complex RpGetPtr=DD_SIM_Action_UpdLogFile -->
<!-- RpItemGroup -->
f0.btnUpload.onclick=click_upload_log;
<!-- RpLastItemInGroup -->
<!-- RpLastItemInGroup -->

<!-- RpDynamicDisplay RpItemCount=2 RpGetType=complex RpGetPtr=DD_SIM_System_Lock_Error -->
<!-- RpItemGroup -->
alert("Device Busy!");
<!-- RpLastItemInGroup -->
<!-- RpItemGroup -->
<!-- RpNamedDisplayText RpGetType=complex RpName="TFTP_In_Progress" RpGetPtr=NDT_SIM_TFTP_In_Progress --><!-- RpEnd -->
<!-- RpLastItemInGroup -->
<!-- RpLastItemInGroup -->
}

var f0=document.forms[0];
setTimeout("init()", 300);

</script>
