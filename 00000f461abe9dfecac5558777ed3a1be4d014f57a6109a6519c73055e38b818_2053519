<script language="Javascript">
    var d = document.snmpview;    
	function AddApply()
	{
		if(!checkTxtInput(d.viewname))
			return false;
		if(!CheckOIDValue(d.Oid))
			return false;
		d.isClear.value = 0;
		d.submit();
	}

	function CheckOIDValue(obj)
	{
		var ipSplit=obj.value.split('.');
		for(i=0; i<ipSplit.length; i++)
		{
			if (!checkNumber(ipSplit[i], 0, 4294967295))
			{
				alert("Invalid OID! ");
				obj.select();
				obj.focus();
				return false;
			}
		}
		return true;
	 }
	 function checkNumber(value, min_val, max_val)
	{
		var i, ch, len = value.length;

		for (i=0; i < len; i++)
		{
			ch = value.charAt(i);
			if (isNaN(ch))
			{
				return false;
			}
		}

		if(value == '' || value.search(" ") != -1 || isNaN(value) || value <  min_val || value > max_val )
		 	return false;
		return true;
	 }

	var snmp_view_count;

	function show_counter()
	{
		var str = "<b>Total Entries: "+ snmp_view_count+"</b>";
		document.getElementById('snmp_view_counter').innerHTML = str;
	}
	 
</script>
</div>
