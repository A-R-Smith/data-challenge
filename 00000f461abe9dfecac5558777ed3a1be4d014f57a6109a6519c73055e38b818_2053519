<script language="Javascript">
function onApply()
{
  
  location.href='52_ACLWizard.html';
}

function selectType(item)
{
	var status='';	
	if(item.selectedIndex==0)
	{
		status=true;
		
	}
	else
	{
		status=false;
	}
	
	for(var i=1;i<8;i++)
	{
	  var obj = MM_findObj('C'+i);	  
	  if(obj)	
	  {
	  	obj.disabled=status;
	  }
	}
	
}
</script>
