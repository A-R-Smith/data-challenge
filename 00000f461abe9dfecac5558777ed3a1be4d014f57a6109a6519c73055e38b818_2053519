    function FindByPort()
    { 
    	var reUrl = "48_MAC_Address_Table_table.html?0,3,"+document.MacTable.D1.value;
    	frames['ifrMac'].location.href=reUrl;
    }
    function ClearByPort()
    { 
    	var reUrl = "48_MAC_Address_Table_table.html?0,6,"+document.MacTable.D1.value;
    	frames['ifrMac'].location.href=reUrl;
    }
    function FindByVlan()
    { 
    	if(!checkTxtInput(document.MacTable.vlanName))
    		return;
    	var reUrl = "48_MAC_Address_Table_table.html?0,1,"+document.MacTable.vlanName.value;
    	frames['ifrMac'].location.href=reUrl;
    }
    function ClearByVlan()
    { 
        if(!checkTxtInput(document.MacTable.vlanName))
    		return;
    	var reUrl = "48_MAC_Address_Table_table.html?0,4,"+document.MacTable.vlanName.value;
    	frames['ifrMac'].location.href=reUrl;
    }
    function FindByMac()
    { 
   	 if(!MAC_Address_Check(document.MacTable.MacAddr))
		return;
    	var reUrl = "48_MAC_Address_Table_table.html?0,2,"+document.MacTable.MacAddr.value;
    	frames['ifrMac'].location.href=reUrl;
    }
    function FindAll()
    { 
    	var reUrl = "48_MAC_Address_Table_table.html?0,0,0"
    	frames['ifrMac'].location.href=reUrl;
    }
    function ClearAll()
    { 
    	var reUrl = "48_MAC_Address_Table_table.html?0,7,0"
    	frames['ifrMac'].location.href=reUrl;
    }
</script>
