							<div id="divPortList">
							<script language="Javascript">
								function showportpanel(unitid, portnum)
								{
									var panstr = "";
									var ports_per_line = (Port_Number.length > 20) ? Port_Number.length /2 : Port_Number.length;
									var ports_in_line;
									panstr += "<table id=mainGray2 class=mainGrayNormal cellpadding=0 cellspacing=0 style=\"width:100%;\"><tbody>";
									panstr += "<tr class=gh><td width=60>&nbsp; Port</td><td></td>";
									// first line ports
									for( i=1; i<=ports_per_line; i++)
									{
										if (i < 10)
										{
											panstr += "<td>&nbsp;0" + i + "</td>";
										}
										else
										{
											panstr += "<td>&nbsp;" + i + "</td>";
										}
									}
									panstr += "</tr>";
									panstr += "<tr class=gw><td></td><td></td>";
									for(i=1; i<=ports_per_line; i++)
										panstr += "<td><input type='checkbox' name='C'"+i+"' id=C"+i+" onclick='showPorts("+i+");'></td>";
									panstr += "</tr>";
									
									ports_in_line = portnum - ports_per_line;
									if(ports_in_line > 0) {
										panstr += "<tr class=gh><td>&nbsp; </td><td></td>";
										for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
											panstr += "<td>&nbsp;" + i + "</td>";
										for( i=0; i<ports_per_line - ports_in_line; i++)
										panstr += "<td></td>";
										panstr += "</tr>";
										panstr += "<tr class=gw><td>&nbsp; </td><td></td>";
										for( i=ports_per_line+1; i<=ports_per_line+ports_in_line; i++)
											panstr += "<td><input type='checkbox' name='C"+i+"' id=C"+i+" onclick='showPorts("+i+");'></td>";
										for( i=0; i<ports_per_line - ports_in_line; i++)
											panstr += "<td></td>";
										panstr += "</tr>";
									}
											
									panstr += "</tbody></table>";
									document.getElementById('divPortList').innerHTML = panstr;		
								}
								showportpanel(1, Port_Number.length);
							</script>
							</div>  <!--div id="divPortList"-->
					</td>
					</tr>
					<tr>
					<td colspan="2">
							<b>Ports&nbsp;&nbsp;</b>
							<input class=flatL type="text" name="boxPorts" id="boxPorts" readonly style='border:solid; border-width:1; border-color:rgb(241,245,251); background:rgb(241,245,251); width:auto;text-align:left; width:80%' onMouseOver="ddrivetip(this.value)"; onMouseOut="hideddrivetip()">
							<script language="javascript">
							document.getElementById("boxPorts").value = VLANTrunk[1];	//ports
							</script>
					</td>
