<link rel=stylesheet href="../jstab/jstab.css" type="text/css" />
<head>
    <script src="../form.js" type="text/javascript"></script>
    <script src="../checkperm.js" type="text/javascript"></script>
    <script src="../form1280.js" type="text/javascript"></script>
    <script src="../jstab/jstab.js" type="text/javascript"></script>
